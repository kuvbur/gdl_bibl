iam_t = str("%.0mm", reinforce_diam[j][i])					diam_inx[k] = diam_t + "." + class					k=k+1				endif			endif		next j	endifnext iparameters diam_inx=diam_inxif vardim1(diam_mat)<>vardim1(diam_inx) then 	dim diam_t[]	for i=1 to vardim1(diam_inx)		diam_t[i] = 0	next i	parameters diam_mat=diam_t, diam_cpen=diam_t, diam_rpen=diam_t, diam_hide = diam_tendifif vardim1(area_mat)<10 or vardim1(area_arr)<10 then 	dim area_mat_t[]	for i=1 to 10		area_mat_t[i] = 0	next i	parameters area_mat=area_mat_t, area_arr=area_mat_tendif