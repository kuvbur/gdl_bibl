if show_all_subpos then LOCK 'show_id','show_no_subposmarker', 'show_subpos'dim n_t[] : n_t[1] = 0for i=1 to vardim1(show_subpos_id)	n_t[i] = 0	for j=1 to vardim2(show_subpos_id)		if show_subpos_id[i][j]<>"" and show_subpos_id[i][j]<>" " then 			n_t[i] = 1			j = vardim2(show_subpos_id)		endif	next jnext iparameters n_id = n_t