===========if bEnableBoard & not(gs_stack_bottom) then	itemID = startItemID_Board	if bDoorWithSidelightSupport then		_bHasAnyBoardOn = (gs_board_inside | \						(gs_sidelight_left  & gs_sidelight_parapet_hgt_left > EPS  & gs_board_inside_sl_left) | \						(gs_sidelight_right & gs_sidelight_parapet_hgt_right > EPS & gs_board_inside_sl_right))	else		_bHasAnyBoardOn = (gs_board_inside)	endif	ui_listitem itemID, listFieldID, "", 							0,	"",								stBoard						: itemID = itemID + 1	if _bHasAnyBoardOn then		if not(isWindow) then			ui_listitem itemID, listFieldID, "gs_pen_2D", 				1,	"ui_list_contourPen_flat(1)",		stContourPen				: itemID = itemID + 1		endif		ui_listitem itemID, listFieldID, "gs_board_pen_sectcont", 		1,	"ui_list_contourPen_flat(1)",		stSectionPen				: itemID = itemID + 1		if gs_bFills then			ui_listitem itemID, listFieldID, "gs_board_fill", 			1,	"ui_list_fillType_flat(1)",			stSectionFillType			: itemID = itemID + 1			ui_listitem itemID, listFieldID, "gs_board_pen_fg", 		1,	"ui_list_fillPen_flat(1)",			stSectionFillPen			: itemID = itemID + 1			ui_listitem itemID, listFieldID, "gs_board_pen_bg", 		1,	"ui_list_fillBgPen_flat(1)",			stSectionFillBackgroundPen	: itemID = itemID + 1		else			itemID = itemID + 3		endif	else		itemID = itemID + 4	endifendif! =============================================================================! Ventilation Profile! =============================================================================if bEnableVentilationProfile then	itemID = startItemID_Ventilation	ui_listitem itemID, listFieldID, "", 						0,	"",								stVentilationProfile		: itemID = itemID + 1	if bVentilationProfile then		ui_listitem itemID, listFieldID, "gs_ventProfile_fill", 	1,	"ui_list_fillType_flat(1)",			stSectionFillType			: itemID = itemID + 1		ui_listitem itemID, listFieldID, "gs_ventProfile_pen_fg", 	1,	"ui_list_fillPen_flat(1)",			stSectionFillPen			: itemID = itemID + 1		ui_listitem itemID, listFieldID, "gs_ventProfile_pen_bg", 	1,	"ui_list_fillBgPen_flat(1)",			stSectionFillBackgroundPen	: itemID = itemID + 1	endifendif! =============================================================================! Sunshade! =============================================================================if bEnableShutter then	itemID = startItemID_Sunshade	ui_listitem itemID, listFieldID, "", 								0,	"",							stSunshade					: itemID = itemID + 1	if bSunshade then		ui_listitem itemID, listFieldID, "gs_sunShade_lineType", 			1,	"ui_list_contLineType_flat(1)",	stLineType					: itemID = itemID + 1		ui_listitem itemID, listFieldID, "gs_sunShade_bShowOpline", 		1,	"",							stShowOpeningLines			: itemID = itemID + 1		ui_listitem itemID, listFieldID, "gs_sunShade_cutLinesPen", 		1,	"ui_list_contourPen_flat(1)",	stCutLinePen 				: itemID = itemID + 1		ui_listitem itemID, listFieldID, "gs_sunShade_unCutLinesPen", 		1,	"ui_list_contourPen_flat(1)",	st3DSectUncutLinePen		: itemID = itemID + 1		if gs_bFills then			ui_listitem itemID, listFieldID, "gs_sunShade_cutFillType", 	1,	"ui_list_fillType_flat(1)",		stCutFillType				: itemID = itemID + 1			ui_listitem itemID, listFieldID, "gs_sunShade_cutFillPen", 		1,	"ui_list_fillPen_flat(1)",		stCutFillPen				: itemID = itemID + 1			ui_listitem itemID, listFieldID, "gs_sunShade_cutFillBackPen", 	1,	"ui_list_fillBgPen_flat(1)",		stCutFillBackgroundPen		: itemID = itemID + 1		else			itemID = itemID + 3		endif		if (gs_sunShade_iType = SUNSHADE_ROLLING | gs_sunShade_iType = SUNSHADE_EXTVENETIAN)	then			ui_listitem itemID, listFieldID, "gs_sunShade_insFillType", 		1,	"ui_list_fillType_flat(1)",		stInsulationFillType		: itemID = itemID + 1			ui_listitem itemID, listFieldID, "gs_sunShade_insFillPen", 			1,	"ui_list_fillPen_flat(1)",		stInsulationFillPen			: itemID = itemID + 1			ui_listitem itemID, listFieldID, "gs_sunShade_insFillBackPen", 		1,	"ui_list_fillBgPen_flat(1)",		stInsulationBackgroundPen	: itemID = itemID + 1		else			itemID = itemID + 3		endif	endifendif! =============================================================================! Opening Head Symbol! =============================================================================itemID = startItemID_OpeningHeadSymbif isEmptyOpening then	ui_listitem itemID, listFieldID, "gs_head_symbol", 			0,	"",	""			: itemID = itemID + 1	if gs_head_symbol then		ui_listitem itemID, listFieldID, "gs_head_symbol_line", 	1,	"ui_list_contLineType_flat(1)",	""			: itemID = itemID + 1		ui_listitem itemID, listFieldID, "gs_head_symbol_pen", 		1,	"ui_list_contourPen_flat(1)",	""			: itemID = itemID + 1	endifendifitemID = startItemID_Opening2DArcif isEmptyOpening & (iWindowShape <> SHAPE_RECTANGULAR) then	ui_listitem itemID, listFieldID, "gs_arc_2D", 			0,	"",	""			: itemID = itemID + 1	if gs_arc_2D then		ui_listitem itemID, listFieldID, "gs_brickw_lt", 	1,	"ui_list_contLineType_flat(1)",	""		: itemID = itemID + 1	endifendif