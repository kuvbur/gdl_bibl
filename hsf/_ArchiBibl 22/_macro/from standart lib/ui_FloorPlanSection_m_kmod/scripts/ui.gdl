utFillPen				= `Перо Штриховки Сечения Коробки`stFrameCutFillBackgroundPen		= `Перо Фона Штриховки Сечения Коробки`stLeafCutFillType				= `Тип Штриховки Сечения Полотна`stLeafCutFillPen				= `Перо Штриховки Сечения Полотна`stLeafCutFillBackgroundPen		= `Перо Фона Штриховки Сечения Полотна`stLeafSashCutFillType			= `Тип Штриховки Сечения Створки и Полотна`stLeafSashCutFillPen			= `Перо Штриховки Сечения Створки и Полотна`stLeafSashCutFillBackgroundPen	= `Перо Фона Штриховки Сечения Створки и Полотна`stGlassCutFillType				= `Тип Штриховки Сечения Остекления`stGlassCutFillPen				= `Перо Штриховки Сечения Остекления`stGlassCutFillBackgroundPen		= `Перо Фона Штриховки Сечения Остекления`stWindow			 		= `Окно`stCutLinePen				= `Перо Линии Сечения`stUncutLinePen				= `Перо Линии Видимого Контура`st3DSectUncutLinePen		= `Перо Линий Видимого Контура в 3D/Сечении`stSashCutFillType			= `Тип Штриховки Сечения Створки`stSashCutFillPen			= `Перо Штриховки Сечения Створки`stSashCutFillBackgroundPen	= `Перо Фона Штриховки Сечения Створки`stOpeningLinesOverride	= `Замена Линий Открывания`stLineTypeOnDoorLeaf	= `Тип Линии Полотна Двери`if not(bDisable2DOverride) & not(bDisable3DOverride) then	stPen					= `Перо в 2D/Сечении`	stPenOnDoorLeaf			= `Перо Полотна Двери в 2D/Сечении`else	if not(bDisable3DOverride) then		stPen					= `Перо Сечения`		stPenOnDoorLeaf			= `Перо Полотна Двери в Сечении`	endifendifstPenOnSidelightTransom	= `Перо Сечения Боковой Панели/Фрамуги`stWallClosure		= `Замыкание Слоев`stContourLineType	= `Тип Линии Контура`stContourPen		= `Перо Контура`stFillType			= `Тип Штриховки`stFillPen			= `Перо Штриховки`stFillBackgroundPen	= `Перо Фона Штриховки`stWallInset					= `Подоконная Ниша`stShowHiddenParapetOn2D		= `Показ Подоконной Ниши в 2D`stHiddenParapetLineType		= `Тип Линии Невидимого Контура Ниши`stMasonryArch				= `Декоративная Перемычка`stSectionPen				= `Перо Сечения`stSectionFillType			= `Тип Штриховки Сечения`stSectionFillPen			= `Перо Штриховки Сечения`stSectionFillBackgroundPen	= `Перо Фона Штриховки Сечения`stThreshold	= `Порог`stLineType	= `Тип Линии`stCasing							= `Наличник`stCutLinePenOnBothSides				= `Перо Линии Сечения с Обеих Сторон`stOutsideSectionFillType			= `Тип Штриховки Сечения Снаружи`stOutsideSectionFillPen				= `Перо Штриховки Сечения Снаружи`stOutsideSectionFillBackgroundPen	= `Перо Фона Штриховки Сечения Снаружи`stInsideSectionFillType				= `Тип Штриховки Сечения Внутри`stInsideSectionFillPen				= `Перо Штриховки Сечения Внутри`stInsideSectionFillBackgroundPen	= `Перо Фона Штриховки Сечения Внутри`stSill		= `Отлив`stSectionInsulationContourPen			= `Перо Контура Сечения Изоляции`stSectionInsulationFillType				= `Тип Штриховки Сечения Изоляции`stSectionInsulationFillPen				= `Перо Штриховки Сечения Изоляции`stSectionInsulationFillBackgroundPen	= `Перо Фона Штриховки Сечения Изоляции`stBoard		= `Доска`stVentilationProfile	= `Приточный Клапан`stSunshade					= `Ставни`stShowOpeningLines			= `Показ Линий Открывания`stCutFillType				= `Тип Штриховки Сечения`stCutFillPen				= `Перо Штриховки Сечения`stCutFillBackgroundPen		= `Перо Фона Штриховки Сечения`stInsulationFillType		= `Тип Штриховки Изоляции`stInsulationFillPen			= `Перо Штриховки Изоляции`stInsulationBackgroundPen	= `Перо Фона Изоляции`startItemID_IndependParams	= 1startItemID_DemolitionNew	= 90startItemID_WallContour		= 100startItemID_OpeningGanging	= 150startItemID_UseFills		= 160startItemID_FrameSash		= 200startItemID_OpeningLines	= 300startItemID_WallClosure		= 400startItemID_WallInset		= 500startItemID_MasonryArch		= 600startItemID_Threshold		= 700startItemID_Casing			= 800startItemID_Sill			= 900startItemID_Board			= 1000startItemID_Ventilation		= 1100startItemID_Sunshade		= 1200startItemID_OpeningHeadSymb = 1300startItemID_Opening2DArc	= 1310! =============================================================================! Built-In Floor Plan and Section! =============================================================================pageStartY = 0ui_listfield listFieldID, 0, pageStartY, 444, 265 itemID = startItemID_IndependParamsui_listitem itemID, listFieldID, "iDWDet2D", 			0,	"",			st2DDetailLevel			: itemID = itemID + 1! itemID = 2 reserved to iRevealOnSymbol, see in Functions macrositemID = itemID + 1if iWindowType = 43 then	! WIN_MultisashVertical	ui_listitem itemID, listFieldID, "gs_ShowSashOnSymbol", 			0,	"",	""		: itemID = itemID + 1endif! =============================================================================! Demolition / New construction SWE only! =============================================================================itemID = startItemID_DemolitionNewif gs_bShowDemolitionPar then	ui_listitem itemID, listFieldID,	"iTypeDemolitionFill",	0,	"",	""		: itemID = itemID + 1	if	iTypeDemolitionFill = RENOVATION_FILL_NEW |\		iTypeDemolitionFill = RENOVATION_FILL_NEW_FULL then		ui_listitem itemID, listFieldID,	"gs_new_symb_fill",		1,	"ui_list_fillType_flat(1)",	""		: itemID = itemID + 1		ui_listitem itemID, listFieldID,	"gs_new_symb_pen_Fg",	1,	"ui_list_fillPen_flat(1)",	""		: itemID = itemID + 1		ui_listitem itemID, listFieldID,	"gs_new_symb_pen_bg",	1,	"ui_list_fillBgPen_flat(1)",	""		: itemID = itemID + 1		ui_listitem itemID, listFieldID,	"gs_new_symb_width_r",	1,	"",	""		: itemID = itemID + 1		ui_listitem itemID, listFieldID,	"gs_new_symb_width_l",	1,	"",	""		: itemID = itemID + 1	else		itemID = itemID + 5	endif	if iTypeDemolitionFill = RENOVATION_FILL_DEMOLISHED & bShowDemolitionLineType then		ui_listitem itemID, listFieldID,	"gs_Demolition_linetype",				1,	"ui_list_pencolor_flat(1)",			`Тип Линии`		endif	itemID = itemID + 1endif! =============================================================================! Wall Contour Lines! =============================================================================bShowSidelightAttributes	= (not(isWindow) & bDoorWithSidelightSupport &	((gs_sidelight_left & gs_sidelight_parapet_hgt_left > EPS) |\																			 (gs_sidelight_right & gs_sidelight_parapet_hgt_right > EPS)))itemID = startItemID_WallContourif bShowSidelightAttributes then	ui_listitem itemID, listFieldID, "", 				0,	"",							stWallContourLines			: itemID = itemID + 1	ui_listitem itemID, listFieldID, "gs_wcont_line", 	1,	"",							stDoorWallContourLines		: itemID = itemID + 1	if iWallContourLine <> WALL_CONTOUR_OFF then		if iWallContourLine = WALL_CONTOUR_OUTSIDE | iWallContourLine = WALL_CONTOUR_BOTHSIDES then			ui_listitem itemID, listFieldID, "gs_wcont_line_out",			1,	"ui_list_visibleLineOut_flat(1)",	stVisibleLineTypeOutside		endif		itemID = itemID + 1		if iWallContourLine = WALL_CONTOUR_INSIDE | iWallContourLine = WALL_CONTOUR_BOTHSIDES then			ui_listitem itemID, listFieldID, "gs_wcont_line_in",			1,	"ui_list_visibleLineIn_flat(1)",		stVisibleLineTypeInside		endif		itemID = itemID + 1		if iWallContourLine = WALL_CONTOUR_OUTSIDE | iWallContourLine = WALL_CONTOUR_BOTHSIDES then			ui_listitem itemID, listFieldID, "gs_wcont_hidden_line_out",	1,	"ui_list_hiddenLineOut_flat(1)",		stHiddenLineTypeOutside			endif		itemID = itemID + 1		if iWallContourLine = WALL_CONTOUR_INSIDE | iWallContourLine = WALL_CONTOUR_BOTHSIDES then			ui_listitem itemID, listFieldID, "gs_wcont_hidden_line_in",	1,	"ui_list_hiddenLineIn_flat(1)",		stHiddenLineTypeInside		endif		itemID = itemID + 1	else		itemID = itemID + 4	endif	ui_listitem itemID, listFieldID, "gs_wcont_line_sl", 				1,	"",								stSidelightWallContourLines	: itemID = itemID + 1	if iWallContourLineSl <> WALL_CONTOUR_OFF then		if iWallContourLineSl = WALL_CONTOUR_OUTSIDE | iWallContourLineSl = WALL_CONTOUR_BOTHSIDES then			ui_listitem itemID, listFieldID, "gs_wcont_line_sl_out",		1,	"ui_list_visibleLineOut_flat(1)",	stVisibleLineTypeOutside		endif		itemID = itemID + 1		if iWallContourLineSl = WALL_CONTOUR_INSIDE | iWallContourLineSl = WALL_CONTOUR_BOTHSIDES then			ui_listitem itemID, listFieldID, "gs_wcont_line_sl_in",			1,	"ui_list_visibleLineIn_flat(1)",		stVisibleLineTypeInside		endif		itemID = itemID + 1		if iWallContourLineSl = WALL_CONTOUR_OUTSIDE | iWallContourLineSl = WALL_CONTOUR_BOTHSIDES then			ui_listitem itemID, listFieldID, "gs_wcont_hidden_line_sl_out",	1,	"ui_list_hiddenLineOut_flat(1)",		stHiddenLineTypeOutside			endif		itemID = itemID + 1		if iWallContourLineSl = WALL_CONTOUR_INSIDE | iWallContourLineSl = WALL_CONTOUR_BOTHSIDES then			ui_listitem itemID, listFieldID, "gs_wcont_hidden_line_sl_in",	1,	"ui_list_hiddenLineIn_flat(1)",		stHiddenLineTypeInside		endif		itemID = itemID + 1	else		itemID = itemID + 4	endif	if iWallContourLine <> WALL_CONTOUR_OFF | iWallContourLineSl <> WALL_CONTOUR_OFF then		ui_listitem itemID, listFieldID, "gs_wcont_line_pen",			1,	"ui_list_contourPen_flat(1)",		stLinePen					: itemID = itemID + 1	else		itemID = itemID + 1	endifelse	ui_listitem itemID, listFieldID, "gs_wcont_line", 				0,	"",							stWallContourLines		: itemID = itemID + 1	if iWallContourLine <> WALL_CONTOUR_OFF then		if not(bEnableSill | bEnableThreshold) then			if not(bEnableThreshold | bEnableBoard) then				stVisibleLineTypeOutside = stLineTypeOutside			endif		endif		if not(bEnableThreshold | bEnableBoard) then			if not(bEnableSill | bEnableThreshold) then				stVisibleLineTypeInside = stLineTypeInside			endif		endif		if iWallContourLine = WALL_CONTOUR_OUTSIDE | iWallContourLine = WALL_CONTOUR_BOTHSIDES then			ui_listitem itemID, listFieldID, "gs_wcont_line_out",			1,	"ui_list_visibleLineOut_flat(1)",	stVisibleLineTypeOutside		endif		itemID = itemID + 1		if iWallContourLine = WALL_CONTOUR_INSIDE | iWallContourLine = WALL_CONTOUR_BOTHSIDES then			ui_listitem itemID, listFieldID, "gs_wcont_line_in",			1,	"ui_list_visibleLineIn_flat(1)",		stVisibleLineTypeInside			endif		itemID = itemID + 1		if (bEnableSill | bEnableThreshold) & (iWallContourLine = WALL_CONTOUR_OUTSIDE | iWallContourLine = WALL_CONTOUR_BOTHSIDES) then			ui_listitem itemID, listFieldID, "gs_wcont_hidden_line_out",	1,	"ui_list_hiddenLineOut_flat(1)",	stHiddenLineTypeOutside		: itemID = itemID + 1		else			itemID = itemID + 1		endif		if (bEnableThreshold | bEnableBoard) & (iWallContourLine = WALL_CONTOUR_INSIDE | iWallContourLine = WALL_CONTOUR_BOTHSIDES) then			ui_listitem itemID, listFieldID, "gs_wcont_hidden_line_in",	1,	"ui_list_hiddenLineIn_flat(1)",		stHiddenLineTypeInside		: itemID = itemID + 1		else			itemID = itemID + 1		endif		ui_listitem itemID, listFieldID, "gs_wcont_line_pen",			1,	"ui_list_contourPen_flat(1)",		stLinePen					: itemID = itemID + 1	else		itemID = itemID + 5	endif	itemID = itemID + 6endif! =============================================================================! Ganging for Empty Openings! =============================================================================if isEmptyOpening then	itemID = startItemID_OpeningGanging	ui_listitem itemID, listFieldID, "", 					0,	"",						`Стыковка`					: itemID = itemID + 1	ui_listitem itemID, listFieldID, "gs_stack_left_contour",		1,	"ui_list_wallContSide1_flat(1)",		""			: itemID = itemID + 1	ui_listitem itemID, listFieldID, "gs_stack_right_contour",		1,	"ui_list_wallContSide2_flat(1)",		""			: itemID = itemID + 1endif! =============================================================================! Use Fills group! =============================================================================itemID = startItemID_UseFillsui_listitem itemID, listFieldID, "gs_bFills", 					0,	"",							stUseFills							: itemID = itemID + 1if bShowSidelightAttributes then	if gs_bFills then		ui_listitem itemID, listFieldID, "gs_wallhole_fill",		1,	"ui_list_fillType_flat(1)",		stDoorPlanSymbolFillType			: itemID = itemID + 1		ui_listitem itemID, listFieldID, "gs_wallhole_pen_fg",		1,	"ui_list_fillPen_flat(1)",		stDoorPlanSymbolFillPen				: itemI