ui_dialog `Kuvbur_АС Пол и потолки`, 600, 410UI_PAGE 1sx = 5sy = 60sxe = 250symax = 5sx1 = 0sx2 = 180sy = sy + 5: UI_OUTFIELD	`Использовать этот вариант настройки`,	sx+20,	sy,	sxe,	20			  UI_INFIELD	"is_use_mview",	sx,	sy-3, 20,	20if is_use_mview thensy = sy + 20: UI_OUTFIELD	`Использовать этот вид для настройки`,	sx+20,	sy,	sxe,	20			  UI_INFIELD	"is_cfg_mview",	sx,	sy-3, 20,	20endifsy = sy + 40: UI_INFIELD	"f_name",	sx,	sy-3, 120,	20sy = sy + 25: UI_OUTFIELD	"Загрузить из файла", sx+20,sy, 120,21UI_INFIELD	"is_load",	sx,	sy-3, 20,	20sy = sy + 25UI_OUTFIELD	"Сохранить в файл", sx+20,sy, 120,21UI_INFIELD	"is_save",	sx,	sy-3, 20,	20sy = sy + 20UI_OUTFIELD	f_info + ".",	sx,	sy,	sxe,	20UI_OUTFIELD	f_info + ".",	sx,	sy,	sxe,	20sy = 250sy = sy + 20: UI_OUTFIELD	`Отображать штриховку пола/потолка`,	sx+20,	sy,	sxe,	20			  UI_INFIELD	"show_fill",	sx,	sy-3, 20,	20sy = sy + 20: UI_OUTFIELD	`Отображать маркировку полов`,	sx+20,	sy,	sxe,	20			  UI_INFIELD	"show_pol",	sx,	sy-3, 20,	20sy = sy + 20: UI_OUTFIELD	`Отображать маркировку потолков`,	sx+20,	sy,	sxe,	20			  UI_INFIELD	"show_pot",	sx,	sy-3, 20,	20sy = sy + 20: UI_OUTFIELD	`Отображать тип потолков`,	sx+20,	sy,	sxe,	20			  UI_INFIELD	"show_type_pot",	sx,	sy-3, 20,	20sy = sy + 20: UI_OUTFIELD	`Отображать высоту помещений`,	sx+20,	sy,	sxe,	20			  UI_INFIELD	"show_h_pot",	sx,	sy-3, 20,	20sy = sy + 20: UI_OUTFIELD	`Отображать номера помещений`,	sx+20,	sy,	sxe,	20			  UI_INFIELD	"show_num",	sx,	sy-3, 20,	20sy = sy + 20: UI_OUTFIELD	`Отображать отделочные слои`,	sx+20,	sy,	sxe,	20			  UI_INFIELD	"show_otd",	sx,	sy-3, 20,	20sy = sy + 20: UI_OUTFIELD	`Отображать несущие слои`,	sx+20,	sy,	sxe,	20			  UI_INFIELD	"show_nes",	sx,	sy-3, 20,	20! go to next-previous page buttons			ui_button ui_prev, " << ", sx1,symax, 32,21, gs_ui_current_page - 1			ui_button ui_next, " >> ", sx2,symax, 32,21, gs_ui_current_page + 1if is_use_mview and is_cfg_mview then	for i=2 to max_tip_p+1			id = i + max_tip_p + 20 - 1			page_name = "Тип " + tip_p[i-1] + " ("+ str("%.0", i-1) + ")"			UI_PAGE i, -1, page_name			sx = 0			sy = symax			sxe = 250			UI_OUTFIELD	page_name,	sx+40,	sy+5,	140,	20			sy = sy + 30: UI_OUTFIELD	`Имя типа пола`,	sx+5,	sy,	80,	15						  UI_INFIELD{3}	tip_p[i-1],	sx+90,	sy-4, 100,	20			sy = sy + 30: UI_OUTFIELD	`Кол-во слоёв (<`+ str("%.0", max_sloi-1)+")",	sx+5,	sy,	80,	20						  UI_INFIELD{3}	n_sloi[i-1],	sx+90,	sy-3, 30,	20			sx = 250			UI_OUTFIELD	`Герметичен?`,	sx-50,	sy-25,	70,	15						  UI_INFIELD{3}	germ_pot[i-1],	sx+30,	sy-28, 20,	20			UI_OUTFIELD	`Плинтус`,	sx-90,	sy,	50,	15						  UI_INFIELD{3}	tip_p_plint[i-1],	sx-40,	sy-3, 80,	20			UI_BUTTON UI_FUNCTION, "Скопировать", sx+60,	sy-59, 90,20, id			UI_INFIELD{3}	copy_from,	sx+155,	sy-59, 180,	20			UI_OUTFIELD	`Штриховка`,	sx+65,	sy-25,	60,	15						  UI_INFIELD{3}	fill_tip[i-1],	sx+155,	sy-28, 95,	20			UI_INFIELD{3}	fill_pen_tip[i-1],	sx+155+97,	sy-28, 40,	20			UI_INFIELD{3}	fill_bac_tip[i-1],	sx+155+99+40,	sy-28, 40,	20			UI_OUTFIELD	`Покрытие`,	sx+65,	sy,	56,	15						  UI_INFIELD{3}	mat_tip[i-1],	sx+155,	sy-3, 180,	20			if n_sloi[i-1]>0 then				for j = 1 to min(n_sloi[i-1]+1,10)					sy = sy + 25					sx = 0					if j = n_sloi[i-1]+1 then 						j = 10						UI_OUTFIELD	`Основа`,	sx+20,	sy,	48,	15					else						UI_OUTFIELD	`Слой `+ str("%.0", j),	sx+20,	sy,	50,	15					endif					sx = 50					UI_OUTFIELD	`Состав`,	sx+20,	sy,	40,	15					UI_INFIELD{3}	s_sloi[i-1][j],	sx+60,	sy-3, 200,	20					sx = sx + 250					UI_OUTFIELD	`t=`,	sx+20,	sy,	18,	15					UI_INFIELD{3}	t_sloi[i-1][j],	sx+40,	sy-3, 40,	20					sx = sx + 50					UI_INFIELD{3}	t_sloi_min[i-1][j],	sx+40,	sy-3, 40,	20					if j <> n_sloi[i-1]+1 then						sx = sx + 80						UI_OUTFIELD	`Несущий`,	sx+20,	sy,	50,	15						UI_INFIELD{3}	v_sloi[i-1][j],	sx+75,	sy-3, 20,	20					endif				next j				sy = sy + 30				sx = 290				UI_OUTFIELD	`Всего `+str("%.0mm", h_tip[i-1])+' мм',	sx+20,	sy,	80,	20				UI_OUTFIELD	`Примечание`,	10,	382,80,	20				UI_INFIELD{3} comment[i-1],	10+80,380, 580,20			endif			! go to next-previous page buttons			ui_button ui_prev, " << ", sx1,symax, 32,21, gs_ui_current_page - 1			ui_button ui_next, " >> ", sx2,symax, 32,21, gs_ui_current_page + 1			UI_BUTTON UI_FUNCTION, "На главную", sx2+40,symax, 80,21,max_tip_p*3	next iendifif GLOB_UI_BUTTON_ID = max_tip_p*3  then	UI_CURRENT_PAGE 1	endif