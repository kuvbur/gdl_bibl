parameters subpos = subposFM_ProductName = "Устройство водосточной системы D"+str("%.0mm", gs_gutter_dd2)PARAMETERS FM_ProductName = FM_ProductNameparameters FM_ObjectWeightUnit = "м"parameters komment = "м"parameters kol_vo_spec = gs_gutter_ds_lgth! -------------------------------------------------------------------------------! Hide all paremeterlist! -------------------------------------------------------------------------------hideparameter all "A", "B", "ZZYZX"parameters gs_gutter_ds_h_positioning = HANGER_UNIFORMVALUES "gs_wall_ang" RANGE (-90,90)values "gs_gutter_ds_ang" range (-90, 90)! ------------------------------------------------------------------------------! UI parameters! ------------------------------------------------------------------------------if GLOB_UI_BUTTON_ID < 0 then	parameters gs_ui_current_page = -GLOB_UI_BUTTON_IDendif! -------------------------------------------------------------------------------! Hanger Positioning! -------------------------------------------------------------------------------values{2} "gs_gutter_ds_h_positioning",	HANGER_CUSTOM,	stHangerPositioning[1],										HANGER_UNIFORM, stHangerPositioning[2]! ------------------------------------------------------------------------------! Profile! ------------------------------------------------------------------------------if LibraryLangCode = "USA" then	values "gs_gutter_ds_profile" stGutterDsProfiles	values "gs_gutter_ds_profile_num" PROFILE_ROUND, PROFILE_RECTANGULAR, PROFILE_CORRUGATED	if gs_gutter_ds_profile = stGutterDsProfiles[1] OR gs_gutter_ds_profile = stGutterDsProfiles[2] OR gs_gutter_ds_profile = stGutterDsProfiles[11] THEN		parameters gs_gutter_ds_profile_num = PROFILE_ROUND	endif	if gs_gutter_ds_profile = stGutterDsProfiles[3] OR gs_gutter_ds_profile = stGutterDsProfiles[4] OR gs_gutter_ds_profile = stGutterDsProfiles[5] OR gs_gutter_ds_profile = stGutterDsProfiles[6] OR gs_gutter_ds_profile = stGutterDsProfiles[12] THEN		parameters gs_gutter_ds_profile_num = PROFILE_RECTANGULAR	endif	if gs_gutter_ds_profile = stGutterDsProfiles[7] OR gs_gutter_ds_profile = stGutterDsProfiles[7] OR gs_gutter_ds_profile = stGutterDsProfiles[8] OR gs_gutter_ds_profile = stGutterDsProfiles[9] OR gs_gutter_ds_profile = stGutterDsProfiles[13] THEN		parameters gs_gutter_ds_profile_num = PROFILE_CORRUGATED	endifelse	values "gs_gutter_ds_profile" `Круглый`,`Прямоугольный`,`Закругленный`	values "gs_gutter_ds_profile_num" PROFILE_ROUND, PROFILE_RECTANGULAR, PROFILE_CORRUGATED	if GLOB_MODPAR_NAME = "gs_gutter_ds_profile" then		gs_gutter_ds_profile_num = PROFILE_RECTANGULAR		if gs_gutter_ds_profile = `Круглый`				then gs_gutter_ds_profile_num = PROFILE_ROUND		if gs_gutter_ds_profile = `Закругленный`			then gs_gutter_ds_profile_num = PROFILE_CORRUGATED		parameters gs_gutter_ds_profile_num = gs_gutter_ds_profile_num	else		gs_gutter_ds_profile = `Прямоугольный`		if gs_gutter_ds_profile_num = PROFILE_ROUND			then gs_gutter_ds_profile = `Круглый`		if gs_gutter_ds_profile_num = PROFILE_CORRUGATED	then gs_gutter_ds_profile = `Закругленный`		parameters gs_gutter_ds_profile = gs_gutter_ds_profile	endifendifVALUES "gs_gutter_aa" RANGE (0,)VALUES "gs_gutter_aa2" RANGE (0,)VALUES "gs_gutter_bb" RANGE (0,)VALUES "gs_gutter_bb2" RANGE (0,)VALUES "gs_gutter_dd" RANGE (0,)VALUES "gs_gutter_dd2" RANGE (0,)if GLOB_MODPAR_NAME = "gs_gutter_dd" or GLOB_MODPAR_NAME = "gs_gutter_ds_profile" then	gs_gutter_dd2 = gs_gutter_dd*2	parameters 	gs_gutter_dd2 = gs_gutter_dd2else	gs_gutter_dd = gs_gutter_dd2/2	parameters 	gs_gutter_dd = gs_gutter_ddendifif GLOB_MODPAR_NAME = "gs_gutter_aa" or GLOB_MODPAR_NAME = "gs_gutter_bb" or GLOB_MODPAR_NAME = "gs_gutter_ds_profile" then	gs_gutter_aa2 = 2 * gs_gutter_aa	gs_gutter_bb2 = 2 * gs_gutter_bb	parameters gs_gutter_aa2 = gs_gutter_aa2,	gs_gutter_bb2 = gs_gutter_bb2else	gs_gutter_aa = gs_gutter_aa2 / 2	gs_gutter_bb = gs_gutter_bb2 / 2	parameters gs_gutter_aa = gs_gutter_aa,	gs_gutter_bb = gs_gutter_bbendifif gs_gutter_ds_profile_num = 1 then	! Round	HIDEPARAMETER "gs_gutter_aa2"	HIDEPARAMETER "gs_gutter_bb2"else	HIDEPARAMETER "gs_gutter_dd2"endif! -----------------------------------------------------------------------------! A, B! -----------------------------------------------------------------------------if gs_gutter_ds_profile_num = 1 then	!round	A = gs_gutter_dd2	Bhalf = gs_gutter_ddelse	A = gs_gutter_aa2	Bhalf = gs_gutter_bbendifB = 2 * Bhalfparameters	A = A,			B = Block "A", "B"values "dis_wall" range [Bhalf, )! ------------------------------------------------------------------------------! ZZYZX! ------------------------------------------------------------------------------minh_elbow = Bminh_downspout = 0minh_offset = 2 * Bminh_HL = 0.05values "ZZYZX" range [gs_elbow * zzyzx_elbow + minh_downspout + gs_downspout_offset * minh_offset + gs_head_leader * minh_HL, )if GLOB_MODPAR_NAME = "ZZYZX" then	gs_gutter_ds_lgth = ZZYZX - gs_elbow * zzyzx_elbow - gs_downspout_offset * DsOffsetLength - gs_head_leader * HL_Height	parameters gs_gutter_ds_lgth = gs_gutter_ds_lgthelse	ZZYZX = gs_elbow * zzyzx_elbow + gs_gutter_ds_lgth + gs_downspout_offset * DsOffsetLength + gs_head_leader * HL_Height	parameters ZZYZX = ZZYZXendif! ------------------------------------------------------------------------------! Elbow! ------------------------------------------------------------------------------zzyzx_elbow = gs_gutter_elb_u_lgth + gs_gutter_elb_l_lgth * cos(gs_gutter_elb_l_ang) + Bhalf * sin(gs_gutter_elb_l_ang)parameters zzyzx_elbow = zzyzx_elbowif GLOB_MODPAR_NAME = "gs_gutter_elb_hor_overhang" then	gs_gutter_elb_l_lgth = (gs_gutter_elb_hor_overhang + Bhalf * cos(gs_gutter_elb_l_ang)) / sin(gs_gutter_elb_l_ang)	gs_gutter_ds_lgth = zzyzx - zzyzx_elbow - gs_downspout_offset * DsOffsetLength - gs_head_leader * HL_Height	parameters gs_gutter_elb_l_lgth = gs_gutter_elb_l_lgth, gs_gutter_ds_lgth = gs_gutter_ds_lgthelse	gs_gutter_elb_hor_overhang = gs_gutter_elb_l_lgth * sin(gs_gutter_elb_l_ang) - Bhalf * cos(gs_gutter_elb_l_ang)	parameters gs_gutter_elb_hor_overhang = gs_gutter_elb_hor_overhangendifVALUES "gs_gutter_elb_u_lgth" RANGE [Bhalf, )VALUES "gs_gutter_elb_l_lgth" RANGE (0, )VALUES "gs_gutter_elb_u_ang" RANGE [0, 90]VALUES "gs_gutter_elb_l_ang" RANGE [0, 90]if not(gs_elbow) then	hideparameter "gs_gutter_elb_u_lgth", "gs_gutter_elb_u_ang",  "gs_gutter_elb_l_lgth", "gs_gutter_elb_l_ang"	lock "gs_gutter_elb_u_lgth", "gs_gutter_elb_u_ang",  "gs_gutter_elb_l_lgth", "gs_gutter_elb_l_ang"endif! ------------------------------------------------------------------------------! Head Lader and Downspout Prameters! ------------------------------------------------------------------------------VALUES "gs_gutter_hl_type" `Тип 1`,`Тип 2`,`Тип 3`,`Тип 4`,`Тип 5`values "gs_gutter_hl_type_m" 1, 2, 3, 4, 5if GLOB_MODPAR_NAME = "gs_gutter_hl_type" then	gs_gutter_hl_type_m = 1	if gs_gutter_hl_type=`Тип 2` then gs_gutter_hl_type_m = 2	if gs_gutter_hl_type=`Тип 3` then gs_gutter_hl_type_m = 3	if gs_gutter_hl_type=`Тип 4` then gs_gutter_hl_type_m = 4	if gs_gutter_hl_type=`Тип 5` then gs_gutter_hl_type_m = 5	parameters gs_gutter_hl_type_m = gs_gutter_hl_type_melse	gs_gutter_hl_type = `Тип 1`	if gs_gutter_hl_type_m = 2 then gs_gutter_hl_type = `Тип 2`	if gs_gutter_hl_type_m = 3 then gs_gutter_hl_type = `Тип 3`	if gs_gutter_hl_type_m = 4 then gs_gutter_hl_type = `Тип 4`	if gs_gutter_hl_type_m = 5 then gs_gutter_hl_type = `Тип 5`	parameters gs_gutter_hl_type = gs_gutter_hl_typeendifVALUES "HL_Width" RANGE [A + 0.05,)VALUES "HL_Width2" RANGE [(A + 0.05) / 2,)VALUES "HL_Depth" RANGE [B + 0.025,)if gs_gutter_ds_profile_num = 1 then	! Round	gs_gutter_aa = gs_gutter_dd	gs_gutter_bb = gs_gutter_dd	if GLOB_MODPAR_NAME = "gs_gutter_dd" or GLOB_MODPAR_NAME = "gs_gutter_dd2" or GLOB_MODPAR_NAME = "gs_gutter_ds_profile" then		HL_Width2 = gs_gutter_dd + HL_Frame		HL_Width = 2 * HL_Width2		HL_Depth = 2 * gs_gutter_dd + HL_Frame		PARAMETERS HL_Width = HL_Width,					HL_Width2 = HL_Width2,					HL_Depth = HL_Depth	endifelse	if GLOB_MODPAR_NAME = "gs_gutter_aa" or GLOB_MODPAR_NAME = "gs_gutter_aa2" or GLOB_MODPAR_NAME = "gs_gutter_ds_profile" then		HL_Width2 = gs_gutter_aa + HL_Frame		HL_Width = 2 * HL_Width2		PARAMETERS HL_Width = HL_Width,					HL_Width2 = HL_Width2	endif	if GLOB_MODPAR_NAME = "gs_gutter_bb" or GLOB_MODPAR_NAME = "gs_gutter_bb2" or GLOB_MODPAR_NAME = "gs_gutter_ds_profile" then		HL_Depth = B + HL_Frame		PARAMETERS HL_Depth = HL_Depth	endifendifif GLOB_MODPAR_NAME = "HL_Width" or GLOB_MODPAR_NAME = "gs_gutter_ds_profile" then	HL_Frame=(HL_Width-2*gs_gutter_aa)/2	HL_Depth=2*gs_gutter_bb+HL_Frame	HL_Width2 = HL_Width/2else	if GLOB_MODPAR_NAME = "HL_Width2" then		HL_Frame=(2*HL_Width2-2*gs_gutter_aa)/2		HL_Depth=2*gs_gutter_bb+HL_Frame		HL_Width = HL_Width2*2	else		if GLOB_MODPAR_NAME = "HL_Frame" then			HL_Width=2*gs_gutter_aa+2*HL_Frame			HL_Width2=gs_gutter_aa+HL_Frame			HL_Depth=2*gs_gutter_bb+HL_Frame		else			HL_Frame=HL_Depth-2*gs_gutter_bb			HL_Width=2*gs_gutter_aa+2*HL_Frame			HL_Width2=gs_gutter_aa+HL_Frame		endif	endifendifPARAMETERS HL_Width = HL_WidthPARAMETERS HL_Width2 = HL_Width2PARAMETERS HL_Frame = HL_FramePARAMETERS HL_Depth = HL_DepthVALUES "HL_Height" RANGE [minh_HL,	)VALUES "gs_gutter_ds_h_off" RANGE [0, )VALUES "gs_gutter_ds_h_dist" RANGE [0, )VALUES "gs_gutter_ds_lgth" RANGE [minh_downspout, )! ------------------------------------------------------------------------------! Head Leader Guard! ------------------------------------------------------------------------------if not(gs_gutter_hl_guard) then LOCK "gs_gutter_guard_mat"if not(gs_head_leader) then	hideparameter "gs_gutter_hl_type","HL_Width","HL_Height","HL_Depth",				"gs_gutter_hl_guard","gs_gutter_hl_mat","gs_gutter_guard_mat",				"gs_gutter_hl_type_m"	lock "gs_gutter_hl_type","HL_Width","HL_Height","HL_Depth",				"gs_gutter_hl_guard","gs_gutter_hl_mat","gs_gutter_guard_mat",				"gs_gutter_hl_type_m"endifif not(gs_gutter_ds_h) then	hideparameter "gs_gutter_ds_h_dist","gs_gutter_ds_h_off",					"gs_gutter_ds_h_pos","gs_gutter_ds_h_pos_mod",					"gs_gutter_ds_h_mat", "gs_gutter_ds_h_positioning"	lock "gs_gutter_ds_h_dist","gs_gutter_ds_h_off",					"gs_gutter_ds_h_pos","gs_gutter_ds_h_pos_mod",					"gs_gutter_ds_h_mat", "gs_gutter_ds_h_positioning"endif! ------------------------------------------------------------------------------! Downspout Offset! ------------------------------------------------------------------------------if GLOB_MODPAR_NAME = "gs_gutter_os_ang" then	!--- felső szakasz és kinyúlás fix, középső és alsó szakaszt változtatjuk	if (gs_gutter_os_ang + EPS) >= 90 then		gs_gutter_os_m_lgth = 0	else if gs_gutter_os_ang > 0 then		gs_gutter_os_m_lgth = gs_gutter_os_off / tan(gs_gutter_os_ang)	endif	gs_gutter_os_l_lgth = DsOffsetLength - gs_gutter_os_u_lgth - gs_gutter_os_m_lgth	parameters	gs_gutter_os_m_lgth = gs_gutter_os_m_lgth,				gs_gutter_os_l_lgth = gs_gutter_os_l_lgthelse	if GLOB_MODPAR_NAME = "DsOffsetLength" and gs_gutter_os_l_lgth > minh_offset / 2 then		!--- középső szakasz dőlés fix, alsó szakasz hosszát változtatjuk		gs_gutter_os_l_lgth = DsOffsetLength - gs_gutter_os_u_lgth - gs_gutter_os_m_lgth		parameters gs_gutter_os_l_lgth = gs_gutter_os_l_lgth	else		!--- középső szakasz dőlést változtatjuk		gs_gutter_os_m_lgth = DsOffsetLength - gs_gutter_os_u_lgth - gs_gutter_os_l_lgth		if gs_gutter_os_m_lgth < EPS then			gs_gutter_os_ang = 90		else			gs_gutter_os_ang = ATN(gs_gutter_os_off / gs_gutter_os_m_lgth)		endif		parameters	gs_gutter_os_m_lgth = gs_gutter_os_m_lgth,					gs_gutter_os_ang = gs_gutter_os_ang	endifendifVALUES "DsOffsetLength" RANGE [minh_offset, )VALUES "gs_gutter_os_u_lgth" RANGE [minh_offset / 2, DsOffsetLength - minh_offset/2]VALUES "gs_gutter_os_l_lgth" RANGE [minh_offset / 2, DsOffsetLength - minh_offset/2]VALUES "gs_gutter_os_off" RANGE [A/2, )if DsOffsetLength <= minh_offset then	VALUES "gs_gutter_os_ang" RANGE [90, 90]else	VALUES "gs_gutter_os_ang" RANGE [atn(gs_gutter_os_off / (DsOffsetLength - minh_offset)), 90]endifVALUES "gs_gutter_os_m_lgth" RANGE [0, )! ------------------------------------------------------------------------------! End Angles! ------------------------------------------------------------------------------Uppmxang = (90-gs_gutter_os_u_ang)/2values "gs_gutter_u_end_ang" RANGE [-Uppmxang, 90-Uppmxang]Lowmxang = (90-gs_gutter_os_l_ang)/2values "gs_gutter_l_end_ang" RANGE [-Lowmxang, 90-Lowmxang]! ------------------------------------------------------------------------------! End Angles (synchronised temporary variables)! ------------------------------------------------------------------------------if up_end_ang_temp < ((360-90)/2)+90 and up_end_ang_temp > 90 then PARAMETERS up_end_ang_temp=90if up_end_ang_temp >= ((360-90)/2)+90 and up_end_ang_temp < 360 then PARAMETERS up_end_ang_temp=0if low_end_ang_temp < ((360-90)/2)+90 and low_end_ang_temp > 90 then PARAMETERS low_end_ang_temp=90if low_end_ang_temp >= ((360-90)/2)+90 and low_end_ang_temp < 360 then PARAMETERS low_end_ang_temp=0if GLOB_MODPAR_NAME = "up_end_ang_temp" then	PARAMETERS gs_gutter_u_end_ang = up_end_ang_temp - (Uppmxang)else	PARAMETERS up_end_ang_temp = gs_gutter_u_end_ang + (Uppmxang)endifif GLOB_MODPAR_NAME = "low_end_ang_temp" then	PARAMETERS gs_gutter_l_end_ang = low_end_ang_temp - (Lowmxang)else	PARAMETERS low_end_ang_temp = gs_gutter_l_end_ang + (Lowmxang)endif! ------------------------------------------------------------------------------! Upper and Lower Angles (synchronised temporary variables)! ------------------------------------------------------------------------------if gs_gutter_os_u_ang < 225 and gs_gutter_os_u_ang > 89 then parameters gs_gutter_os_u_ang = 89if gs_gutter_os_u_ang >= 225 and gs_gutter_os_u_ang < 360 then parameters gs_gutter_os_u_ang = 0if gs_gutter_os_l_ang < 225 and gs_gutter_os_l_ang > 89 then parameters gs_gutter_os_l_ang = 89if gs_gutter_os_l_ang >= 225 and gs_gutter_os_l_ang < 360 then parameters gs_gutter_os_l_ang = 0if gs_gutter_os_u_ang < 0 then PARAMETERS gs_gutter_os_u_ang=0if gs_gutter_os_l_ang < 0 then PARAMETERS gs_gutter_os_l_ang=0if not(gs_downspout_offset) then	hideparameter "DsOffsetLength", "gs_gutter_os_u_lgth", "gs_gutter_os_u_ang", "gs_gutter_u_end_ang", "gs_gutter_os_off", "gs_gutter_os_l_lgth", "gs_gutter_os_ang"	lock "DsOffsetLength", "gs_gutter_os_u_lgth", "gs_gutter_os_u_ang", "gs_gutter_u_end_ang", "gs_gutter_os_off", "gs_gutter_os_l_lgth", "gs_gutter_os_ang"endif! ------------------------------------------------------------------------------! 3D Representation! ------------------------------------------------------------------------------values "gs_detlevel_3D" `Детальный`, `Простой`, `Откл.`values "gs_detlevel_3D_m" DETLEVEL_3D_OFF, DETLEVEL_3D_SIMPLE, DETLEVEL_3D_DETAILEDif GLOB_MODPAR_NAME = "gs_detlevel_3D" then	gs_detlevel_3D_m = DETLEVEL_3D_DETAILED	if gs_detlevel_3D = `Простой`					then gs_detlevel_3D_m = DETLEVEL_3D_SIMPLE	if gs_detlevel_3D = `Откл.`						then gs_detlevel_3D_m = DETLEVEL_3D_OFF	parameters gs_detlevel_3D_m = gs_detlevel_3D_melse	gs_detlevel_3D = `Детальный`	if gs_detlevel_3D_m = DETLEVEL_3D_SIMPLE		then gs_detlevel_3D = `Простой`	if gs_detlevel_3D_m = DETLEVEL_3D_OFF			then gs_detlevel_3D = `Откл.`	parameters gs_detlevel_3D = gs_detlevel_3Dendif! ------------------------------------------------------------------------------! Simple Model! ------------------------------------------------------------------------------if gs_detlevel_3D_m = 1 then		! Simple	! --- Hanger	LOCK "gs_gutter_ds_h","gs_gutter_ds_h_dist","gs_gutter_ds_h_off","gs_gutter_ds_h_pos","gs_gutter_ds_h_pos_mod","gs_gutter_ds_h_mat"	! --- Gutter Guard	LOCK "gs_gutter_hl_guard", "gs_gutter_guard_mat", "gs_resol"endif! ------------------------------------------------------------------------------! 2D Representation! ------------------------------------------------------------------------------VALUES "floorPlanDisplayType" `Сечение и Низ`, `Сечение и Верх`, `Символический`, `Только Контуры`, `Вся Проекция`VALUES "iFloorPlanDisplayType" 	DISPLAY_PROJECTED, \								DISPLAY_OVERHEAD_PROJ, \								DISPLAY_SYMBOLIC, \								DISPLAY_OUTLINES, \								DISPLAY_OVERHEAD_ALLif GLOB_MODPAR_NAME = "floorPlanDisplayType" then	iFloorPlanDisplayType = DISPLAY_OVERHEAD_PROJ	if floorPlanDisplayType = `Сечение и Низ`		then iFloorPlanDisplayType = DISPLAY_PROJECTED	if floorPlanDisplayType = `Символический`		then iFloorPlanDisplayType = DISPLAY_SYMBOLIC	if floorPlanDisplayType = `Только Контуры`	then iFloorPlanDisplayType = DISPLAY_OUTLINES	if floorPlanDisplayType = `Вся Проекция`	then iFloorPlanDisplayType = DISPLAY_OVERHEAD_ALL	parameters iFloorPlanDisplayType = iFloorPlanDisplayTypeelse	floorPlanDisplayType = `Сечение и Верх`	if iFloorPlanDisplayType = DISPLAY_PROJECTED		then floorPlanDisplayType = `Сечение и Низ`	if iFloorPlanDisplayType = DISPLAY_SYMBOLIC			then floorPlanDisplayType = `Символический`	if iFloorPlanDisplayType = DISPLAY_OUTLINES			then floorPlanDisplayType = `Только Контуры`	if iFloorPlanDisplayType = DISPLAY_OVERHEAD_ALL		then floorPlanDisplayType = `Вся Проекция`	parameters floorPlanDisplayType = floorPlanDisplayTypeendifif iFloorPlanDisplayType = DISPLAY_PROJECTED then	HIDEPARAMETER "overhead_Line_Type"endifif iFloorPlanDisplayType = DISPLAY_SYMBOLIC or iFloorPlanDisplayType = DISPLAY_OUTLINES then	HIDEPARAMETER "cut_pen", "overhead_Line_Type"endifif iFloorPlanDisplayType = DISPLAY_OVERHEAD_ALL then	HIDEPARAMETER "cut_pen", "gs_line_type"endif! ------------------------------------------------------------------------------! Basic geometric parameters! ------------------------------------------------------------------------------ac_bottomlevel = 0ac_toplevel = zzyzxparameters ac_bottomlevel = ac_bottomlevelparameters ac_toplevel = ac_toplevelDSoffset_ver_offset = zzyzx - (gs_gutter_os_u_lgth + gs_gutter_os_m_lgth + gs_gutter_os_l_lgth + HL_Height*gs_head_leader)parameters DSoffset_ver_offset = DSoffset_ver_offsetHeadLeader_ver_offset = zzyzx - HL_Heightparameters HeadLeader_ver_offset = HeadLeader_ver_offsetif gs_downspout_offset then	HeadLeader_hor_offset = -gs_gutter_os_off-gs_gutter_os_u_lgth * TAN(gs_gutter_os_u_ang)	parameters HeadLeader_hor_offset = HeadLeader_hor_offsetelse	HeadLeader_hor_offset = -dis_wall + Bhalf	parameters HeadLeader_hor_offset = HeadLeader_hor_offsetendifnaen = "Труба металлическая для водосточных систем, окрашенная, диаметр " + str("%.0mm", gs_gutter_aa2) + " мм, длина 3000 мм"kol_vo_spec = round_int(gs_gutter_lgth/3)parameters naen = naen, kol_vo_spec = kol_vo_spec