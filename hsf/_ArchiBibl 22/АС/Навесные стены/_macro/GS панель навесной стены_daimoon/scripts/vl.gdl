!VALUES "Shrift_fille" "Откл", CUSTOM!++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++!					  Чтение текстовых стилей!++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++!if Shrift_fille <> "Откл" then!	file = Shrift_fille + "_font_info.txt"!	ch1 = OPEN ("TEXT", file, "SEPARATOR = ';', MODE = RO, LIBRARY")!	font_f = ""!	tip_f = 0!	razmer_f = 0!	nr = INPUT (ch1, 1, 1, font_f, tip_f, razmer_f, v)!	close ch1!	PARAMETERS Shrift = font_f, Shrift_tip = tip_f, Shrift_razm = razmer_f!endif!++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++hir_t = ROUND_INT((ac_width + zazor) * 50)/50vis_t = ROUND_INT((ac_height + zazor) * 50)/50hir = hir_tvis = vis_tparameters razm = str("%.2m", hir) + "x" + str("%.2m", vis) + "(h)"if post_vis then parameters razm = str("%.2m", hir) + "x" + str("%.2m", b_pan) + "(h)"VALUES "type_pan" 1, 2, 3if type_pan = 1 then	HIDEPARAMETER 'spec_dw'		LOCK 'spec_dw'	values 'tip' "Сэндвич","Кассета","Ж/б панель",CUSTOM	if tip="Кассета" then	parameters naen="ВФ МП 1005/23/20"+" "+str("%.0mm",hir)+"x"+str("%.0mm",vis)+"(h)"	parameters obozn="ТУ 5285-001-78334080-2006"	HIDEPARAMETER 'proisvod','marka','dlin'	parameters dlin=0	endif	if tip="Сэндвич" then		values 'proisvod' "МЕТАЛЛпрофиль","Термопанель","Ruukki", "ГОСТ"		if proisvod="МЕТАЛЛпрофиль" then			values 'marka' "ТСП-Z","ТСП-S","ТСП-К"!,CUSTOM			parameters obozn="ТУ 5284-001-78099614-2007"			otdel_out = "(ПЭ-01-"+color_out+"-0.7)"			otdel_in = "(ПЭ-01-"+color_in+"-0.7)"			end_naen = "-Г-Г-МВ" + otdel_out + "/" + otdel_in			if obmen=0 then				temp=vis					if post_vis then vis=b_pan					parameters naen="МП "+marka+"-"+str("%.0mm",ac_thickness)+"-"+str("%.0mm",vis)+ end_naen					parameters dlin=hir					vis=temp				else					temp=hir					if post_vis then hir=b_pan					parameters naen="МП "+marka+"-"+str("%.0mm",ac_thickness)+"-"+str("%.0mm",hir)+ end_naen					parameters dlin=vis					hir=temp				endif		endif		if proisvod="Термопанель" then			parameters obozn="ТУ 5284-013-01395087-2001"			values 'marka' "ПСБ","ПКБ"!,CUSTOM			parameters naen=marka+"-"+str("%.0mm",hir)+"x"+str("%.0mm",vis)+"x"+str("%.0mm",ac_thickness)		endif		if proisvod = "ГОСТ" then			values 'marka' "ТСП-Z","ТСП-S","ТСП-К",CUSTOM			values 'obozn' "ГОСТ 32603-2012",CUSTOM			otdel_out = "(ПЭ-01-"+color_out+"-0.7)"			otdel_in = "(ПЭ-01-"+color_in+"-0.7)"			end_naen = " нар. " + color_out + "/вн. " +color_in			if obmen=0 then				temp=vis				if post_vis then vis=b_pan				parameters naen=marka+"-"+str("%.0mm",ac_thickness)+"-"+str("%.0mm",vis)+ end_naen				parameters dlin=hir				vis=temp			else				temp=hir				if post_vis then hir=b_pan				parameters naen=marka+"-"+str("%.0mm",ac_thickness)+"-"+str("%.0mm",hir)+ end_naen				parameters dlin=vis				hir=temp			endif		endif				if proisvod="Ruukki" then			parameters obozn="ТУ 5284-001-48363367-04"			values 'marka' "Венталл-СЗ","Венталл-КЗ"!,CUSTOM			parameters naen=marka			parameters dlin=hir			if hir = 1.2 then parameters dlin=vis		endif	endif	if tip <> "Кассета" and tip <> "Сэндвич" then		if marka <> "" then parameters naen = marka + " " + razm		HIDEPARAMETER 'proisvod'	endifelse	HIDEPARAMETER 'spec_pan'		LOCK 'spec_pan'	parameters tip_spec = "Проём за вычетом допусков"	parameters ac_unit_width = hir	parameters ac_unit_height = visendif