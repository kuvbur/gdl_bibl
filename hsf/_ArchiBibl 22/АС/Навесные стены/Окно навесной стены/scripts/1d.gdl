eps = 0.0001! opening typeOPTYPE_SIDEHUNG			= 2OPTYPE_SIDEHUNGMIRRORED = 3OPTYPE_BOTTOMHUNG		= 4OPTYPE_TOPHUNG			= 5OPTYPE_TILTTURN			= 6OPTYPE_TILTTURNMIRRORED	= 7! opening directionOPDIR_IN  = 1OPDIR_OUT = 2PANEL_EMPTY = 0PANEL_FIX = 1PANEL_BOX = 2PANEL_2MAT = 3PANEL_WINDOW = 10PANEL_WINDOW2 = 11PANEL_DOOR = 20PANEL_DOOR2 = 21PANEL_DOOR_SLIDING2 = 22PANEL_DOOR_SLIDING4 = 23PANEL_DOOR_REVOLVING = 24PANEL_CUSTOM = 99nCorners = vardim1 (AC_PanelCoords)nCorners_in = vardim1 (AC_clampFreeRegionCoords)leftSide_X  = AC_clampFreeRegionCoords[1][1]rightSide_X = AC_clampFreeRegionCoords[1][1]downSide_Y  = AC_clampFreeRegionCoords[1][2]upperSide_Y = AC_clampFreeRegionCoords[1][2]for i = 1 to nCorners_in	if AC_clampFreeRegionCoords[i][1] < leftSide_X  then leftSide_X  = AC_clampFreeRegionCoords[i][1]	if AC_clampFreeRegionCoords[i][1] > rightSide_X then rightSide_X = AC_clampFreeRegionCoords[i][1]	if AC_clampFreeRegionCoords[i][2] < downSide_Y  then downSide_Y  = AC_clampFreeRegionCoords[i][2]	if AC_clampFreeRegionCoords[i][2] > upperSide_Y then upperSide_Y = AC_clampFreeRegionCoords[i][2]next ileftClamp_X  = AC_PanelCoords[1][1]rightClamp_X = AC_PanelCoords[1][1]downClamp_Y  = AC_PanelCoords[1][2]upperClamp_Y = AC_PanelCoords[1][2]for i = 1 to nCorners	if AC_PanelCoords[i][1] < leftClamp_X  then leftClamp_X  = AC_PanelCoords[i][1]	if AC_PanelCoords[i][1] > rightClamp_X then rightClamp_X = AC_PanelCoords[i][1]	if AC_PanelCoords[i][2] < downClamp_Y  then downClamp_Y  = AC_PanelCoords[i][2]	if AC_PanelCoords[i][2] > upperClamp_Y then upperClamp_Y = AC_PanelCoords[i][2]next iif abs (gs_updirection) < eps or abs (gs_updirection - 180) < eps or GLOB_SCRIPT_TYPE = 2 then	windowWidth = rightSide_X - leftSide_X	windowHeight = upperSide_Y - downSide_Yelse	windowHeight = rightSide_X - leftSide_X	windowWidth = upperSide_Y - downSide_Yendifsash_X = windowWidth  - 2 * (frameWidth - recessWidth) - 2 * adapterWidth * (iFrameType = 2)sash_Y = windowHeight - 2 * (frameWidth - recessWidth) - 2 * adapterWidth * (iFrameType = 2)