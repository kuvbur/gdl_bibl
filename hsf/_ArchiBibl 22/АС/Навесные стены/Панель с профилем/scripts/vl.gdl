values "bOverridepen" "Сложный профиль", "Материал панели", "Ручная настройка"values "bOverridebmat" "Сложный профиль", "Материал панели"values "bOverridebpokr" "Сложный профиль", "Материал панели"if not(bOverrideEdgeSurf) then lock "edgeSurface"values{2} "force_structure_display" iforce_structure_display, tforce_structure_displayif bOverridepen<>"Ручная настройка" then lock "gs_fill_type", "gs_fill_pen", "gs_back_pen"if not(use_profile_det) then lock "myProfileIdx_det"if not(use_profile_simp) then lock "myProfileIdx_simp"values 'type_panel' "ТСП-Z","ТСП-S","ТСП-К",CUSTOMparameters obozn = "ГОСТ 32603-2012"values "krat_txt" 50 / 1000, 10 / 1000, 5 / 1000, 1 / 1000if abs(krat_txt-50/1000)<EPS then krat = 20if abs(krat_txt-10/1000)<EPS then krat = 100if abs(krat_txt-5/1000)<EPS then krat = 200if abs(krat_txt-1/1000)<EPS then krat = 1000! --- Size parameters for listing ---------------------------------------------------parameters ac_panel_type = 0parameters ac_openingDir = 0parameters ac_width = rightSide_X - leftSide_X parameters ac_nominalWidth = ROUND_INT((ac_width) * krat)/kratparameters ac_height = upperSide_Y - downSide_Yparameters ac_nominalHeight = ROUND_INT((ac_height) * krat)/kratparameters centerX = centerXparameters centerY = centerYparameters ac_thickness = ac_clampWidthwidth = ROUND_INT((ac_width) * krat)/kratdlin = ROUND_INT((ac_height) * krat)/kratif post_vis then width = b_panparameters razm = str("%.2m", dlin) + "x" + str("%.2m", width) + "(h)"naen = type_panel+"-"+str("%.0mm",ac_thickness)+"-"+str("%.0mm",width)+"-"+type_sheet_out+"-"+type_sheet_in+"-"+"МВ"parameters naen=naenparameters dlin=dlin, width=width