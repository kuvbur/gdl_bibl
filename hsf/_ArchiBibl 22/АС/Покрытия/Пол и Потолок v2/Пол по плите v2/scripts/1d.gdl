rot2 -SYMB_ROTANGLEunID=1model solidEPS=0.000001unID=1dim nul[][]dim otr_otv[][]dim otr_pl[][]k_otr_pl=0k_otv=0n_otr=0i=1!!Удалим лишнее - новый способotr_otv=nulotr_pl=nulwhile ac_coords[i+1][1]<>0 doif i>1000000 then end !Защита от бесконечного цикла	if ac_coords[i][1]*1000-25.4>EPS and i>1 then		k_otv=k_otv+1 !После первого 50,8 начинаются отверстия		if k_otv>1 then k_otr_otv[k_otv-1]=n_otr - n_t		n_t = n_otr	else		!!Поворот		kx1=ac_coords[i][2]		ky1=ac_coords[i][3]		kx2=ac_coords[i+1][2]		ky2=ac_coords[i+1][3]		ugol=SYMB_ROTANGLE		GOSUB "koord_ang"		!на выходе kxx1 kyy1 kxx2 kyy2		if k_otv=0 then !Для контура плиты			k_otr_pl=k_otr_pl+1			otr_pl[k_otr_pl][1]=kxx1			otr_pl[k_otr_pl][2]=kyy1			otr_pl[k_otr_pl][3]=kxx2			otr_pl[k_otr_pl][4]=kyy2		endif		if k_otv>0 then !Для отверстий			n_otr=n_otr+1			otr_otv[n_otr][1]=kxx1			otr_otv[n_otr][2]=kyy1			otr_otv[n_otr][3]=kxx2			otr_otv[n_otr][4]=kyy2!			strt = str("%.0", k_otr_otv[k_otv]) + " " + str("%.0", k_otv)!			text2 kxx2 + (kxx1-kxx2)/2, kyy2 + (kyy1-kyy2)/2, strt		endif	endifi=i+1ENDWHILEif k_otv > 0 then k_otr_otv[k_otv]=n_otr - n_tCALL "Пол_Потолок_v2" parameters allgoto 20"koord_ang":	kxx1=kx1*cos(ugol)-ky1*sin(ugol)	kyy1=kx1*sin(ugol)+ky1*cos(ugol)	kxx2=kx2*cos(ugol)-ky2*sin(ugol)	kyy2=kx2*sin(ugol)+ky2*cos(ugol)return20: