if bst_use<EPS then endif show_perem_3d<EPS then endif GLOB_VIEW_TYPE=9 then end!-------------------------------------------------------------!---------- Отображение ж/б усиления в 3д --------------------!-------------------------------------------------------------SHADOW OFF, OFFRESOL 5del topaddz -otstup_2mulx -1!if bRotated=1 and SYMB_MIRRORED=0 then mulx -1mulz sgn(otstup)if not((AC_Wall_Direction_Type=0) or (AC_Wall_Direction_Type=2)) then	if abs(otstup)-900<EPS then addz -abs(otstup)	mulz -1	addz -max_t/2else	if abs(otstup)-900<EPS then addz abs(otstup)	mulz -1	addz -max_t/2endifshow_hole = (abs(GLOB_STRUCTURE_DISPLAY-upr_n)<EPS or (GLOB_VIEW_TYPE = 9))add -ac_wallhole_width/2,-WIDO_SILL_HEIGHT,0if bst_raspol = irasp_BOTH or bst_raspol = irasp_LEFT or bst_raspol = irasp_NOUP or bst_raspol = irasp_LEFTNOUP then	start_x = -bst_th : start_y = 0 : start_z = 0 : gosub "hbst_l_down"	start_x = -bst_th : start_y = 0 : start_z = max_t : gosub "hbst_l_down"endifif bst_raspol = irasp_BOTH or bst_raspol = irasp_RIGHT or bst_raspol = irasp_NOUP or bst_raspol = irasp_RIGHTNOUP then	start_x = ac_wallhole_width+bst_th : start_y = 0 : start_z = 0 : gosub "hbst_l_down"	start_x = ac_wallhole_width+bst_th : start_y = 0 : start_z = max_t : gosub "hbst_l_down"endifif bst_raspol = irasp_NOUP or bst_raspol = irasp_LEFTNOUP or bst_raspol = irasp_LEFT then	if bst_raspol = irasp_LEFT then		start_y = h_0+bst_th	else		start_y = h_0	endif	start_x = -bst_th : start_z = 0 : gosub "hbst_l_up"	start_x = -bst_th : start_z = max_t : gosub "hbst_l_up"endifif bst_raspol = irasp_NOUP or bst_raspol = irasp_RIGHTNOUP or bst_raspol = irasp_RIGHT then	if bst_raspol = irasp_RIGHT then		start_y = h_0+bst_th	else		start_y = h_0	endif	start_x = ac_wallhole_width+bst_th : start_z = 0 : gosub "hbst_l_up"	start_x = ac_wallhole_width+bst_th : start_z = max_t : gosub "hbst_l_up"endifif bst_raspol = irasp_BOTH then	start_y = h_0 + bst_th	if bst_l_right > EPS then		start_x = ac_wallhole_width+bst_th	else		start_x = -bst_th	endif	start_z = 0 : gosub "hbst_l_up"	start_z = max_t : gosub "hbst_l_up"endifif bst_raspol = irasp_UP or bst_raspol = irasp_LEFT or bst_raspol = irasp_BOTH then	if bst_raspol = irasp_BOTH then		start_x = ac_wallhole_width + bst_th	else		start_x = ac_wallhole_width	endif	start_y = h_0+bst_th : start_z = 0 : gosub "hbst_l_left"	start_y = h_0+bst_th : start_z = max_t : gosub "hbst_l_left"endifif bst_raspol = irasp_UP or bst_raspol = irasp_RIGHT or bst_raspol = irasp_BOTH then	if bst_raspol = irasp_BOTH then		start_x = -bst_th	else		start_x = 0	endif	start_y = h_0+bst_th : start_z = 0 : gosub "hbst_l_right"	start_y = h_0+bst_th : start_z = max_t : gosub "hbst_l_right"endifif bst_raspol = irasp_BOTH then	!Правая часть	put 0,-bst_l_down,15 !Низ стены на границе проёма	put -bst_th,-bst_l_down,15 !Отступ на толщину сердечника, низ стены	put -bst_th,h_0,15	!Верх проёма	!Выступ справа	if bst_l_right>EPS then		put -bst_l_right-bst_th,h_0,15		put -bst_l_right-bst_th,h_0+bst_th,15	endif	put -bst_th,h_0+bst_th,15 !Верх сердечника над проёмом справа	!Выступ вверх справа	if bst_l_up>EPS and bst_l_right < EPS then		put -bst_th,h_0+bst_th+bst_l_up,15		put 0,h_0+bst_th+bst_l_up,15	endif	put 0,h_0+bst_th,15		!Левая часть	put ac_wallhole_width,h_0+bst_th,15	!Выступ вверх слева	if bst_l_up>EPS and bst_l_right > EPS then		put ac_wallhole_width,h_0+bst_th+bst_l_up,15		put ac_wallhole_width+bst_th,h_0+bst_th+bst_l_up,15	endif	put ac_wallhole_width+bst_th,h_0+bst_th,15	!Выступ слева	if bst_l_left>EPS then		put ac_wallhole_width+bst_th+bst_l_left,h_0+bst_th,15		put ac_wallhole_width+bst_th+bst_l_left,h_0,15	endif	put ac_wallhole_width+bst_th,h_0,15	put ac_wallhole_width+bst_th,-bst_l_down,15	put ac_wallhole_width,-bst_l_down,15	put ac_wallhole_width,h_0,15	put 0,h_0,15	gosub "model"endifif bst_raspol = irasp_LEFT then 	put 0,-bst_l_down,15	put -bst_th,-bst_l_down,15	put -bst_th,h_0+bst_th,15	!Выступ вверх справа	if bst_l_up>EPS and bst_l_right < EPS then		put -bst_th,h_0+bst_th+bst_l_up,15		put 0,h_0+bst_th+bst_l_up,15		put 0,h_0+bst_th,15	endif	put ac_wallhole_width+bst_l_left,h_0+bst_th,15	put ac_wallhole_width+bst_l_left,h_0,15	put 0,h_0,15	gosub "model"endifif bst_raspol = irasp_RIGHT then	put ac_wallhole_width,-bst_l_down,15	put ac_wallhole_width+bst_th,-bst_l_down,15	put ac_wallhole_width+bst_th,h_0+bst_th,15	!Выступ вверх слева	if bst_l_up>EPS and bst_l_right > EPS then		put ac_wallhole_width+bst_th,h_0+bst_th+bst_l_up,15		put ac_wallhole_width,h_0+bst_th+bst_l_up,15		put ac_wallhole_width,h_0+bst_th,15	endif	put -bst_l_right,h_0+bst_th,15	put -bst_l_right,h_0,15	put ac_wallhole_width,h_0,15	gosub "model"endifif bst_raspol = irasp_LEFTNOUP then 	put 0,-bst_l_down,15	put -bst_th,-bst_l_down,15	put -bst_th,h_0+bst_l_up,15	put 0,h_0+bst_l_up,15	gosub "model"endifif bst_raspol = irasp_RIGHTNOUP then	put ac_wallhole_width,-bst_l_down,15	put ac_wallhole_width+bst_th,-bst_l_down,15	put ac_wallhole_width+bst_th,h_0+bst_l_up,15	put ac_wallhole_width,h_0+bst_l_up,15	gosub "model"endifif bst_raspol = irasp_UP then	put -bst_l_right,h_0+bst_th,15	put -bst_l_right,h_0,15	put ac_wallhole_width+bst_l_left,h_0,15	put ac_wallhole_width+bst_l_left,h_0+bst_th,15	gosub "model"endifif bst_raspol = irasp_NOUP then	put 0,-bst_l_down,15	put -bst_th,-bst_l_down,15	put -bst_th,h_0+bst_l_up,15	put 0,h_0+bst_l_up,15	gosub "model"	put ac_wallhole_width,-bst_l_down,15	put ac_wallhole_width+bst_th,-bst_l_down,15	put ac_wallhole_width+bst_th,h_0+bst_l_up,15	put ac_wallhole_width,h_0+bst_l_up,15	gosub "model"endifdel topend"model":	pen pen_cont	building_material bst_material	material _mat	sect_attrs{2} pen_cut, 1	if show_hole then		PRISM_ NSP/3,max_t,use(nsp)		WALLHOLE NSP/3,1,get(nsp)	else		PRISM_ NSP/3,max_t,get(nsp)	endifreturn"hbst_l_down":	HOTSPOT start_x, start_y, start_z, unID, bst_l_down, 1+128: unID=unID+1	HOTSPOT start_x, start_y+1, start_z, unID, bst_l_down, 3: unID=unID+1	HOTSPOT start_x, start_y-bst_l_down, start_z, unID, bst_l_down, 2: unID=unID+1return"hbst_l_up":	HOTSPOT start_x, start_y, start_z, unID, bst_l_up, 1+128: unID=unID+1	HOTSPOT start_x, start_y-1, start_z, unID, bst_l_up, 3: unID=unID+1	HOTSPOT start_x, start_y+bst_l_up, start_z, unID, bst_l_up, 2: unID=unID+1return"hbst_l_left":	HOTSPOT start_x, start_y, start_z, unID, bst_l_left, 1+128: unID=unID+1	HOTSPOT start_x-1, start_y, start_z, unID, bst_l_left, 3: unID=unID+1	HOTSPOT start_x+bst_l_left, start_y, start_z, unID, bst_l_left, 2: unID=unID+1return"hbst_l_right":	HOTSPOT start_x, start_y, start_z, unID, bst_l_right, 1+128: unID=unID+1	HOTSPOT start_x+1, start_y, start_z, unID, bst_l_right, 3: unID=unID+1	HOTSPOT start_x-bst_l_right, start_y, start_z, unID, bst_l_right, 2: unID=unID+1return