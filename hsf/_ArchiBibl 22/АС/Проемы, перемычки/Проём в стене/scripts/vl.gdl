call "Перемычки" parameters all, otstup=otstup, max_t=max_t, otstup_2=otstup_2call "Жб обрамление проёма" parameters all, otstup=otstup, max_t=max_t, otstup_2=otstup_2parameters AC_WallContours = 0parameters AC_Hole_Hotspot_Control = 2parameters AC_show2DHotspotsin3D = 0PARAMETERS WALL_COMPS_NAME_ = WALL_COMPS_NAME_, WALL_THICKNESS_ = WALL_THICKNESS_, WALL_SKINS_PARAMS_ = WALL_SKINS_PARAMS_, WALL_SKINS_NUMBER_ = WALL_SKINS_NUMBER_PARAMETERS otstup = otstup, otstup_2 = otstup_2 , max_t = max_tif abs(max_t)<EPS and WALL_COMPS_NAME_<>"" then	values "max_t" range (,3]else	PARAMETERS max_t = max_tendifvalues "gs_wcont_line" "Вкл", "Откл"values "krat_txt" 1/1000, 5/1000, 10/1000, 50/1000, 100/1000, 500/1000if manual_elev then	values 'n_elev' numb_elevelse	parameters n_elev = n_elev	LOCK "n_elev"endifif main_name = "" then 	values "nazn" "АС", "ОВ", "ВК", "ТХ", "ЭЛ", "ЭМ","ТС",CUSTOM	values 'firest' "Нет" , "EI 15","EI 30","EI 60","EIW 15","EIW 30","EIW 60"	parameters ac_wallhole_height = B, ac_wallhole_width = A	if kirp then		values "A" range [0, 10] step 0.14, 0.13		values "B" range [krat_txt, 1000] step krat_txt, krat_txt 	else		values "A" range [krat_txt, 1000] step krat_txt, krat_txt 		values "B" range [krat_txt, 1000] step krat_txt, krat_txt 	endifendifvalues "upr_rezhim" "Откл" ,"Только ядро", "Без отделки", "Вся модель"if upr_rezhim = "Откл" then PARAMETERS upr_n = -1if upr_rezhim = "Без отделки" then PARAMETERS upr_n = 2if upr_rezhim = "Только ядро" then PARAMETERS upr_n = 1if upr_rezhim = "Вся модель" then PARAMETERS upr_n = 0if krugl then	parameters razm = "D" + str("%.0mm", A)	parameters B = A	parameters razm_otv_B = A	parameters razm_otv_H = B	parameters porog = 0else	if GLOB_MODPAR_NAME="A" or GLOB_MODPAR_NAME="B" then		parameters pos_x = A*0.1		parameters pos_y = B*0.8	else		if abs(pos_x)<EPS or abs(pos_y)<EPS then			parameters pos_x = A*0.1			parameters pos_y = B*0.8		endif	endif	if pos_x<0.01 or pos_x-A>0.01 then parameters pos_x = A*0.1	if pos_y<0.01 or pos_y-B>0.01 then parameters pos_y = B*0.8	if sbros then		parameters pos_x = A*0.1		parameters pos_y = B*0.8		parameters sbros = 0	endif	parameters razm = str("%.0mm", A)+"x"+str("%.0mm", B+porog)+"(h)"	parameters razm_otv_B = A	parameters razm_otv_H = B+porogendifif otdel_otk then	l = ac_wallhole_height*2 + ac_wallhole_width*2	l = l + 0.03	b = max(abs(WALL_THICKNESS),abs(AC_wido_sill),0.02)	PARAMETERS s_otk = l * belse	PARAMETERS s_otk = 0endiftxt_x = str("%.0mm", SYMB_POS_X+WALL_POSITION[1])txt_y = str("%.0mm", SYMB_POS_Y+WALL_POSITION[2])txt_z = urtest_string = razm + "_" + txt_x + "_"+ txt_y + "_" + txt_zif test_string<>FM_SerialNumber then	PARAMETERS FM_SerialNumber = test_string	PARAMETERS is_struct_approve = 0endif