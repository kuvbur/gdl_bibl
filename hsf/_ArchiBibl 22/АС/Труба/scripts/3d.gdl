! Tubeif gs_detlevel_3D=`Откл.` then Endif gs_detlevel_3D=`Простой` then gs_resol=18if gs_shadow then	shadow onelse	shadow offendifif GLOB_CONTEXT<20 or GLOB_CONTEXT>40 then	resol gs_resolelse	resol 12endifpen gs_cont_penunID=1hotspot 0,0,0,unID : unID=unID+1!!-----------------------!!!! Base Geometry Editing !!!!-----------------------!!if edit_mode=`Координаты` then	hotspot 0,0,zzyzx,unID,dist_x,1+128 : unID=unID+1	hotspot -1,0,zzyzx,unID,dist_x,3 : unID=unID+1	hotspot dist_x,0,zzyzx,unID,dist_x,2 : unID=unID+1	hotspot dist_x,0,0,unID,zzyzx,1+128 : unID=unID+1	hotspot dist_x,0,-1,unID,zzyzx,3 : unID=unID+1	hotspot dist_x,0,zzyzx,unID,zzyzx,2 : unID=unID+1else	hotspot 0,0,0,unID,angle_1,6 : unID=unID+1	hotspot 1,0,0,unID,angle_1,4+128 : unID=unID+1	hotspot 0,-1,0,unID,angle_1,7 : unID=unID+1	hotspot 0.5*tube_length*cos(angle_1),0,0.5*tube_length*sin(angle_1),unID,angle_1,5 : unID=unID+1	roty 90-angle_1	hotspot 0,0,0,unID,tube_length,1+128 : unID=unID+1	hotspot 0,0,-1,unID,tube_length,3 : unID=unID+1	hotspot 0,0,tube_length,unID,tube_length,2 : unID=unID+1	del 1endifroty 90-angle_1!!------------------------------!!!! Structural Dimension Editing !!!!------------------------------!!hotspot 0,0,0,unID,radius_1,1+128 : unID=unID+1hotspot -1,0,0,unID,radius_1,3 : unID=unID+1hotspot radius_1,0,0,unID,radius_1,2 : unID=unID+1hotspot 0,0,0,unID,radius_1,1+128 : unID=unID+1hotspot 1,0,0,unID,radius_1,3 : unID=unID+1hotspot -radius_1,0,0,unID,radius_1,2 : unID=unID+1hotspot 0,0,0,unID,radius_1,1+128 : unID=unID+1hotspot 0,-1,0,unID,radius_1,3 : unID=unID+1hotspot 0,radius_1,0,unID,radius_1,2 : unID=unID+1hotspot 0,0,0,unID,radius_1,1+128 : unID=unID+1hotspot 0,1,0,unID,radius_1,3 : unID=unID+1hotspot 0,-radius_1,0,unID,radius_1,2 : unID=unID+1hotspot 0,0,tube_length,unID,radius_1,1+128 : unID=unID+1hotspot -1,0,tube_length,unID,radius_1,3 : unID=unID+1hotspot radius_1,0,tube_length,unID,radius_1,2 : unID=unID+1hotspot 0,0,tube_length,unID,radius_1,1+128 : unID=unID+1hotspot 1,0,tube_length,unID,radius_1,3 : unID=unID+1hotspot -radius_1,0,tube_length,unID,radius_1,2 : unID=unID+1hotspot 0,0,tube_length,unID,radius_1,1+128 : unID=unID+1hotspot 0,-1,tube_length,unID,radius_1,3 : unID=unID+1hotspot 0,radius_1,tube_length,unID,radius_1,2 : unID=unID+1hotspot 0,0,tube_length,unID,radius_1,1+128 : unID=unID+1hotspot 0,1,tube_length,unID,radius_1,3 : unID=unID+1hotspot 0,-radius_1,tube_length,unID,radius_1,2 : unID=unID+1!----------!! Geometry !!----------!material gs_ifc_matgroup "cylind"	cylind tube_length,radius_1endgroup!!!!placegroup "elbow"if tube_thk>0 and tube_thk<radius_1 then	group "hole"		addz -0.05		cylind tube_length+0.10,radius_1-tube_thk		del 1	endgroup	!!!!placegroup "hole"	tub=subgroup("cylind","hole")	placegroup tubelse	placegroup "cylind"endifdel 1end