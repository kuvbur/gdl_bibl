PAPER_TO_MODEL = GLOB_SCALE / 1000l_ended = 1*PAPER_TO_MODELHOTSPOT2 0, 0, unID, pos_1_x1, 1+128: unID=unID+1HOTSPOT2 -1, 0, unID, pos_1_x1, 3: unID=unID+1HOTSPOT2 pos_1_x1, 0, unID, pos_1_x1, 2: unID=unID+1HOTSPOT2 0, diam_arm_1, unID, pos_1_x1, 1+128: unID=unID+1HOTSPOT2 -1, diam_arm_1, unID, pos_1_x1, 3: unID=unID+1HOTSPOT2 pos_1_x1, diam_arm_1, unID, pos_1_x1, 2: unID=unID+1HOTSPOT2 0, pos_1_y, unID, pos_1_x2, 1+128: unID=unID+1HOTSPOT2 -1, pos_1_y, unID, pos_1_x2, 3: unID=unID+1HOTSPOT2 pos_1_x2, pos_1_y, unID, pos_1_x2, 2: unID=unID+1HOTSPOT2 0, pos_1_y-diam_arm_1, unID, pos_1_x2, 1+128: unID=unID+1HOTSPOT2 -1, pos_1_y-diam_arm_1, unID, pos_1_x2, 3: unID=unID+1HOTSPOT2 pos_1_x2, pos_1_y-diam_arm_1, unID, pos_1_x2, 2: unID=unID+1HOTSPOT2 0, 0, unID, pos_1_y, 1+128: unID=unID+1HOTSPOT2 0, -1, unID, pos_1_y, 3: unID=unID+1HOTSPOT2 0, pos_1_y, unID, pos_1_y, 2: unID=unID+1HOTSPOT2 maxrad_1+diam_arm_1,maxrad_1+diam_arm_1, unID, pos_1_yt, 1+128: unID=unID+1HOTSPOT2 maxrad_1+diam_arm_1,maxrad_1+diam_arm_1-1, unID, pos_1_yt, 3: unID=unID+1HOTSPOT2 maxrad_1+diam_arm_1,maxrad_1+diam_arm_1+pos_1_yt, unID, pos_1_yt, 2: unID=unID+1HOTSPOT2 diam_arm_1, diam_arm_1, unID, maxrad_1, 1+128: unID=unID+1HOTSPOT2 diam_arm_1, diam_arm_1-1, unID, maxrad_1, 3: unID=unID+1HOTSPOT2 diam_arm_1, diam_arm_1+maxrad_1, unID, maxrad_1, 2: unID=unID+1HOTSPOT2 diam_arm_1, diam_arm_1, unID, maxrad_1, 1+128: unID=unID+1HOTSPOT2 diam_arm_1, diam_arm_1-1, unID, maxrad_1, 3: unID=unID+1HOTSPOT2 diam_arm_1, diam_arm_1+maxrad_1, unID, maxrad_1, 2: unID=unID+1HOTSPOT2 0, 0,unID: unID=unID+1HOTSPOT2 maxrad_1+diam_arm_1,maxrad_1+diam_arm_1, unIDsn = 1r_arm_1 = diam_arm_1/2centr_1 = maxrad_1+diam_arm_1if show_arm = 0 then gosub "solid"if show_arm = 1 then gosub "wire"LINE_PROPERTY 2frame_fill = 1+2PEN pen_1FILL fill_1POLY2_B NSP/3, frame_fill,fill_1_pen, fill_1_background_pen,USE(NSP)for i = 1 to NSP step 6	if NSP>EPS then x1 = get(1)	if NSP>EPS then y1 = get(1)	if NSP>EPS then sn1 = get(1)	if NSP>EPS then x2 = get(1)	if NSP>EPS then y2 = get(1)	if NSP>EPS then sn2 = get(1)	if sn1=sn and sn2=sn then		hotline2 x1,y1,x2,y2,unID: unID=unID+1		if show_hotspot then			hotspot2 x1,y1,unID: unID=unID+1			hotspot2 x2,y2,unID: unID=unID+1		endif	endifnext iif show_ended then gosub "ended"end"solid":	put centr_1,0,sn	put pos_1_x1,0,sn	put pos_1_x1,diam_arm_1,sn	put centr_1,diam_arm_1,sn	put centr_1,centr_1,900+sn	put 0,-90,4000+sn	put diam_arm_1,centr_1,sn	put diam_arm_1,pos_1_y-centr_1,sn	put centr_1,pos_1_y-centr_1,900+sn	put 0,-90,4000+sn	put centr_1,pos_1_y-diam_arm_1,sn	put pos_1_x2,pos_1_y-diam_arm_1,sn	put pos_1_x2,pos_1_y,sn	put centr_1,pos_1_y,sn	put centr_1,pos_1_y-centr_1,900+sn	put 0,90,4000+sn	put 0,centr_1,sn	put centr_1,centr_1,900+sn	put 0,90,4000+snreturn"wire":	put pos_1_x1,r_arm_1,sn	put centr_1,r_arm_1,sn	put centr_1,centr_1,900+sn	put 0,-90,4000+sn	put r_arm_1,centr_1,sn	put r_arm_1,pos_1_y-centr_1,sn	put centr_1,pos_1_y-centr_1,900+sn	put 0,-90,4000+sn	put centr_1,pos_1_y-r_arm_1,sn	put pos_1_x2,pos_1_y-r_arm_1,snreturn"ended":	if NSP>0 then tt = max(GET(NSP))	HOTSPOT2 pos_1_x1,r_arm_1, unID, ang_1_ended, 6: unID=unID+1	HOTSPOT2 pos_1_x1+0.9*l_ended,0, unID, ang_1_ended, 4: unID=unID+1	HOTSPOT2 pos_1_x1+0.9*l_ended*COS(ang_1_ended), r_arm_1+0.9*l_ended*SIN(ang_1_ended), unID,ang_1_ended, 5: unID=unID+1	if show_arm = 1 then		put pos_1_x1,r_arm_1,600+sn	else		if ang_1_ended<EPS then			put pos_1_x1,0,600+sn		else			put pos_1_x1,diam_arm_1,600+sn		endif	endif	put l_ended,ang_1_ended,200+sn	frame_fill = 1+2	POLY2_ NSP/3, frame_fill,GET(NSP)	HOTSPOT2 pos_1_x2,pos_1_y-r_arm_1, unID, ang_2_ended, 6: unID=unID+1	HOTSPOT2 pos_1_x2+0.9*l_ended,0, unID, ang_2_ended, 4: unID=unID+1	HOTSPOT2 pos_1_x2+0.9*l_ended*COS(ang_2_ended), pos_1_y-r_arm_1+0.9*l_ended*SIN(ang_2_ended), unID,ang_2_ended, 5: unID=unID+1	if show_arm = 1 then		put pos_1_x2,pos_1_y-r_arm_1,600+sn	else		if ang_2_ended>EPS then			put pos_1_x2,pos_1_y,600+sn		else			put pos_1_x2,pos_1_y-diam_arm_1,600+sn		endif	endif	put l_ended,ang_2_ended,200+sn	frame_fill = 1+2	POLY2_ NSP/3, frame_fill,GET(NSP)return