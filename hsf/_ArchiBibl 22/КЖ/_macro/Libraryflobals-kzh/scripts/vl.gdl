values{2} "show_direction" ishow_direction, tshow_directionvalues{2} "show_type_2d" ishow_type, tshow_typevalues{2} "show_type_3d" ishow_type, tshow_typevalues{2} "show_constr" ishow_constr, tshow_constrvalues{2} "show_layer" ishow_layer, tshow_layerif not(overr_diam) then	LOCK "diam_mat", "diam_cpen", "diam_rpen", "diam_hiden", "diam_inx"endifif not(overr_area) then	parameters area_lvl = 1	LOCK "area_mat", "area_lvl"else	values "area_lvl" range[1, 10]	if auto_area then		if GLOB_MODPAR_NAME = "area_end" or GLOB_MODPAR_NAME = "area_start" or GLOB_MODPAR_NAME = "area_lvl"  then			diff_area = abs(area_start - area_end) / area_lvl			summ_area = area_start			dim area_t[]			for i=1 to area_lvl				area_t[i] = summ_area				summ_area = summ_area + diff_area 			next i			area_t[area_lvl] = area_end			parameters area_arr = area_t		endif		LOCK "area_arr"	else		area_start = area_arr[1]		area_end = area_arr[10]		parameters area_end = area_end, area_start = area_start		LOCK "area_start", "area_end"	endifendif