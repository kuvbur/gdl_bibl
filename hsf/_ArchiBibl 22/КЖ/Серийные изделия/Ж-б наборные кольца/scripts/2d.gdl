pen gs_cont_penmask=7fill gs_fill_typejt = 1+16hotspot2 0,0,unID : unID=unID+1if type_elem=WALL or type_elem=WALL_HOLE then	put 0,0,900+jt	put radius_1,360,4000+jt	put radius_1-tube_thk,360,4000+jt	hotspot2 0,radius_1,unID : unID=unID+1	hotspot2 radius_1,0,unID : unID=unID+1	hotspot2 0,-radius_1,unID : unID=unID+1	hotspot2 -radius_1,0,unID : unID=unID+1	hotspot2 0,radius_1-tube_thk,unID : unID=unID+1	hotspot2 radius_1-tube_thk,0,unID : unID=unID+1	hotspot2 0,-radius_1+tube_thk,unID : unID=unID+1	hotspot2 -radius_1+tube_thk,0,unID : unID=unID+1	hotarc2 0,0,radius_1,0,180,unID : unID=unID+1	hotarc2 0,0,radius_1,180,360,unID : unID=unID+1	hotarc2 0,0,radius_1-tube_thk,0,180,unID : unID=unID+1	hotarc2 0,0,radius_1-tube_thk,180,360,unID : unID=unID+1endifif type_elem = SLABDOWN then	put 0,0,900+jt	put radius_1,360,4000+jt	hotspot2 0,radius_1,unID : unID=unID+1	hotspot2 radius_1,0,unID : unID=unID+1	hotspot2 0,-radius_1,unID : unID=unID+1	hotspot2 -radius_1,0,unID : unID=unID+1	hotarc2 0,0,radius_1,0,180,unID : unID=unID+1	hotarc2 0,0,radius_1,180,360,unID : unID=unID+1endifif type_elem = SLABROAD then	put -sh/2,-dl/2,jt	put -sh/2,dl/2,jt	put sh/2,dl/2,jt	put sh/2,-dl/2,jt	put -sh/2,-dl/2,-1	put 0,0,900+jt	put radius_1,360,4000+jt	circle2 0,0,radius_2	hotspot2 -sh/2,-dl/2,unID : unID=unID+1	hotspot2 sh/2,-dl/2,unID : unID=unID+1	hotspot2 sh/2,dl/2,unID : unID=unID+1	hotspot2 -sh/2,dl/2,unID : unID=unID+1	hotspot2 0,radius_1,unID : unID=unID+1	hotspot2 radius_1,0,unID : unID=unID+1	hotspot2 0,-radius_1,unID : unID=unID+1	hotspot2 -radius_1,0,unID : unID=unID+1	hotarc2 0,0,radius_1,0,180,unID : unID=unID+1	hotarc2 0,0,radius_1,180,360,unID : unID=unID+1endifif type_elem=SLABUP then	put 0,0,900+jt	put radius_1,360,4000+jt	put 0,tube_thk,900+jt	put radius_2,360,4000+jt	hotspot2 0,radius_1,unID : unID=unID+1	hotspot2 radius_1,0,unID : unID=unID+1	hotspot2 0,-radius_1,unID : unID=unID+1	hotspot2 -radius_1,0,unID : unID=unID+1	hotspot2 0,radius_2+tube_thk,unID : unID=unID+1	hotspot2 radius_2+tube_thk,0,unID : unID=unID+1	hotspot2 0,-radius_2+tube_thk,unID : unID=unID+1	hotspot2 -radius_2+tube_thk,0,unID : unID=unID+1	hotspot2 0,tube_thk,unID : unID=unID+1	hotarc2 0,0,radius_1,0,180,unID : unID=unID+1	hotarc2 0,0,radius_1,180,360,unID : unID=unID+1	hotarc2 0,tube_thk,radius_2,0,180,unID : unID=unID+1	hotarc2 0,tube_thk,radius_2,180,360,unID : unID=unID+1endifif NSP>0 then POLY2_ NSP/3, 1+2+4, get(NSP)