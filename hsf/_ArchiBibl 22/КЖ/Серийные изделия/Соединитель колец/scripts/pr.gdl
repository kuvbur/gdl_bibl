if mat_pokr<>"Нет" then	tip_el = "30"	komment_t = "кв.м."	obozn_t = "см. примечания"	naen_t = "Покрытие " + mat_pokr	ves_t = 0	kol_vo_spec_t=area_pokr	gosub "out"endifif type_spec="Металл" then	subpos = subpos + "'" + pos	for i=1 to kol_vo_spec		ndet = 1		z_th = t1		z_at = max(hout, bb)		z_bt = min(hout, bb)		gosub "out_sheet"	next i	tip_el="45"	pos = subpos	kol_vo_spec_t=kol_vo_spec	obozn_t = obozn	naen_t = naen	komment=""	ves_t = ves	gosub "out"endifif type_spec="Изделие" then	tip_el="40"	kol_vo_spec_t=kol_vo_spec	ves_t = ves	obozn_t = obozn	naen_t = naen	komment=""	gosub "out"endifgoto "eend""out":	call "Спецификатор" parameters tip_el=tip_el,									subpos = subpos, 									pos = pos, 									obozn=obozn_t,									naen=naen_t,									ves=ves_t,									kol_vo_spec=kol_vo_spec_t,									komment=kommentreturn"out_sheet":	z_s_plit_obsh = z_at * z_bt	z_naen = "--" + str("%.0mm", z_th) + "x" + str("%.0mm", z_at) + "x" + str("%.0mm", z_bt)	z_ves_p = z_th * 7850	z_tip_profile = "-- " + str('%.0mm', z_th)	call "Спецификатор" parameters tip_el="20",									subpos = pos, 									pos = str("%.0",ndet),									tip_konstr="Прочее",									stal_gost="ГОСТ 27772-2015",									mater="С235",									obozn="ГОСТ 19903-74",									tip_profile=z_tip_profile,									dlin_prof=z_s_plit_obsh,									ves=z_ves_p, 									naen=z_naenreturn"eend":