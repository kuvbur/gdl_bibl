DEFINE STYLE "aa" "Arial", 30, 7, 0SET STYLE "aa"SHADOW OFF, OFFRESOL 5if is_serd then	start_x = 0 : start_y = 0 : start_z = 0	HOTSPOT start_x, start_y, start_z, unID, serd_height, 1+128: unID=unID+1	HOTSPOT start_x, start_y, start_z-1, unID, serd_height, 3: unID=unID+1	HOTSPOT start_x, start_y, start_z+serd_height, unID, serd_height, 2: unID=unID+1	start_x = bst : start_y = 0	HOTSPOT start_x, start_y, start_z, unID, serd_height, 1+128: unID=unID+1	HOTSPOT start_x, start_y, start_z-1, unID, serd_height, 3: unID=unID+1	HOTSPOT start_x, start_y, start_z+serd_height, unID, serd_height, 2: unID=unID+1	start_x = bst : start_y = max_t 	HOTSPOT start_x, start_y, start_z, unID, serd_height, 1+128: unID=unID+1	HOTSPOT start_x, start_y, start_z-1, unID, serd_height, 3: unID=unID+1	HOTSPOT start_x, start_y, start_z+serd_height, unID, serd_height, 2: unID=unID+1	start_x = 0 : start_y = max_t 	HOTSPOT start_x, start_y, start_z, unID, serd_height, 1+128: unID=unID+1	HOTSPOT start_x, start_y, start_z-1, unID, serd_height, 3: unID=unID+1	HOTSPOT start_x, start_y, start_z+serd_height, unID, serd_height, 2: unID=unID+1	hotspot bst/2,max_t/2,0	hotspot 0,max_t/2,0	hotspot bst,max_t/2,0	hotspot bst/2,0,0	hotspot bst/2,max_t,0	hotspot 0,0,0	pen p_st	material "!Бетон вспомогательный со штриховкой"	SECT_ATTRS f_st, 0, p_f_st, p_sech_st	add -0.001,-0.001,0	BRICK bst+0.002, max_t+0.002, serd_height	del 1	endendifstart_x = ac_wall_length/2 : start_y = otstup+max_t : start_z = 0 : gosub "hl_down"start_x = ac_wall_length/2 : start_y = otstup : start_z = 0 : gosub "hl_down"start_x = ac_wall_length/2 : start_y = otstup+max_t : start_z = ac_wall_height : gosub "hl_up"start_x = ac_wall_length/2 : start_y = otstup : start_z = ac_wall_height : gosub "hl_up"group "sten"	add t_konop_left+otstup_left, otstup, otstup_down	if left_str = "Сердечник" then		start_x = 0 : start_y = 0 : start_z = 0 : gosub "hl_down_left"		start_x = 0 : start_y = 0 : start_z = ac_wall_height_nw : gosub "hl_up_left"		addz -l_down_left		pen p_st		material "!Бетон вспомогательный со штриховкой"		SECT_ATTRS f_st, 0, p_f_st, p_sech_st		add -0.001,-0.001,0		BRICK bst+0.002, max_t+0.002, h_serd_1		del 1		addz l_down_left	endif	if left_str <> "Нет" and t_konop_left>0 then		addx -t_konop		material "!Минвата"		SECT_ATTRS sh_kon, 0, p_f_st, p_sech_st		BRICK t_konop, max_t, ac_wall_height_nw	endif	del top	material "!Бетон вспомогательный со штриховкой"	SECT_ATTRS f_st, 0, p_f_st, p_sech_st	add (left_str = "Сердечник")*bst + t_konop_left + otstup_left, otstup, otstup_down	BRICK l_st, max_t, t_rastv		if show_vert then		for i=shag_vert to ac_wall_height_nw-t_konop step shag_vert			lin_ -t_konop_left,-diam_arm_vert/2,i,l_wall,-diam_arm_vert/2,i			lin_ -t_konop_left,max_t+diam_arm_vert/2,i,l_wall,max_t+diam_arm_vert/2,i		next i		for i=shag_vert to l_wall step shag_vert			lin_ i,-3*diam_arm_vert/2,0,i,-3*diam_arm_vert/2,ac_wall_height_nw			lin_ i,max_t+3*diam_arm_vert/2,0,i,max_t+3*diam_arm_vert/2,ac_wall_height_nw		next i	endif	if show_gor then		for i=shag_gor to ac_wall_height_nw-t_konop step shag_gor			for j=20/1000 to max_t-20/1000 step 50/1000				lin_ -t_konop_left,j,i+diam_arm_gor/2,l_wall+max_t*(right_str = "Пересечение"),j,i+diam_arm_gor/2			next j			for j=50/1000 to l_wall+max_t*(right_str = "Пересечение")-50/1000 step 50/1000				lin_ j,-5/1000,i-diam_arm_gor/2, j,max_t+5/1000,i-diam_arm_gor/2			next j		next i	endif	material "!Краска-06"	fst = (l_wall - (int(l_wall/shag_krep)-1)*shag_krep)/2	for i=fst to l_wall step shag_krep		addx i		if mirr_in then			addx l_ug			rotz 180 			addy -max_t*2-wt		endif		rasp = 2 : if is_krep_down then gosub "ug_niz"		rasp = 3 : if is_krep_up then gosub "ug_niz"		if mirr_in then del 3		if mirr_out then			addx l_ug			rotz 180 			addy wt		endif		rasp = 1 : if is_krep_down then gosub "ug_niz"		rasp = 4 : if is_krep_up then gosub "ug_niz"		if mirr_out then del 3		del 1	next i	del top	material "!Минвата"	SECT_ATTRS sh_kon, 0, p_f_st, p_sech_st	add (left_str = "Сердечник")*bst + t_konop_left + otstup_left, otstup, ac_wall_height_nw - t_konop+otstup_down	BRICK l_st, max_t, t_konop	del top	add ac_wall_length-t_konop_right-otstup_right, otstup, otstup_down	if right_str = "Сердечник" then		start_x = 0 : start_y = 0 : start_z = 0 : gosub "hl_down_right"		start_x = 0 : start_y = 0 : start_z = ac_wall_height_nw : gosub "hl_up_right"		addx -bst		addz -l_down_right		material "!Бетон вспомогательный со штриховкой"		SECT_ATTRS f_st, 0, p_f_st, p_sech_st		add -0.001,-0.001,0		BRICK bst+0.002, max_t+0.002, h_serd_2		del 1		addz l_down_right		addx bst	endif		if left_str <> "Нет" and t_konop_right>0 then		material "!Минвата"		SECT_ATTRS sh_kon, 0, p_f_st, p_sech_st		BRICK t_konop, max_t, ac_wall_height_nw	endifendgroupgroup "cutting body"	del top	dim cut_set[5]	cut_set[1]=0	! Lower Cut	cut_set[2]=0	! Upper Cut	cut_set[3]=0	! End Cuts	cut_set[4]=1	! Window/Door	cut_set[5]=0	! Connecting Walls	call "Wall Cutting Body" parameters ref_thickness=max_t*2, gs_mat=base_mat, cut_set=cut_set,										ac_accessory_debug=0, gs_gdl_debug=0,										ac_wall_height_nw=ac_wall_height_nw, ac_wall_radius=ac_wall_radius, ac_wall_length=ac_wall_length,										ac_wall_resol=ac_wall_resol, ac_refside=ac_refside, ac_angles=ac_angles, ac_side_poly=ac_side_poly,										ac_top_poly=ac_top_poly, ac_bot_poly=ac_bot_poly,										ac_wd_poly=ac_wd_poly, ac_curved_wall=ac_curved_wall, ac_cont_pen=ac_cont_pen,										bot_ext=0, top_ext=0endgroup!!------------------!!!! Solid Operations !!!!------------------!!wall_1=subgroup("sten","cutting body")placegroup wall_1!placegroup "sten"basevert 0, 0, 0vert 1, 0, 0vert 0, 1, 0vert 0, 0, 1coor 2, -1, -2, -3, -4body 1end"ug_niz":	type_krep = "ug"	if rasp = 1 then		rotx 90		roty 180		mulx -1		if plast_out_down then 			type_krep = "plast"			rotx -90			addx (l_ug-l_plast)/2			addz h_plast-zas_plast		endif	endif	if rasp = 2 then		rotx 90		addz -max_t		if plast_in_down then 			type_krep = "plast"			rotx -90			addx (l_ug-l_plast)/2			addz h_plast-zas_plast		endif	endif	if rasp = 3 then		addy max_t		addz ac_wall_height_nw		if plast_in_up then 			type_krep = "plast"			addx (l_ug-l_plast)/2			addz zas_plast		endif	endif	if rasp = 4 then		muly -1		addz ac_wall_height_nw		if plast_out_up then 			type_krep = "plast"			addx (l_ug-l_plast)/2			addz zas_plast		endif	endif	roty 90	if type_krep = "ug" then		PRISM_ 7, l_ug,				0,0,15,				ws,0,15,				ws,wt,15,				wt,wt,15,				wt,ds,15,				0,ds,15,				0,0,-1	else		PRISM_ 5, l_plast,				0,0,15,				h_plast,0,15,				h_plast,t_plast,15,				0,t_plast,15,				0,0,-1	endif	if rasp = 1 then		if plast_out_down then del 3		del 4	else		if rasp = 2 and plast_in_down then del 3		if rasp = 3 and plast_in_up then del 2		if rasp = 4 and plast_out_up then del 2		del 3	endifreturn"hl_down_left":	HOTSPOT start_x, start_y, start_z, unID, l_down_left, 1+128: unID=unID+1	HOTSPOT start_x, start_y, start_z+1, unID, l_down_left, 3: unID=unID+1	HOTSPOT start_x, start_y, start_z-l_down_left, unID, l_down_left, 2: unID=unID+1return"hl_down_right":	HOTSPOT start_x, start_y, start_z, unID, l_down_right, 1+128: unID=unID+1	HOTSPOT start_x, start_y, start_z+1, unID, l_down_right, 3: unID=unID+1	HOTSPOT start_x, start_y, start_z-l_down_right, unID, l_down_right, 2: unID=unID+1return"hl_up_left":	HOTSPOT start_x, start_y, start_z, unID, l_up_left, 1+128: unID=unID+1	HOTSPOT start_x, start_y, start_z-1, unID, l_up_left, 3: unID=unID+1	HOTSPOT start_x, start_y, start_z+l_up_left, unID, l_up_left, 2: unID=unID+1return"hl_up_right":	HOTSPOT start_x, start_y, start_z, unID, l_up_right, 1+128: unID=unID+1	HOTSPOT start_x, start_y, start_z-1, unID, l_up_right, 3: unID=unID+1	HOTSPOT start_x, start_y, start_z+l_up_right, unID, l_up_right, 2: unID=unID+1return"hl_down":	HOTSPOT start_x, start_y, start_z, unID, otstup_down, 1+128: unID=unID+1	HOTSPOT start_x, start_y, start_z-1, unID, otstup_down, 3: unID=unID+1	HOTSPOT start_x, start_y, start_z+otstup_down, unID, otstup_down, 2: unID=unID+1return"hl_up":	HOTSPOT start_x, start_y, start_z, unID, otstup_up, 1+128: unID=unID+1	HOTSPOT start_x, start_y, start_z+1, unID, otstup_up, 3: unID=unID+1	HOTSPOT start_x, start_y, start_z-otstup_up, unID, otstup_up, 2: unID=unID+1return