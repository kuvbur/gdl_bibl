 GLOB_STRUCTURE_DISPLAY=0 then		show_zhb = 1		show_pokr = 1	endif	if GLOB_STRUCTURE_DISPLAY=1 then		show_zhb = 1		show_pokr = 0	endif	if GLOB_STRUCTURE_DISPLAY=2 then		show_zhb = 1		show_pokr = 0	endif	chStoryDist = GLOB_CH_STORY_DIST	IF gs_StorySensitive = 0 THEN chStoryDist = 0	if chStoryDist > EPS then		t_symb = type_symb_2	else		t_symb = type_symb_1	endif	PTM = GLOB_SCALE / 1000	! Convert paper size to model size	k_str = l_stup*0.6!5 * PTM	k_ogr = l_stup*0.3!3 * PTMendifif el_version = 2 then	success = LIBRARYGLOBAL ("Libraryflobals-pokr", "is_use_mview", is_use_mview)	success = LIBRARYGLOBAL ("Libraryflobals-pokr", "is_cfg_mview", is_cfg_mview)	read_flag = 0	if success > 0 then		if is_use_mview and is_cfg_mview then			read_flag = 1			else			read_flag = 0		endif	endif	if read_flag then		n_sloi_r = 0		CALL "macro_read_tip" PARAMETERS tip_pol = tip_pol_pl RETURNED_PARAMETERS n_sloi_r, tolsh_r, tolsh_min_r, strih_r, pokaz_r, mat_r		if n_sloi_r > 0 then			n_sloi = n_sloi_r			mat_stup = mat_r			h_p = 0			for i = 1 to n_sloi_r				h_p = h_p + tolsh_r[i]			next i			strih = strih_r			tolsh = tolsh_r			PARAMETERS mat_stup = mat_stup			PARAMETERS t_pol_plosh = h_p			PARAMETERS n_sloi = n_sloi			PARAMETERS strih = strih			PARAMETERS tolsh = tolsh		endif		n_sloi_r = 0		CALL "macro_read_tip" PARAMETERS tip_pol = tip_pol_l RETURNED_PARAMETERS n_sloi_r, tolsh_r, tolsh_min_r, strih_r, pokaz_r, mat_r		if n_sloi_r > 0 then			h_p = 0			for i = 1 to n_sloi_r				h_p = h_p + tolsh_r[i]			next i			PARAMETERS h_pirog_l = h_p			PARAMETERS t_pol_l = h_p		endif		n_sloi_r = 0		CALL "macro_read_tip" PARAMETERS tip_pol = tip_pol_niz RETURNED_PARAMETERS n_sloi_r, tolsh_r, tolsh_min_r, strih_r, pokaz_r, mat_r		if n_sloi_r > 0 then			h_p = 0			for i = 1 to n_sloi_r				h_p = h_p + tolsh_r[i]			next i			PARAMETERS t_pol_niz = h_p		endif		n_sloi_r = 0		CALL "macro_read_tip" PARAMETERS tip_pol = tip_pol_verh RETURNED_PARAMETERS n_sloi_r, tolsh_r, tolsh_min_r, strih_r, pokaz_r, mat_r		if n_sloi_r > 0 then			h_p = 0			for i = 1 to n_sloi_r				h_p = h_p + tolsh_r[i]			next i			PARAMETERS t_pol_verh = h_p		endif	endifendiflt = t_l/sin(ang_l) !Толщина жб части по Xl_pol_t = t_pol_l/sin(ang_l) !Отступ на отделкуl_cut = cos(angle_cut) * b_ldx_cut = sin(angle_cut) * b_l!Определение координат ж/бif issecondm then 	h_1m = h_plosh	ns_1m = int(h_1m/h_stup)	l_1m = ns_1m * l_stup	n_stup = ns_1m	l_l_t = l_1m	h_l_t = h_1m	dl_zhb_verh_t = 0	dl_zhb_niz_t = dl_zhb_niz	isendstuppokr_t = 1	itype_pr_niz_t = itype_pr_niz	itype_pr_verh_t = 3		gosub "marsh"	l_zhb_niz_1m = l_zhb_niz	peres_niz_1_1m = peres_niz_x	peres_niz_2_1m = peres_niz_2	peres_verh_1m = peres_verh_x	l_zhb_verh_1m = l_zhb_verh+t_pol_l*2*is_eq_bet	h_2m = h_l - h_plosh	ns_2m = int(h_2m/h_stup)	l_2m = ns_2m * l_stup	n_stup = ns_2m	l_l_t = l_2m	h_l_t = h_2m	dl_zhb_verh_t = dl_zhb_verh	dl_zhb_niz_t = peres_verh_1m - l_1m + l_stup	isendstuppokr_t = isendstuppokr	itype_pr_niz_t = 3	itype_pr_verh_t = itype_pr_verh	gosub "marsh"	l_zhb_niz_2m = l_zhb_niz	peres_niz_1_2m = peres_niz_x	peres_niz_2_2m = peres_niz_2	peres_verh_2m = peres_verh_x		l_zhb_verh_2m = l_zhb_verh else	h_1m = h_l	ns_1m = int(h_1m/h_stup)	l_1m = ns_1m * l_stup	n_stup = ns_1m	l_l_t = l_1m	h_l_t = h_1m	dl_zhb_verh_t = dl_zhb_verh	dl_zhb_niz_t = dl_zhb_niz	isendstuppokr_t = isendstuppokr	itype_pr_niz_t = itype_pr_niz	itype_pr_verh_t = itype_pr_verh	gosub "marsh"	l_zhb_niz_1m = l_zhb_niz	peres_niz_1_1m = peres_niz_x	peres_niz_2_1m = peres_niz_2	peres_verh_1m = peres_verh_x	l_zhb_verh_1m = l_zhb_verh	l_zhb_niz_2m = 0	peres_niz_1_2m = 0	peres_niz_2_2m = 0	peres_verh_2m = 0	l_zhb_verh_2m = 0endifh_ogr = 1.2!----------- Площади -----------------s_pol_pl_1 = 0 : if isplosh then s_pol_pl_1 = l_2_plosh*b_2_ploshs_pol_pl_2 = 0 : if issecondm then s_pol_pl_2 = (b_zazor+b_l*2)*l_ploshs_pol_pl = s_pol_pl_1 + s_pol_pl_2 + s_zap*(s_pol_pl_1 + s_pol_pl_2)s_pol_verh = dl_zhb_verh * b_l + s_zap*(dl_zhb_niz * b_l)s_pol_niz = dl_zhb_niz * b_l + s_zap*(dl_zhb_niz * b_l)s_pol_l = (ns_1m+ns_2m+2)*(h_stup*b_l + l_stup*b_l) + s_zap*((ns_1m+ns_2m+2)*(h_stup*b_l + l_stup*b_l))s_pot_l = s_pol_pl+(ns_1m+ns_2m+2)*(SQR(h_stup*h_stup + l_stup*l_stup))*b_ls_pot_l = s_pot_l+s_pot_l*s_zapperim_pl_1 = 0 : if isplosh then perim_pl_1 = (l_2_plosh+b_2_plosh)*2perim_pl_2 = 0 : if issecondm then perim_pl_2 = (b_zazor+b_l*2+l_plosh)*2perim_pl = perim_pl_1+perim_pl_2+s_zap*(perim_pl_1+perim_pl_2)s_pol = s_pol_pl_1 + s_pol_pl_2 + s_pol_verh + s_pol_niz + (l_zhb_verh_1m+l_zhb_verh_2m-l_stup*2)*b_l+b_zazor*(max(l_zhb_verh_1m, l_zhb_verh_2m)-l_stup*2)goto 20"marsh":	!======================================================================================================================================================================	!============= Ж/б ====================	!--------- Первая ступень --------------	!Опирание на плиту	if itype_pr_niz_t = 1 then		!Точка пересечения нижней плиты и низа марша		x1 = -1 : x2 = l_l_t + 1 : y1 = -t_pol_niz : y2 = y1  !Линия верха нижней плиты		x3 = l_stup+t_pol_l+lt : x4 = l_stup*(n_stup+2)+t_pol_l+lt : y3 = h_stup-t_pol_l : y4 = h_stup*(n_stup+2)-t_pol_l  !Линия низа марша		gosub "getXY"		peres_niz_x = x		peres_niz_y = -t_pol_niz	else		!Сбоку к плите		if itype_pr_niz_t = 2 then			t_pol_niz_t = t_pol_niz			t_pl_niz_t = t_pl_niz		else			t_pol_niz_t = t_pol_plosh			t_pl_niz_t = t_plosh		endif		!Точка пересечения низа нижней плиты и низа марша		x1 = -3 : x2 = 3 : y1 = - t_pol_niz_t - t_pl_niz_t : y2 = y1  !Линия низа нижней плиты		x3 = l_stup+t_pol_l+lt : x4 = l_stup*(n_stup+2)+t_pol_l+lt : y3 = h_stup-t_pol_l : y4 = h_stup*(n_stup+2)-t_pol_l  !Линия низа марша		gosub "getXY"		peres_niz_x = x		peres_niz_y = - t_pol_niz_t - t_pl_niz_t		if peres_niz_x > 0 then			l_zhb_niz = - dl_zhb_niz_t		else			l_zhb_niz = peres_niz_x - dl_zhb_niz_t		endif		!Точка пересечения верха нижней плиты и верха марша		x1 = -6 : x2 = 6 : y1 = -t_pol_niz_t : y2 = -t_pol_niz_t !Линия низа нижней плиты		x3 = l_stup + t_pol_l : x4 = l_stup * (n_stup + 2) + t_pol_l : y3 = h_stup - t_pol_l : y4 = h_stup * (n_stup + 2) - t_pol_l  !Линия низа марша		gosub "getXY"		peres_niz_2 = min(t_pol_l,x)		if peres_niz_2 < l_zhb_niz then peres_niz_2 = l_zhb_niz	endif	!--------- Последняя ступень --------------	!Сбоку к плите	if itype_pr_verh_t = 2 then		t_pol_verh_t = t_pol_verh		t_pl_verh_t = t_pl_verh	else		t_pol_verh_t = t_pol_plosh		t_pl_verh_t = t_plosh	endif	if isendstuppokr_t then		t_pol_l_t = t_pol_verh_t	else		t_pol_l_t = t_pol_l	endif	!Точка пересечения низа площадки и низа верхней плиты	x1 = -1 : x2 = l_l_t + 1 : y1 = h_l_t - t_pol_verh_t - t_pl_verh_t : y2 = y1  !Линия низа площадки	x3 = l_stup+t_pol_l+lt : x4 = l_stup*(n_stup+2)+t_pol_l+lt : y3 = h_stup-t_pol_l : y4 = h_stup*(n_stup+2)-t_pol_l  !Линия низа марша	gosub "getXY"	peres_verh_x = x	if x < l_l_t then		l_zhb_verh = l_l_t + dl_zhb_verh_t	else		l_zhb_verh = peres_verh_x + dl_zhb_verh_t	endif	!======================================================================================================================================================================return"getXY":    x=(((x1*y2-x2*y1)*(x4-x3)-(x3*y4-x4*y3)*(x2-x1))/((y1-y2)*(x4-x3)-(y3-y4)*(x2-x1)))    y=(((y3-y4)*x-(x3*y4-x4*y3))/(x4-x3))	x = - x	tt = x : gosub "getRound" : x = tt	tt = y : gosub "getRound" : y = ttreturn"getRound":	tt = round_int(tt*100)/100return20:                                                              