if GLOB_VIEW_TYPE = 9 then show_pokr = 0scode = 15!hotspot 0,0,0pen gs_cont_pendel topif is_pot then mat_l_niz=VOTSGROUP "first_m"	del top	rotx 90	addx -t_pol_l*2*is_eq_bet	n_stup = ns_1m	l_l_t = l_1m	h_l_t = h_1m	if issecondm then 		dl_zhb_verh_t = 0		dl_zhb_niz_t = dl_zhb_niz		isendstuppokr_t = 1		itype_pr_niz_t = itype_pr_niz		itype_pr_verh_t = 3	else		dl_zhb_verh_t = dl_zhb_verh		dl_zhb_niz_t = dl_zhb_niz		isendstuppokr_t = isendstuppokr		itype_pr_niz_t = itype_pr_niz		itype_pr_verh_t = itype_pr_verh	endif	l_zhb_niz = l_zhb_niz_1m	peres_niz_x = peres_niz_1_1m	peres_niz_2 = peres_niz_2_1m	peres_verh_x = peres_verh_1m	l_zhb_verh = l_zhb_verh_1mif show_ogr and show_pokr then	add -l_stup/2,0,-b_l		lin_ 0,h_ogr,0,0,0,0		lin_ l_stup*n_stup, h_l_t+1.2,0, l_stup*n_stup, h_l_t,0		lin_ 0,h_ogr,0, l_stup*n_stup, h_l_t+h_ogr,0		lin_ 0,h_ogr/2,0, l_stup*n_stup, h_l_t+h_ogr/2,0	del 1endif	gosub "zhb"	SECT_ATTRS sht_zhb, 19, 1, pen_zhb, ltype	material mat_l_niz	PRISM_  NSP/3, -b_l, get(NSP)	if t_pol_l > 0 and show_pokr then		gosub "pokr"		SECT_ATTRS sht_stup, 19, 1, pen_stup		material mat_stup		PRISM_  NSP/3, -b_l, get(NSP)	endifENDGROUPGROUP "second_m"	if issecondm then 		del top		rotx 90		addz -(b_l + b_zazor)		addy h_plosh		addx (int(h_plosh/h_stup)-1)*l_stup		mulx -1		n_stup = ns_2m		l_l_t = l_2m		h_l_t = h_2m		dl_zhb_verh_t = dl_zhb_verh		dl_zhb_niz_t = peres_verh_1m - l_1m + l_stup		isendstuppokr_t = isendstuppokr		itype_pr_niz_t = 3		itype_pr_verh_t = itype_pr_verh		l_zhb_niz = l_zhb_niz_2m		peres_niz_x = peres_niz_1_2m		peres_niz_2 = peres_niz_2_2m		peres_verh_x = peres_verh_2m		l_zhb_verh = l_zhb_verh_2mif show_ogr and show_pokr then	add -l_stup/2,0,0		lin_ 0,h_ogr,0, 0,0,0		lin_ l_stup*n_stup, h_l_t+h_ogr,0, l_stup*n_stup, h_l_t,0		lin_ 0,h_ogr,0, l_stup*n_stup, h_l_t+h_ogr,0		lin_ 0,h_ogr/2,0, l_stup*n_stup, h_l_t+h_ogr/2,0	del 1endif		gosub "zhb"                                                                  		SECT_ATTRS sht_zhb, 19, 1, pen_zhb, ltype		material mat_l_niz		PRISM_  NSP/3, -b_l, get(NSP)		if t_pol_l > 0 and show_pokr then			gosub "pokr"			SECT_ATTRS sht_stup, 19, 1, pen_stup			material mat_stup			PRISM_ NSP/3, -b_l, get(NSP)		endif	endifENDGROUPGROUP "plosh"	if issecondm then		del top		mulx -1		rotz 180		addz h_plosh - t_pol_plosh		addy - 2 * b_l - b_zazorif show_ogr and show_pokr then	lin_ l_1m-l_stup/2,b_l,h_ogr+t_pol_plosh, l_1m-l_stup/2,b_l+b_zazor,h_ogr+t_pol_plosh	lin_ l_1m-l_stup/2,b_l,h_ogr/2+t_pol_plosh, l_1m-l_stup/2,b_l+b_zazor,h_ogr/2+t_pol_ploshendif		addx l_zhb_verh_1m-t_pol_l*2*is_eq_bet		gosub "plosh"		SECT_ATTRS sht_zhb, 19, 1, pen_zhb, ltype		material mat_l_niz			if t_pol_plosh > 0 and show_pokr then			PRISM_ NSP/3, -t_plosh, use(NSP)			material mat_stup			SECT_ATTRS sht_stup, 19, 1, pen_stup			PRISM_ NSP/3, t_pol_plosh, get(NSP)		else			PRISM_ NSP/3, -t_plosh, get(NSP)		endif	endifENDGROUPGROUP "plosh_2"	if isplosh then		del top		if issecondm then			rotz 180			addz h_l - t_pol_plosh			addx -l_1m+l_2m			addy - 2 * b_l - b_zazor			HOTSPOT 0, b_2_plosh, 0, unID, l_2_plosh, 1+128: unID=unID+1			HOTSPOT l_2_plosh, b_2_plosh, 0, unID, l_2_plosh, 2: unID=unID+1			HOTSPOT l_2_plosh, 0, 0, unID, b_2_plosh, 1+128: unID=unID+1			HOTSPOT l_2_plosh, b_2_plosh, 0, unID, b_2_plosh, 2: unID=unID+1		else			addx l_1m - l_stup			addz h_l - t_pol_plosh			HOTSPOT 0, b_2_plosh, 0, unID, l_2_plosh, 1+128: unID=unID+1			HOTSPOT l_2_plosh, b_2_plosh, 0, unID, l_2_plosh, 2: unID=unID+1			HOTSPOT l_2_plosh, 0, 0, unID, b_2_plosh, 1+128: unID=unID+1			HOTSPOT l_2_plosh, b_2_plosh, 0, unID, b_2_plosh, 2: unID=unID+1			l_zhb_verh_2m = l_zhb_verh_1m			l_2m = l_1m		endif		gosub "plosh_2"		SECT_ATTRS sht_zhb, 19, 1, pen_zhb, ltype		material mat_l_niz		if t_pol_plosh > 0 and show_pokr then			PRISM_ NSP/3, -t_plosh, use(NSP)			material mat_stup			SECT_ATTRS sht_stup, 19, 1, pen_stup			PRISM_ NSP/3, t_pol_plosh, get(NSP)		else			PRISM_ NSP/3, -t_plosh, get(NSP)		endif	endifENDGROUPif itype_pr_niz = 2 then	startx = peres_niz_1_1melse	startx = peres_niz_2_1mendifstarty = 0.5 * b_lHOTSPOT startx, starty,0, unID, dl_zhb_niz, 1+128: unID=unID+1HOTSPOT startx+1, starty,0, unID, dl_zhb_niz, 3: unID=unID+1HOTSPOT startx-dl_zhb_niz, starty,0, unID, dl_zhb_niz, 2: unID=unID+1if issecondm then		del top		startx = (h_plosh/h_stup)*l_stup - ((h_l-h_plosh)/h_stup)*l_stup		starty = 1.5 * b_l + b_zazor		HOTSPOT startx, starty,0, unID, h_l, 1+128: unID=unID+1		HOTSPOT startx, starty,h_l, unID, h_l, 2: unID=unID+1			startx = l_1m - l_2m - l_stup		starty = 1.5 * b_l + b_zazor		HOTSPOT startx, starty,h_l - t_pol_plosh, unID, dl_zhb_verh, 1+128: unID=unID+1		HOTSPOT startx+1, starty,h_l - t_pol_plosh, unID, dl_zhb_verh, 3: unID=unID+1		HOTSPOT startx-dl_zhb_verh, starty,h_l - t_pol_plosh, unID, dl_zhb_verh, 2: unID=unID+1		startx = (int(h_plosh/h_stup)-1)*l_stup		starty = b_l + b_zazor/2		HOTSPOT startx, starty,0, unID, h_plosh, 1+128: unID=unID+1		HOTSPOT startx, starty,h_plosh, unID, h_plosh, 2: unID=unID+1			HOTSPOT startx, starty,h_plosh, unID, l_plosh, 1+128: unID=unID+1		HOTSPOT startx+l_plosh, starty,h_plosh, unID, l_plosh, 2: unID=unID+1		PLACEGROUP "first_m"		PLACEGROUP "second_m"		PLACEGROUP "plosh"else		del top		startx = (h_l/h_stup)*l_stup		starty = 0.5 * b_l		HOTSPOT startx, starty, 0, unID, h_l, 1+128: unID=unID+1		HOTSPOT startx, starty, h_l, unID, h_l, 2: unID=unID+1		startx = peres_verh_1m		starty = 0.5 * b_l		HOTSPOT startx, starty,h_l - t_pol_plosh, unID, dl_zhb_verh, 1+128: unID=unID+1		HOTSPOT startx-1, starty,h_l - t_pol_plosh, unID, dl_zhb_verh, 3: unID=unID+1		HOTSPOT startx+dl_zhb_verh, starty,h_l - t_pol_plosh, unID, dl_zhb_verh, 2: unID=unID+1		PLACEGROUP "first_m"endifdel topif isplosh then PLACEGROUP "plosh_2"end"plosh":	!======================================================================================================================================================================	!============= Площадка ====================	put 0, 0, scode	put 0, b_l, scode	put l_zhb_niz+t_pol_l-t_pol_l*2*is_eq_bet, b_l, scode	put l_zhb_niz+t_pol_l-t_pol_l*2*is_eq_bet, b_l + b_zazor, scode	put 0, b_l + b_zazor, scode	put 0, 2* b_l + b_zazor, scode	put l_plosh + l_zhb_niz, 2* b_l + b_zazor, scode	put l_plosh + l_zhb_niz, 0, scode	!======================================================================================================================================================================return"plosh_2":	!======================================================================================================================================================================	!============= Площадка ====================	put (l_zhb_verh_2m-l_2m+l_stup), 0, scode	if b_2_plosh > b_l then		put (l_zhb_verh_2m-l_2m+l_stup), b_l, scode		put t_pol_l, b_l, scode		put t_pol_l, b_2_plosh, scode	else		put (l_zhb_verh_2m-l_2m+l_stup), b_2_plosh, scode	endif	put l_2_plosh, b_2_plosh, scode	put l_2_plosh, 0, scode	!======================================================================================================================================================================return"zhb":	!======================================================================================================================================================================	!============= Ж/б ====================	!--------- Первая ступень --------------	!Опирание на плиту	if itype_pr_niz_t = 1 then		put t_pol_l, -t_pol_niz, scode		peres_niz_y = -t_pol_niz	else		!Сбоку к плите		if itype_pr_niz_t = 2 then			t_pol_niz_t = t_pol_niz			t_pl_niz_t = t_pl_niz		else			t_pol_niz_t = t_pol_plosh			t_pl_niz_t = t_plosh		endif		peres_niz_y = - t_pol_niz_t - t_pl_niz_t		put peres_niz_x, peres_niz_y, scode		put l_zhb_niz, -t_pol_niz_t-t_pl_niz_t, scode		put l_zhb_niz, -t_pol_niz_t, scode		put peres_niz_2, -t_pol_niz_t, scode		put t_pol_l, -t_pol_l, scode	endif	!--------- Промежуточные ступени --------------	for i = 1 to n_stup-1		x = l_stup * i+t_pol_l		y = h_stup * i-t_pol_l		put x-l_stup, y, scode		put x, y, scode	next i	!--------- Последняя ступень --------------	!Сбоку к плите	if itype_pr_verh_t = 2 then		t_pol_verh_t = t_pol_verh		t_pl_verh_t = t_pl_verh	else		t_pol_verh_t = t_pol_plosh		t_pl_verh_t = t_plosh	endif	if isendstuppokr_t then		t_pol_l_t = t_pol_verh_t	else		t_pol_l_t = t_pol_l	endif	put l_stup*(n_stup-1)+t_pol_l, h_stup*n_stup-t_pol_l_t, scode	if not(isendstuppokr_t) then		put l_l_t, h_l_t - t_pol_l_t, scode		put l_l_t, h_l_t - t_pol_verh_t, scode	endif	put l_zhb_verh, h_l_t - t_pol_verh_t, scode	put l_zhb_verh, h_l_t - t_pol_verh_t - t_pl_verh_t, scode	put peres_verh_x, h_l_t - t_pol_verh_t - t_pl_verh_t, scode	put peres_niz_x, peres_niz_y, scode	for i = 0 to 1		th = -i * b_l		hotspot t_pol_l,-t_pol_l,th		hotspot peres_niz_2,-t_pol_niz_t,th		hotspot peres_niz_x,-t_pl_niz_t-t_pol_niz_t,th		hotspot l_zhb_niz,-t_pl_niz_t-t_pol_niz_t,th		hotspot l_zhb_niz,-t_pol_niz_t,th		hotspot l_l_t,h_l_t-t_pol_verh_t,th		hotspot l_l_t,h_l_t-t_pol_verh_t,th		hotspot peres_verh_x,h_l_t - t_pol_verh_t - t_pl_verh_t,th		hotspot l_zhb_verh,h_l_t - t_pol_verh_t - t_pl_verh_t,th		hotspot l_zhb_verh,h_l_t-t_pol_verh_t,th	next i	!======================================================================================================================================================================return"pokr":	!======================================================================================================================================================================	!============= Покрытие ====================	!--------- Первая ступень	!Опирание на плиту	if itype_pr_niz_t = 1 then		put t_pol_l, -t_pol_niz_t, scode		put 0, -t_pol_niz_t, scode		put 0, 0, scode	endif	!Сбоку к плите	if itype_pr_niz_t = 2 or itype_pr_niz_t = 3 then		put t_pol_l, -t_pol_l, scode		put peres_niz_2, -t_pol_niz_t, scode		put l_zhb_niz, -t_pol_niz_t, scode		put l_zhb_niz, 0, scode	endif	put 0, 0, scode	!--------- Верх покрытия ---------	for i = 1 to n_stup-1		y = h_stup * i		x = l_stup * i		put x-l_stup, y, scode		put x, y, scode	next i	put l_l_t-l_stup, h_stup*n_stup, scode	if isendstuppokr_t then		t_pol_l_t = t_pol_verh_t	else		t_pol_l_t = t_pol_l	endif	!--------- Поворачиваем вниз и вбок ---------	put l_zhb_verh, h_stup*n_stup, scode	put l_zhb_verh, h_stup*n_stup-t_pol_verh_t, scode	put l_zhb_verh, h_stup*n_stup-t_pol_verh_t, scode	put l_zhb_verh, h_stup*n_stup-t_pol_l_t, scode	put l_stup*(n_stup-1)+t_pol_l, h_stup*n_stup-t_pol_l_t, scode	!--------- Низ покрытия ---------	for i = n_stup-1 to 1 step -1		x = l_stup*(i)+t_pol_l		y = h_stup*(i)-t_pol_l		put x, y, scode		put x-l_stup, y, scode	next i	!--------- Поворачиваем вниз и вбок ---------	put t_pol_l, h_stup-t_pol_l, scode	put t_pol_l, 0, scode	!======================================================================================================================================================================return