PEN gs_cont_penFILL gs_fill_type! --- CHECK for VISIBILITY ---bShowCage = ((ZZYZX+EPS) > saftyLimit)bCageTopView = (not(bStorySensitive) | (bStorySensitive & ((cutplaneHeight+EPS) > sr_bl)))bShowPlatform = landingbShowRail = top_railif bStorySensitive then	if cutplaneHeight < ac_toplevel then bShowPlatform = 0	if cutplaneHeight < ac_toplevel then bShowRail = 0endif! --- HOTSPOTS ---unID=1HOTSPOT2 0,0,unID :unID=unID+1HOTSPOT2 a/2-4",0,unID :unID=unID+1HOTSPOT2 -a/2+4",0,unID :unID=unID+1if bShowCage then	HOTSPOT2 a/2,-1',unID :unID=unID+1	HOTSPOT2 -a/2,-1',unID :unID=unID+1	HOTSPOT2 a/2,-1'8 1/2",unID :unID=unID+1	HOTSPOT2 0,-a/2-0.515,unID :unID=unID+1	HOTSPOT2 0,-a/2-0.642,unID :unID=unID+1	HOTSPOT2 -a/2,-1'8 1/2",unID :unID=unID+1endif! --- EDITABLE HOTSPOTS ---if bShowPlatform then	HOTSPOT2 0,0,unID,lnd_w,1+128 :unID=unID+1	HOTSPOT2 0,-0.1,unID,lnd_w,3 :unID=unID+1	HOTSPOT2 0,lnd_w,unID,lnd_w,2 :unID=unID+1endif! --- EDITABLE HOTSPOTS ---POLY2_B 5,1+2+4,gs_fill_pen, gs_back_pen,	-a/2+4",-7"+sr_d/2,1,	-a/2+4",-7"-sr_d/2,1,	-a/2+4"-sr_w,-7"-sr_d/2,1,	-a/2+4"-sr_w,-7"+sr_d/2,1,	-a/2+4",-7"+sr_d/2,-1RECT2 -a/2+4"-h_w,-7"+sr_d/2-h_w,-a/2+4"-sr_w+h_w,-7"-sr_d/2+h_wPOLY2_B 5,1+2+4,gs_fill_pen, gs_back_pen,	a/2-4",-7"+sr_d/2,1,	a/2-4",-7"-sr_d/2,1,	a/2-4"+sr_w,-7"-sr_d/2,1,	a/2-4"+sr_w,-7"+sr_d/2,1,	a/2-4",-7"+sr_d/2,-1RECT2 a/2-4"+h_w,-7"+sr_d/2-h_w,a/2-4"+sr_w-h_w,-7"-sr_d/2+h_wPOLY2_B 4,1+2+4,gs_fill_pen, gs_back_pen,	-a/2+4"+h_w,-7"+rw/2,1,	-a/2+4"+h_w,-7"-rw/2,1,	a/2-4"-h_w,-7"-rw/2,1,	a/2-4"-h_w,-7"+rw/2,1! --- Safety Cage ---if bShowCage then	if bCageTopView then		POLY2_B 20,1+2+4,gs_fill_pen, gs_back_pen,			a/2-4"-f_w,0,1,			a/2-4",0,1,			a/2-4",-7"-sr_d/2,1,			a/2,-12",1,			a/2,-3'2"+1'1/2",1,			-a/2,-3'2"+1'1/2",1001,			-a/2,-12",1,			-a/2+4",-7"-sr_d/2,1,			-a/2+4",0,1,			-a/2+4"+f_w,0,1,			-a/2+4"+f_w,-h_w,1,			-a/2+4"+h_w,-h_w,1,			-a/2+4"+h_w,-7"-sr_d/2-h_w/2,1,			-a/2+h_w,-12"-h_w/2,1,			-a/2+h_w,-3'2"+1'1/2",1,			a/2-h_w,-3'2"+1'1/2",1001,			a/2-h_w,-12"-h_w/2,1,			a/2-4"-h_w,-7"-sr_d/2-h_w/2,1,			a/2-4"-h_w,-h_w,1,			a/2-4"-f_w,-h_w,1		POLY2_B 20,1+2+4,gs_fill_pen, gs_back_pen,			a/2-4"-f_w,0,1,			a/2-4",0,1,			a/2-4",-7"-sr_d/2,1,			a/2,-12",1,			a/2,-2'9"+1'1/2",1,			-a/2,-2'9"+1'1/2",1001,			-a/2,-12",1,			-a/2+4",-7"-sr_d/2,1,			-a/2+4",0,1,			-a/2+4"+f_w,0,1,			-a/2+4"+f_w,-h_w,1,			-a/2+4"+h_w,-h_w,1,			-a/2+4"+h_w,-7"-sr_d/2-h_w/2,1,			-a/2+h_w,-12"-h_w/2,1,			-a/2+h_w,-2'9"+1'1/2",1,			a/2-h_w,-2'9"+1'1/2",1001,			a/2-h_w,-12"-h_w/2,1,			a/2-4"-h_w,-7"-sr_d/2-h_w/2,1,			a/2-4"-h_w,-h_w,1,			a/2-4"-f_w,-h_w,1	else		POLY2_B 6,1+2+4,gs_fill_pen, gs_back_pen,			-a/2+4",-7"-sr_d/2,1,			-a/2+4",0,1,			-a/2+4"+f_w,0,1,			-a/2+4"+f_w,-h_w,1,			-a/2+4"+h_w,-h_w,1,			-a/2+4"+h_w,-7"-sr_d/2,1		POLY2_B 6,1+2+4,gs_fill_pen, gs_back_pen,			a/2-4"-f_w,0,1,			a/2-4",0,1,			a/2-4",-7"-sr_d/2,1,			a/2-4"-h_w,-7"-sr_d/2,1,			a/2-4"-h_w,-h_w,1,			a/2-4"-f_w,-h_w,1		line_type gs_LineTypeStUp		POLY2_B 14,1+4,gs_fill_pen, gs_back_pen,			a/2-4",-7"-sr_d/2,1,			a/2,-12",1,			a/2,-3'2"+1'1/2",1,			-a/2,-3'2"+1'1/2",1001,			-a/2,-12",1,			-a/2+4",-7"-sr_d/2,1,			-a/2+4"+h_w,-7"-sr_d/2,1,			-a/2+4"+h_w,-7"-sr_d/2-h_w/2,1,			-a/2+h_w,-12"-h_w/2,1,			-a/2+h_w,-3'2"+1'1/2",1,			a/2-h_w,-3'2"+1'1/2",1001,			a/2-h_w,-12"-h_w/2,1,			a/2-4"-h_w,-7"-sr_d/2-h_w/2,1,			a/2-4"-h_w,-7"-sr_d/2,1		POLY2_B 14,1+4,gs_fill_pen, gs_back_pen,			a/2-4",-7"-sr_d/2,1,			a/2,-12",1,			a/2,-2'9"+1'1/2",1,			-a/2,-2'9"+1'1/2",1001,			-a/2,-12",1,			-a/2+4",-7"-sr_d/2,1,			-a/2+4"+h_w,-7"-sr_d/2,1,			-a/2+4"+h_w,-7"-sr_d/2-h_w/2,1,			-a/2+h_w,-12"-h_w/2,1,			-a/2+h_w,-2'9"+1'1/2",1,			a/2-h_w,-2'9"+1'1/2",1001,			a/2-h_w,-12"-h_w/2,1,			a/2-4"-h_w,-7"-sr_d/2-h_w/2,1,			a/2-4"-h_w,-7"-sr_d/2,1		line_type 1	endifendif! --- LANDING PLATFORM ---IF bShowPlatform THEN	RECT2 -a/2+4",0,a/2-4",lnd_w	HOTSPOT2 a/2-4",lnd_w,unID :unID=unID+1	HOTSPOT2 -a/2+4",lnd_w,unID :unID=unID+1ENDIF! --- RAIL on Top ---unID = 50IF bShowRail THEN	POLY2_B 4,1+2+4,gs_fill_pen, gs_back_pen,		-a/2+4",-7"+sr_d/2,1,		-a/2+4"-sr_w,-7"+sr_d/2,1,		-a/2+4"-sr_w,-7"+sr_d/2+tr_oh,1,		-a/2+4",-7"+sr_d/2+tr_oh,1	POLY2_B 4,1+2+4,gs_fill_pen, gs_back_pen,		a/2-4",-7"+sr_d/2,1,		a/2-4"+sr_w,-7"+sr_d/2,1,		a/2-4"+sr_w,-7"+sr_d/2+tr_oh,1,		a/2-4",-7"+sr_d/2+tr_oh,1	HOTSPOT2 a/2-4"+sr_w,-7"+sr_d/2+tr_oh,unID :unID=unID+1	HOTSPOT2 -a/2+4"-sr_w,-7"+sr_d/2+tr_oh,unID :unID=unID+1ENDIF