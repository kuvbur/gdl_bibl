if not(show) then endGROUP "nastil"MATERIAL mat_plt_nast=0.005sj=1+2+4+8+16+32addx bshvaddz hshv+t_nastPUT 0,0,sjPUT -A,0,sjPUT -A,-B,sjPUT 0,-B,sjPRISM_ NSP/3, -t_nast, get(NSP)ENDGROUPGROUP "krep_ogr"MATERIAL mat_ostsj=1+2+4+8+16+32addx tstaddy hshv-a_krep_ogr-tpolPUT 0,0,sjPUT b_krep_ogr,0,sjPUT b_krep_ogr,a_krep_ogr,sjPUT 0,a_krep_ogr,sjhotspot bshv,a_krep_ogr,0PRISM_ NSP/3, t_krep_ogr, get(NSP)ENDGROUPGROUP "sred_pol"MATERIAL mat_ostsj=1+2+4+8+16+32!addx tstaddy hshv-a_krep_ogrPUT 0,0,sjPUT -sh_svet,0,sjPUT -sh_svet,a_krep_ogr,sjPUT 0,a_krep_ogr,sjPRISM_ NSP/3, t_krep_ogr, get(NSP)ENDGROUPdel topGROUP "mass_sred_pol"MATERIAL mat_ostrotx 90addz l1for i=1 to nPLACEGROUP "sred_pol"addz l2next iENDGROUPPLACEGROUP "mass_sred_pol"del topGROUP "mass_krep_ogr"MATERIAL mat_ostrotx 90addz 0.3for i=1 to n1PLACEGROUP "krep_ogr"addz l3next iENDGROUPGROUP "shveller"MATERIAL mat_ostrotx 90sj=1+2+4+8+16+32PUT 0,0,sj	if htspt then hotspot 0,0,0	if htspt then hotspot 0,0,lshvelPUT 0,hshv,sj	if htspt then hotspot 0,hshv,0	if htspt then hotspot 0,hshv,lshvelPUT bshv,hshv,sj	if htspt then hotspot bshv,hshv,0	if htspt then hotspot bshv,hshv,lshvelPUT bshv,hshv-tpol,sj	if htspt then hotspot bshv,hshv-tpol,0	if htspt then hotspot bshv,hshv-tpol,lshvelPUT tst,hshv-tpol,sj	if htspt then hotspot tst,hshv-tpol,0	if htspt then hotspot tst,hshv-tpol,lshvelPUT tst,tpol,sj	if htspt then hotspot tst,tpol,0	if htspt then hotspot tst,tpol,lshvelPUT bshv,tpol,sj	if htspt then hotspot bshv,tpol,0	if htspt then hotspot bshv,tpol,lshvelPUT bshv,0,sj	if htspt then hotspot bshv,0,0	if htspt then hotspot bshv,0,lshvelPRISM_ NSP/3, B, get(NSP)ENDGROUPGROUP "ogr"MATERIAL mat_ostmuly -1addx bshv+0.05addz hshv-b_krep_ogrif ogr then call "Мет. ограждение по серии", parameters l1_krep_ogr=0.3,l2_krep_ogr=l3,n_krep_ogr=n1,l_s=B,l_v=B+2*0.06,h_ogr=h_ogr,b_krep_ogr=b_krep_ogrdel topENDGROUPGROUP "verh_ugol"MATERIAL mat_ostrotx 90roty 90addx -b_ugaddy hshv-b_ugsj=1+2+4+8+16+32h_ug=b_ugPUT 0,0,sjPUT 0,h_ug,sjPUT b_ug,h_ug,sjPUT b_ug,h_ug-t_ug,sjPUT t_ug,h_ug-t_ug,sjPUT t_ug,0,sjPRISM_ NSP/3, -sh_svet, get(NSP)ENDGROUPMATERIAL mat_plPLACEGROUP "nastil"MATERIAL mat_ostPLACEGROUP "verh_ugol"addy -Bmuly -1PLACEGROUP "verh_ugol"del topPLACEGROUP "shveller"if krogr then PLACEGROUP "mass_krep_ogr"PLACEGROUP "ogr"del topmulx -1addx sh_svetPLACEGROUP "shveller"if krogr then PLACEGROUP "mass_krep_ogr"PLACEGROUP "ogr"