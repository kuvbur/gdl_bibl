if not(show) then endif otobr_simv = 0 then PROJECT2 3,270,2if type_uch = "Прямой" then	HOTSPOT2 0, 0, unID: unID=unID+1	HOTSPOT2 0, 0, unID, l_ogr, 1+128: unID=unID+1	HOTSPOT2 l_ogr, 0, unID, l_ogr, 2: unID=unID+1	for i = 1 to n_stoek		if i = n_stoek then			del top			end_st = (n_stoek - 2) * shag_stoek + b_plast/2			line2 end_st, 0, A - b_plast/2, 0			add2 A,0			hotspot2 0,0			RECT2 -b_plast/2, -a_plast/2, b_plast/2, a_plast/2			del top		else			hotspot2 0,0			RECT2 -b_plast/2, -a_plast/2, b_plast/2, a_plast/2			if i < n_stoek - 1 then line2 b_plast/2,0,shag_stoek-b_plast/2,0			add2 shag_stoek,0		endif	next i	del topendifif type_uch = "Угловой" then	HOTSPOT2 0, 0, unID: unID=unID+1	HOTSPOT2 0, 0, unID, r_ug, 1+128: unID=unID+1	HOTSPOT2 -r_ug, 0, unID, r_ug, 2: unID=unID+1	rot2 180	ARC2 0, 0, r_ug, 0, a_ug	hotspot2 0, r_ugendifif type_uch = "Доборный" then	HOTSPOT2 0, 0, unID: unID=unID+1	HOTSPOT2 0, 0, unID, l_ogr, 1+128: unID=unID+1	HOTSPOT2 l_ogr, 0, unID, l_ogr, 2: unID=unID+1	line2 0, 0, l_ogr, 0endif