values "AC_Type" `Линия`,`Дугообразный`,"Line","Arc"values "AC_LineVisibility" `Видимый`,`Видимый Наполовину`,`Скрытая`,"Visible","Semi-Visible","Hidden"values "AC_Radius" range[,-A/2], range [A/2,]if (ABS(AC_Radius) < ABS(A/2) ) then	AC_Radius = A/2endifif (AC_Type = `Линия` or AC_Type = "Line") then	lock "AC_Radius"endif