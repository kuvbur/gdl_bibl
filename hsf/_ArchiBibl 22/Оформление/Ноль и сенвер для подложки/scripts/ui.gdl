ui_dialog `Стиль Символа`,444,266ui_current_page gs_ui_current_page! =============================================================================! Tabpage Selector Control! =============================================================================file_dependence "uiTab_Dimension_flat(1)"file_dependence "uiTab_2DRepr_flat(1)"TABID_ROOT				= -1TABID_GENERAL			= 1TABID_APPEARANCE		= 2if TABID_GENERAL > 0 then	ui_page TABID_GENERAL, TABID_ROOT, `Общие Параметры`, "uiTab_Dimension_flat(1)"	gosub "generalPage"endifif TABID_APPEARANCE > 0 then	ui_page TABID_APPEARANCE, TABID_ROOT, `Отображение и Покрытия`, "uiTab_2DRepr_flat(1)"	gosub "appearancePage"endif! =============================================================================end! ============================================================================="generalPage":! =============================================================================! General Settings Tabpage! =============================================================================	UI_STYLE 0,0	pageStartY	= 5		x1 = 10	x2 = 100	x3 = 120	x4 = 280	x5 = 330	y1 = pageStartY + 3			UI_PICT 1,		x1,		y1,		100,	110,	1	! sizing demonstration		ui_infield{3} "symbolStyles_m",	290,	pageStartY + 3,		112,	102,			2, 2, 15, 5,			! type chooser pict			130,102,100,90,			1, `Стиль 1`, 1,			2, `Стиль 2`, 2,			3, `Стиль 3`, 3,			4, `Стиль 4`, 4,			5, `Стиль 5`, 5,			6, `Стиль 6`, 6,			7, `Стиль 7`, 7,			8, `Стиль 8`, 8,			9, `Стиль 9`, 9,			10,`Стиль 10`,10,			13,`Стиль 11`,11,			14,`Стиль 12`,12,			15,`Стиль 13`,13		if scaleValues_m = 2 then	! Scale independent (Paper Size)		form = ""		n = REQUEST ("Model_text_size_unit", "", form)		unit = "mm"		n = SPLIT (form, "%%.%n%s", variable1, variable2, unit)		if unit = "mm" then			UI_INFIELD	"fontSize_mm",    x3,	 	pageStartY + 10,		80,  15 ui_tooltip `Размер Шрифта`			UI_INFIELD	"gs_TextPosY_mm", x3,		pageStartY + 33,		62,  15 ui_tooltip `Расположение Текста по Y (Размер Бумаги)`			UI_INFIELD	"B_mm",           x3,		pageStartY + 79,		80,  15 ui_tooltip `Диаметр (Размер бумаги)`		else			UI_INFIELD	"fontSize_pt",    x3,	 	pageStartY + 10,		80,  15 ui_tooltip `Размер Шрифта`			UI_INFIELD	"gs_TextPosY_pt", x3,		pageStartY + 33,		62,  15 ui_tooltip `Расположение Текста по Y (Размер Бумаги)`			UI_INFIELD	"B_pt",           x3,		pageStartY + 79,		80,  15 ui_tooltip `Диаметр (Размер бумаги)`		endif		UI_OUTFIELD	unit,			x3+90,	pageStartY + 10,		40,	 	15		UI_OUTFIELD	unit,			x3+90,	pageStartY + 33,		40,	 	15		UI_OUTFIELD	unit,			x3+90,	pageStartY + 79,		40,	 	15	else		form = ""		n = request("Working_length_unit", "", form)		if STRSTR(form, "m") then			n = SPLIT (form, "%%.%n%s", variable1, variable2, unit)			if n = 0 then				n = SPLIT (form, "%%0.%n%s", variable1, variable2, unit)			endif		else			unit = ""		endif			UI_INFIELD	"fontSize_model",	x3,		pageStartY + 10,	80,	 	15	ui_tooltip `Размер Шрифта`		UI_OUTFIELD	unit,				x3+90,	pageStartY + 10,	40,	 	15		UI_INFIELD	"gs_TextPosY",		x3,		pageStartY + 33,	62,	 	15	ui_tooltip `Расположение Текста по Y`		UI_OUTFIELD	unit,			x3+90,		pageStartY + 33,	40,	 	15		UI_INFIELD	"B",				x3,		pageStartY + 79,	80,	 	15	ui_tooltip `Диаметр`		UI_OUTFIELD	unit,			x3+90,		pageStartY + 79,	40,	 	15	endif		y1 = y1 + 140		UI_OUTFIELD	`Масштаб`,				x1,		y1,		x3-x1-1,15	UI_INFIELD	"scaleValues",			x3, 	y1-2,	130,	20	ui_tooltip `Масштаб`		UI_outFIELD	`Текст`,  				x4,		y1,	 	x5-x4-1,15	UI_INFIELD	"txt",					x5, 	y1-2, 	68,		20	ui_tooltip `Текст`	y1 = y1 + 23		UI_OUTFIELD	`Шрифт`,  				x4,		y1,	 	x5-x4-1,15	UI_INFIELD	"fontType",				x5, 	y1-2, 	85,		20	ui_tooltip `Шрифт`		UI_OUTFIELD	`Ориентация Текста`,	x1,		y1,		x3-x1-1,15	UI_INFIELD	"typeTextRotation",				x3, 	y1-2,	130,	20	ui_tooltip `Элемент (Размер Бумаги)`	y1 = y1 + 23		UI_OUTFIELD	`Стиль`,  				x4,		y1,	 	x5-x4-1,15		ui_infield{3} "gs_text_style_bold", x5, y1, 24, 24,				6, "",				2,				1, 16, 16, 16, 16,				"ui_bold_flat(1)", "", 0,				"ui_bold_flat(1)", "", 1\				ui_tooltip `Жирный`			ui_infield{3} "gs_text_style_italic", x5+30, y1, 24, 24,				6, "",				2,				1, 16, 16, 16, 16,				"ui_italic_flat(1)", "", 0,				"ui_italic_flat(1)", "", 1\				ui_tooltip `Курсив`			ui_infield{3} "gs_text_style_underline", x5+60, y1, 24, 24,				6, "",				2,				1, 16, 16, 16, 16,				"ui_underline_flat(1)", "", 0,				"ui_underline_flat(1)", "", 1\				ui_tooltip `Подчеркнутый`			UI_STYLE 0,1	ui_infield{3} "gs_follow_north",	x1,		y1,		200,	18,			7,"",2,1,			0,0,0,0,			1,`Согласно Северу Проекта`, 0,			2,`Согласно Северу Проекта`, 1	UI_STYLE 0,0return"appearancePage":! =============================================================================! Representation Tabpage! =============================================================================	itemID = 1	listFieldID = TABID_APPEARANCE	ui_listfield listFieldID, 0, 0, 444, 266, 1	ui_listitem{2} itemID, listFieldID, NorthSymbo_sp1, 		0							: itemID = itemID + 1	ui_listitem{2} itemID, listFieldID, gs_FontPen, 			1,	"ui_list_pencolor_flat(1)"	: itemID = itemID + 1	ui_listitem{2} itemID, listFieldID, gs_cont_pen, 			1,	"ui_list_contourPen_flat(1)"	: itemID = itemID + 1	if not( symbolStyles_m = 1 |\			symbolStyles_m = 5 |\			symbolStyles_m = 9 |\			symbolStyles_m = 10) then		ui_listitem{2} itemID, listFieldID, gs_fill_type, 			1,	"ui_list_fillType_flat(1)"	: itemID = itemID + 1		ui_listitem{2} itemID, listFieldID, gs_fill_pen, 			1,	"ui_list_fillPen_flat(1)"	: itemID = itemID + 1		ui_listitem{2} itemID, listFieldID, gs_back_pen, 			1,	"ui_list_fillBgPen_flat(1)"	: itemID = itemID + 1	else		itemID = itemID + 3	endif	if not( symbolStyles_m = 1 |\			symbolStyles_m = 3 |\			symbolStyles_m = 5 |\			symbolStyles_m = 7 |\			symbolStyles_m = 10) then		ui_listitem{2} itemID, listFieldID, gs_fill2_type, 			1,	"ui_list_fillPen_flat(1)"	: itemID = itemID + 1		ui_listitem{2} itemID, listFieldID, gs_fill2_pen, 			1,	"ui_list_fillPen_flat(1)"	: itemID = itemID + 1		ui_listitem{2} itemID, listFieldID, gs_back2_pen, 			1,	"ui_list_fillBgPen_flat(1)"	: itemID = itemID + 1	else		itemID = itemID + 3	endif	if not(	symbolStyles_m = 4 |\			symbolStyles_m = 5 |\			symbolStyles_m = 6 |\			symbolStyles_m = 11) then		ui_listitem{2} itemID, listFieldID, gs_BackFillType, 		1,	"ui_list_fillType_flat(1)"	: itemID = itemID + 1		ui_listitem{2} itemID, listFieldID, gs_BackFillPen, 		1,	"ui_list_fillPen_flat(1)"	: itemID = itemID + 1		ui_listitem{2} itemID, listFieldID, gs_BackFillBackPen, 	1,	"ui_list_fillBgPen_flat(1)"	: itemID = itemID + 1	else		itemID = itemID + 3	endif	ui_listitem{2} itemID, listFieldID, gs_3D_representation, 	0							: itemID = itemID + 1	ui_listitem{2} itemID, listFieldID, gs_detlevel_3D,			1							: itemID = itemID + 1	ui_listitem{2} itemID, listFieldID, gs_shadow,				1,	"ui_list_shadow_flat(1)"		: itemID = itemID + 1	ui_listitem{2} itemID, listFieldID, gs_material, 			0							: itemID = itemID + 1	ui_listitem{2} itemID, listFieldID, gs_base_mat,			1,	"ui_list_surface_flat(1)"	: itemID = itemID + 1	ui_listitem{2} itemID, listFieldID, gs_sign_mat,			1,	"ui_list_surface_flat(1)"	: itemID = itemID + 1return