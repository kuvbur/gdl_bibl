unID=1EPS = 0.0001is = 1 : ie = 2ix = 1 : iy = 2define style{2} "textStyleName" AC_TextFont_1, AC_TextSize_1, AC_TextStyle_1_parAlignment = 1 	! unecessary function for one row_parLineSpace = 1 	! default_blockAnchor = 2	! topCenter_blockHeight = 1	! scale independent paper sizedim paramname_coord[2][3]paramname_coord[is][ix] = "X начало"paramname_coord[is][iy] = "Y начала"paramname_coord[ie][ix] = "X конца"paramname_coord[ie][iy] = "Y конца"inx = 1!p_name = paramname_coord[is][ix] + " оси " + axis_name_x[inx]!HOTSPOT2	0,					pos_prof[is][iy],	unID,	pos_prof[is][ix],	1+128, pos_prof[is][ix], p_name: unID=unID+1!HOTSPOT2	pos_prof[is][ix],	pos_prof[is][iy],	unID,	pos_prof[is][ix],	2, pos_prof[is][ix], p_name: unID=unID+1!HOTSPOT2	-1,					pos_prof[is][iy],	unID,	pos_prof[is][ix],	3, pos_prof[is][ix], p_name: unID=unID+1rrr = REQUEST ("View_Rotangle", "", angleViewRot)totalRotate = (SYMB_ROTANGLE + angleViewRot)if totalRotate > 360 then totalRotate = totalRotate - 360Marker_Diameter = GLOB_SCALE/1000*AC_MarkerSizetextStyleHeight = 0textStyleDescent = 0textStyleLeading = 0req_id = REQUEST ("Height_of_style", "textStyleName", textStyleHeight, textStyleDescent, textStyleLeading)textStyleAscent = (textStyleHeight - textStyleDescent - textStyleLeading) markerTextBlockPosY = (textStyleAscent - AC_TextSize_1 / 2) * GLOB_SCALE / 1000anxinx = str("%.0", inx)PARAGRAPH "parName"+anxinx _parAlignment, 0, 0, 0, _parLineSpace , 0	pen AC_TextPen_1	style "textStyleName"ENDPARAGRAPHTEXTBLOCK "markerTextBlock"+anxinx 0, _blockAnchor, 0, AC_TextCharWidthFactor_1, AC_TextCharSpaceFactor_1, _blockHeight, "parName"+anxinxROT2 (-1+SYMB_MIRRORED*2)*totalRotaterichtext2 0, markerTextBlockPosY, "markerTextBlock"+anxinx