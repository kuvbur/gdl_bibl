values "upr_rezhim" "Откл", "Только ядро", "Без отделки", "Вся модель"if upr_rezhim = "Откл" then PARAMETERS upr_n = -1if upr_rezhim = "Без отделки" then PARAMETERS upr_n = 2if upr_rezhim = "Только ядро" then PARAMETERS upr_n = 1if upr_rezhim = "Вся модель" then PARAMETERS upr_n = 0parameters lenB=shaftLengthcall "Условный_болт" PARAMETERS ALL!Check for User Input	rrr = Application_Query ("Parameter_Script", "FirstOccasion_in_Progress", isFirst)	if isFirst then		buttonID = glob_ui_button_ID		modparName = glob_modpar_name	else		buttonID = 0		modparName = ""	endif!End of Check for User Input!Menu	for i = 1 to nUIPages		put i	next i	values "uiPage" get(nsp)	if modparName = "uiPage" then		uiPart = 1		parameters uiPart = 1	endif	if displayPropertyControl then		if bittest(displayPropertyControl, 0) then put 1		if bittest(displayPropertyControl, 1) then put 2		if bittest(displayPropertyControl, 2) then put 3		values "editProperty" get(nsp)	endif	if nUIParts > 1 then		for i = 1 to nUIParts			put i		next i		values "uiPart" get(nsp)	endif	if nUISubParts > 1 then		for i = 1 to nUISubParts			put i		next i		values "uiSubPart" get(nsp)	endif!End of Menu!Washer Shape	values "washerType" "None",		"Round",		"Square"!End of Washer Shape!if offsetLgt > threadLength then!	parameters offsetLgt = threadLength!endifif modparName = "shaftLength" then	gosub "Check Shaft Length"	gosub "Check Inside Length"	gosub "Check Thread Length"	parameters shaftLength2D = shaftLength2Dendifif modparName = "shaftLength2D" then	shaftLength = shaftLength2D/cos(incline)	gosub "Check Shaft Length"	gosub "Check Inside Length"	gosub "Check Thread Length"	parameters shaftLength = shaftLengthendifif modparName = "insideLength" then	gosub "Check Inside Length"	gosub "Check Thread Length"endifif modparName = "insideLength2D" then	insideLength = insideLength2D/cos(incline)	gosub "Check Inside Length"	gosub "Check Thread Length"endifif modparName = "threadLength" then	gosub "Check Thread Length"endifif modparName = "threadLength2D" then	threadLength = threadLength2D/cos(incline)	gosub "Check Thread Length"endifif modparName = "Incline" then	shaftLength2D = shaftLength*cos(incline)	insideLength2D = insideLength*cos(incline)	threadLength2D = threadLength*cos(incline)	parameters shaftLength2D = shaftLength2D,		insideLength2D = insideLength2D,		threadLength2D = threadLength2Dendif!Plan View Symbol	values "showParts" 1,		2,		3,		7!End of Plan View Symbolvalues "nNuts" 0, 1, 2values "show_hole2D" 0, 1end"Check Shaft Length":	shaftLength = max(shaftLength, 2*washerThick + nutThick*nNuts)	shaftLength2D = shaftLength*cos(incline)	parameters shaftLength = shaftLength,		shaftLength2D = shaftLength2Dreturn"Check Inside Length":	insideLength = min(insideLength, shaftLength - 2*washerThick - nutThick*nNuts)	insideLength = max(insideLength, shaftLength - washerThick - threadLength)	insideLength = max(insideLength, 0)	insideLength2D = insideLength*cos(incline)	parameters insideLength = insideLength,		insideLength2D = insideLength2Dreturn"Check Thread Length":	threadLength = max(threadLength, shaftLength - insideLength - washerThick)	threadLength = min(threadLength, shaftLength)	threadLength2D = threadLength*cos(incline)	parameters threadLength = threadLength,		threadLength2D = threadLength2Dreturn	