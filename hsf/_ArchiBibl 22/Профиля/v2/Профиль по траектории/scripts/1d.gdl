EPS=0.00001ix = 1 : iy = 2 : iz = 3n = REQUEST ("Name_of_main", "", name)is_call = 0 : if name<>"" then is_call = 1!!ac_toplevel_t = max(pos_prof[ie][iz], pos_prof[is][iz])!ac_bottomlevel_t = min(pos_prof[ie][iz],pos_prof[is][iz])!if ac_toplevel_t = ac_bottomlevel_t then!	ac_toplevel_t = ac_toplevel_t + aa!	ac_bottomlevel_t = ac_bottomlevel_t - aa!endif!ac_toplevel = ac_toplevel_t!ac_bottomlevel = ac_bottomlevel_tcall "Polyline" parameters allshag = 1.2dim priv[3]dim coord_s[][5]priv[ix] = 0priv[iy] = 0priv[iz] = 0n_otr = vardim1(otr)n_c = 0for i = 1 to n_otr	r = otr[i][7]	phi = otr[i][8]	etta = otr[i][9]	n_int = round_int(r / shag)	delta = r - n_int * shag	addr = 0	if delta > EPS then addr = delta/2	for jj = 1 to n_int		n_c = n_c + 1		coord_s[n_c][ix] = otr[i][ix+3] + addr * sin(etta+90) * cos(phi)		coord_s[n_c][iy] = otr[i][iy+3] + addr * sin(etta+90) * sin(phi)		coord_s[n_c][iz] = otr[i][iz+3] + addr * cos(etta+90)		coord_s[n_c][4] = phi		coord_s[n_c][5] = etta		text2 coord_s[n_c][ix], coord_s[n_c][iy], addr		addr = addr + shag	next jjnext i!		if delta > EPS and i = 1 then!			n_c = n_c + 1!			coord_s[n_c][ix] = otr[i][ix]!			coord_s[n_c][iy] = otr[i][iy]!			coord_s[n_c][iz] = otr[i][iz]!			coord_s[n_c][4] = phi!			coord_s[n_c][5] = etta!		endif!	if delta > EPS and i = n_otr then!		n_c = n_c + 1!		coord_s[n_c][ix] = otr[i][ix+3]!		coord_s[n_c][iy] = otr[i][iy+3]!		coord_s[n_c][iz] = otr[i][iz+3]!		coord_s[n_c][4] = phi!		coord_s[n_c][5] = etta!	endif