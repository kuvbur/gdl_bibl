if iscall then 30!Header	pageWidth = 444	pageHeight = 266	pageMargin = 130	pageTopMargin = 21	ui_dialog "Настройки болта", pageWidth, pageHeight	useArrow = 0	uiX0 = 150	uiY0 = 6	uiX = uiX0	uiY = uiY0	alternateText = ""	uiGridHeight = 75	groupTitle = ""	showTitle = 1	uiY = 51!End of HeaderUI_outFIELD	"Угол-1 (в плоскости)", 0, 110, 	130,	15UI_outFIELD	rang, 30, 130, 	100,	15UI_outFIELD	"Угол-2 (из плоскости)", 0, 150, 	130,	15UI_outFIELD	incline_r, 30, 170, 	100,	15UI_outFIELD	"Скрыть в виде", 0, 190, 	90,	15ui_infield "el_view", 0, 210, 	130,	15ui_infield "show_hole", 100, 190, 	15,	15 ui_tooltip "Оставить отверстие"!Model	if uiPage = 1 then		!Components			if uiPart = 1 then				!Model					if editProperty = 1 then						uiY = uiY + 40						!Shaft Diameter							ui_pict 1, uiX, uiY - 35, 57, 31, 1	! CT_SEL_DDD Bolt_ShaftDiameter							ui_infield "diaB", uiX, uiY, 60, 19 ui_tooltip "Размер"							gosub "Next Cell"						!Shaft Diameter							ui_pict 1, uiX, uiY - 35, 57, 31, 1	! CT_SEL_DDD Bolt_ShaftDiameter							ui_infield "shaftThick", uiX, uiY, 60, 19 ui_tooltip "Диаметр"							gosub "Next Cell"						!Shaft Length							ui_pict 2, uiX, uiY - 35, 57, 31, 1	! CT_SEL_DDD Bolt_ShaftLength							ui_infield "shaftLength", uiX, uiY, 60, 19 ui_tooltip "Длина болта"							gosub "Next Cell"						!Thread Length							ui_pict 3, uiX, uiY - 35, 57, 31, 1	! CT_SEL_DDD Bolt_ThreadLength							ui_infield "threadLength", uiX, uiY, 60, 19 ui_tooltip "Длина резьбы"							gosub "Next Cell"						!Flat Width							ui_pict 4, uiX, uiY - 35, 57, 31, 1	! CT_SEL_DDD Bolt_FlatWidth							ui_infield "flatWidth", uiX, uiY, 60, 19 ui_tooltip "Размер гайки"							gosub "Next Cell"						!Head Thickness							ui_pict 5, uiX, uiY - 35, 57, 31, 1	! CT_SEL_DDD Bolt_HeadThick							ui_infield "headThick", uiX, uiY, 60, 19 ui_tooltip "Толщина верха болта"							gosub "Next Cell"						!Washer Size							ui_pict 6, uiX, uiY - 35, 57, 31, 1	! CT_SEL_DDD Bolt_WasherDiameter							ui_infield "washerSize", uiX, uiY, 60, 19 ui_tooltip "Диаметр шайбы"							gosub "Next Cell"						!Washer Thickness							ui_pict 7, uiX, uiY - 35, 57, 31, 1	! CT_SEL_DDD Bolt_WasherThick							ui_infield "washerThick", uiX, uiY, 60, 19 ui_tooltip "Толщина шайбы"							gosub "Next Cell"						!Nut Thickness							ui_pict 8, uiX, uiY - 35, 57, 31, 1	! CT_SEL_DDD Bolt_NutThick							ui_infield "nutThick", uiX, uiY, 60, 19 ui_tooltip "Толщина шайбы"							gosub "Next Cell"						!Incline							ui_pict 9, uiX + 10, uiY - 43, 40, 40, 1	! CT_SEL_DDD Bolt_Incline							ui_infield "incline", uiX, uiY, 60, 19 ui_tooltip "Pitch Angle"							gosub "Next Cell"						uiY = uiY + 23						gosub "Separator"					endif				!End of Model				!Surfaces					if editProperty = 2 then						!Bolt							uiX = uiX0							ui_infield{2} boltMat, uiX, uiY, 330 - uiX, 23							uiX = 335							ui_outfield "Bolt", uiX, uiY + 5, 440 - uiX, 14							uiY = uiY + 26						!End of Bolt						gosub "Separator"					endif				!End of Surfaces				!Appearance					if editProperty = 3 then						!Bolt							ui_outfield "View Attributes", uiX, uiY, 440 - uiX, 14							uiY = uiY + 18							!View Outline								ui_pict 10, uiX, uiY + 2, 12, 12, 1	! CT_PenSect								uiX = uiX + 13								penID = gs_cont_pen								gosub "Pen ID"								ui_infield{2} gs_cont_pen, uiX, uiY, 45, 17 ui_tooltip "Outline Pen"								uiX = uiX + 57							!View Fill Pen								ui_pict 11, uiX, uiY + 2, 12, 12, 1	! CT_PenSectFill								uiX = uiX + 13								penID = gs_fill_pen								gosub "Pen ID"								ui_infield{2} gs_fill_pen, uiX, uiY, 45, 17 ui_tooltip "Fill Pen (Plan View)"								uiX = uiX + 56							!View Fill Background Pen								ui_pict 12, uiX, uiY + 2, 12, 12, 1	! CT_PenSectBkgd								uiX = uiX + 13								penID = gs_back_pen								gosub "Pen ID"								ui_infield{2} gs_back_pen, uiX, uiY, 45, 17 ui_tooltip "Background Pen (Plan View)"								uiX = uiX + 56								uiX = uiX0								uiY = uiY + 18							!Line Type								uiX = uiX0								ui_pict 13, uiX, uiY + 7, 12, 12, 1	! CT_LineType								uiX = uiX + 14								ui_infield{2} gs_line_type, uiX, uiY, 120, 27 ui_tooltip "Line Type (Plan View)"								uiX = uiX + 125							!Fill								ui_pict 14, uiX, uiY + 7, 12, 12, 1	! CT_SectFill								uiX = uiX + 14								ui_infield{2} gs_fill_type, uiX, uiY, min(237, 440 - uiX), 27 ui_tooltip "Fill Type (Plan View)"								uiY = uiY + 29							gosub "Separator"						!End of Pole						!Center Line							ui_outfield "Center Line Attributes", uiX, uiY, 440 - uiX, 14							uiY = uiY + 18							!Pen								ui_pict 10, uiX, uiY + 2, 12, 12, 1	! CT_PenSect								uiX = uiX + 13								penID = centerLinePen								gosub "Pen ID"								ui_infield{2} clPen, uiX, uiY, 45, 17 ui_tooltip "Center Line Pen"								uiX = uiX + 57							!Line Type								ui_pict 13, uiX, uiY + 2, 12, 12, 1	! CT_LineType								uiX = uiX + 14								ui_infield{2} clType, uiX, uiY, 440 - uiX, 27 ui_tooltip "Center Line Type"								uiX = uiX + 125								uiY = uiY + 29							gosub "Separator"						!End of Center Line						!Symbol							ui_outfield "Plan View Symbol", uiX, uiY, 440 - uiX, 14							uiY = uiY + 18							uiY = uiY + 40							ui_infield{3} "showParts", uiX, uiY - 40, 60, 60,								2, 15, 4, 4,	! CT_SEL_DDD Bolt_SymbolOptions								50, 30, 50, 30,								1, "Head Only", 1,								2, "Nut Only", 2,								3, "Head and Nut", 3,								4, "Complete Bolt", 7							gosub "Next Cell"							uiY = uiY + 23							gosub "Separator"						!End of Symbol					endif				!End of Appearance			endif		!End of Components		!Options			if uiPart = 2 then				ui_outfield "Washers", uiX, uiY, 440 - uiX, 14				uiY = uiY + 16				uiY = uiY + 40				!Washer Type					ui_infield{3} "washerType", uiX, uiY - 40, 60, 60, 						2, 16, 3, 3,	! CT_SEL_DDD Bolt_WasherType						50, 30, 50, 30, 						1, "None", "None",						2, "Round", "Round",						3, "Square", "Square"					gosub "Next Cell"				uiY = uiY + 23				gosub "Separator"				ui_outfield "Nut", uiX, uiY, 440 - uiX, 14				uiY = uiY + 16				uiY = uiY + 40				!Nut Type					ui_infield{3} "nNuts", uiX, uiY - 40, 60, 60,						2, 17, 3, 3, 	! CT_SEL_DDD Bolt_ShowNut						50, 30, 50, 30, 						1, "No Nut", 0,						2, "1 Nut", 1,						3, "2 Nuts", 2					gosub "Next Cell"				uiY = uiY + 23				gosub "Separator"			endif		!End of Options	endif!End of Model!Hole	if uiPage = 2 then		!Model			if editProperty = 1 then!				ui_outfield "Hole", uiX, uiY, 440 - uiX, 14!				uiY = uiY + 16				uiY = uiY + 40				!Hole Length					ui_pict 18, uiX, uiY - 40, 57, 37, 1	! CT_SEL_DDD Bolt_HoleLength					ui_infield "insideLength", uiX, uiY, 60, 19 ui_tooltip "Hole Length"					gosub "Next Cell"				!Hole Tolerance					ui_pict 19, uiX, uiY - 40, 57, 37, 1	! CT_SEL_DDD Bolt_HoleOffset					ui_infield "holeTolerance", uiX, uiY, 60, 19 ui_tooltip "Tolerance"					gosub "Next Cell"				uiY = uiY + 23				gosub "Separator"			endif		!End of Model		!Appearance			if editProperty = 3  then				ui_outfield "Hole Attributes", uiX, uiY, 440 - uiX, 14				uiY = uiY + 18				!Symbol					ui_outfield "Plan View Symbol", uiX, uiY, 440 - uiX, 14					uiY = uiY + 18					uiY = uiY + 40					!Options						ui_infield{3} "showHole2D", uiX, uiY - 40, 60, 60,							2, 20, 2, 2,	! CT_SEL_DDD Bolt_ShowHole2D							50, 30, 50, 30,							2, "Show Hole Outlines in 2D", 1,							1, "Show the Bolt Only", 0						gosub "Next Cell"					!Pen						ui_pict 10, uiX, uiY + 2, 12, 12, 1	! CT_PenSect						uiX = uiX + 13						penID = holePen						gosub "Pen ID"						ui_infield{2} holePen, uiX, uiY, 45, 17 ui_tooltip "Center Line Pen"						uiX = uiX + 57					uiY = uiY + 23					gosub "Separator"				!End of Symbol			endif		!End of Appearance	endif!End of Hole!Page Layout	W = pageWidth	H = pageHeight	M = pageMargin	T = pageTopMargin	uiY = 0	!Page Menu		uiY = uiY + 32		arrowY = 0		ui_style 0, 0		menuSpace = 17		for i = 1 to nUIPages			if uiPageName[i] # "" then				truncateText = 0				if uiPage = i then					arrowY = uiY - 3				endif				ui_radiobutton uiPage, i, uiPageName[i], 0, uiY, M - 5, 16				uiY = uiY + 17				if nUIParts and i = uiPage then					for j = 1 to nUIParts						ui_radiobutton uiPart, j, uiPartName[j], 10, uiY, M - 15, 16						uiY = uiY + 17					next j				endif			endif		next i	!End of Page Menu	!Page Border		!Top Left Corner			ui_pict 23, M + 4, T + 2, 1, 2	! CT_Border2			ui_pict 23, M + 5, T + 1, 2, 1	! CT_Border2		!Top Right Corner			ui_pict 23, W - 4, T + 1, 2, 1	! CT_Border2			ui_pict 23, W - 2, T + 2, 1, 2	! CT_Border2		!Bottom Right Corner			ui_pict 23, W - 2, H - 4, 1, 2	! CT_Border2			ui_pict 23, W - 4, H - 2, 2, 1	! CT_Border2		!Bottom Left Corner			ui_pict 23, M + 5, H - 2, 2, 1	! CT_Border2			ui_pict 23, M + 4, H - 4, 1, 2	! CT_Border2		!Top Edge			ui_pict 23, M + 7, T, W - M - 11, 1	! CT_Border2		!Left Edge			ui_pict 23, M + 3, T + 4, 1, H - T - 8	! CT_Border2		!Right Edge			ui_pict 23, W - 1, T + 4, 1, H - T - 8	! CT_Border2		!Bottom Edge			ui_pict 23, M + 7, H - 1, W - M - 11, 1	! CT_Border2		!Separator			ui_pict 23, M + 4, T + 25, W - M - 5, 1	! CT_Border2		!Arrow			ui_pict 24, M - 1, arrowY + 5, 3, 5, 1	! CT_BorderArrow_2012	!End of Page Border	!Page Title		if uiPage then			ui_outfield uiPageName[uiPage], M + 3, 3, W - M - 60, 14		endif	!End of Page Title	!Edit Property Control		displayModel = bittest(displayPropertyControl, 0)		displayMaterial = bittest(displayPropertyControl, 1)		displayAppearance = bittest(displayPropertyControl, 2)		displayListing = bittest(displayPropertyControl, 3)		nControls = displayModel + displayMaterial + displayAppearance + displayListing		if nControls then			controlWidth = nControls*23 + (nControls - 1)*2			uiXi = W - 3 - controlWidth			ui_infield{3} editProperty, uiXi, T + 2 , controlWidth, 22,				4, 25, 3, 1,	! CT_Properties				17, 17, 17, 17,				1, "Model", 1,				2, "Surfaces", 2,				3, "Appearance", 3		endif	!End of Edit Property Control	!SubPart Control		ui_style 0, 1		uiY = T + 3		if nControls then			subPartControlWidth = W - M - controlWidth - 16		else			subPartControlWidth = W - M - 9		endif		if nUISubParts > 1 then			for i = vardim1(uiSubPartName) + 1 to 51				uiSubPartName[i] = ""			next i			ui_infield{3} uiSubPart, M + 6, T + 2, subPartControlWidth, 22,				2, "", 0, 0, 				6, 11, 6, 11,				"", uiSubPartName[1], 1,				"", uiSubPartName[2], 2,				"", uiSubPartName[3], 3,				"", uiSubPartName[4], 4,				"", uiSubPartName[5], 5,				"", uiSubPartName[6], 6,				"", uiSubPartName[7], 7,				"", uiSubPartName[8], 8,				"", uiSubPartName[9], 9,				"", uiSubPartName[10], 10,				"", uiSubPartName[11], 11,				"", uiSubPartName[12], 12,				"", uiSubPartName[13], 13,				"", uiSubPartName[14], 14,				"", uiSubPartName[15], 15,				"", uiSubPartName[16], 16,				"", uiSubPartName[17], 17,				"", uiSubPartName[18], 18,				"", uiSubPartName[19], 19,				"", uiSubPartName[20], 20,				"", uiSubPartName[21], 21,				"", uiSubPartName[22], 22,				"", uiSubPartName[23], 23,				"", uiSubPartName[24], 24,				"", uiSubPartName[25], 25,				"", uiSubPartName[26], 26,				"", uiSubPartName[27], 27,				"", uiSubPartName[28], 28,				"", uiSubPartName[29], 29,				"", uiSubPartName[30], 30,				"", uiSubPartName[31], 31,				"", uiSubPartName[32], 32,				"", uiSubPartName[33], 33,				"", uiSubPartName[34], 34,				"", uiSubPartName[35], 35,				"", uiSubPartName[36], 36,				"", uiSubPartName[37], 37,				"", uiSubPartName[38], 38,				"", uiSubPartName[39], 39,				"", uiSubPartName[40], 40,				"", uiSubPartName[41], 41,				"", uiSubPartName[42], 42,				"", uiSubPartName[43], 43,				"", uiSubPartName[44], 44,				"", uiSubPartName[45], 45,				"", uiSubPartName[46], 46,				"", uiSubPartName[47], 47,				"", uiSubPartName[48], 48,				"", uiSubPartName[49], 49,				"", uiSubPartName[50], 50,				"", uiSubPartName[51], 51		else			ui_outfield uiSubPartName[1], M + 13, T + 6, subPartControlWidth - 5, 14		endif		ui_style 0, 0	!End of SubPart Control!End of Page Layoutend"Separator":	if uiX = uiX0 then		uiY = uiY - uiGridHeight	endif	if uiY < pageHeight - 6 then		ui_pict 26, uiX0, uiY, pageWidth - 4 - uiX0, 1	! CT_Separator2	endif	uiY = uiY + 5	uiX = uiX0return"Next Cell":	uiX = uiX + 75	if uiX > pageWidth - 50 then		gosub "Next Row"	endifreturn"Next Row":	uiX = uiX0	uiY = uiY + uiGridHeightreturn"Pen ID":	if penID < 100 then		ui_outfield "- " + str(penID, 1, 0) + " -", uiX, uiY + 2, 30, 14, 2	else		ui_outfield "-" + str(penID, 1, 0) + "-", uiX, uiY + 2, 30, 14, 2	endif	uiX = uiX + 31return30: