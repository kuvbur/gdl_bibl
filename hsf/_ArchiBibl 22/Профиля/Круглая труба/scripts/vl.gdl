UIPicts[7] = 54 !! "gs_Cut1Bottom_O" 		!!(7)parameters gs_UIPicts[8] = 60 !! "gs_Cut1Up_O" 			!!(8)parameters gs_UIPicts[9] = 66 !! "gs_Cut2Bottom_O" 		!!(9)parameters gs_UIPicts[10] = 72 !! "gs_Cut2Up_O" 			!!(10)parameters gs_UIPicts[11] = 78 !! "gs_Cut3Bottom_O" 		!!(11)parameters gs_UIPicts[12] = 84 !! "gs_Cut3Up_O" 			!!(12)parameters gs_UIPicts[13] = 90 !! "gs_SectRotBottom_O" 	!!(13)parameters gs_UIPicts[14] = 102 !! "gs_SectRotUp_O" 		!!(14)parameters gs_UIPicts[15] = 38 !! "gs_AxisRotCentre_O" 	!!(15)parameters gs_UIPicts[16] = 96 !! "gs_SectRotCentre_O" 	!!(16)if gs_cont_pen=0 then parameters gs_cont_pen=28IF gs_fill_pen=0 THEN PARAMETERS gs_fill_pen=19IF axis_line_pen=0 THEN PARAMETERS axis_line_pen=68values "gs_rot_axis"    rot_axis_mtrvalues "gs_stationary"    `Вверху`,`По центру`,`Внизу`IF gs_rot_axis=rot_axis_mtr[1] or ang>60 THEN	values "gs_cutmode_bot" cut_mod_mtr[1],cut_mod_mtr[3]	values "gs_cutmode_up" cut_mod_mtr[1],cut_mod_mtr[3]ELSE	values "gs_cutmode_bot" cut_mod_mtr	values "gs_cutmode_up" cut_mod_mtrENDIFif gs_cut_planes=0 then lock "gs_cutmode_bot","gs_cutang1_bot",							 "gs_cutang2_bot","gs_cutmode_up",							 "gs_cutang3_up","gs_cutang4_up"IF GLOB_MODPAR_NAME="gs_rot_axis" THEN	IF gs_rot_axis=rot_axis_mtr[2] THEN Parameters a_temp=A*cos(ang)	IF gs_rot_axis=rot_axis_mtr[1] THEN Parameters a_temp=AELSE	IF GLOB_MODPAR_NAME="A" or GLOB_MODPAR_NAME="ang" OR GLOB_MODPAR_NAME="temp_ang" THEN		IF gs_rot_axis=rot_axis_mtr[2] THEN Parameters a_temp=A*cos(ang)		IF gs_rot_axis=rot_axis_mtr[1] THEN Parameters a_temp=A		PARAMETERS aa_ = 0	ELSE		IF NOT(aa_) or GLOB_MODPAR_NAME="a_temp" THEN		  IF gs_rot_axis=rot_axis_mtr[2] THEN Parameters A=a_temp/cos(ang)		  IF gs_rot_axis=rot_axis_mtr[1] THEN Parameters A=a_temp		ENDIF	ENDIFENDIFif axis=0 THEN	LOCK "only_axis"	LOCK "axis_line_type"	LOCK "axis_line_pen"ENDIFIF ang=0 AND gs_cutmode_bot<>cut_mod_mtr[3] AND gs_cutmode_up<>cut_mod_mtr[3] OR ang<>0 THEN	HIDEPARAMETER "DashedLineType"ENDIF! ---------- Rotation Angle --------------------------------------------------VALUES "ang" range[-90,90]if temp_ang >= 270 and temp_ang <= 360 THEN PARAMETERS temp_ang = 0		!! Bottomif temp_ang < 270 and temp_ang >= 180 THEN PARAMETERS temp_ang = 180IF GLOB_MODPAR_NAME = "temp_ang" THEN	PARAMETERS ang = temp_ang - 90ELSE	PARAMETERS temp_ang = ang + 90ENDIF! ---------- Cutting Angles --------------------------------------------------VALUES "gs_cutang1_bot" range[-60,60]VALUES "gs_cutang2_bot" range[-60,60]VALUES "gs_cutang3_up" range[-60,60]VALUES "gs_cutang4_up" range[-60,60]if temp_cutang1_bot > 120 and temp_cutang1_bot < (360-120)/2+120 THEN PARAMETERS temp_cutang1_bot = 120if temp_cutang1_bot < 360 and temp_cutang1_bot >= (360-120)/2+120 THEN PARAMETERS temp_cutang1_bot = 0if temp_cutang2_bot > 120 and temp_cutang2_bot < (360-120)/2+120 THEN PARAMETERS temp_cutang2_bot = 120if temp_cutang2_bot < 360 and temp_cutang2_bot >= (360-120)/2+120 THEN PARAMETERS temp_cutang2_bot = 0if temp_cutang3_up > 120 and temp_cutang3_up < (360-120)/2+120 THEN PARAMETERS temp_cutang3_up = 120if temp_cutang3_up < 360 and temp_cutang3_up >= (360-120)/2+120 THEN PARAMETERS temp_cutang3_up = 0if temp_cutang4_up > 120 and temp_cutang4_up < (360-120)/2+120 THEN PARAMETERS temp_cutang4_up = 120if temp_cutang4_up < 360 and temp_cutang4_up >= (360-120)/2+120 THEN PARAMETERS temp_cutang4_up = 0IF GLOB_MODPAR_NAME = "temp_cutang1_bot" THEN	PARAMETERS gs_cutang1_bot = temp_cutang1_bot - 60ELSE	PARAMETERS temp_cutang1_bot = gs_cutang1_bot + 60ENDIFIF GLOB_MODPAR_NAME = "temp_cutang2_bot" THEN	PARAMETERS gs_cutang2_bot = temp_cutang2_bot - 60ELSE	PARAMETERS temp_cutang2_bot = gs_cutang2_bot + 60ENDIFIF GLOB_MODPAR_NAME = "temp_cutang3_up" THEN	PARAMETERS gs_cutang3_up = temp_cutang3_up - 60ELSE	PARAMETERS temp_cutang3_up = gs_cutang3_up + 60ENDIFIF GLOB_MODPAR_NAME = "temp_cutang4_up" THEN	PARAMETERS gs_cutang4_up = temp_cutang4_up - 60ELSE	PARAMETERS temp_cutang4_up = gs_cutang4_up + 60ENDIF