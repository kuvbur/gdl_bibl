EPS = 0.00001if GLOB_SCRIPT_TYPE <> 5 then	if GLOB_CONTEXT = 6 then	! Window Schedule - 3D view should NOT display sash in open position		gs_open_3D = 0		gs_open3D = 0	endifendifDET2D_OFF				= 0DET2D_SCALESENS			= -1DET2D_SIMPLE			= 11DET2D_MIDDLE			= 21DET2D_DETAILED			= 30DET2D_SIMPLE_CHI		= 51DET2D_DETAILED_CHI		= 52! gs_iSwingType:LINE_SHAPE_SWING_NONE			= 0LINE_SHAPE_SWING_CURVED			= 1LINE_SHAPE_SWING_STRAIGHT		= 2LINE_SHAPE_SWING_SIMPLE_LINE	= 3LINE_SHAPE_SWING_LEAF_LINE		= 4unID = 450track_dz = 0.063gs_track_thk = 0.057fb = b+gs_frwtgs_a=ags_b=bgs_pv=gs_frame_pthk 	! Profile thicknessgs_mot_m=0gs_btype_m=1IF iDoorPanelType=1 THEN gs_mot_m=3		! HORIZONTAL RIBBEDIF iDoorPanelType=2 THEN gs_mot_m=2		! VERTICAL RIBBEDIF iDoorPanelType=3 THEN gs_mot_m=1		! CASETTEDIF iDoorPanelType=31 THEN gs_mot_m=4		! CUSTOM PANELgs_bkx=6"gs_bky=5"gs_ufw=1"gs_gtav=0.053gm=0.045gs_gm = 0.045gt=0.088+track_dz*0sza = gs_a + gs_frame_width_left + gs_frame_width_right - 0.11szb=gs_b+gs_frwt-0.063sszb=(szb-gm/2)/gs_section_numIF (SQR(sszb**2/2))+gt<0.42 THEN gs_track_radius=(SQR(sszb**2/2))+gt ELSE gs_track_radius=0.42rk = gs_track_radiusrkk=rk-gttokb=gs_b+gs_frwtszut=tokb-rk-gm/2+0.5*PI*(rk-gt)op3D=gs_open3D/szut*100szute=tokb-rk-gm/2IF ((rkk**2+rkk**2-sszb**2)/2/rkk/rkk)>=-1 AND ((rkk**2+rkk**2-sszb**2)/2/rkk/rkk)<=1 THEN teta=ACS((rkk**2+rkk**2-sszb**2)/2/rkk/rkk)zeta=90-tetautv=op3D/100*szutgs_szekc_m=(szb-gs_gm/2)/gs_section_numrah=gs_rail_h-gs_szekc_m*(gs_section_num-1)IF gs_mot_m=1 THEN	gs_bet_sz=(sza-(gs_hor_pnum+1)*gs_bkx)/gs_hor_pnum	gs_passdoorWidth_=gs_bet_sz+gs_bkxENDIFIF gs_mot_m=2 OR gs_mot_m=3 THEN	IF gs_rail_h<(gs_section_num-1)*sszb+gs_bky THEN gs_rail_h=(gs_section_num-1)*sszb+gs_bky	IF gs_bGlazedDoor=1 THEN		gs_bet_sz=(sza-(gs_hor_pnum+1)*gs_bkx)/gs_hor_pnum		gs_passdoorWidth_=gs_bet_sz+gs_bkx	ENDIFENDIF!!gs_door_hif gs_door_b<EPS then	gs_passdoorWidth = gs_passdoorWidth_else	gs_passdoorWidth = gs_door_bendif