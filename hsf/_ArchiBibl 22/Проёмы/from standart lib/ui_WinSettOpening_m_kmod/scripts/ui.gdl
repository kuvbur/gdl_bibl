dim stDetail[6]	stDetail[1] = `Зависит от Масштаба`	stDetail[2] = `Детальный`	stDetail[3] = `Простой`	stDetail[4] = `Откл.`	stDetail[5] = `Китайский Простой`	stDetail[6] = `Китайский Детальный`dim stDetail2D[12]	stDetail2D[1]	= `по ПМВ`	stDetail2D[2]	= `Зависит от Масштаба`	stDetail2D[3]	= `1:50`	stDetail2D[4]	= `1:100`	stDetail2D[5]	= `Средняя Детализация - без Аксесс.`	stDetail2D[6]	= `Простой - Две Линии`	stDetail2D[7]	= `1:200`	stDetail2D[8]	= `Откл.`	stDetail2D[9]	= `Средняя США`	stDetail2D[10]	= `Китайский Простой`	stDetail2D[11]	= `Китайский Детальный`	stDetail2D[12]	= `1:20`pageStartY = 5y0 			= pageStartY dy			= 23x1			= 5x2			= 165x3 			= 229heightOutf	= 15heightInf	= 19! ======================================================================! Detail Level! ======================================================================bVertSeparator = 0if iWindowType = WIN_WHalfRound |\ 	iWindowType = WIN_WMultiside |\ 	iWindowType = WIN_WOval 	|\ 	iWindowType = WIN_WRound 	|\ 	iWindowType = WIN_WPentagon |\ 	iWindowType = WIN_Skewed	|\ 	iWindowType = WIN_WTriangle |\ 	iWindowType = WIN_WEyebrows |\ 	iWindowType = WIN_Rhomb then	y0 = pageStartY + 203	x4 = 157	widthOutf = 150	widthInf = 135	sepy = pageStartY + 200else	bVertSeparator = 1	if iWindowType = WIN_Curved then		y0 = pageStartY + dy + 13		x4 = 130		widthOutf = 124		widthInf = 88		sepy = pageStartY + 27	else		y0 = pageStartY + 194		x4 = 130		widthOutf = 124		widthInf = 88		sepy = pageStartY + 188	endifendifgosub "DetailLevel"! ======================================================================! Natural Ventilation! ======================================================================if iWindowType <> WIN_Curved &\	iWindowType <> WIN_Bow &\	iWindowType <> WIN_SplayBay &\	iWindowType <> WIN_SquareBay &\	iWindowType <> WIN_WRound &\	iWindowType <> WIN_WOval &\	iWindowType <> WIN_WMultiside &\	iWindowType <> WIN_WHalfRound &\	iWindowType <> WIN_WQuarterRound &\	iWindowType <> WIN_WPentagon &\	iWindowType <> WIN_Skewed &\	iWindowType <> WIN_WTriangle &\	iWindowType <> WIN_WEyebrows &\	iWindowType <> WIN_Rhomb then	gosub "VentilationType"endif! ======================================================================! Number of Sides! ======================================================================if iWindowType = WIN_WMultiside then	y0 = pageStartY + 176	ui_style 0, 1	ui_outfield `Количество Сторон`, x1, y0, widthOutf, heightOutf	ui_style 0, 0	ui_infield "gs_numOfSides", x4 + 85, y0-4, 50, heightInfendif! ======================================================================end! ======================================================================"DetailLevel":	ui_style 0,1	ui_outfield `Детализация`, x1, y0, widthOutf, heightOutf	y0 = y0 + 19	ui_style 0,0	ui_outfield `3D-детализация`, x1,y0, widthOutf, heightOutf		ui_infield{3} "iDWDet3D",x4, y0-4, widthInf,heightInf,			8, "",			0, 0,			0, 0, 0, 0,			"",	stDetail[1],	DETLEVEL_DW3D_SCALESENS,			"",	stDetail[2],	DETLEVEL_DW3D_DETAILED,			"",	stDetail[3],	DETLEVEL_DW3D_SIMPLE,			"",	stDetail[4],	DETLEVEL_DW3D_OFF,			"",	stDetail[5],	DETLEVEL_DW3D_CHI_SIMPLE,			"",	stDetail[6],	DETLEVEL_DW3D_CHI_DETAILED	y0 = y0 + dy	ui_outfield `2D-детализация`, x1,y0, widthOutf, heightOutf		ui_infield{3} "iDWDet2D", x4, y0-4, widthInf, heightInf,			8, "",			0, 0,			0, 0, 0, 0,			"",	stDetail2D[1],	DETLEVEL_DW2D_MVO,			"",	stDetail2D[2],	DETLEVEL_DW2D_SCSENS,			"",	stDetail2D[3],	DETLEVEL_DW2D_50,			"",	stDetail2D[4],	DETLEVEL_DW2D_100,			"",	stDetail2D[5],	DETLEVEL_DW2D_MIDDET,			"",	stDetail2D[6],	DETLEVEL_DW2D_SMPL2L,			"",	stDetail2D[7],	DETLEVEL_DW2D_200,			"",	stDetail2D[8],	DETLEVEL_DW2D_OFF,			"",	stDetail2D[9],	DETLEVEL_DW2D_MIDUSA,			"",	stDetail2D[10],	DETLEVEL_DW2D_CHISMPL,			"",	stDetail2D[11],	DETLEVEL_DW2D_CHIDET,			"",	stDetail2D[12],	DETLEVEL_DW2D_20	y0 = y0 + dy	ui_separator x1 - 3, sepy, 442, sepy	if bVertSeparator = 1 then		ui_separator 222, y0-67, 222, y0 + 2	endif return"VentilationType":	y0 = pageStartY + 194	ui_style 0,1		ui_infield{3} "bVentilationProfile", x3, y0, 212, heightInf,		7, "",		2,		1, 20, 20, 20, 20,		0, `Приточная Вентиляция`, 0,			1, `Приточная Вентиляция`, 1				y0 = y0 + 20	ui_style 0, 0	if gs_VentilationProfileTypeEnable > 0 then		ui_infield{3} "iVentilationProfileType", x3 + 72, y0, 140, heightInf,				8, "",				3,				1, 20, 20, 20, 20,				1, stVentilationProfileType[1], VENTPROFILE_NONE,			! Block				2, stVentilationProfileType[2], VENTPROFILE_OVER_FRAME,		! Double Rebate				3, stVentilationProfileType[3], VENTPROFILE_OVER_GLAZING	! Simple Rebate				y0 = y0 + dy	endif	ui_pict_button ui_function, "", "uiFunctionVentilation_flat(1)", 405, y0, 36, 22, -iUiID_VentProfile ! Set GLOB_UI_BUTTON_IDreturn