targetGUID = TO_GUID! ==============================================================================! Subroutines! ==============================================================================gosub "RenameMinSpaceParams_BWM"gosub "DoorPanelType_BWM"gosub "deleteUnusedListParams_BWM"gosub "NewDemolitionSymbol_BWM"gosub "B406_NewFuctionsCommon_BWM"! ==============================================================================! Set migration GUID! ==============================================================================setmigrationguid targetGuid! ==============================================================================end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en! ==============================================================================! ==============================================================================! RenameMinSpaceParams! =============================================================================="RenameMinSpaceParams_BWM":	if targetGUID # "" then	    newparameter "ms_openingSWidth1", "Length"		newparameter "ms_openingSWidth2", "Length"	    newparameter "ms_oppositeSWidth1", "Length"		newparameter "ms_oppositeSWidth2", "Length"				ms_openingSWidth1 = msOpeningSideWidth2		ms_openingSWidth2 = msOpeningSideWidth1	    ms_oppositeSWidth1 = msOppositeSideWidth2	    ms_oppositeSWidth2 = msOppositeSideWidth1				parameters 	ms_openingSWidth1 = ms_openingSWidth1,					ms_openingSWidth2 = ms_openingSWidth2,	                ms_oppositeSWidth1 = ms_oppositeSWidth1,	                ms_oppositeSWidth2 = ms_oppositeSWidth2	endifreturn! ==============================================================================! DoorPanelType! =============================================================================="DoorPanelType_BWM":	if targetGUID # "" then		newparameter "gs_door_typ_m", "Integer"			gs_door_typ_m = iDoorPanelType		parameters gs_door_typ_m = gs_door_typ_m	endifreturn! ==============================================================================! deleteUnusedListParams! =============================================================================="deleteUnusedListParams_BWM":	if targetGUID # "" then		! Empty Step migrate backward every case	endifreturn! ==============================================================================! NewDemolitionSymbol! =============================================================================="NewDemolitionSymbol_BWM":	if targetGUID # "" then		if (iTypeDemolitionFill <> 1) then		! not RENOVATION_FILL_EXIST			targetGUID = "" 		endif	endifreturn! ==============================================================================! B406_NewFuctionsCommon! =============================================================================="B406_NewFuctionsCommon_BWM":	if targetGUID # "" then		bMigrationSuccess = 1		call "Migration_DoorWindow" parameters all	iMigrationStep			= 1,													bMigrateDoorLeaf		= 0,													bMigrateDoorHandle		= 0,													bMigrateKickBoard		= 1,													bMigrateKickBoard2		= 1,													bMigrateCasingType		= 0,													bMigrateBottomCasing	= 0,													bMigrateCasingCover		= 0,								returned_parameters bMigrationSuccess			if not(bMigrationSuccess) then			targetGUID = ""		endif	endifreturn