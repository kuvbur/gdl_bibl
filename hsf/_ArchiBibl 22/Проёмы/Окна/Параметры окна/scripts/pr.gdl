razd_ceil = "."ignore_id = "!!"if STRSTR(GLOB_ID, ignore_id)=0 then	area_zap = ac_wallhole_width*ac_wallhole_height	if tip_spec="Размер проёма" then		area_zap = ac_wallhole_width*ac_wallhole_height	else		area_zap = ac_unit_height*ac_unit_width	endif	subpos = GLOB_ID	pos = ""	call "Спецификатор" parameters tip_el="60",									subpos = subpos,									pos = "окно"+pos,									obozn=obozn,									naen=naen,									ves=ves,									kol_vo_spec=1,									n_elev = n_elev,									mun_elev = 1,									komment=' ', spec_add_param = "@S="+str("%.2",area_zap)	if gs_board_inside then		call "Спецификатор" parameters tip_el="60",										subpos = subpos,										pos = gs_list_headdet, 										obozn=board_gost,										naen="ПВХ доска подоконная шириной "+ str("%.0mm",t_podok) + " мм.",										ves=1.5,										kol_vo_spec=l_podok,										n_elev = n_elev,										mun_elev = 1,										komment='п.м.', spec_add_param = "@S="+str("%.2",t_podok*l_podok)	endif	if gs_sill_outside and gs_sill_width>EPS then		call "Спецификатор" parameters tip_el="60",										subpos = subpos, 										pos = " ", 										obozn=" ",										naen="Стальной окрашеный отлив шириной "+ str("%.0mm",gs_sill_width) + " мм.",										ves=0.5,										kol_vo_spec=gs_sill_ovhg_left+gs_sill_ovhg_right+ac_wallhole_width,										n_elev = n_elev,										mun_elev = 1,										komment='п.м.', spec_add_param = "@S="+str("%.2",gs_sill_width*(gs_sill_ovhg_left+gs_sill_ovhg_right+ac_wallhole_width))	endifendif