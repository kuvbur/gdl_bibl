dim ttype_shrift_tip[]dim itype_shrift_tip[] : i=0i=i+1 : itype_shrift_tip[i]=0 : ttype_shrift_tip[i] = "Обычный"i=i+1 : itype_shrift_tip[i]=1 : ttype_shrift_tip[i] = "Жирный(Ж)"i=i+1 : itype_shrift_tip[i]=2 : ttype_shrift_tip[i] = "Курсив(К)"i=i+1 : itype_shrift_tip[i]=4 : ttype_shrift_tip[i] = "Подчеркнутый(П)"i=i+1 : itype_shrift_tip[i]=3 : ttype_shrift_tip[i] = "Ж+К"i=i+1 : itype_shrift_tip[i]=5 : ttype_shrift_tip[i] = "Ж+П"i=i+1 : itype_shrift_tip[i]=6 : ttype_shrift_tip[i] = "К+П"i=i+1 : itype_shrift_tip[i]=7 : ttype_shrift_tip[i] = "Ж+К+П"values{2} "Shrift_otm_tip" itype_shrift_tip, ttype_shrift_tipvalues{2} "Shrift_razm_tip" itype_shrift_tip, ttype_shrift_tipvalues "Shrift_otm_razm" range[2,]values "Shrift_razm_razm" range[2,]dim fontnames[]n = REQUEST ("FONTNAMES_LIST", "", fontnames)values "Shrift" fontnames, CUSTOMif Shrift_from_as then LOCK "Shrift_otm_tip", "Shrift_razm_tip", "Shrift_otm_razm", "Shrift_razm_razm", "Shrift"PARAMETERS type_perem = t_type_peremvalues{2} "show_perem_2d" ishow_perem, tshow_peremvalues{2} "show_perem_3d" ishow_perem, tshow_peremvalues{2} "perem_ved_pos" ived_pos, tved_posif perem_pen_eq_sim then	lock "perem_pen_sim_m"else	lock "perem_pen_sim"endifif not(perem_ved_wall_show) then	lock "perem_ved_wall_pen", "perem_ved_wall_eq", "perem_ved_wall"endifif perem_fill_eq then	lock "perem_fill", "perem_fill_pen", "perem_fill_bg"endifif GLOB_MODPAR_NAME = "is_load_pos" and is_load_pos then	parameters is_load_pos = 0	f_info_pos = "ОШИБКА"	gosub "load_txt_pos"	if n_load>EPS then		PARAMETERS perem_pos_in = t_pos, perem_naen_in = t_naen, perem_obozn_in=t_obozn		PARAMETERS perem_pos_in_read = 1	endif	dstr = ""	ch22 = OPEN ("DateTime", "", "%y/%m/%d %H:%M")	n = INPUT (ch22, "", "", dstr)	CLOSE (ch22)	parameters f_info_pos = "Прочитано поз " + str("%.0",n_load) + "\n" + dstrendifif GLOB_MODPAR_NAME = "is_load_marka" and is_load_marka then	parameters is_load_marka = 0	f_info_marka = "ОШИБКА"	gosub "load_txt_mark"	if n_load>EPS then		PARAMETERS perem_sum_in=t_chsum,perem_otm_in=t_otm,perem_marka_in=t_mark		PARAMETERS perem_marka_in_read = 1	endif	dstr = ""	ch22 = OPEN ("DateTime", "", "%y/%m/%d %H:%M")	n = INPUT (ch22, "", "", dstr)	CLOSE (ch22)	parameters f_info_marka = "Прочитано марок " + str("%.0",n_load) + "\n" + dstrendif!---- Чтение сортамента брусковых перемычек ----	dim naen_m_br[] : naen_m_br[1] = ""	dim dlin_m_br[]	dim max_pr_m_br[]	dim tolsh_m_br[]	dim vis_m_br[]	dim nagr_m_br[]	dim ves_m_br[]	dim gost_m_br[] : gost_m_br[1] = ""	ch3=OPEN("TEXT","перемычки.txt","SEPARATOR='\t',MODE=RO,LIBRARY")	i = 1: n=1: j=1 : v0="" : v1=0 : v2=0 : v3=0 : v4=0 : v5=0	"nxt":	if n <> -1 then		n=INPUT(ch3,i,1,v0,v1,v2,v3,v4,v5)		if vartype(v0)=2 then			if STRSTR(v0,"-п")=0 then				naen_m_br[j]=v0				dlin_m_br[j]=v1/1000				tolsh_m_br[j]=v2/1000				vis_m_br[j]=v3/1000				max_pr_m_br[j] = 0				nagr_m_br[j]=v4				ves_m_br[j]=v5				gost_m_br[j] = "ГОСТ 948-2016"				j = j + 1			endif		endif		i = i + 1		goto "nxt"	else		close (ch3)	endif	if j>1 then		PARAMETERS naen_m_br=naen_m_br		PARAMETERS dlin_m_br=dlin_m_br		PARAMETERS tolsh_m_br=tolsh_m_br		PARAMETERS vis_m_br=vis_m_br		PARAMETERS nagr_m_br=nagr_m_br		PARAMETERS ves_m_br=ves_m_br		PARAMETERS max_pr_m_br=max_pr_m_br		PARAMETERS gost_m_br = gost_m_br	endif!--------------------------------------------!---- Чтение сортамента фибробетонных перемычек ----	dim naen_m_fb[] : naen_m_fb[1] = ""	dim dlin_m_fb[]	dim max_pr_m_fb[]	dim tolsh_m_fb[]	dim vis_m_fb[]	dim nagr_m_fb[]	dim ves_m_fb[]	dim gost_m_fb[] : gost_m_fb[1] = ""	ch3=OPEN("TEXT","перемычки фибробетон.txt","SEPARATOR='\t',MODE=RO,LIBRARY")	i = 1: n=1: j=1 : v0="" : v1=0 : v2=0 : v3=0 : v4=0 : v5=0 : v6=0	"nxt2":	if n <> -1 then		n=INPUT(ch3,i,1,v0,v1,v2,v3,v4,v5,v6)		if vartype(v0)=2 then			naen_m_fb[j]=v0			dlin_m_fb[j]=v1/1000			tolsh_m_fb[j]=v2/1000			vis_m_fb[j]=v3/1000			nagr_m_fb[j]=v4			ves_m_fb[j]=v5			max_pr_m_fb[j]=v6/1000			gost_m_fb[j]="ТУ 5828-002-80392712-2015"			j = j + 1		endif		i = i + 1		goto "nxt2"	else		close (ch3)	endif	if j>1 then		PARAMETERS naen_m_fb=naen_m_fb		PARAMETERS dlin_m_fb=dlin_m_fb		PARAMETERS tolsh_m_fb=tolsh_m_fb		PARAMETERS vis_m_fb=vis_m_fb		PARAMETERS nagr_m_fb=nagr_m_fb		PARAMETERS ves_m_fb=ves_m_fb		PARAMETERS max_pr_m_fb=max_pr_m_fb		PARAMETERS gost_m_fb = gost_m_fb	endif!--------------------------------------------!---- Чтение сортамента уголков ----	dim naen_m_ug[] : naen_m_ug[1] = ""	dim ves_m_ug[] : ves_m_ug[1] = 0	dim b_ug[] : b_ug[1] = 0	dim h_ug[] : h_ug[1] = 0	dim t_ug[] : t_ug[1] = 0	dim gost_ug[] : gost_ug[1] = ""	ch4=OPEN("TEXT","перемычки уголки.txt","SEPARATOR='\t',MODE=RO,LIBRARY")	i = 1: n=1: j=1 : v0="" : v1=0 : v2=0 : v3=0 : v4=0 : v5=""	"nxt21":	if n <> -1 then		n=INPUT(ch4,i,1,v0,v1,v2,v3,v4,v5,v6)		if vartype(v0)=2 then			naen_m_ug[j]=v0			ves_m_ug[j]=v1			b_ug[j]=v2/1000			h_ug[j]=v3/1000			t_ug[j]=v4/1000			gost_ug[j]=v5			j = j + 1		endif		i = i + 1		goto "nxt21"	else		close (ch4)	endif	if j>1 then		PARAMETERS naen_m_ug=naen_m_ug		PARAMETERS ves_m_ug=ves_m_ug		PARAMETERS b_ug=b_ug		PARAMETERS h_ug=h_ug		PARAMETERS t_ug=t_ug		PARAMETERS gost_ug=gost_ug	endif!--------------------------------------------dim mark_perem[]i=0i = i + 1 : mark_perem[i] = "Любая брусковая"i = i + 1 : mark_perem[i] = "Любая фибро"i = i + 1 : mark_perem[i] = "1ПБ"i = i + 1 : mark_perem[i] = "2ПБ"i = i + 1 : mark_perem[i] = "3ПБ"i = i + 1 : mark_perem[i] = "4ПБ"i = i + 1 : mark_perem[i] = "5ПБ"i = i + 1 : mark_perem[i] = "6ПБ"i = i + 1 : mark_perem[i] = "7ПБ"i = i + 1 : mark_perem[i] = "7ПП"i = i + 1 : mark_perem[i] = "8ПП"i = i + 1 : mark_perem[i] = "9ПП"i = i + 1 : mark_perem[i] = "10ПП"i = i + 1 : mark_perem[i] = "1ПБФ"i = i + 1 : mark_perem[i] = "2ПБФ"i = i + 1 : mark_perem[i] = "3ПБФ"i = i + 1 : mark_perem[i] = "4ПБФ"i = i + 1 : mark_perem[i] = "5ПБФ"i = i + 1 : mark_perem[i] = "6ПБФ"i = i + 1 : mark_perem[i] = "7ПБФ"!---- Чтение пользовательского сортамента ----	dim naen_m_u[] : naen_m_u[1] = ""	dim dlin_m_u[]	dim max_pr_m_u[]	dim tolsh_m_u[]	dim vis_m_u[]	dim nagr_m_u[]	dim ves_m_u[]	dim gost_m_u[] : gost_m_u[1] = ""	ch5=OPEN("TEXT","перемычки custom.txt","SEPARATOR='\t',MODE=RO,LIBRARY")	if ch5>EPS then		i = 1: n=1: j=1 : v0="" : v1=0 : v2=0 : v3=0 : v4=0 : v5=0 : v6=0 : v7=""		"nxt5":		if n <> -1 then			n=INPUT(ch5,i,1,v0,v1,v2,v3,v4,v5,v6,v7)			if vartype(v0)=2 then				naen_m_u[j]=v0				dlin_m_u[j]=v1/1000				tolsh_m_u[j]=v2/1000				vis_m_u[j]=v3/1000				nagr_m_u[j]=v4				ves_m_u[j]=v5				max_pr_m_u[j] = v6/1000				gost_m_u[j] = v7				j = j + 1			endif			i = i + 1			goto "nxt5"		else			close (ch5)		endif		if j>1 then			PARAMETERS naen_m_u=naen_m_u			PARAMETERS dlin_m_u=dlin_m_u			PARAMETERS tolsh_m_u=tolsh_m_u			PARAMETERS vis_m_u=vis_m_u			PARAMETERS nagr_m_u=nagr_m_u			PARAMETERS ves_m_u=ves_m_u			PARAMETERS max_pr_m_u=max_pr_m_u			PARAMETERS gost_m_u = gost_m_u		endif		i = vardim1(mark_perem) + 1 : mark_perem[i] = "Любой пользовательский"		has_custom_perem = 1	else		has_custom_perem = 0	endif	PARAMETERS has_custom_perem = has_custom_perem!--------------------------------------------PARAMETERS mark_perem = mark_peremdim naen_m[] : naen_m[1] = ""dim dlin_m[]dim max_pr_m[]dim tolsh_m[]dim vis_m[]dim nagr_m[]dim ves_m[]j = 0for i=1 to vardim1(naen_m_br)	j = j + 1	naen_m[j]=naen_m_br[i]	dlin_m[j]=dlin_m_br[i]	tolsh_m[j]=tolsh_m_br[i]	vis_m[j]=vis_m_br[i]	nagr_m[j]=nagr_m_br[i]	ves_m[j]=ves_m_br[i]	max_pr_m[j]=max_pr_m_br[i]	gost_m[j] = gost_m_br[i]	type_perem_m[j] = 1next ifor i=1 to vardim1(naen_m_fb)	j = j + 1	naen_m[j]=naen_m_fb[i]	dlin_m[j]=dlin_m_fb[i]	tolsh_m[j]=tolsh_m_fb[i]	vis_m[j]=vis_m_fb[i]	nagr_m[j]=nagr_m_fb[i]	ves_m[j]=ves_m_fb[i]	max_pr_m[j]=max_pr_m_fb[i]	gost_m[j] = gost_m_fb[i]	type_perem_m[j] = 2next iif has_custom_perem then	for i=1 to vardim1(naen_m_u)		j = j + 1		naen_m[j]=naen_m_u[i]		dlin_m[j]=dlin_m_u[i]		tolsh_m[j]=tolsh_m_u[i]		vis_m[j]=vis_m_u[i]		nagr_m[j]=nagr_m_u[i]		ves_m[j]=ves_m_u[i]		max_pr_m[j]=max_pr_m_u[i]		gost_m[j] = gost_m_u[i]		type_perem_m[j] = 3	next iendifif j>1 then	PARAMETERS naen_m=naen_m	PARAMETERS dlin_m=dlin_m	PARAMETERS tolsh_m=tolsh_m	PARAMETERS vis_m=vis_m	PARAMETERS nagr_m=nagr_m	PARAMETERS ves_m=ves_m	PARAMETERS max_pr_m=max_pr_m	PARAMETERS gost_m=gost_m	PARAMETERS type_perem_m = type_perem_mendifend"load_txt_pos":	dstr = ""	ch22 = OPEN ("DateTime", "", "%y%m%d%H%M%S")	n = INPUT (ch22, "", "", dstr)	CLOSE (ch22)	ch4 = OPEN ("TEXT", dstr, "SEPARATOR=';', MODE = RO, DIALOG")	n_load = 0 : err = 0	i = 1: n = 1	"nxt3":	if n <> -1 then		v0=0 : v1=0 : v2=0		n=INPUT(ch4,i,1,v0,v1,v2)		if vartype(v1)=2 then			if vartype(v0)=1 then v0 = str("%.0", v0)			if vartype(v2)=1 then v2 = ""			n_load = n_load + 1			t_pos[n_load] = v0			t_naen[n_load] = v1			t_obozn[n_load] = v2		endif		i = i + 1		if i<10000 then goto "nxt3"	endif	close (ch4)return"load_txt_mark":	dstr = ""	ch22 = OPEN ("DateTime", "", "%y%m%d%H%M%S")	n = INPUT (ch22, "", "", dstr)	CLOSE (ch22)	ch4 = OPEN ("TEXT", dstr+"1", "SEPARATOR=';', MODE = RO, DIALOG")	n_load = 0 : err = 0	i = 1: n = 1	"nxt4":	if n <> -1 then		v0=0 : v1=0 : v2=0		n=INPUT(ch4,i,1,v0,v1,v2)		if vartype(v0)=2 then			if vartype(v1)=1 then v1 = str("%.0", v1)			if vartype(v2)=1 then v2 = str("%.0", v2)			n_load = n_load + 1			t_chsum[n_load] = v0			t_otm[n_load] = v1			t_mark[n_load] = v2		endif		i = i + 1		if i<10000 then goto "nxt4"	endif	close (ch4)return