call "Перемычки" parameters all, otstup=otstup, max_t=max_t, otstup_2=otstup_2call "Жб обрамление проёма" parameters all, otstup=otstup, max_t=max_t, otstup_2=otstup_2flag_pr = 0 : if (GLOB_VIEW_TYPE) <> 9 and ((GLOB_STRUCTURE_DISPLAY = upr_n) or asotv or main_name = "") then flag_pr = 1SHADOW OFF, OFFif flag_pr then	pen i_pen!	MATERIAL gs_ifc_mat	del top	k = 1addz -otstup_2if bRotated=0 and SYMB_MIRRORED=1 then mulx -1if bRotated=1 and SYMB_MIRRORED=0 then mulx -1if not((AC_Wall_Direction_Type=0) or (AC_Wall_Direction_Type=2)) then roty 180mulz sgn(otstup)if abs(otstup)-900<EPS then addz abs(otstup)addz max_t/2!if (AC_Wall_Direction_Type=0) or (AC_Wall_Direction_Type=2) then roty 180if asotv and (((B+porog)<min_h_otv) and (A<min_b_otv) or not(show_asotv)) then 	WALLHOLE 3, 1,			0.001, 0, 15,			0, 0, 15,			0, 0.001, 15	endelse	if krugl then 		WALLHOLE  2,1, 0,0,900, A/2, 360, 4000		HOTSPOT 0, 0, 0, unID: unID=unID+1		HOTSPOT -A/2, 0, 0, unID: unID=unID+1		HOTSPOT A/2, 0, 0, unID: unID=unID+1		HOTSPOT 0, A/2, 0, unID: unID=unID+1		HOTSPOT 0, -A/2, 0, unID: unID=unID+1		HOTSPOT 0, 0, -max_t, unID: unID=unID+1		HOTSPOT -A/2, 0, -max_t, unID: unID=unID+1		HOTSPOT A/2, 0, -max_t, unID: unID=unID+1		HOTSPOT 0, A/2, -max_t, unID: unID=unID+1		HOTSPOT 0, -A/2, -max_t, unID: unID=unID+1	endifendif	ango = 160	if upr_n = -1 or GLOB_STRUCTURE_DISPLAY = upr_n then		if krugl then			model solid			SECT_ATTRS GLOB_FILL_INDEX_BACKGROUND, 0, 0, 0			x = sin(32.75) * A/2			y = cos(32.75) * A/2			poly_ 7, 			-x, -y, 1, 			A * 0.15, -A * 0.15, 901, 			0, -155, 4000, 			x, y, 1, 			0, 0, 901, 			0, 205, 4001, 			-x, -y, -1			HOTSPOT 0, 0, 0, unID: unID=unID+1			HOTSPOT -A/2, 0, 0, unID: unID=unID+1			HOTSPOT A/2, 0, 0, unID: unID=unID+1			HOTSPOT 0, A/2, 0, unID: unID=unID+1			HOTSPOT 0, -A/2, 0, unID: unID=unID+1			HOTSPOT 0, 0, -max_t, unID: unID=unID+1			HOTSPOT -A/2, 0, -max_t, unID: unID=unID+1			HOTSPOT A/2, 0, -max_t, unID: unID=unID+1			HOTSPOT 0, A/2, -max_t, unID: unID=unID+1			HOTSPOT 0, -A/2, -max_t, unID: unID=unID+1		else			addx -A/2				WALLHOLE  4,1,					0,-porog,15,					0,B,15,					A,B,15,					A,-porog,15			HOTSPOT 0, 0, 0, unID: unID=unID+1			HOTSPOT pos_x, 0, 0, unID, pos_y, 1+128: unID=unID+1			HOTSPOT pos_x, pos_y, 0, unID, pos_y, 2: unID=unID+1			HOTSPOT 0, pos_y, 0, unID, pos_x, 1+128: unID=unID+1			HOTSPOT pos_x, pos_y, 0, unID, pos_x, 2: unID=unID+1			POLY  4,				0,-porog,				0,B,				A,B,				pos_x,pos_y			endif			hotspot 0,-porog,0, unID : unID=unID+1			hotspot 0,B,0, unID : unID=unID+1			hotspot A,B,0, unID : unID=unID+1			hotspot A,-porog,0, unID : unID=unID+1			hotspot 0,-porog,-max_t, unID : unID=unID+1			hotspot 0,B,-max_t, unID : unID=unID+1			hotspot A,B,-max_t, unID : unID=unID+1			hotspot A,-porog,-max_t, unID : unID=unID+1	endif	del topendif