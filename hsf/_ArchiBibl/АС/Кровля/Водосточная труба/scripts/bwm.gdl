targetGUID = TO_GUID! ==============================================================================! Subroutines! ==============================================================================gosub "repairHeight_BWM"gosub "newparameter_BWM"! ==============================================================================! Set migration GUID! ==============================================================================setmigrationguid targetGUID! ==============================================================================end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en! ==============================================================================! ==============================================================================! Newparameter: dis_wall! =============================================================================="newparameter_BWM":	if targetGUID # "" then		if gs_gutter_ds_profile_num = 1 then  !	if Downspout Profile is round			if abs(dis_wall - gs_gutter_dd) > EPS then				targetGuid = ""			else				gs_gutter_dd = dis_wall				parameters gs_gutter_dd = gs_gutter_dd			endif		else	!	if Downspout Profile is rectangular or corrugated			if abs(dis_wall - gs_gutter_bb) > EPS then				targetGuid = ""			else				gs_gutter_bb = dis_wall				parameters gs_gutter_bb = gs_gutter_bb			endif		endif	endifreturn! ==============================================================================! Head leader could be placed incorrrectly in 19! =============================================================================="repairHeight_BWM":	if targetGUID # "" then		!don't migrate angled downspout + offset, different geometry in 19		if (gs_downspout_offset | gs_head_leader) & abs(gs_gutter_ds_ang) > EPS  then targetGUID = ""	endifreturn