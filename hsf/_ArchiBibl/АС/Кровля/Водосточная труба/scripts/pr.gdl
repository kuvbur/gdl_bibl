rz = ";"tip_el = "40"obozn = " "gid = ""name_syst = "оцинк. сталь 0,5мм с покрытием"Dtxt = "D" + str("%.0mm", gs_gutter_dd2)ros = 7850*0.0005komment = "шт."!Трубаpos = "ТВ"kol_vo_spec = round_int(gs_gutter_ds_lgth/3)ves = gs_gutter_dd2*3.14*ros* kol_vo_spec*3naen = "Труба водосточная " + Dtxt +" L=3м"gosub "spec"pos = " "!Крепёжif gs_gutter_ds_h then	kol_vo_spec = round_int(gs_gutter_ds_lgth/gs_gutter_ds_h_dist)	ves = 0.2	naen = "Держатель трубы"	gosub "spec"endif!Воронкаif gs_head_leader then	kol_vo_spec = 1	ves = 0.4	naen = "Воронка D"+ str("%.0mm", HL_Width)+"/" + Dtxt	gosub "spec"endif!Коленоif gs_elbow then	kol_vo_spec = 1	ves = 0.4	naen = "Колено "+ Dtxt + "("+str("%.0", gs_gutter_elb_l_ang)+")"	gosub "spec"endif!Колено 2kol_vo_spec = 1ves = 0.4naen = "Колено "+ Dtxt + "(60)"gosub "spec"end"spec":	call "Спецификатор" parameters tip_el=tip_el,									subpos = subpos, 									pos = pos, 									obozn=obozn,									naen=naen+", "+name_syst,									ves=ves,									kol_vo_spec=kol_vo_spec,									komment=komment, gid = gidreturn