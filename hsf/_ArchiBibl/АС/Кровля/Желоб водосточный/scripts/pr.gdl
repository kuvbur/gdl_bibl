rz = ";"tip_el = "40"obozn = "МеталлПрофиль"name_syst = "МП Проект"Dtxt = "D" + str("%.0mm", gs_gutter_p_r*2)ros = 7850*0.0005komment = "шт."!Желобpos = "ЖВ"kol_vo_spec = round_int(gs_gutter_lgth2/3)ves = gs_gutter_p_r*2*3.14*ros*3naen = "Желоб водосточный " + Dtxt +"х3"gosub "spec"!Крепёжpos = " "if gs_gutter_hanger then	kol_vo_spec = round_int(gs_gutter_lgth/gs_gutter_hanger_dist)	ves = 0.2	naen = "Держатель желоба"	gosub "spec"endif!Воронкаif gs_gutter_downspout then	kol_vo_spec = 1	ves = 0.4	naen = "Воронка выпускная "+Dtxt+"/"+str("%.0mm", gs_gutter_dd2)	gosub "spec"endif!Заглушкиif gs_gutter_endcap_l then	kol_vo_spec = 1	ves = 0.1	naen = "Заглушка желоба "+Dtxt	gosub "spec"endifif gs_gutter_endcap_r then	kol_vo_spec = 1	ves = 0.1	naen = "Заглушка желоба "+Dtxt	gosub "spec"endif!Свесif ksves then	kol_vo_spec = round_int(gs_gutter_lgth/2)	ves = 0.169*ros* kol_vo_spec*2	naen = "Планка карнизная 100х69х2000"	gosub "spec"endifend"spec":	call "Спецификатор" parameters tip_el=tip_el,									subpos = "-", 									pos = pos, 									obozn=obozn,									naen=name_syst + " " + naen,									ves=ves,									kol_vo_spec=kol_vo_spec,									komment=kommentreturn