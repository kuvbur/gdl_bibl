show = 0call "Показ_по_сборкам" PARAMETERS ALL subpos=subpos, pos=GLOB_ID RETURNED_PARAMETERS show_subpos, show_markershow = show_subposif not(show) then endpen gs_sec_pensuccess = LIBRARYGLOBAL ("Libraryflobals-as", "gs_detlevel_3D_m", gs_detlevel_3D_m)gs_resol=16gs_shadow = 1if gs_detlevel_3D_m = 0 then ENDif gs_detlevel_3D_m = 1 or (GLOB_CONTEXT>20 and GLOB_CONTEXT<40) then 	gs_resol=6	gs_shadow = 0endifRESOL gs_resolif gs_shadow=0 then SHADOW OFFif gs_shadow=1 then SHADOW ON_bmat = mat : gosub "set_mat"addz -ZZYZXHOTSPOT 0,0,0HOTSPOT A,0,0HOTSPOT A,B,0HOTSPOT 0,B,0HOTSPOT 0,0,ZZYZXHOTSPOT A,0,ZZYZXHOTSPOT 0.3,0,ZZYZXHOTSPOT A-0.3,0,ZZYZXZZYZX_ = ZZYZXif spec_prim then	B = B - t_shov	addz -(th_in_1+th_in_2+th_in_3+th_in_4)	mat_ = mat_shov_1 : gosub "set_mat" : BRICK A, t_shov, h_shov-0.015-0.03	addz h_shov-0.015-0.03	mat_ = mat_shov_2 : gosub "set_mat" : BRICK A, t_shov, 0.03	addz 0.03	mat_ = mat_shov_3 : gosub "set_mat" : BRICK A, t_shov, 0.015	del 3	addy t_shovendifsumm_th = th_in_1+th_in_2+th_in_3+th_in_4summ_th_ = 0if vh_pl then	HOTSPOT A,B,ZZYZX	HOTSPOT 0,B,ZZYZX	group "pl"		if has_stup>0 and vh_pl then			if stup_side = 1 then			endif			if stup_side = 2 then				put 0,0			endif			if stup_side = 3 then			endif		endif		mat_ = mat : gosub "set_mat"!		h_pl = ZZYZX : b_pl = A : l_pl = B : th_mat = 0!!		put 0,0!		put 0,h_pl!		put b_pl,h_pl!		if has_stup>0 then!			stx = 0 : sty = stup_h!			put b_pl, h_pl-sty!			for i=1 to has_stup!				stx = stx+stup_b!				put b_pl+stx, h_pl-sty!				sty = sty+stup_h!				put b_pl+stx, h_pl-sty!			next i!		endif!		put b_pl+stx, 0!!		rotx 90!		addz -l_pl!		PRISM NSP/2, l_pl, GET(NSP)!		del 2!		model wire		mat_ = mat_out_1 : th_ = th_out_1 : gosub "layer_out"		mat_ = mat_out_2 : th_ = th_out_2 : gosub "layer_out"		mat_ = mat_out_3 : th_ = th_out_3 : gosub "layer_out"		mat_ = mat_out_4 : th_ = th_out_4 : gosub "layer_out"		mat_ = mat : gosub "set_mat"		addx summ_th_		BRICK A-summ_th_*2, B+summ_th, ZZYZX		HOTSPOT A-summ_th_*2, B+summ_th, ZZYZX		HOTSPOT 0, B+summ_th, ZZYZX		HOTSPOT A-summ_th_*2, 0, ZZYZX		del 1		ZZYZX = 0		mat_ = mat_in_1 : th_ = th_in_1 : gosub "layer_in"		mat_ = mat_in_2 : th_ = th_in_2 : gosub "layer_in"		mat_ = mat_in_3 : th_ = th_in_3 : gosub "layer_in"		mat_ = mat_in_4 : th_ = th_in_4 : gosub "layer_in"	endgroup	group "op"		zaz1 =  0.01		mat_ = mat : gosub "set_mat"		add (A-0.69-zaz1*2)/2,B-0.48-0.3-zaz1,ZZYZX_-0.043		BRICK 0.69+zaz1*2, 0.48+zaz1*2, 0.043		del 1		zaz2 =  0.03		add (A-0.69+zaz2*2)/2,B-0.48-0.3+zaz2,ZZYZX_-0.05-0.043		BRICK 0.69-zaz2*2, 0.48-zaz2*2, 0.05		del 1	endgroup	if gs_detlevel_3D_m = 1 then 		placegroup "pl"	else		if resh then			res1 = SUBGROUP("pl", "op")			placegroup res1			add (A-0.69)/2,B-0.48-0.3,ZZYZX_-0.04			call    "Решётка для чистки ног РН-7.5-2",					PARAMETERS A =         0.69, B =         0.48, ZZYZX =         0.04,						AC_show2DHotspotsIn3D =      1, ac_bottomlevel =            1,						ac_toplevel =            0, obozn = "Серия 1.100.2-5",						naen = "Решётка для чистки ног РН-7.5-2", ves =        12.84,						kol_vo_spec =      1, komment = " ", subpos = subpos, tip_el = "40"			del 1		else			placegroup "pl"		endif	endifendifif pand then	mat_ = mat : gosub "set_mat"	HOTSPOT A,B,tniz	HOTSPOT 0,B,tniz	rotx 90	roty 90	if th_out_1>0 then		_mat = 1 : bSucceed = request{2} ("Building_Material_info", mat_out_1, "gs_bmat_surface", _mat)		material _mat	endif	PRISM 4, A,		0, 0,		B, 0,		B, tniz,		0, ZZYZX	del 2	ZZYZX = 0	mat_ = mat_in_1 : th_ = th_in_1 : gosub "layer_in"	mat_ = mat_in_2 : th_ = th_in_2 : gosub "layer_in"	mat_ = mat_in_3 : th_ = th_in_3 : gosub "layer_in"	mat_ = mat_in_4 : th_ = th_in_4 : gosub "layer_in"endifdel topmodel solidZZYZX = ZZYZX_end"set_mat":	n = REQUEST{2} ("Building_Material_info", mat_, "gs_bmat_cutfill_properties", _fillType, _fillPen, _fillBgPen)	_mat = 1 : bSucceed = request{2} ("Building_Material_info", mat_, "gs_bmat_surface", _mat)	building_material mat_	material _mat	sect_attrs{2} gs_sec_pen, 1return"layer_pand":	if mat_>0 and th_>EPS then		gosub "set_mat"		ZZYZX = ZZYZX - th_		add -summ_th, 0, ZZYZX		BRICK A+summ_th*2, B+summ_th, th_		HOTSPOT A+summ_th*2, B+summ_th, th_		HOTSPOT 0, B+summ_th, th_		HOTSPOT A+summ_th*2, 0, th_		del 1		PRISM 8, ZZYZX,		A+summ_th, 0,		A+summ_th, B+summ_th,		-summ_th, B+summ_th,		-summ_th, 0,		-summ_th+th_, 0,		-summ_th+th_, B+summ_th-th_,		A+summ_th-th_, B+summ_th-th_,		A+summ_th-th_, 0		summ_th = summ_th - th_		summ_th_ = summ_th_ + th_	endifreturn"layer_out":	if mat_>0 and th_>EPS then		gosub "set_mat"		ZZYZX = ZZYZX - th_		add -summ_th, 0, ZZYZX		BRICK A+summ_th*2, B+summ_th, th_		HOTSPOT A+summ_th*2, B+summ_th, th_		HOTSPOT 0, B+summ_th, th_		HOTSPOT A+summ_th*2, 0, th_		del 1		PRISM 8, ZZYZX,		A+summ_th, 0,		A+summ_th, B+summ_th,		-summ_th, B+summ_th,		-summ_th, 0,		-summ_th+th_, 0,		-summ_th+th_, B+summ_th-th_,		A+summ_th-th_, B+summ_th-th_,		A+summ_th-th_, 0		summ_th = summ_th - th_		summ_th_ = summ_th_ + th_	endifreturn"layer_in":	if mat_>0 and th_>EPS then		gosub "set_mat"		ZZYZX = ZZYZX - th_		add -in_offset, 0, ZZYZX		BRICK A+in_offset*2, B+in_offset, th_		del 1	endifreturn