! ==============================================================================! Basin 02! ==============================================================================unID = 1hotspot2 0, 0, unID : unID = unID + 1      ! Default Hotspot! === Minimal Space ========================================================reqBMinSpace = 0if MSFront > EPS or MSSide > EPS or MSSide2 > EPS then	drawindex 20	call "fa_minimalspace" parameters all MSType = 8, unID = 3000,								returned_parameters reqBMinSpace,													MSFillType,													MSFillPen,													MSFillBackPen,													MSContLineType,													MSContLinePen	if reqBMinSpace <> 0 then		FILL MSFillType		line_type MSContLineType		pen MSContLinePen		for i = 1 to 2			if iSymbolType = 2 then				mul2 a/0.46,b/0.33				POLY2_B 9, 1+2+4,MSFillPen,MSFillBackPen,						0.229995, 			-0.330000002987,    (abs(MSSide2) < EPS),						0.229995, 			-0.1691788864483,   0,						0.09112000116124, 	-0.1679999999998,   900,						0, 					-61.03907623342,   	4000,						0.1573336153633, 	-0.2900795329692,   0,						0, 					0,    				900,						0, 					-28.47456123756,   	4001,						0, 					-0.330000002987,    (abs(MSFront) < EPS),						0.229995, 			-0.330000002987,    -1				del 1			else				mul2 a/0.46,b/0.373				POLY2_B 7, 1+2+4,MSFillPen,MSFillBackPen,						0.2226530996926, 	-0.06484830776218,  0,						-0.02360937645825, 	-0.1254495343801,   900,						0, 					-13.8248713831,   	4000,						0.23, 				-0.125449534378,    (abs(MSSide2) < EPS),						0.23,            	0,      			1,						0.1983349842818,    0,      			0,						0.2226530996926, 	-0.06484830776218,  0				POLY2_B 10, 1+2+4,MSFillPen,MSFillBackPen,						0.23,       		-0.373,      		(abs(MSSide2) < EPS),						0.23, 				-0.1254495343781,   0,						-0.02360937645849, 	-0.1254495343781,   900,						0, 					-64.72963475838,   	4000,						0.08465397919317, 	-0.3547893731561,   0,						0, 					-0.166363164793,    900,						0, 					-24.19288669681,   	4000,						0, 					-0.3729321174246,   0,						0,       			-0.373,      		(abs(MSFront) < EPS),						0.23,       		-0.373,     		0				del 1			endif			mul2 -1, 1			MSSide2 = MSSide		next i		del 2	endif	line_type 1endifDrawingNum = 1IF DrawingNum = 1 THEN	! Top	pen gs_cont_pen	fill gs_fill_type	! ==========================================================================	! Scale	! ==========================================================================	call "2dDetailLevel" parameters gs_detlevel_2D_m = gs_detlevel_2D_m,					returned_parameters gs_detlevel_2D_m	! === Basin Symbol =========================================================	unID = 10	drawindex 20	if iSymbolType = 1 then		! Symbolic		mul2 a/0.46,b/0.373		hotspot2  0,             -0.372932117425		hotspot2 -0.198334984282, 0		hotspot2  0.198334984282, 0		hotspot2 -0.23,          -0.125449534378		hotspot2  0.23,          -0.125449534378		line2    -0.175991166279, -0.0297868765001, -0.225108589216, -0.0758804978919		line2    -0.11637938332,  -0.0869027156432, 0.11637938332, -0.0869027156432		line2     0.175991166279, -0.0297868765001, 0.225108589216, -0.0758804978919		line2    -0.147739955928, -0.0186068610771, 0.147739955928, -0.0186068610771		arc2     -0.147739955928, -0.059891403014, 0.0412845419369, 90, 133.1809583718		arc2      0, -0.147775582766, 0.200953570541, 190.5602330844, 349.4397669157		arc2     -0.11637938332,  -0.169471799517, 0.0825690838737, 90, 190.5602330844		circle2  -0.0818132353062, -0.0536699045179, 0.0134917043668		circle2   0, -0.142402397023, 0.0189703783166		circle2   0, -0.142402397023, 0.0260997688604		circle2   0.0818132353062, -0.0536699045179, 0.0134917043668		arc2      0.11637938332, -0.169471799517, 0.0825690838737, 349.4397669157, 90		arc2      0.147739955928, -0.059891403014, 0.0412845419369, 46.81904162846, 90		poly2_b 12, 1+2+4,gs_fill_pen,gs_back_pen,			-0.222653099694, -0.06484830776589,      1,			0.02360937645848, -0.1254495343782,    900,			0, 78.55450614068,   4001,			-0.08465397919297, -0.3547893731562,      1,			0, -0.166363164793,    900,			0, 48.3857733935,   4001,			0.08465397919298, -0.3547893731562,      1,			-0.02360937645846, -0.1254495343781,    900,			0, 78.55450614068,   4001,			0.222653099694, -0.06484830776589,      1,			0.1983349842818,            0,      1,			-0.1983349842818,            0,      1		del 1	else					! Model Based		hotspot2 -A/2,  0,   unID : unID = unID + 1		hotspot2  A/2,  0,   unID : unID = unID + 1		hotspot2 -A/2, -B/2, unID : unID = unID + 1		hotspot2  A/2, -B/2, unID : unID = unID + 1		hotspot2  0,   -B,   unID : unID = unID + 1		mul2 a/0.46,b/0.33		poly2_b 8, 1+2+4,gs_fill_pen,gs_back_pen,			-0.23,   0,     1,			 0.23,   0,     1,			 0.23,  -0.168, 1,			 0,     -1,     800,			 0.1573,-0.2901,1001,			-0.1573,-0.2901,1001,			-0.23,  -0.168, 1001,			-0.23,   0,     -1		if gs_detlevel_2D_m = 2 or gs_detlevel_2D_m = 3 then			hotspot2  0,    -0.168, unID : unID = unID + 1			arc2   0,       0,     0.28, 241.5254390862, 298.4745609132			line2  0.0911, -0.0677, -0.0911, -0.0677			for i = 1 to 2				line2  0.18,   -0.1344,  0.18,   -0.168				arc2   0.09112, -0.168, 0.08888, 298.4745602165, 360				add2 0.091, 0				mul2 88.05/66, 1				arc2 0, -0.1344, 0.0667, 0, 90				del 2				mul2 -1, 1			next i			del 2		endif		del 1		if gs_detlevel_2D_m = 3 then	! 1/50			circle2 0, ConPosY_3, 0.0175          ! Drain		endif	endif	! === Tap ==================================================================	if gs_tap_type_m <> 0 and (gs_detlevel_2D_m = 2 or gs_detlevel_2D_m = 3) then		drawindex 40		add2 0, yWallPos * NOT(bWallMountedTap)		call "tapType_m" parameters all bShower 			= 0,										yWallPos 			= yWallPos,										bEnableTapEdit		= 1,										bSinkTap			= 0		del 1	endifENDIF! --- start of modifying to Shaftif bShowMEP then!-----------2d---------------------if show_el < EPS then endIF gs_detlevel_2D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, gs_fill_type, gs_fill_pen, gs_back_pen, MEP_cline_type, MEP_cline_pen, show_el	PEN gs_cont_pen				! --- OTHER VIEWS --- !	ROT2 -SYMB_ROTANGLE * (DrawingNum > 1 AND NOT(SYMB_MIRRORED))+SYMB_ROTANGLE * (DrawingNum > 1 AND SYMB_MIRRORED)	IF DrawingNum = 2 THEN PROJECT2 -3, 90, 2	! Bottom	IF DrawingNum = 3 THEN PROJECT2 4, 270, 2	! Front	IF DrawingNum = 4 THEN PROJECT2 4, 90, 2	! Back	IF DrawingNum = 5 THEN PROJECT2 4, 180*(SYMB_MIRRORED), 2		! Right	IF DrawingNum = 6 THEN PROJECT2 4, 180*NOT(SYMB_MIRRORED), 2	! Left	if bWallmountedTap then		add2 xTapPos,0		corrMEPx = xTapPos	else		corrMEPx = 0	endif				! --- CONNECTIONS --- !	conedit = 0	IF iSuptyp = SEMI_PEDESTAL THEN conedit = 1	CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,		ui_current_con = ui_current_con,		MEP_NumberConnections = MEP_NumberConnections,		MEP_NumConnectionData = gs_Connections,		gs_AddConnections = gs_AddConnections,		DrawingNum = DrawingNum,		MEP_InsShow = 0,		MEP_cline_show = 0,		gs_cont_pen = gs_cont_pen,		gs_fill_type=gs_fill_type,		gs_fill_pen=gs_fill_pen,		gs_back_pen=gs_back_pen,		ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConLength_1=ConLength_1, ConEdit_1 = 12,		ConPosX_2=ConPosX_2, ConPosY_2=ConPosY_2, ConLength_2=ConLength_2, ConEdit_2 = 12,		ConPosX_3=ConPosX_3-corrMEPx, ConPosY_3=ConPosY_3, ConLength_3=ConLength_3, ConEdit_3 = conedit, MEP_StrConnectionData = MEP_StrConnectionData	DEL 1	if bWallmountedTap then del 1ENDIF! --- end of modifying to Shaft