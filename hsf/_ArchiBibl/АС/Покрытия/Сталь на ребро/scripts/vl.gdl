CALL "Общий шаблон элемента" parameters allvalues "det" "Откл", "Просто", "Детально"!values "A" RANGE[0.01,30] STEP 0,krat_txt!values "B" RANGE[0.01,10] STEP 0,krat_txtPARAMETERS zzyzx = hvalues "type_pokr" "Пресснастил", "Композитная решётка", "Композитная решётка в кв.м."naen = ""if type_pokr = "Пресснастил" then	naen = "P " + str("%.0mm",t1) +"*"+str("%.0mm",q1)+"/"+str("%.0mm",h)+"*"+str("%.0mm",t)+" S2,Zn,тип А " + str("%.0mm",A-0.01)+"*"+str("%.0mm",B-0.01)	dim tip_konstr_m[]	success = LIBRARYGLOBAL ("Libraryflobals-km", "konstr_type_arr", tip_konstr_m)	values "tip_konstr" tip_konstr_m, CUSTOM	dim type_spec_type_arr[]	success = LIBRARYGLOBAL ("Libraryflobals-km", "type_spec_type_arr", type_spec_type_arr)	values "type_spec" type_spec_type_arrelse	PARAMETERS q1 = t1	naen = "Композитная решётка яч."+str("%.0mm",t1)+ "*"+str("%.0mm",t1)+"*"+str("%.0mm",h) +"(h)"	if type_pokr = "Композитная решётка в кв.м." then		PARAMETERS kol_vo_spec = A * B		PARAMETERS komment = "кв.м."		PARAMETERS ves = ves_p	else		PARAMETERS kol_vo_spec = 1		naen = naen + " " + str("%.0mm",A-0.01)+"х"+str("%.0mm",B-0.01)		PARAMETERS komment = "шт."		PARAMETERS ves = ves_p*A*B + ves_p*A*B*0.05	endif	PARAMETERS type_spec = "Изделие"endifPARAMETERS naen = naen