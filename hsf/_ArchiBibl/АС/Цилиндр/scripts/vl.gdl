values "FM_Type" `Объекты по умолчанию`, `Ист.света по умолчанию`, `Люди`, `Лифты`, `Безопасность и эвакуация`, `Лестницы`, `Рабочее пространство`,         		`Электрические устройства`, `Распределительные щиты`, `Измеряемые места`, `Источники света`, `Механическое оборудование`,         		`Санитария`, `Замки`, `Встроенные гардеробы`, `Мебель`, `Потолки`, `Полы`, `Объекты снаружи`,         		`Компьютеры`, `Мониторы`, `Программное обеспечение`, `Телефоны`, `Контейнеры`, `Другой`values "gs_detlevel_3D" `Детальный`,`Простой`,`Откл.`values "end_mode" `Перпендикулярно`,`Горизонтально`,`Специально`values "edit_mode" `Угол-длина`,`Координаты`if end_mode=`Горизонтально` or end_mode=`Перпендикулярно` then hideparameter "angle_1","angle_2"if gs_cont_pen=0 then parameters gs_cont_pen=2if gs_fill_pen=0 then parameters gs_fill_pen=91If gs_resol<3 then parameters gs_resol=3if gs_detlevel_3D=`Простой` then lock "gs_resol"VALUES "GAMMA" range [0,360]if abs(gamma-90)>=eps AND abs(gamma-270)>=eps then lock "gs_fill_pen"if glob_modpar_name="radius_1" then parameters radius_1_gc=radius_1*cos(90-gamma)if glob_modpar_name="radius_1_gc" then parameters radius_1=radius_1_gc/cos(90-gamma)if glob_modpar_name="gamma" then parameters radius_1_gc=radius_1*cos(90-gamma)hideparameter "radius_1_gc"VALUES "nazn" "Бетонный элемент", "Анкер", "Пластина", CUSTOMif nazn="Бетонный элемент" then	VALUES "bet_m" "B7.5","B12.5","B15","B17","B20","B25", CUSTOM	VALUES "bet_w" "W2","W4", CUSTOM	parameters obozn="ГОСТ 7473-94"	parameters naen="Бетон "+prim+" "+bet_m+" "+bet_w	parameters ves=3.14*radius_1*radius_1*lengthelsehideparameter "_sp0"endifif nazn="Анкер" then	VALUES "tip_ank" "Hilti","ГОСТ 24379.1-2012", "БСР"	if tip_ank = "Hilti" then		VALUES "naen_ank" "HIT-RE500-SD", CUSTOM		VALUES "naen_sp" "HIT-V-R", CUSTOM		VALUES "d_ank" 0.006,0.008,0.012		parameters radius_1 = d_ank/2		parameters obozn="Hilti"		parameters naen= naen_sp + " M"+str("%.0mm",radius_1*2) +" "+ naen_ank + " l="+str("%.0mm",length)+"мм."		parameters ves=3.14*radius_1*radius_1*length*7850		parameters komment="глубина заделки " + str("%.0mm",length-0.04) + "мм."	endif	if tip_ank = "ГОСТ 24379.1-2012" then		VALUES "naen_ank" "Болт 1.1",  "Болт 1.2"		VALUES "naen_sp" " "		hideparameter "naen_sp"		VALUES "d_ank" 0.006,0.008,0.012,0.016,0.020,0.024		parameters radius_1 = d_ank/2		parameters obozn="ГОСТ 24379.1-2012"		parameters naen= naen_ank + " M"+str("%.0mm",radius_1*2) +"x"+str("%.0mm",length)+" ВСт3пс2"		parameters ves=3.14*radius_1*radius_1*length*7850		parameters komment=" "	endif	if tip_ank = "БСР" then		hideparameter "naen_ank"		hideparameter "naen_sp"		VALUES "d_ank" 0.006,0.008,0.010,0.012,0.016,0.020,0.022,0.024		if d_ank = 0.006 then parameters length = 65/1000		if d_ank = 0.008 then parameters length = 85/1000		if d_ank = 0.010 then parameters length = 100/1000		if d_ank = 0.012 then parameters length = 110/1000		if d_ank = 0.016 then parameters length = 150/1000		if d_ank = 0.020 then parameters length = 200/1000		if d_ank = 0.022 then parameters length = 250/1000		if d_ank = 0.024 then parameters length = 300/1000		parameters radius_1 = d_ank/2		parameters obozn="ГОСТ 28778-90"		parameters naen=  "БСР M"+str("%.0mm",radius_1*2) +"x"+str("%.0mm",length)+" ВСт3пс2"		parameters ves=3.14*radius_1*radius_1*length*7850		parameters komment=" "	endifelsehideparameter "_sp2"endifif nazn="Пластина" then	call "Шаблон профиля" parameters allelsehideparameter "_sp3"endif