CALL "Сортамент_арматуры" parameters alldim diam_temp[] : diam_temp[1] = 0values "marka" reinforce_classif GLOB_MODPAR_NAME = "marka" or diam_temp[1] = 0  then	class = marka : GOSUB "Class2Gost" : parameters gost = gost	GOSUB "GetDiametr" : PARAMETERS reinforce_diam_val = diam_tempendifvalues "diam_arm" diam_tempif GLOB_MODPAR_NAME = "diam_arm" then 	diametr = diam_arm : GOSUB "Diametr2Weight" : parameters weight = weightendifPARAMETERS r_arm=diam_arm*0.5!!dim vid[]!vid[1] = "тело"!vid[2] = "стержень"!vid[3] = "участок"!vid[4] = "откл"!values "vid0" vid!values "vid1" vid!values "vid2" vid!!if GLOB_STRUCTURE_DISPLAY=0 then !vse!	if vid0=vid[1] or vid0=vid[3] then PARAMETERS sterzh=0!	if vid0=vid[2] then PARAMETERS sterzh=1!endif!if GLOB_STRUCTURE_DISPLAY=1 then !nes!	if vid1=vid[1] or vid1=vid[3] then PARAMETERS sterzh=0!	if vid1=vid[2] then PARAMETERS sterzh=1!endif!if GLOB_STRUCTURE_DISPLAY=2 then !bezot!	if vid2=vid[1] or vid2=vid[3] then PARAMETERS sterzh=0!	if vid2=vid[2] then PARAMETERS sterzh=1!endifvalues "dlin_ed_st" 6, 11.7values "l_nahl" range[0.5,)PARAMETERS maxrad = diam_armif dlin_st_model > dlin_ed_st then	PARAMETERS fon = 1	PARAMETERS kol_vo_spec = 1	n_st = round_int(dlin_st_model / dlin_ed_st)	PARAMETERS dlin_st = round_int((n_st * l_nahl + dlin_st_model)*10*kol_vo_model)/10else	if fon then		PARAMETERS kol_vo_spec = 1		PARAMETERS dlin_st = round_int((dlin_st_model*kol_vo_model)*10)/10	else		PARAMETERS kol_vo_spec = kol_vo_model		PARAMETERS dlin_st = round_int(dlin_st_model*krat)/krat	endifendifPARAMETERS obozn = gostPARAMETERS naen = str("%.0mm",diam_arm)+"-"+marka+" L="+str("%.0mm",dlin_st)+"мм."PARAMETERS ves = dlin_st*weightif gnut then PARAMETERS prim="*"if not(gnut) then PARAMETERS prim=" "if strstr(marka,"240")<>0 AND diam_arm<=0.02 then PARAMETERS maxrad=2.5*diam_arm/2+diam_arm/2if strstr(marka,"240")<>0 AND diam_arm>0.02 then PARAMETERS maxrad=2.5*diam_arm/2+diam_arm/2if strstr(marka,"300")<>0 AND diam_arm<=0.02 then PARAMETERS maxrad=4*diam_arm/2+diam_arm/2if strstr(marka,"300")<>0 AND diam_arm>0.02 then PARAMETERS maxrad=6*diam_arm/2+diam_arm/2if strstr(marka,"400")<>0 AND diam_arm<=0.02 then PARAMETERS maxrad=5*diam_arm/2+diam_arm/2if strstr(marka,"400")<>0 AND diam_arm>0.02 then PARAMETERS maxrad=8*diam_arm/2+diam_arm/2if strstr(marka,"500")<>0 AND diam_arm<=0.02 then PARAMETERS maxrad=6*diam_arm/2+diam_arm/2if strstr(marka,"500")<>0 AND diam_arm>0.02 then PARAMETERS maxrad=8*diam_arm/2+diam_arm/2values "krat_txt" 50/1000,10/1000, 5/1000,1/1000if krat_txt=50/1000 then PARAMETERS krat=20if krat_txt=10/1000 then PARAMETERS krat=100if krat_txt=5/1000 then PARAMETERS krat=200if krat_txt=1/1000 then PARAMETERS krat=1000end"Class2Gost":	nstolb = 0	gost = ""	for i = 1 to vardim1(reinforce_class)		if class = reinforce_class[i] then nstolb = i	next i	gost = reinforce_gost[nstolb]return"GetDiametr":	i = 1 : usl = 0	if nstolb <= 0 then nstolb = 1	REPEAT		diam_temp[i] = reinforce_diam[i][nstolb]		i = i + 1		if i <= vardim1(reinforce_diam) and i>0 then			usl = (reinforce_diam[i][nstolb] = 0)		else			usl = 1		endif	UNTIL uslreturn"Diametr2Weight":	nstrok = 1	weight = 0	if nstolb = 0 then nstolb = 1	for i = 1 to vardim1(reinforce_diam)		if diametr = reinforce_diam[i][nstolb] then nstrok = i	next i	weight = reinforce_weight[nstrok][nstolb]return