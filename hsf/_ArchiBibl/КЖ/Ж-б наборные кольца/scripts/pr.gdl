CALL "Общий шаблон бетон" parameters allmat_area = 0		!Площадьmat_volume = 0	!Объёмmat_thickness = 0v_up = pi * (th_layer_up+th_layer_down) * (radius_1*radius_1 - (radius_1-tube_thk) * (radius_1-tube_thk))if v_up>EPS then	mat_inx = layer_mat		!Индекс строительного материала	mat_volume = v_up	!Объём	gosub "spec_mat"endifgoto "prop_end""spec_mat":	if mat_inx > EPS then		call "Спецификатор" parameters tip_el="35",										subpos=subpos, 										mat_inx=mat_inx,										mat_area=mat_area,										mat_thickness=mat_thickness,										mat_volume=mat_volume,										mat_length=mat_length	endifreturn"prop_end":