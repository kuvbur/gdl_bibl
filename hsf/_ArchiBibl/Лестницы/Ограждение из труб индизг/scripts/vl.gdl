values "type_spec" "Металл", "Изделие"! ==============================================================================! General Dimensions! ==============================================================================if vardim1(el_views)<=1 then	dim el_tmp[]	for i=1 to 20		el_tmp[i] = " "	next i	parameters el_views = el_tmpendifcall "railParameterControl_mod" parameters all!parameters hr = 1050/1000parameters hrw = 60/1000parameters hrh = 40/1000parameters pth = 50/1000parameters pw = 50/1000parameters bth = 10/1000parameters bw = 40/1000parameters nb = 1!parameters hroL = 50/1000!parameters hroR = 50/1000values "type_ogrtxt" "Серийная площадка", "Сверху на бетон", "Сбоку на бетон", "Сверху на металл", "Сбоку на металл", "Калитка"if type_ogrtxt = "Серийная площадка" then 	parameters type_ogr = 1	ttxt = "-Ш "	parameters gs_PostBaseOverhang = 140/1000endifif type_ogrtxt = "Сверху на бетон" then 	parameters type_ogr = 2	ttxt = "-ВБ "	parameters gs_PostBaseOverhang = 10/1000endifif type_ogrtxt = "Сбоку на бетон" then 	parameters type_ogr = 3	ttxt = "-ББ "	parameters gs_PostBaseOverhang = 190/1000endifif type_ogrtxt = "Сверху на металл" then 	parameters type_ogr = 4	ttxt = "-ВМ "	parameters gs_PostBaseOverhang = 10/1000endifif type_ogrtxt = "Сбоку на металл" then 	parameters type_ogr = 5	ttxt = "-БМ "	parameters gs_PostBaseOverhang = 100/1000endifif type_ogrtxt = "Калитка" then 	parameters type_ogr = 6	ttxt = "-К "	parameters del_plast = 1	parameters gs_PostBaseOverhang = -50/1000	parameters hroL = 0.025	parameters hroR = 0.025	parameters ds = 0	parameters addl = 0.025	parameters addr = 0.025	parameters addl2 = 0.025	parameters addr2 = 0.025	parameters hroLDiagonal = 0.025	parameters hroRDiagonal = 0.025endifvalues "mat_pokr" "тип 1","тип 2","тип 3","тип 4"if not(mun_subpos) then	if rom = "Нет наклона" then		parameters subpos = "ОГ" + ttxt + str("%.0m",(l1_2+hrh)*100) + "."+ str("%.0m",lrah*100)		parameters naen = "Ограждение " + subpos		parameters tip_profile = "Ограждение ОГ"+ttxt	endif	if rom = "Перпендикулярно" then		parameters subpos = "ОГЛП" + ttxt + str("%.0",ani) + "."+ str("%.0m",(l1_2+hrh)*100)+ "." + str("%.0m",lrah*100)		parameters naen = "Ограждение лестничное " + subpos		parameters tip_profile = "Ограждение лестничное ОГЛП"+ttxt	endif	if rom = "Вертикально" then		parameters subpos = "ОГЛ" + ttxt + str("%.0",ani) + "."+ str("%.0m",(l1_2+hrh)*100)+ "." + str("%.0m",lrah*100)		parameters naen = "Ограждение лестничное " + subpos		parameters tip_profile = "Ограждение лестничное ОГЛ"+ttxt	endifendif! ==============================================================================! Structural Dimensions! ==============================================================================parameters pd=pw,pth=pwparameters bd=bw,bth=bwlock "pd","bd","pth","bth"dim stHandrailType[2]	stHandrailType[1] = `Прямоугольные`	stHandrailType[2] = `Круглые`parameters hrt = stHandrailType[1]values "hrt_m" 1, 2if GLOB_MODPAR_NAME = "hrt" then	hrt_m = 1	if hrt = stHandrailType[2] then hrt_m = 2	parameters hrt_m = hrt_melse	hrt = stHandrailType[1]	if hrt_m = 2 then hrt = stHandrailType[2]	parameters hrt = hrtendifdim stPostType[2]	stPostType[1] = `Прямоугольные`	stPostType[2] = `Круглые`values "pt" stPostTypevalues "pt_m" 1, 2if GLOB_MODPAR_NAME = "pt" then	pt_m = 1	if pt = stPostType[2] then pt_m = 2	parameters pt_m = pt_melse	pt = stPostType[1]	if pt_m = 2 then pt = stPostType[2]	parameters pt = ptendifdim stBarType[2]	stBarType[1] = `Прямоугольные`	stBarType[2] = `Круглые`values "bt" stBarTypevalues "bt_m" 1, 2if GLOB_MODPAR_NAME = "bt" then	bt_m = 1	if bt = stBarType[2] then bt_m = 2	parameters bt_m = bt_melse	bt = stBarType[1]	if bt_m = 2 then bt = stBarType[2]	parameters bt = btendifif ptyp_m <> 1 then	if hrt_m = 1 then values "b" hrw	if hrt_m = 2 then values "b" hrd	lock "a","b","zzyzx"	hideparameter "LengthSize"else	if hrt_m = 1 then		if GLOB_MODPAR_NAME = "hrw" then			B = hrw			parameters B = B		else			hrw = B			parameters hrw = hrw		endif	else		if GLOB_MODPAR_NAME = "hrd" then			B = hrd			parameters B = B		else			hrd = B			parameters hrd = hrd		endif	endif	lock "a","zzyzx"endif! ======================================================================! UI! ======================================================================call "ui_tabcontrol" parameters		controlType			= 1,									nTabs				= 10,									gs_ui_current_page	= gs_ui_current_page