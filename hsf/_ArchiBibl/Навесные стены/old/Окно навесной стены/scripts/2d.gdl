unID = 1tranWidth = 0tranX = 0call "CW Transformation" parameters A = A, AC_PanelCoords = AC_PanelCoords, ac_originIsFrameCenter = ac_originIsFrameCenter returned_parameters tranWidth, tranXmul2 tranWidth, 1add2 -tranX, 0! --- setting detail level ---if gs_detlevel_2d_m = 0 then endif gs_detlevel_2d_m < 0 then	if GLOB_SCALE >= 150 then gs_detlevel_2d_m = 200	if GLOB_SCALE < 150 and GLOB_SCALE >= 75 then gs_detlevel_2d_m = 100	if GLOB_SCALE < 75 then gs_detlevel_2d_m = 50endif! --- setting center of window ---add2 (leftSide_X + rightSide_X) / 2, 0add2 0, AC_ClampWidth / 2 + offsetPanel! --- defining dimensions ---frameLeftIn		= -windowWidth / 2 + frameWidth + adapterWidth * (iFrameType = 2)frameRightIn	=  windowWidth / 2 - frameWidth - adapterWidth * (iFrameType = 2)frameLeftOut	= -windowWidth / 2 + adapterWidth * (iFrameType = 2)	! adapterframeRightOut	=  windowWidth / 2 - adapterWidth * (iFrameType = 2)	! adaptersashLeftOut		= frameLeftIn  - recessWidthsashRightOut	= frameRightIn + recessWidthsashLeftIn		= sashLeftOut  + sashWidthsashRightIn		= sashRightOut - sashWidthsashInside		= offsetGlass - sashThksashOutside		= offsetGlassif gs_opening_dir_m = OPDIR_IN then	frameInside = offsetGlass - recessThkelse	frameInside = offsetGlass - sashThk - (frameThk - recessThk)endifframeOutside = frameInside + frameThkadapterOutside	= max (0, frameOutside)adapterInside	= adapterOutside - adapterThk! --- drawing symbol ---! --- 1:200 ---if gs_detlevel_2d_m = 200 then	pen penFrame	line2 -windowWidth / 2,	0,	frameLeftIn  ,		0	line2  frameRightIn,	0,	windowWidth / 2,	0	pen penSymbol	line2  frameLeftIn,		0,	frameRightIn,		0endif! --- 1:100 ---if gs_detlevel_2d_m = 100 then	pen penSymbol	fill fillCover	poly2_b 5, 1+2+4+64, fillPenCover, fillPenBgCover,				 sashLeftIn,	max (frameOutside, sashOutside),	1,				 sashRightIn,	max (frameOutside, sashOutside),	0,				 sashRightIn,	min (frameInside, sashInside),	1,				 sashLeftIn,	min (frameInside, sashInside),	0,				 sashLeftIn,	max (frameOutside, sashOutside),	-1	pen penFrame	fill fillFrame	poly2_b 4, 1+2+4+32, fillPenFrame, fillPenBgFrame,				frameLeftOut,	max (frameOutside, sashOutside),	1,				sashLeftIn,		max (frameOutside, sashOutside),	1,				sashLeftIn,		min (frameInside, sashInside),		1,				frameLeftOut,	min (frameInside, sashInside),		1	poly2_b 4, 1+2+4+32, fillPenFrame, fillPenBgFrame,				 frameRightOut,	max (frameOutside, sashOutside),	1,				 sashRightIn,	max (frameOutside, sashOutside),	1,				 sashRightIn,	min (frameInside, sashInside),		1,				 frameRightOut,	min (frameInside, sashInside),		1	if iFrameType = 2 then		! adapter		poly2_b 4, 1+2+4+32, fillPenFrame, fillPenBgFrame,				-windowWidth / 2,	adapterOutside,	1,				 frameLeftOut,		adapterOutside,	1,				 frameLeftOut,		adapterInside,	1,				-windowWidth / 2,	adapterInside,	1		poly2_b 4, 1+2+4+32, fillPenFrame, fillPenBgFrame,				 windowWidth / 2,	adapterOutside,	1,				 frameRightOut,		adapterOutside,	1,				 frameRightOut,		adapterInside,	1,				 windowWidth / 2,	adapterInside,	1	endif	pen penGlass	line2 sashLeftIn, 0, sashRightIn, 0endif! --- 1:50 ---if gs_detlevel_2d_m = 50 then	pen penFrame	fill fillFrame	if iFrameType = 2 then		! adapter		poly2_b 4, 1+2+4+32, fillPenFrame, fillPenBgFrame,				-windowWidth / 2,	adapterOutside,	1,				 frameLeftOut,		adapterOutside,	1,				 frameLeftOut,		adapterInside,	1,				-windowWidth / 2,	adapterInside,	1		poly2_b 4, 1+2+4+32, fillPenFrame, fillPenBgFrame,				 windowWidth / 2,	adapterOutside,	1,				 frameRightOut,		adapterOutside,	1,				 frameRightOut,		adapterInside,	1,				 windowWidth / 2,	adapterInside,	1	endif	if gs_opening_dir_m = OPDIR_IN then		poly2_b 6, 1+2+4+32, fillPenFrame, fillPenBgFrame,				frameLeftOut,					frameOutside,		1,				frameLeftIn,					frameOutside,		1,				frameLeftIn,					sashOutside,		1,				frameLeftIn - recessWidth,		sashOutside,		1,				frameLeftIn - recessWidth,		frameInside,		1,				frameLeftOut,					frameInside,		1		poly2_b 6, 1+2+4+32, fillPenFrame, fillPenBgFrame,				frameRightOut,					frameOutside,		1,				frameRightIn,					frameOutside,		1,				frameRightIn,					sashOutside,		1,				frameRightIn + recessWidth,		sashOutside,		1,				frameRightIn + recessWidth, 	frameInside,		1,				frameRightOut,					frameInside,		1	else		poly2_b 6, 1+2+4+32, fillPenFrame, fillPenBgFrame,				frameLeftOut,					frameInside,		1,				frameLeftIn,					frameInside,		1,				frameLeftIn,					sashInside,			1,				frameLeftIn - recessWidth,		sashInside,			1,				frameLeftIn - recessWidth,		frameOutside,		1,				frameLeftOut,					frameOutside,		1		poly2_b 6, 1+2+4+32, fillPenFrame, fillPenBgFrame,				frameRightOut,					frameInside,		1,				frameRightIn,					frameInside,		1,				frameRightIn,					sashInside,			1,				frameRightIn + recessWidth,		sashInside,			1,				frameRightIn + recessWidth, 	frameOutside,		1,				frameRightOut,					frameOutside,		1	endif	pen penSymbol	fill fillCover	poly2_b 5, 1+2+4+64, fillPenCover, fillPenBgCover,				 sashLeftIn,	sashOutside,	1,				 sashRightIn,	sashOutside,	0,				 sashRightIn,	sashInside,		1,				 sashLeftIn,	sashInside,		0,				 sashLeftIn,	sashOutside,	-1	pen penGlass	line2 sashLeftIn, 0, sashRightIn, 0	fill fillSash	pen penSymbol	poly2_b 4, 1+2+4+32, fillPenSash, fillPenBgSash,				 sashLeftOut,	sashOutside,	1,				 sashLeftIn,	sashOutside,	1,				 sashLeftIn,	sashInside,		1,				 sashLeftOut,	sashInside,		1	poly2_b 4, 1+2+4+32, fillPenSash, fillPenBgSash,				 sashRightOut,	sashOutside,	1,				 sashRightIn,	sashOutside,	1,				 sashRightIn,	sashInside,		1,				 sashRightOut,	sashInside,		1endifend