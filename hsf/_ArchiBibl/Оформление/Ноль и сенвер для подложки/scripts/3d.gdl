IF gs_detlevel_3D_int = 3 THEN ENDif gs_follow_north then	muly 1-2*SYMB_MIRRORED	rotz GLOB_NORTH_DIR + 180*SYMB_MIRRORED - SYMB_ROTANGLE - 90endifunID=1HOTSPOT2 0,0,unID : unID=unID+1PEN gs_cont_penif symbolStyles_m = 1 or symbolStyles_m = 2 or symbolStyles_m = 3 or \		symbolStyles_m = 7 or symbolStyles_m = 8 or \		symbolStyles_m = 9 or symbolStyles_m = 10 or symbolStyles_m = 12 or symbolStyles_m = 13 then	alpha = 45	if symbolStyles_m = 12 then		alpha = 25	endif	! RADIUS HOTSPOT	HOTSPOT 0, 0, 0, unID, radius_model, 1+128, B:	unID=unID+1	HOTSPOT -1, 0, 0, unID, radius_model, 3+128, B: unID=unID+1	HOTSPOT radius_model, 0, 0, unID, radius_model, 2, B: unID=unID+1	HOTSPOT 0, 0, 0, unID, radius_model, 1+128, B:	unID=unID+1	HOTSPOT 1, 0, 0, unID, radius_model, 3+128, B: unID=unID+1	HOTSPOT -radius_model, 0, 0, unID, radius_model, 2, B: unID=unID+1	HOTSPOT 0, 0, 0, unID, radius_model, 1+128, B:	unID=unID+1	HOTSPOT 0, -1, 0, unID, radius_model, 3+128, B: unID=unID+1	HOTSPOT 0, radius_model, 0, unID, radius_model, 2, B: unID=unID+1	HOTSPOT 0, 0, 0, unID, radius_model, 1+128, B:	unID=unID+1	HOTSPOT 0, 1, 0, unID, radius_model, 3+128, B: unID=unID+1	HOTSPOT 0, -radius_model, 0, unID, radius_model, 2, B: unID=unID+1	MATERIAL gs_base_mat	!!!!CYLIND zzyzx-SignHeight,b/2	HillHeight = (zzyzx-SignHeight)/2	FPRISM_ gs_base_mat,gs_base_mat,gs_base_mat,gs_base_mat,		2,zzyzx-SignHeight,alpha,HillHeight,		0,0,979,		b/2,360,4079endifif symbolstyles_m = 1 then	alfa1=acs(((b/2)*0.045)/(b/2))	ADDz zzyzx-SignHeight	MATERIAL gs_sign_mat	 PRISM_ 6,SignHeight,		0,0.01,15,		b/2*0.045,0.01,15,		b/2*0.045,sin(alfa1)*b/2-HillHeight,15,		0,0,979,		0,b/2-HillHeight,15,		0,0.01,-1	 PRISM_ 4,SignHeight/5,		-B/2+HillHeight,0,15,		-B/2+HillHeight ,-0.01,15,		B/2-HillHeight ,-0.01,15,		B/2-HillHeight ,0,15	 PRISM_ 4,SignHeight/5,		 0,-0.02,15,		0.01,-0.02,15,		0.01,-B/2+HillHeight,15,		0,-B/2+HillHeight,15	DEL 1endifif symbolstyles_m = 2 then	ADDz zzyzx-SignHeight	MATERIAL gs_sign_mat	 PRISM_ 4,SignHeight,		0,0,15,		0,b/2-HillHeight,15,		-b/2+HillHeight,0,15,		0,0,-1	 PRISM_ 7,SignHeight/5,		0.01,0,15,		b/2-HillHeight,0,15,		0.01,b/2-HillHeight,15,		0.01,b/2-HillHeight-0.0141,15,		b/2-HillHeight-0.0141,0.01,15,		0.01,0.01,15,		0.01,0,-1	 PRISM_ 4,SignHeight/5,		 0,-0.02,15,		0.01,-0.02,15,		0.01,-B/2+HillHeight,15,		0,-B/2+HillHeight,15	DEL 1endifif symbolstyles_m = 3 then	alfa2=acs(((b/2-HillHeight)*0.49)/(b/2-HillHeight))	ADDz zzyzx-SignHeight	MATERIAL gs_sign_mat	 PRISM_ 4,SignHeight,		0,b/2-HillHeight, 15,		-((b/2-HillHeight)*0.49),-(sin (alfa2)*b/2-HillHeight), 15,		((b/2-HillHeight)*0.49),-(sin (alfa2)*(b/2-HillHeight)), 15,		0,b/2-HillHeight, -1	 PRISM_ 4,SignHeight/5,		-B/2+HillHeight,0,15,		-B/2+HillHeight ,-0.01,15,		-((b/2-HillHeight)*0.49) ,-0.01,15,		-((b/2-HillHeight)*0.49) ,0,15	 PRISM_ 4,SignHeight/5,		B/2-HillHeight,0,15,		B/2-HillHeight ,-0.01,15,		((b/2-HillHeight)*0.49) ,-0.01,15,		((b/2-HillHeight)*0.49) ,0,15	DEL 1ENDIFif symbolStyles_m = 4 then	alfa3=acs(((b/2)*0.45)/((b/2)*1.097))	ADDy -B/2	GROUP "Cutting_Poly"		PRISM_ 3, ZZYZX,			0,((sin (alfa3)*((b/2)*1.096)))-((b/2-(b/2)*0.259)),15,			 -(((b/2)*0.5)),-0.01,15,			 (((b/2)*0.5)),-0.01,15	ENDGROUP	GROUP "NorthSign"		ADDZ ZZYZX/2		ROTx -90		 MATERIAL gs_sign_mat		 PYRAMID 3,b/2+((sin (alfa3)*((b/2)*1.096))),1+4+16,			-(((b/2)*0.45)),0,1,			0,ZZYZX/2,1,			0,-ZZYZX/2,1		 MATERIAL gs_base_mat		 PYRAMID 3,b/2+((sin (alfa3)*((b/2)*1.096))),1+4+16,			(((b/2)*0.45)),0,1,			0,ZZYZX/2,1,			0,-ZZYZX/2,1		DEL 2	ENDGROUP	RESULT1 = SUBGROUP ("NorthSign","Cutting_Poly")	PLACEGROUP RESULT1	KILLGROUP RESULT1	KILLGROUP "NorthSign"	KILLGROUP "Cutting_Poly"	DEL 1	! RADIUS HOTSPOT	HOTSPOT -(((b/2)*0.45)), -b/2, ZZYZX/2, unID:	unID=unID+1	HOTSPOT (((b/2)*0.45)), -b/2, ZZYZX/2, unID:	unID=unID+1	HOTSPOT 0,((sin (alfa3)*((b/2)*1.096)))-((b/2-(b/2)*0.259))-b/2, ZZYZX/2, unID:	unID=unID+1	HOTSPOT 0, 0, ZZYZX/2, unID, radius_model, 1+128, B:	unID=unID+1	HOTSPOT 0, -1, ZZYZX/2, unID, radius_model, 3+128, B: unID=unID+1	HOTSPOT 0, radius_model, ZZYZX/2, unID, radius_model, 2, B: unID=unID+1ENDIFIF symbolStyles_m = 5 THEN	MATERIAL gs_sign_mat	PLANE 4,		0,(b/2),0,		0,(b/2),ZZYZX,		0,-b/2,ZZYZX,		0,-b/2,0	PLANE 4,		0,b/2,0,		0,b/2,ZZYZX,		((b/2)*0.553),((b/2)*0.681),ZZYZX,		((b/2)*0.553),((b/2)*0.681),0	PLANE 4,		0,b/2,0,		0,b/2,ZZYZX,		-((b/2)*0.553),((b/2)*0.681),ZZYZX,		-((b/2)*0.553),((b/2)*0.681),0	PLANE 4,		0,(b/2)*0.904,0,		0,(b/2)*0.904,ZZYZX,		-((b/2)*0.553),((b/2)*0.584),ZZYZX,		-((b/2)*0.553),((b/2)*0.584),0	! RADIUS HOTSPOT	HOTSPOT 0, 0, 0,  unID, radius_model, 1+128, B:	unID=unID+1	HOTSPOT 0, 1, 0,  unID, radius_model, 3, B: unID=unID+1	HOTSPOT 0, -radius_model, 0, unID, radius_model, 2, B: unID=unID+1	HOTSPOT 0, 0, 0,  unID, radius_model, 1+128, B:	unID=unID+1	HOTSPOT 0, -1, 0,  unID, radius_model, 3, B: unID=unID+1	HOTSPOT 0, radius_model, 0, unID, radius_model, 2, B: unID=unID+1ENDIFIF symbolStyles_m = 6 THEN	ADDz ZZYZX/2	ROTx -90	MATERIAL gs_sign_mat	PYRAMID 3,b/2,1+4+16,		-((b/2)*0.078),0,1,		0,ZZYZX/2,1,		0,-ZZYZX/2,1	MATERIAL gs_base_mat	PYRAMID 3,b/2,1+4+16,		((b/2)*0.078),0,1,		0,ZZYZX/2,1,		0,-ZZYZX/2,1	ROTx 180	MATERIAL gs_base_mat	PYRAMID 3,b/2,1+4+16,		-((b/2)*0.078),0,1,		0,ZZYZX/2,1,		0,-ZZYZX/2,1	MATERIAL gs_sign_mat	PYRAMID 3,b/2,1+4+16,		((b/2)*0.078),0,1,		0,ZZYZX/2,1,		0,-ZZYZX/2,1	DEL 3	MATERIAL gs_sign_mat	PLANE 4,		-(b/2)*0.297,0,2*ZZYZX/5,		-(b/2)*0.297,0,3*ZZYZX/5,		(b/2)*0.297,0,3*ZZYZX/5,		(b/2)*0.297,0,2*ZZYZX/5	PLANE 4,		0,b/2,2*ZZYZX/5,		0,b/2,3*ZZYZX/5,		0,-b/2,3*ZZYZX/5,		0,-b/2,2*ZZYZX/5	! RADIUS HOTSPOT	HOTSPOT 0, 0, ZZYZX/2, unID, radius_model, 1+128, B:	unID=unID+1	HOTSPOT 0, -1, ZZYZX/2, unID, radius_model, 3, B: unID=unID+1	HOTSPOT 0, radius_model, ZZYZX/2, unID, radius_model, 2, B: unID=unID+1	HOTSPOT 0, 0, unID, ZZYZX/2, radius_model, 1+128, B:	unID=unID+1	HOTSPOT 0, 1, unID, ZZYZX/2, radius_model, 3, B: unID=unID+1	HOTSPOT 0, -radius_model, ZZYZX/2, unID, radius_model, 2, B: unID=unID+1endifif symbolstyles_m = 7 then	material gs_sign_mat	radius_model = radius_model*0.85	signThk = radius_model*0.1	addz zzyzx-SignHeight	for i = 1 to 2		prism_ 8, SignHeight,				0,          radius_model,      15,				-radius_model,        0,       15,				0, 0, 900,				-radius_model,        -signThk*sqr(2),    3015,				-signThk/2, radius_model-signThk/2-signThk*sqr(2),   15,				-signThk/2, -cos(asn(signThk/2/radius_model))*radius_model,    15,				0,          -radius_model,   3000,				0,          radius_model,      -1		mulx -1	next iendifif symbolstyles_m = 8 then	radius_model = radius_model*0.85	dy=radius_model*sin(36)	dx=radius_model*cos(36)	addz zzyzx-SignHeight	rotz 90	prism 3, SignHeight,		radius_model, 0,		-dx, -dy,		-dx*2/3, 0	material gs_sign_mat	prism 3, SignHeight,		radius_model, 0,		-dx, dy,		-dx*2/3, 0	del 1endifif symbolstyles_m = 9 then	material gs_sign_mat	alfa2=acs(((b/2-HillHeight)*0.49)/(b/2-HillHeight))	addz zzyzx-SignHeight	prism_ 4,SignHeight,		0,b/2-HillHeight, 15,		-((b/2-HillHeight)*0.49),-(sin (alfa2)*b/2-HillHeight), 15,		((b/2-HillHeight)*0.49),-(sin (alfa2)*(b/2-HillHeight)), 15,		0,b/2-HillHeight, -1	del 1endifif symbolstyles_m = 10 then	rotx 90	material gs_sign_mat	for i=1 to 4		rect (radius_model*7/6), zzyzx-SignHeight/3		roty 90	next i	del 4	roty 90	addz -radius_model/6	addx radius_model/6	rect (radius_model+radius_model/6), ZZYZX	del 3endifif symbolStyles_m = 11 or symbolStyles_m = 12 or symbolStyles_m = 13 then	rotz 90	if symbolStyles_m = 13 then		radius_model = radius_model*0.85	endif	if symbolStyles_m = 11 then		SignHeight = zzyzx	endif	dx1=radius_model*tan(11)	dx2=radius_model*sin(22)	dy2=radius_model*cos(22)	addz zzyzx-SignHeight	material gs_base_mat	prism 3, SignHeight,			0, 0,			radius_model, 0,			0, dx1	material gs_sign_mat	prism 3, SignHeight,			0, 0,			radius_model, 0,			0, -dx1	material gs_base_mat	prism 4, SignHeight,			0, 0,			0, -dx1,			-dy2, -dx2,			-dy2 * 2/3, 0	material gs_sign_mat	prism 4, SignHeight,			0, 0,			0, dx1,			-dy2, dx2,			-dy2 * 2/3, 0	del 2	addz zzyzx-SignHeight	PLANE 4,		  0, -dy2*2/3, 0,		  0, -dy2*2/3, SignHeight,		  0, -radius_model, SignHeight,		  0, -radius_model, 0	del 1endifif symbolStyles_m = 12 or symbolStyles_m = 13 then	MATERIAL gs_sign_mat	addz zzyzx-SignHeight	PLANE 4,		  -dx1, 0, 0,		  -dx1, 0, SignHeight,		  -radius_model, 0, SignHeight,		  -radius_model, 0, 0	PLANE 4,		  dx1, 0, 0,		  dx1, 0, SignHeight,		  radius_model, 0, SignHeight,		  radius_model, 0, 0	del 1endifif symbolStyles_m = 11 then	PLANE 4,		-dx1, 0, 0,		-dx1, 0, SignHeight,		-dx2, 0, SignHeight,		-dx2, 0, 0	PLANE 4,		dx1, 0, 0,		dx1, 0, SignHeight,		dx2, 0, SignHeight,		dx2, 0, 0	! RADIUS HOTSPOT	HOTSPOT -dx2, -dy2, ZZYZX/2, unID:	unID=unID+1	HOTSPOT dx2, -dy2, ZZYZX/2, unID:	unID=unID+1	HOTSPOT 0, -radius_model, ZZYZX/2, unID:	unID=unID+1	HOTSPOT 0, 0, ZZYZX/2, unID, radius_model, 1+128, B:	unID=unID+1	HOTSPOT 0, -1, ZZYZX/2, unID, radius_model, 3+128, B: unID=unID+1	HOTSPOT 0, radius_model, ZZYZX/2, unID, radius_model, 2, B: unID=unID+1endif