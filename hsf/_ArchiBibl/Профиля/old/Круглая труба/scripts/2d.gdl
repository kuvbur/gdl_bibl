call "Шаблон профиля" parameters allhotspot2 0,0if not(show) then endif GLOB_CONTEXT=6 then	k = 5	pen SYMB_SECT_PEN	MUL2 A_/1000, A_/1000 	circle2 0,0,k/2	endendif!!!!ROTATION Angle=0					LABEL 100:!!!!	AXIS							LABEL 100:!!!!	gs_cutmode_bot=cut_mod_mtr[1]	LABEL 110:!!!!	gs_cutmode_bot=cut_mod_mtr[2]	LABEL 110:!!!!	gs_cutmode_up=cut_mod_mtr[1]	LABEL 120:!!!!	gs_cutmode_up=cut_mod_mtr[2]	LABEL 120:!!!!	gs_cutmode_bot=cut_mod_mtr[3]	LABEL 130:!!!!	gs_cutmode_up=cut_mod_mtr[3]	LABEL 140:!!!!ROTATION Angle=90 OR Angle=-90		LABEL 200:!!!!	gs_rot_axis=rot_axis_mtr[1]		LABEL 210:!!!!	gs_rot_axis=rot_axis_mtr[2]		LABEL 220:!!!!ROTATION 0<Angle<90 OR -90<Angle<0	LABEL 300:if show_only_axsis then	axis_line_pen = only_axis_pen	axis_line_type = only_axis_line	only_axis = 1	if (ang_pl<>0) and (gs_rot_axis=rot_axis_mtr[2]) and (ang<>0) and (ang<>90) then 		axis=0	else		axis=1	endifendifPEN gs_cont_penFILL gs_fill_typeLINE_TYPE 1IF ang=0 THEN GOTO 100IF ang=90 OR ang=-90 THEN GOTO 200IF ang>0 AND ang<90 OR ang<0 AND ang>-90 THEN GOTO 300100:unID=100IF (axis=1) THEN	PEN axis_line_pen	LINE_TYPE axis_line_type	LINE2 0, 0, A*cos(ang),0	HOTLINE2 0, 0, A*cos(ang),0ENDIFif angl_pl<>0 then 	HOTSPOT2 0, 0, unID, a, 1+256: unID=unID+1	! AXIS (Length)	HOTSPOT2 -1, 0, unID, a, 3: unID=unID+1	HOTSPOT2 a, 0, unID, a, 2: unID=unID+1	HOTSPOT2 a/2, 0,unID : unID=unID+1			! AXIS (Length/2)endifPEN gs_cont_penFILL gs_fill_typeLINE_TYPE 1IF axis=1 AND only_axis=0 OR axis=0 THEN	110:	IF gs_cutmode_bot=cut_mod_mtr[1] OR gs_cutmode_bot=cut_mod_mtr[2] THEN		! Cut. Angle (1)		IF gs_cutang1_bot=0 THEN			P1xCutMod12=0		ELSE			P1xCutMod12=(ws/2)*TAN(gs_cutang1_bot)		ENDIF		! Cut. Angle (2)		IF gs_cutang2_bot=0 THEN			P2xCutMod12=0		ELSE			P2xCutMod12=(ws/2)*TAN(gs_cutang2_bot)		ENDIF		! Contour		POLY2_b 6, 1, gs_fill_pen, gs_back_pen,				0,0,1,				P1xCutMod12,ws/2,1,				A/2,ws/2,0,				A/2,-ws/2,1,				P2xCutMod12,-ws/2,1,				0,0,-1		!!! *** Background Fill		IF GLOB_CONTEXT<>22 THEN	!! (DO NOT SHOW FILL IN HOTSPOT EDITING)			POLY2_b 6, 2, gs_fill_pen, gs_back_pen,					0,0,0,					P1xCutMod12,ws/2,0,					A/2,ws/2,0,					A/2,-ws/2,0,					P2xCutMod12,-ws/2,0,					0,0,-1		ENDIF		! Hotspots		! Cut. Angle (1)		P1CutMod12_RR=(ws/2)/COS(gs_cutang1_bot)		IF gs_cut_planes=1 THEN			ROT2 -30			unID=110			HOTSPOT2 0,0, unID, temp_cutang1_bot, 6+512, gs_cutang1_bot : unID=unID+1 !Center			HOTSPOT2 -P1CutMod12_RR,0, unID, temp_cutang1_bot, 4+128,  gs_cutang1_bot : unID=unID+1 !Base			HOTSPOT2 -COS(temp_cutang1_bot) * P1CutMod12_RR, SIN(temp_cutang1_bot) * P1CutMod12_RR,  unID, temp_cutang1_bot, 5,  gs_cutang1_bot : unID=unID+1 !Move			DEL 1		ELSE			unID=113			HOTSPOT2 0,ws/2 : unID=unID+1		ENDIF		! Cut. Angle (2)		P2CutMod12_RR=(ws/2)/COS(gs_cutang2_bot)		IF gs_cut_planes=1 THEN			ROT2 30			unID=114			HOTSPOT2 0,0, unID, temp_cutang2_bot, 6, gs_cutang2_bot : unID=unID+1 !Center			HOTSPOT2 -P2CutMod12_RR,0, unID, temp_cutang2_bot, 4+128,  gs_cutang2_bot : unID=unID+1 !Base			HOTSPOT2 -COS(temp_cutang2_bot) * P2CutMod12_RR, -SIN(temp_cutang2_bot) * P2CutMod12_RR,  unID, temp_cutang2_bot, 5,  gs_cutang2_bot : unID=unID+1 !Move			DEL 1		ELSE			unID=117			HOTSPOT2 0,-ws/2 : unID=unID+1		ENDIF	ENDIF	120:	IF gs_cutmode_up=cut_mod_mtr[1] OR gs_cutmode_up=cut_mod_mtr[2] THEN		! Cut. Angle (3)		IF gs_cutang3_up=0 THEN			P3xCutMod12=0		ELSE			P3xCutMod12=(ws/2)*TAN(gs_cutang3_up)		ENDIF		! Cut. Angle (4)		IF gs_cutang4_up=0 THEN			P4xCutMod12=0		ELSE			P4xCutMod12=(ws/2)*TAN(gs_cutang4_up)		ENDIF		! Contour		POLY2_b 6, 1, gs_fill_pen, gs_back_pen,				A,0,1,				A-P3xCutMod12,ws/2,1,				A/2,ws/2,0,				A/2,-ws/2,1,				A-P4xCutMod12,-ws/2,1,				A,0,-1		! Contour and Background Fill		IF GLOB_CONTEXT<>22 THEN			! (DO NOT SHOW FILL IN HOTSPOT EDITING)			POLY2_b 6, 2, gs_fill_pen, gs_back_pen,					A,0,0,					A-P3xCutMod12,ws/2,0,					A/2,ws/2,0,					A/2,-ws/2,0,					A-P4xCutMod12,-ws/2,0,					A,0,-1		ENDIF		! Hotspots		! Cut. Angle (3)		P3CutMod3_RR=(ws/2)/COS(gs_cutang3_up)		IF gs_cut_planes=1 THEN			ADD2 A,0			ROT2 30			unID=120			HOTSPOT2 0,0, unID, temp_cutang3_up, 6, gs_cutang3_up : unID=unID+1 !Center			HOTSPOT2 P3CutMod3_RR,0, unID, temp_cutang3_up, 4+128,  gs_cutang3_up : unID=unID+1 !Base			HOTSPOT2 COS(temp_cutang3_up) * P3CutMod3_RR, SIN(temp_cutang3_up) * P3CutMod3_RR,  unID, temp_cutang3_up, 5,  gs_cutang3_up : unID=unID+1 !Move			DEL 2		ELSE			unID=123			HOTSPOT2 a,ws/2 : unID=unID+1		ENDIF		! Cut. Angle (4)		P4CutMod3_RR=(ws/2)/COS(gs_cutang4_up)		IF gs_cut_planes=1 THEN			ADD2 A,0			ROT2 -30			unID=124			HOTSPOT2 0,0, unID, temp_cutang4_up, 6+512, gs_cutang4_up : unID=unID+1 !Center			HOTSPOT2 P4CutMod3_RR,0, unID, temp_cutang4_up, 4+128,  gs_cutang4_up : unID=unID+1 !Base			HOTSPOT2 COS(temp_cutang4_up) * P4CutMod3_RR, -SIN(temp_cutang4_up) * P4CutMod3_RR,  unID, temp_cutang4_up, 5,  gs_cutang4_up : unID=unID+1 !Move			DEL 2		ELSE			unID=127			HOTSPOT2 a,-ws/2 : unID=unID+1		ENDIF	ENDIF	130:	unID=130	IF gs_cutmode_bot=cut_mod_mtr[3]THEN		IF gs_cutang1_bot=0 THEN			P1xCutMod3=0		ELSE			P1xCutMod3=(ws/2)*TAN(gs_cutang1_bot)		ENDIF		IF gs_cutang2_bot=0 THEN			P2xCutMod3=0		ELSE			P2xCutMod3=(ws/2)*TAN(gs_cutang2_bot)		ENDIF		! MIN. Horiz. Length		tempP12CutMod3=MIN(0,P1xCutMod3,P2xCutMod3)		! Lines Types		IF P1xCutMod3<0 THEN			tempLineOrigo=DashedLineType		ELSE			tempLineOrigo=1		ENDIF		IF P2xCutMod3<P1xCutMod3 AND  P2xCutMod3<=0 THEN			tempLineAng2=1		ELSE			tempLineAng2=DashedLineType		ENDIF		! ORIGO		LINE_TYPE tempLineOrigo		IF gs_cutang1_bot<>-gs_cutang2_bot OR -gs_cutang1_bot<>gs_cutang2_bot THEN LINE2 0,ws/2,0,-ws/2		! Cut. Angle (1)		LINE_TYPE 1		LINE2 P1xCutMod3,ws/2,P1xCutMod3,-ws/2		IF gs_ShowMoreHtps2D=1 THEN HOTSPOT2 P1xCutMod3,0,unID : unID=unID+1		! Cut. Angle (2)		LINE_TYPE tempLineAng2		IF gs_cutang1_bot<>gs_cutang2_bot THEN			LINE2 P2xCutMod3,ws/2,P2xCutMod3,-ws/2			IF gs_ShowMoreHtps2D=1 THEN HOTSPOT2 P2xCutMod3,0,unID : unID=unID+1		ENDIF		LINE_TYPE 1		! Contour		POLY2_b 5, 1, gs_fill_pen, gs_back_pen,				tempP12CutMod3,ws/2,1,				A/2,ws/2,0,				A/2,-ws/2,1,				tempP12CutMod3,-ws/2,0,				tempP12CutMod3,ws/2,-1		IF gs_ShowMoreHtps2D=1 THEN HOTSPOT2 tempP12CutMod3,ws/2,unID : unID=unID+1		IF gs_ShowMoreHtps2D=1 THEN HOTSPOT2 tempP12CutMod3,-ws/2,unID : unID=unID+1		! Background Fill		IF GLOB_CONTEXT<>22 THEN	!! (DO NOT SHOW FILL IN HOTSPOT EDITING)			POLY2_b 5, 2, gs_fill_pen, gs_back_pen,					tempP12CutMod3,ws/2,0,					A/2,ws/2,0,					A/2,-ws/2,0,					tempP12CutMod3,-ws/2,0,					tempP12CutMod3,ws/2,-1		ENDIF	ENDIF	140:	unID=140	IF gs_cutmode_up=cut_mod_mtr[3]THEN		IF gs_cutang3_up=0 THEN			P3xCutMod3=0		ELSE			P3xCutMod3=(ws/2)*TAN(gs_cutang3_up)		ENDIF		IF gs_cutang4_up=0 THEN			P4xCutMod3=0		ELSE			P4xCutMod3=(ws/2)*TAN(gs_cutang4_up)		ENDIF		! MIN. Horiz. Length		tempP34CutMod3=MIN(0,P3xCutMod3,P4xCutMod3)		! Lines Types		IF P3xCutMod3<0 THEN			tempLineOrigo=DashedLineType		ELSE			tempLineOrigo=1		ENDIF		IF P4xCutMod3<P3xCutMod3 AND  P4xCutMod3<=0 THEN			tempLineAng4=1		ELSE			tempLineAng4=DashedLineType		ENDIF		! ORIGO		LINE_TYPE tempLineOrigo		IF gs_cutang3_up<>-gs_cutang4_up OR -gs_cutang3_up<>gs_cutang4_up THEN LINE2 A,ws/2,A,-ws/2		! Cut. Angle (3)		LINE_TYPE 1		LINE2 A-P3xCutMod3,ws/2,A-P3xCutMod3,-ws/2		IF gs_ShowMoreHtps2D=1 THEN HOTSPOT2 A-P3xCutMod3,0,unID : unID=unID+1		! Cut. Angle (4)		LINE_TYPE tempLineAng4		IF gs_cutang3_up<>gs_cutang4_up THEN			LINE2 A-P4xCutMod3,ws/2,A-P4xCutMod3,-ws/2			IF gs_ShowMoreHtps2D=1 THEN HOTSPOT2 A-P4xCutMod3,0,unID : unID=unID+1		ENDIF		LINE_TYPE 1		! Contour		POLY2_b 5, 1, gs_fill_pen, gs_back_pen,				A-tempP34CutMod3,ws/2,1,				A/2,ws/2,0,				A/2,-ws/2,1,				A-tempP34CutMod3,-ws/2,1,				A-tempP34CutMod3,ws/2,-1		IF gs_ShowMoreHtps2D=1 THEN HOTSPOT2 A-tempP34CutMod3,ws/2,unID : unID=unID+1		IF gs_ShowMoreHtps2D=1 THEN HOTSPOT2 A-tempP34CutMod3,-ws/2,unID : unID=unID+1		! Background Fill		IF GLOB_CONTEXT<>22 THEN	!! (DO NOT SHOW FILL IN HOTSPOT EDITING)			POLY2_b 5, 7, gs_fill_pen, gs_back_pen,					A-tempP34CutMod3,ws/2,0,					A/2,ws/2,0,					A/2,-ws/2,0,					A-tempP34CutMod3,-ws/2,0,					A-tempP34CutMod3,ws/2,-1		ENDIF	ENDIFENDIF150:END200:210:unID=210IF gs_rot_axis=rot_axis_mtr[1] THENPEN gs_cont_penIF ((axis=1 AND only_axis=0) OR axis=0) or show_only_axsis THEN PROJECT2{2} 3,270,3+32, gs_back_pen,0,0,0	IF gs_stationary=`Вверху` AND ang=90 OR gs_stationary=`Внизу` AND ang=-90 THEN ADD2 0,ds/2	IF gs_stationary=`Внизу` AND ang=90 OR gs_stationary=`Вверху` AND ang=-90 THEN ADD2 0,-ds/2	HOTSPOT2 0, 0, unID, a, 1+256: unID=unID+1	!!! AXIS (Length)	HOTSPOT2 -1, 0, unID, a, 3: unID=unID+1	HOTSPOT2 a, 0, unID, a, 2: unID=unID+1	HOTSPOT2 a/2, 0,unID : unID=unID+1			!!! AXIS (Length/2)	IF gs_ShowMoreHtps2D=1 THEN		HOTSPOT2 0, ds/2,unID : unID=unID+1			!!! BOUNDING BOX		HOTSPOT2 A, ds/2,unID : unID=unID+1		HOTSPOT2 A, -ds/2,unID : unID=unID+1		HOTSPOT2 0, -ds/2,unID : unID=unID+1	ENDIF	IF (axis=1) THEN		PEN axis_line_pen		LINE_TYPE axis_line_type		LINE2 0, 0, A,0		HOTLINE2 0, 0, A,0	ENDIF	IF gs_stationary <> `По центру` THEN DEL 1PEN gs_cont_penENDIF220:unID=220IF gs_rot_axis=rot_axis_mtr[2] THEN	PEN gs_cont_pen	IF gs_stationary=`Вверху` AND ang=90 OR gs_stationary=`Внизу` AND ang=-90 THEN ADD2 0,-ws/2	IF gs_stationary=`По центру` THEN ADD2 -ds/2,-ws/2	IF gs_stationary=`Внизу` AND ang=90 OR gs_stationary=`Вверху` AND ang=-90 THEN ADD2 -ds,-ws/2	HOTSPOT2 ds/2,ws/2,unID : unID=unID+1		!!! ORIGO	IF gs_ShowMoreHtps2D=1 THEN HOTSPOT2 ds,ws/2,unID : unID=unID+1	IF gs_ShowMoreHtps2D=1 THEN HOTSPOT2 0,ws/2,unID : unID=unID+1	IF ((axis=1 AND only_axis=0) OR axis=0) or show_only_axsis THEN		POLY2_b 3, 7, gs_fill_pen, gs_back_pen,				ws/2,ws/2,901,				ws/2,360,4001,				ws/2-wt,360,4001		IF gs_ShowMoreHtps2D=1 THEN			HOTSPOT2 ds,ws,unID : unID=unID+1	!!! BOUNDING BOX			HOTSPOT2 ds,0,unID : unID=unID+1			HOTSPOT2 0,0,unID : unID=unID+1			HOTSPOT2 0,ws,unID : unID=unID+1		ENDIF	ENDIF	IF (axis=1) THEN		PEN axis_line_pen		LINE_TYPE axis_line_type		LINE2 -0.05, ws/2, ds+0.05,ws/2		LINE2 ds/2, -0.05, ds/2,ws+0.05		PEN gs_cont_pen		IF gs_ShowMoreHtps2D=1 THEN HOTSPOT2 ds/2,0,unID : unID=unID+1		IF gs_ShowMoreHtps2D=1 THEN HOTSPOT2 ds/2,ws,unID : unID=unID+1	ENDIF	DEL 1	PEN gs_cont_penENDIFEND300:unID=300PEN gs_cont_penIF ((axis=1 AND only_axis=0) OR axis=0) or show_only_axsis THEN PROJECT2{2} 3,270,3+32, gs_back_pen,0,0,0IF (axis=1) and not(show_only_axsis) THEN	PEN axis_line_pen	LINE_TYPE axis_line_type	IF gs_rot_axis=rot_axis_mtr[1] THEN		LINE2 0, 0, A,0		HOTLINE2 0, 0, A,0		HOTSPOT2 0, 0, unID, a, 1+256: unID=unID+1	!!! AXIS (Length)		HOTSPOT2 -1, 0, unID, a, 3: unID=unID+1		HOTSPOT2 a, 0, unID, a, 2: unID=unID+1		HOTSPOT2 A/2,0,unID : unID=unID+1			!!! AXIS (Length/2)	ENDIF	IF gs_rot_axis=rot_axis_mtr[2] THEN		IF gs_stationary=`Внизу` THEN ADD2 -ds/2*sin(ang),0		IF gs_stationary=`Вверху` THEN ADD2 ds/2*sin(ang),0		LINE2 0, 0, A*cos(ang),0		HOTLINE2 0, 0, A*cos(ang),0		HOTSPOT2 0, 0, unID, a_temp, 1+256, A: unID=unID+1		!!! AXIS (Length/2)		HOTSPOT2 -1, 0, unID, a_temp, 3, A: unID=unID+1		HOTSPOT2 a_temp, 0, unID, a_temp, 2, A: unID=unID+1		HOTSPOT2 A*cos(ang)/2,0,unID : unID=unID+1	!!! AXIS (Length)		IF gs_ShowMoreHtps2D=1 THEN			HOTSPOT2 0,ws/2,unID : unID=unID+1			!!! BOUNDING BOX			HOTSPOT2 0,-ws/2,unID : unID=unID+1			HOTSPOT2 A*cos(ang),ws/2,unID : unID=unID+1			HOTSPOT2 A*cos(ang),-ws/2,unID : unID=unID+1		ENDIF		IF gs_stationary <> `По центру` THEN DEL 1	ENDIFENDIFPEN gs_cont_penEND