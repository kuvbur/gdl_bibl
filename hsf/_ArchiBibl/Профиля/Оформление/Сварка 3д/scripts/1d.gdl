EPS=0.00001unID=1 : unIDR=800 : unIDD=4000is = 1 : ie = 2ix = 1 : iy = 2 : iz = 3n = REQUEST ("Name_of_main", "", name)is_call = 0 : if name<>"" then is_call = 1dim ttype_weld[]dim itype_weld[] : i=0i=i+1 : itype_weld[i]=0 : ttype_weld[i] = "Автоматическая"i=i+1 : itype_weld[i]=1 : ttype_weld[i] = "Ручная"dim ttype_weld_ang[]dim itype_weld_ang[] : i=0i=i+1 : itype_weld_ang[i]=0 : ttype_weld_ang[i] = "Угловой"i=i+1 : itype_weld_ang[i]=1 : ttype_weld_ang[i] = "Стыковой"dim tcalltype_weld[]dim icalltype_weld[] : i=0i=i+1 : icalltype_weld[i]=0 : tcalltype_weld[i] = "Нет"i=i+1 : icalltype_weld[i]=1 : tcalltype_weld[i] = "Автоматический угловой"i=i+1 : icalltype_weld[i]=2 : tcalltype_weld[i] = "Автоматический стыковой"i=i+1 : icalltype_weld[i]=3 : tcalltype_weld[i] = "Ручной угловой"i=i+1 : icalltype_weld[i]=4 : tcalltype_weld[i] = "Ручной стыковой"if is_call then	if calltype_weld>EPS then		if calltype_weld=1 then			type_weld = 0			type_weld_ang = 0		endif		if calltype_weld=2 then			type_weld = 0			type_weld_ang = 1		endif		if calltype_weld=3 then			type_weld = 1			type_weld_ang = 0		endif		if calltype_weld=4 then			type_weld = 1			type_weld_ang = 1		endif	endif	dx = pos_weld[ie][ix] - pos_weld[is][ix]	dy = pos_weld[ie][iy] - pos_weld[is][iy]	dz = pos_weld[ie][iz] - pos_weld[is][iz]	if abs(dx)<EPS and abs(dy)<EPS and abs(dz)<EPS then end	gosub "xytorphi"	l_pos_weld = r : ang_pl = phi : ang = etta	dx = pos_weld[ie][ix] - pos_weld[is][ix]	dy = pos_weld[ie][iy] - pos_weld[is][iy]	l_pos_weld_2d = SQR(dx*dx+dy*dy)else	!-- Перешли в сферичискую систему координат ---	if GLOB_MODPAR_NAME="pos_weld" or GLOB_MODPAR_NAME = ""  then		dx = pos_weld[ie][ix] - pos_weld[is][ix]		dy = pos_weld[ie][iy] - pos_weld[is][iy]		dz = pos_weld[ie][iz] - pos_weld[is][iz]		gosub "xytorphi"		parameters l_pos_weld = r, ang_pl = phi, ang = etta		dx = pos_weld[ie][ix] - pos_weld[is][ix]		dy = pos_weld[ie][iy] - pos_weld[is][iy]		l_axis_2d = SQR(dx*dx+dy*dy)		parameters l_pos_weld_2d=l_axis_2d	else		!-- Перешли в радиальную систему координат ---		if GLOB_MODPAR_NAME="l_pos_weld" or GLOB_MODPAR_NAME="ang_pl" or GLOB_MODPAR_NAME="ang" then			r = l_pos_weld : phi = ang_pl : etta = ang			gosub "rphitoxy"			pos_weld[ie][ix] = pos_weld[is][ix] + x			pos_weld[ie][iy] = pos_weld[is][iy] + y			pos_weld[ie][iz] = pos_weld[is][iz] + z			parameters pos_weld = pos_weld		endif	endifendifgoto 323"xytorphi":	r = SQR(dx*dx+dy*dy+dz*dz)	if dx>0 and dy>=0 then phi = atn(dy/dx)	if dx>0 and dy<0 then phi = atn(dy/dx)+360	if dx<0 then phi = atn(dy/dx)+180	if abs(dx)<EPS and dy>0 then phi = 90	if abs(dx)<EPS and dy<0 then phi = 270	if abs(dx)<EPS and abs(dy)<EPS then phi = 0	etta = acs(dz/r)return"rphitoxy":	x = r * sin(etta) * cos(phi)	y = r * sin(etta) * sin(phi)	z = r * cos(etta)return323: