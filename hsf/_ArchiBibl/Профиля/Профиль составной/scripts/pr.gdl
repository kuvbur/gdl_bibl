show_marker = 1 : show_subpos = 1 : call "Показ_по_сборкам" PARAMETERS ALL subpos=subpos, pos=GLOB_ID RETURNED_PARAMETERS show_subpos, show_markerif show_subpos = 0 and show_marker = 0 then endsuccess = LIBRARYGLOBAL ("Libraryflobals-km", "ignore_id", ignore_id)success = LIBRARYGLOBAL ("Libraryflobals-km", "show_ingore", show_ingore)show_spec = 0 : IF STRSTR (GLOB_ID, ignore_id, 1) > 0 or type_spec="Не специфицировать" THEN show_spec = 1success = LIBRARYGLOBAL ("Libraryflobals-km", "show_izd", show_izd)success = LIBRARYGLOBAL ("Libraryflobals-km", "show_metall", show_metall)if type_spec = "Металл" and not(show_metall) and show_spec = 0 then show_spec = 1if type_spec = "Изделие" and not(show_izd) and show_spec = 0 then show_spec = 1if not(show_spec) then 	dlin_prof = B	spokr = SURFACE3D()	ves = VOLUME3D()*7850	ves = ves + ves*proc_zap	dlin_prof = ves / ves_p	pos = GLOB_ID	gosub "spec_ed"	if uch_pokr then		tip_el = "30"		komment = "кв.м."		obozn = "см. примечания"		naen = "Покрытие " + mat_pokr		call "Спецификатор" parameters tip_el=tip_el,										subpos = subpos,  										obozn=obozn,										naen=naen,										kol_vo_spec=spokr,										komment=komment, otm_elem=ac_bottomlevel+SYMB_POS_Z		endif	if r_fire > 0 then		tip_el = "30"		komment = "кг."		obozn = " "		naen = "Тонкослойное огнезащитное покрытие"		kol_vo_spec = r_fire		call "Спецификатор" parameters tip_el=tip_el,										subpos = subpos,  										obozn=obozn,										naen=naen,										kol_vo_spec=kol_vo_spec,										komment=komment, otm_elem=ac_bottomlevel+SYMB_POS_Z		else		if t_pokr_txt <> "Обеспечивается металлом" then			tip_el = "30"			komment = "кв.м."			obozn = " "			naen = "Конструктивная огнезащита"			call "Спецификатор" parameters tip_el=tip_el,											subpos = subpos,  											obozn=obozn,											naen=naen,											kol_vo_spec=spokr,											komment=komment, otm_elem=ac_bottomlevel+SYMB_POS_Z			endif	endifendifend"spec_ed":	if type_spec = "Металл" then		if pm then naen = naen + "@@п.м."		tip_el = "20"		call "Спецификатор" parameters tip_el=tip_el,										subpos = subpos, 										pos = pos,										tip_konstr=tip_konstr,										stal_gost=stal_gost,										mater=mater,										obozn=obozn,										tip_profile=tip_profile,										dlin_prof=dlin_prof,										ves=ves_p, 										naen=naen, otm_elem=ac_bottomlevel+SYMB_POS_Z		else		tip_el = "40"		komment="кг." : if pm then komment="п.м."		call "Спецификатор" parameters tip_el=tip_el,										subpos = subpos, 										pos = pos, 										obozn=obozn,										naen=naen,										ves=ves_p,										kol_vo_spec=1,										komment=komment, otm_elem=ac_bottomlevel+SYMB_POS_Z			endifreturn