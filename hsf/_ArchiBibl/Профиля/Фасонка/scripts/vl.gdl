PARAMETERS name_prof = "Лист"call "Шаблон профиля" parameters alldim param_name_out_t[] : i=1param_name_out_t[1] = "!!Профиль "+name_prof+"!!"i=i+1 : param_name_out_t[i] = "naen - наименование"i=i+1 : param_name_out_t[i] = "obozn - обозначение"i=i+1 : param_name_out_t[i] = "tip_profile - сечение"i=i+1 : param_name_out_t[i] = "mater - марка стали"i=i+1 : param_name_out_t[i] = "a_m - толщ. фасонки"i=i+1 : param_name_out_t[i] = "b_m - ширина фасонки"i=i+1 : param_name_out_t[i] = "c_m - длина фасонки"parameters param_name_out = param_name_out_tdim param_temp_out_t[] : i=0i=i+1 : param_temp_out_t[i] = "%subpos%"i=i+1 : param_temp_out_t[i] = "%naen%"i=i+1 : param_temp_out_t[i] = "%tip_profile% %mater%"i=i+1 : param_temp_out_t[i] = "t%a_m% %mater%"i=i+1 : param_temp_out_t[i] = "t%a_m%x%b_m%x%c_m% %mater%"parameters param_temp_out = param_temp_out_tdim tip_f_m[]i = 0i = i + 1 : tip_f_m[i] = "Квадратная" 				!1i = i + 1 : tip_f_m[i] = "Прямоугольная" 			!2i = i + 1 : tip_f_m[i] = "Треугольная" 				!3i = i + 1 : tip_f_m[i] = "Треугольная с подрезкой"  !4i = i + 1 : tip_f_m[i] = "Прямоугольная с подрезкой"  !5i = i + 1 : tip_f_m[i] = "Трапеция с подрезкой"  !5values "tip_f" "Специальный", tip_f_mif tip_f = "Специальный" then HIDEPARAMETER "r_1", "r_2", "r_sk"if tip_f = "Квадратная" then	HIDEPARAMETER "r_1", "r_sk"	parameters n_t = 4	i = 0	i = i + 1 : kontur[i][1] = 0 : kontur[i][2] = 0 	i = i + 1 : kontur[i][1] = r_1 : kontur[i][2] = 0	i = i + 1 : kontur[i][1] = r_1 : kontur[i][2] = r_1	i = i + 1 : kontur[i][1] = 0 : kontur[i][2] = r_1endifif tip_f = "Прямоугольная" then	HIDEPARAMETER "r_sk"	parameters n_t = 4	i = 0	i = i + 1 : kontur[i][1] = 0 : kontur[i][2] = 0 	i = i + 1 : kontur[i][1] = r_1 : kontur[i][2] = 0	i = i + 1 : kontur[i][1] = r_1 : kontur[i][2] = r_2	i = i + 1 : kontur[i][1] = 0 : kontur[i][2] = r_2endifif tip_f = "Треугольная" then	HIDEPARAMETER "r_sk"	parameters n_t = 3	i = 0	i = i + 1 : kontur[i][1] = 0 : kontur[i][2] = 0 	i = i + 1 : kontur[i][1] = r_1 : kontur[i][2] = 0	i = i + 1 : kontur[i][1] = 0 : kontur[i][2] = r_2endifif tip_f = "Треугольная с подрезкой" then	parameters n_t = 6	i = 0	i = i + 1 : kontur[i][1] = 0 : kontur[i][2] = 0 	i = i + 1 : kontur[i][1] = -r_sk : kontur[i][2] = r_sk	i = i + 1 : kontur[i][1] = -r_sk : kontur[i][2] = r_1	i = i + 1 : kontur[i][1] = 0 : kontur[i][2] = r_1	i = i + 1 : kontur[i][1] = r_2-r_sk : kontur[i][2] = r_sk	i = i + 1 : kontur[i][1] = r_2-r_sk : kontur[i][2] = 0endifif tip_f = "Прямоугольная с подрезкой" then	parameters n_t = 6	i = 0	i = i + 1 : kontur[i][1] = 0 : kontur[i][2] = 0 	i = i + 1 : kontur[i][1] = -r_sk : kontur[i][2] = r_sk	i = i + 1 : kontur[i][1] = -r_sk : kontur[i][2] = r_1-r_sk	i = i + 1 : kontur[i][1] = 0 : kontur[i][2] = r_1	i = i + 1 : kontur[i][1] = r_2-r_sk : kontur[i][2] = r_1	i = i + 1 : kontur[i][1] = r_2-r_sk : kontur[i][2] = 0endifif tip_f = "Трапеция с подрезкой" then	parameters n_t = 7	i = 0	i = i + 1 : kontur[i][1] = 0 : kontur[i][2] = 0 	i = i + 1 : kontur[i][1] = 0 : kontur[i][2] = -r_1/2	i = i + 1 : kontur[i][1] = r_sk : kontur[i][2] = -r_1/2	i = i + 1 : kontur[i][1] = r_3-r_sk : kontur[i][2] = -(r_2-r_sk)/2	i = i + 1 : kontur[i][1] = r_3-r_sk : kontur[i][2] = (r_2-r_sk)/2	i = i + 1 : kontur[i][1] = r_sk : kontur[i][2] = r_1/2	i = i + 1 : kontur[i][1] = 0 : kontur[i][2] = r_1/2endifif GLOB_MODPAR_NAME = "prim" and prim then	parameters prim = 0	parameters kontur = kontur	parameters tip_f = "Специальный"endifk = 200values{2} "weld_side" itype_weld, ttype_weldvalues{2} "weld_side_up" itype_weld, ttype_weldvalues "weld_side_coord" 0,1/k, 2/k, 3/k, 4/kvalues "weld_side_ang" 0,90,180,270,360,-90,-180,-270,-360values "weld_side_ang_up" 0,90,180,270,360,-90,-180,-270,-360if GLOB_MODPAR_NAME = "weld_side_coord" then	for i=1 to vardim1(weld_side_coord)		weld_side[i] = int(weld_side_coord[i][1]*k)		weld_side_up[i] = int(weld_side_coord[i][2]*k)	next i	parameters weld_side = weld_side, weld_side_up = weld_side_upelse	for i=1 to vardim1(weld_side)		weld_side_coord[i][1] = weld_side[i]/k		weld_side_coord[i][2] = weld_side_up[i]/k	next i	parameters weld_side_coord = weld_side_coordendifif GLOB_MODPAR_NAME = "reset_weld" then	for i=1 to vardim1(weld_side)		weld_side_coord[i][1] = 0		weld_side[i] = 0		weld_side_coord[i][2] = 0		weld_side_up[i] = 0	next i	parameters weld_side = weld_side, weld_side_up = weld_side_up, weld_side_coord = weld_side_coord, reset_weld = 0endifvalues 'type_ves' "По геометрическим размерам", "По габаритам"aa1 = -100000000: aa2 = 100000000: bb1 = -100000000: bb2 = 100000000for i = 1 to n_t+1	aa1 = max(aa1, kontur[i][1])	aa2 = min(aa2, kontur[i][1])	bb1 = max(bb1, kontur[i][2])	bb2 = min(bb2, kontur[i][2])next itA = aa1 - aa2tB = bb1 - bb2A = round_int(tA*krat)/kratB = round_int(tB*krat)/kratif (tA-A)>EPS then A = A + krat_txtif (tB-B)>EPS then B = B + krat_txtparameters A = Aparameters B = Bif type_ves = "По геометрическим размерам" then	parameters s_plit_obsh = abs(s_plit_o)endifif type_ves = "По габаритам" then	parameters s_plit_obsh = A*Bendifif uch_pokr then	parameters spokr = s_plit_obsh * 2 + s_plit_obsh * 2 * 0.03else	parameters spokr = 0endifif mun = 0 then	PARAMETERS ves_p = 7850 * zzyzx	vt = 7850 * zzyzx * s_plit_obsh + 7850 * zzyzx * s_plit_obsh * proc_zap	PARAMETERS ves = vt	if abs(mun_t)<EPS then mun_t=ZZYZX	PARAMETERS naen = "-- " + str("%.0mm", A) + "x" + str("%.0mm", B) + "x" + str("%.0mm", mun_t)	PARAMETERS tip_profile = "-- " + str('%.0mm', mun_t)else	values 'ves' CUSTOM	values 'naen' CUSTOM	values 'tip_profile' CUSTOM	values 'obozn' CUSTOMendifif abs(mun_t)<EPS then mun_t=ZZYZXPARAMETERS a_m = mun_tPARAMETERS c_m = APARAMETERS b_m = B