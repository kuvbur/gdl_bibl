bEnableCustomCornerRight	= 1


! =============================================================================
! Lock Parameters
! =============================================================================

bLockOplineStyle = 1
if	iWindowType = WIN_MultisashHorizontal	|\
	iWindowType = WIN_MultisashVertical		then

	for i = 1 to gs_panes_number
		if (optypeIDArray[i] <> SASH_FIXED) & (optypeIDArray[i] <> SASH_VER_PIVOT) then
			!one sash is different from pivot or fixed
			bLockOplineStyle = 0
		endif
	next i
else
	if	((CasementIDArray[1][1] > 0) & gs_UTrans & gs_UTrans_Div & (gs_optype_m_10 <> SASH_FIXED) & (gs_optype_m_10 <> SASH_VER_PIVOT)) |\
		((CasementIDArray[1][2] > 0) & gs_UTrans & (gs_optype_m_02 <> SASH_FIXED) & (gs_optype_m_02 <> SASH_VER_PIVOT)) |\
		((CasementIDArray[1][3] > 0) & gs_UTrans & gs_UTrans_Div & (gs_optype_m_06 <> SASH_FIXED) & (gs_optype_m_06 <> SASH_VER_PIVOT)) |\
		((CasementIDArray[2][1] > 0) & gs_bLeftSLight & (gs_optype_m_09 <> SASH_FIXED) & (gs_optype_m_09 <> SASH_VER_PIVOT)) |\
		((CasementIDArray[2][2] > 0) & (gs_optype_m_01 <> SASH_FIXED) & (gs_optype_m_01 <> SASH_VER_PIVOT)) |\
		((CasementIDArray[2][3] > 0) & gs_bSLight & (gs_optype_m_05 <> SASH_FIXED) & (gs_optype_m_05 <> SASH_VER_PIVOT)) |\
		((CasementIDArray[3][1] > 0) & gs_LTrans & gs_LTrans_Div & (gs_optype_m_11 <> SASH_FIXED) & (gs_optype_m_11 <> SASH_VER_PIVOT)) |\
		((CasementIDArray[3][2] > 0) & gs_LTrans & (gs_optype_m_03 <> SASH_FIXED) & (gs_optype_m_03 <> SASH_VER_PIVOT)) |\
		((CasementIDArray[3][3] > 0) & gs_LTrans & gs_LTrans_Div & (gs_optype_m_07 <> SASH_FIXED) & (gs_optype_m_07 <> SASH_VER_PIVOT)) then

		!one sash is different from pivot or fixed
		bLockOplineStyle = 0
	endif
endif


