! =============================================================================
! Corner Window Type Enable
! =============================================================================

if iWindowType = WIN_MultisashHorizontal then
	bCornerWindow = (ac_cw_function & ac_corner_window)

	gs_CornConnTypeEnableLeft  = 3
	gs_CornConnTypeEnableRight = 3

	if bLeftCornerFunction & (optypeIDArray[1] = SASH_FIXED) then
		gs_CornConnTypeEnableLeft = 7
	endif
	if (bRightCornerFunction | bCornerWindow) & (optypeIDArray[gs_panes_number] = SASH_FIXED) then
		gs_CornConnTypeEnableRight = 7
	endif

	gs_CornConnTypeEnable = max(gs_CornConnTypeEnableLeft, gs_CornConnTypeEnableRight)
else
	gs_CornConnTypeEnable = 3
endif


