	gs_LTransom_Middle		= 1
endif

if	iWindowType = WIN_W3			|\
	iWindowType = WIN_WSingleHung3	|\
	iWindowType = WIN_WDoubleHung3	then

	gs_bSLight				= 1
	gs_bLeftSLight			= 1
	if gs_UTrans_Div then
		gs_Mullion_Top		= 1
		gs_LeftMullion_Top	= 1
	else
		gs_Mullion_Top		= 0
	endif
	if gs_LTrans_Div then
		gs_Mullion_Bott		= 1
		gs_LeftMullion_Bott	= 1
	else
		gs_Mullion_Bott		= 0
	endif
	gs_UTransom_toLeft		= 1
	gs_UTransom_toRight		= 1
	gs_LTransom_toLeft		= 1
	gs_LTransom_toRight		= 1
	gs_RightMullion_Middle	= 1
	gs_LeftMullion_Middle	= 1
	gs_UTransom_Middle		= 1
	gs_LTransom_Middle		= 1
endif

if iWindowType = WIN_WDoubleSash then
	gs_bSLight				= 1
	gs_Mullion_Top			= (gs_UTrans & gs_UTrans_Div)
	gs_Mullion_Bott			= (gs_LTrans & gs_LTrans_Div)
	gs_UTransom_toLeft		= 1
	gs_UTransom_toRight		= 1
	gs_LTransom_toLeft		= 1
	gs_LTransom_toRight		= 1
	gs_UTransom_Middle		= 1
	gs_LTransom_Middle		= 1
endif

