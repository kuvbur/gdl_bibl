	gs_LeftMullion_Bott		= 0
	gs_UTransom_toLeft		= 0
	gs_UTransom_toRight		= 0
	gs_LTransom_toLeft		= 0
	gs_LTransom_toRight		= 0
	gs_RightMullion_Middle	= 0
	gs_LeftMullion_Middle	= 0
	gs_UTransom_Middle		= 0
	gs_LTransom_Middle		= 0
endif


