	gs_Mullion_Bott			= 1
	gs_UTransom_toLeft		= 1
	gs_LTransom_toLeft		= 1
	gs_RightMullion_Middle	= 1
	gs_UTransom_Middle		= 1
	gs_LTransom_Middle		= 1
endif

if iWindowType = WIN_WVariable then
	if not(gs_bSLight) then
		gs_Mullion_Top	= 0
		gs_Mullion_Bott	= 0
	endif
	gs_UTransom_toLeft		= 1
	gs_UTransom_toRight		= not(gs_bSLight & gs_Mullion_Top)
	gs_LTransom_toLeft		= 1
	gs_LTransom_toRight		= not(gs_bSLight & gs_Mullion_Bott)
	gs_RightMullion_Middle	= 1
	gs_UTransom_Middle		= 1
	gs_LTransom_Middle		= 1
endif

if iWindowType = WIN_WVariableDoubleSash then
	gs_bLeftSLight			= 1
	if not(gs_bSLight) then
		gs_Mullion_Top	= 0
		gs_Mullion_Bott	= 0
	endif
	gs_UTransom_toLeft		= 1
	gs_UTransom_toRight		= (gs_bSLight & not(gs_Mullion_Top)) | not(gs_bSLight)
	gs_LTransom_toLeft		= 1
	gs_LTransom_toRight		= (gs_bSLight & not(gs_Mullion_Bott)) | not(gs_bSLight)
	gs_RightMullion_Middle	= 1
	gs_UTransom_Middle		= 1
	gs_LTransom_Middle		= 1
endif

if	iWindowType = WIN_MultisashHorizontal	|\
	iWindowType = WIN_MultisashVertical		then

	gs_UTrans				= 0
	gs_LTrans				= 0
