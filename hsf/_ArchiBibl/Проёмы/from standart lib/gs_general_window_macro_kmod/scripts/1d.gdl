	gs_UTransom_toRight		= 1
	gs_LTransom_toLeft		= 1
	gs_LTransom_toRight		= 1
	gs_UTransom_Middle		= 1
	gs_LTransom_Middle		= 1
endif

if	iWindowType = WIN_W2			|\
	iWindowType = WIN_WSingleHung2	|\
	iWindowType = WIN_WDoubleHung2	then

	gs_bSLight				= 1
	gs_Mullion_Top			= (gs_UTrans_Div)
	gs_Mullion_Bott			= (gs_LTrans_Div)
	gs_UTransom_toLeft		= 1
	gs_UTransom_toRight		= 1
	gs_LTransom_toLeft		= 1
	gs_LTransom_toRight		= 1
	gs_RightMullion_Middle	= 1
