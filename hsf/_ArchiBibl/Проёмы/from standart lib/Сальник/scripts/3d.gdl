call "Проём в стене" parameters all, WALL_COMPS_NAME_ = WALL_COMPS_NAME,WALL_SKINS_NUMBER_ = WALL_SKINS_NUMBER, WALL_SKINS_PARAMS_ = WALL_SKINS_PARAMS, WALL_THICKNESS_ = WALL_THICKNESS del topaddz -otstup_2if bRotated=0 and SYMB_MIRRORED=1 then mulx -1if bRotated=1 and SYMB_MIRRORED=0 then mulx -1if not((AC_Wall_Direction_Type=0) or (AC_Wall_Direction_Type=2)) then roty 180mulz sgn(otstup)if abs(otstup)-900<EPS then addz abs(otstup)addz max_t/2	model solid	HOTSPOT 0, 0, 0, unID: unID=unID+1	HOTSPOT -dns/2, 0, 0, unID: unID=unID+1	HOTSPOT dns/2, 0, 0, unID: unID=unID+1	HOTSPOT 0, dns/2, 0, unID: unID=unID+1	HOTSPOT 0, -dns/2, 0, unID: unID=unID+1	HOTSPOT 0, 0, -max_t, unID: unID=unID+1	HOTSPOT -dns/2, 0, -max_t, unID: unID=unID+1	HOTSPOT dns/2, 0, -max_t, unID: unID=unID+1	HOTSPOT 0, dns/2, -max_t, unID: unID=unID+1	HOTSPOT 0, -dns/2, -max_t, unID: unID=unID+1	WALLHOLE  2,1, 0,0,900, A/2, 360, 4000	addz -dlin_sal-(max_t-dlin_sal)/2	CYLIND dlin_sal,dns/2		del top