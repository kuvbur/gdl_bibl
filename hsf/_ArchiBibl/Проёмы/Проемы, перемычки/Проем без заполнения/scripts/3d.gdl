if max(A,B)<min_h_otv and min(A,B)<min_b_otv then endif not(show_asotv) then endshow_th = 0success = LIBRARYGLOBAL ("Libraryflobals-as", "show_th", show_th)success = LIBRARYGLOBAL ("LibraryGlobals13", "MSFillType", reqMSFillType)success = LIBRARYGLOBAL ("LibraryGlobals13", "MSFillPen", reqMSFillPen)success = LIBRARYGLOBAL ("LibraryGlobals13", "MSFillBackPen", reqMSFillBackPen)success = LIBRARYGLOBAL ("LibraryGlobals13", "MSContLineType", reqMSContLineType)success = LIBRARYGLOBAL ("LibraryGlobals13", "MSContLinePen", reqMSContLinePen)if type_sal<>"Нет" then	HOTSPOT A/2, 0,0, unID: unID=unID+1	HOTSPOT -A/2, 0,0, unID: unID=unID+1	HOTSPOT 0, A/2,0, unID: unID=unID+1	HOTSPOT 0, -A/2,0, unID: unID=unID+1	HOTSPOT 0, 0,0, unID: unID=unID+1	HOTSPOT A/2, 0,ZZYZX, unID: unID=unID+1	HOTSPOT -A/2, 0,ZZYZX, unID: unID=unID+1	HOTSPOT 0, A/2,ZZYZX, unID: unID=unID+1	HOTSPOT 0, -A/2,ZZYZX, unID: unID=unID+1	HOTSPOT 0, 0,ZZYZX, unID: unID=unID+1	jt = 13	put 0,0,900+jt	put A/2,360,4000+jt	put A/2-tst,360,4000+jt	PRISM_ NSP/3, dlin_sal, get(NSP)	A = A - tst*2endifrotx anggosub "attr"if tip_proem = "Прямоугольный" then	if in3d then		HOTSPOT A, 0, 0		HOTSPOT A, B, 0		HOTSPOT 0, B, 0		if oper then			HOTSPOT 0, 0, ZZYZX			HOTSPOT A, 0, ZZYZX			HOTSPOT A, B, ZZYZX			HOTSPOT 0, B, ZZYZX			if GLOB_SEO_TOOL_MODE = 1 or show_op_otv then				gosub "attr"				BLOCK A, B, ZZYZX				if GLOB_SEO_TOOL_MODE = 1 then end			endif		endif		if th_priyam>EPS and priyam and show_th then			pen reqMSContLinePen			SECT_ATTRS reqMSFillType, 0, reqMSContLinePen, reqMSContLinePen,reqMSContLinePen			HOTSPOT2 a/2, b, 901, th_priyam, 1+128			HOTSPOT2 a/2, th_priyam+b, 902, th_priyam, 2			HOTSPOT2 a/2, -1, 903, th_priyam, 3			put -th_priyam,-th_priyam,15			put -th_priyam,th_priyam+b,15			put th_priyam+a,th_priyam+b,15			put th_priyam+a,-th_priyam,15			put -th_priyam,-th_priyam,-1				put 0,0,15			put 0,b,15			put a,b,15			put a,0,15			put 0,0,-1			PRISM_ NSP/3, ZZYZX, get(NSP)			put -th_priyam,-th_priyam,15			put -th_priyam,th_priyam+b,15			put th_priyam+a,th_priyam+b,15			put th_priyam+a,-th_priyam,15			put -th_priyam,-th_priyam,-1			addz -th_priyam			PRISM_ NSP/3, th_priyam, get(NSP)				del 1		endif		if upr_n = -1 or GLOB_STRUCTURE_DISPLAY = upr_n then			HOTSPOT 0, 0, 0, unID: unID=unID+1			HOTSPOT pos_x, 0, 0, unID, pos_y, 1+128: unID=unID+1			HOTSPOT pos_x, pos_y, 0, unID, pos_y, 2: unID=unID+1			HOTSPOT 0, pos_y, 0, unID, pos_x, 1+128: unID=unID+1			HOTSPOT pos_x, pos_y, 0, unID, pos_x, 2: unID=unID+1			gosub "attr_on"			POLY  4,			0,0,			0,B,			A,B,			pos_x,pos_y		endif	endif	if (GLOB_STRUCTURE_DISPLAY <> upr_n or upr_n=-1) then 		if pokr or obraml then 			addz ZZYZX-0.101			gosub "attr_on"			CALL "Щит" parameters all			del 1		endif	endif	else	if in3d then		if oper then			HOTSPOT 0, 0, 0			HOTSPOT A/2, 0, 0			HOTSPOT 0, A/2, 0			HOTSPOT 0, 0, ZZYZX			HOTSPOT A/2, 0, ZZYZX			HOTSPOT 0, A/2, ZZYZX			if GLOB_SEO_TOOL_MODE = 1 or show_op_otv then				gosub "attr"				CYLIND ZZYZX, A/2				if GLOB_SEO_TOOL_MODE = 1 then end			endif		endif		if upr_n = -1 or GLOB_STRUCTURE_DISPLAY = upr_n then			gosub "attr_on"			HOTSPOT 0, 0, 0			HOTSPOT A/2, 0, 0			HOTSPOT 0, A/2, 0			HOTSPOT -A/2, 0, 0			HOTSPOT 0, -A/2, 0			HOTSPOT A/2, 0, ZZYZX			HOTSPOT 0, A/2, ZZYZX			HOTSPOT -A/2, 0, ZZYZX			HOTSPOT 0, -A/2, ZZYZX			x = sin(32.75) * A/2			y = cos(32.75) * A/2			poly_ 7, 			-x, -y, 1, 			A * 0.15, - A * 0.15, 900, 			0, -155, 4001, 			y, x, 1, 			0, 0, 900, 			0, 205, 4001, 			-x, -y, 1		endif	endifendifend"attr":	pen i_pen	n = REQUEST{2} ("Building_Material_info", _bmat, "gs_bmat_cutfill_properties", _fillType, _fillPen, _fillBgPen)	bSucceed = request{2} ("Building_Material_info", _bmat, "gs_bmat_surface", _mat)	building_material _bmat	material _mat	sect_attrs{2} i_pen, 1		SHADOW OFFreturn"attr_on":	pen i_pen	SECT_FILL "Сталь", 0, i_pen, i_pen,i_pen	SECT_ATTRS "Сталь", 0, i_pen, i_pen,i_pen	MATERIAL "!Краска-05"		SHADOW ONreturn