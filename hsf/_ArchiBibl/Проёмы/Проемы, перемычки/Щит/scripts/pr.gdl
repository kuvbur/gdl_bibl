ter='С245'	gosub "pr"else	kol_vo_spec = 1	komment = " "	gosub "izd"endifreturn"izd":	!Изделие	tip_el = "40"	call "Спецификатор" parameters tip_el=tip_el,									subpos = subpos, 									pos = pos, 									obozn=obozn,									naen=naen,									ves=ves,									kol_vo_spec=kol_vo_spec,									komment=komment, otm_elem=ac_bottomlevel+SYMB_POS_Zreturn"mat":	!Материал	tip_el = "30"	call "Спецификатор" parameters tip_el=tip_el,									subpos = subpos, 									obozn=obozn,									naen=naen,									kol_vo_spec=kol_vo_spec,									komment=komment, otm_elem=ac_bottomlevel+SYMB_POS_Zreturn"pr":	!Прокат	tip_el = "20"	call "Спецификатор" parameters tip_el=tip_el,									subpos = subpos, 									pos = pos,									tip_konstr=tip_konstr,									stal_gost=stal_gost,									mater=mater,									obozn=obozn,									tip_profile=tip_profile,									dlin_prof=dlin_prof,									ves=ves/dlin_prof,									naen=naen, otm_elem=ac_bottomlevel+SYMB_POS_Zreturn"eend":