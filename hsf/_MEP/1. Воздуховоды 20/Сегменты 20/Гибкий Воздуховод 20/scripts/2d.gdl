!-----------2d---------------------if show_el < EPS then endIF gs_detlevel_2D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, gs_fill_type, gs_fill_pen, gs_back_pen, MEP_cline_type, MEP_cline_pen, show_elnumPoint = NumFlexibleDuctPointsPEN gs_cont_penIF gs_fill_pen THEN SET FILL gs_fill_typeFOR i = 1 TO VARDIM1(MEP_ControlPoints)	unID = i-1	IF i < 2 THEN unID = hceID_1	IF i > VARDIM1(MEP_ControlPoints)-1 THEN unID = hceID_2	HOTSPOT2 MEP_ControlPoints[i][1], MEP_ControlPoints[i][2], unIDNEXT iFOR i = 1 TO numPoint-1	HOTLINE2 MEP_FlexibleDuctPoints[i][1], MEP_FlexibleDuctPoints[i][2], MEP_FlexibleDuctPoints[i+1][1], MEP_FlexibleDuctPoints[i+1][2]NEXT iPEN gs_cont_penIF gs_fill_pen THEN PEN gs_fill_penif not(show_axis) then PROJECT2{3} 3, 270, 3+32, 4, gs_back_pen, 0, 0, 0, PARAMETERS gs_detlevel_3D_m = DETLEV_3D_DETAILED, duct_show = 1, MEP_InsShow = 0, MEP_cline_show_3D = 0PEN gs_cont_penif not(show_axis) then PROJECT2{3} 3, 270, 2, 8, 0, 0, 0, 0, PARAMETERS gs_detlevel_3D_m = DETLEV_3D_DETAILED, duct_show = 1, MEP_InsShow = 0, MEP_cline_show_3D = 0IF MEP_InsShow and show_isol and not(show_axis) THEN PROJECT2{3} 3, 270, 2, 8, gs_back_pen, 0, 0, 0, PARAMETERS gs_detlevel_3D_m = DETLEV_3D_DETAILED, duct_show = 0, MEP_InsShow = 1, MEP_cline_show_3D = 0IF ((MEP_cline_show and show_axis_and) or show_axis) THEN	PEN MEP_cline_pen	SET LINE_TYPE MEP_cline_type	FOR i = 1 TO numPoint-1		LINE2 MEP_FlexibleDuctPoints[i][1], MEP_FlexibleDuctPoints[i][2], MEP_FlexibleDuctPoints[i+1][1], MEP_FlexibleDuctPoints[i+1][2]	NEXT iENDIF