!-----------2d---------------------if show_el < EPS then endIF gs_detlevel_2D_m < EPS THEN ENDif GLOB_PREVIEW_MODE = 0 and GLOB_FEEDBACK_MODE = 0 then CALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, gs_fill_type, gs_fill_pen, gs_back_pen, MEP_cline_type, MEP_cline_pen, show_elPEN gs_cont_penSET FILL gs_fill_typeHptsUnIDBeginForConnEnds = 30000hceID_1 = HptsUnIDBeginForConnEnds+100hceID_2 = HptsUnIDBeginForConnEnds+200	unID = 1	HOTSPOT2 outCoords[7][1], outCoords[7][2], unID: unID=unID+1!!	HOTSPOT2 outCoords[8][1], outCoords[8][2]!!	HOTSPOT2 outCoords[9][1], outCoords[9][2]!!	HOTSPOT2 outCoords[10][1], outCoords[10][2]!!	HOTSPOT2 outCoords[11][1], outCoords[11][2]!!	HOTSPOT2 outCoords[12][1], outCoords[12][2]!!	HOTSPOT2 outCoords[13][1], outCoords[13][2]!!	HOTSPOT2 outCoords[14][1], outCoords[14][2]!!	HOTSPOT2 outCoords[15][1], outCoords[15][2]call "2dmacro" parameters all, type_symb = type_symb, dirAngOnXYPlane = dirAngOnXYPlaneif show_axis then 	hotspot2 0,0	endendifIF ABS(dZz-1) < EPS THEN	ADD2 MEP_Position[1][1], MEP_Position[1][2]	ROT2 dirAngOnXYPlane	DIM StraightPosition[][]	StraightPosition[1][1] = -A/2	StraightPosition[1][2] = 0	StraightPosition[1][3] = 0	StraightPosition[2][1] = 1	StraightPosition[2][2] = 0	StraightPosition[2][3] = 0	StraightPosition[3][1] = 0	StraightPosition[3][2] = 1	StraightPosition[3][3] = 0	CALL "MEP_m_Duct_4" PARAMETERS 	MEP_Position = StraightPosition,									gs_Connections = gs_Connections,									MEP_NumberConnections = MEP_NumberConnections,									MEP_NumConnectionData = MEP_NumConnectionData,									MEP_StrConnectionData = MEP_StrConnectionData,									MEP_StraightLength = A,									gs_AddConnections = gs_AddConnections,									duct_show = 1,									MEP_InsShow = 0,									MEP_cline_show = 0,									gs_detlevel_3D_m = gs_detlevel_3D_m,									gs_cont_pen = gs_cont_pen,									gs_fill_type = gs_fill_type,									gs_fill_pen = gs_fill_pen,									gs_back_pen = gs_back_pen	DEL 2ELSE	HOTSPOT2 outCoords[1][1], outCoords[1][2], hceID_1	HOTSPOT2 outCoords[4][1], outCoords[4][2], hceID_2	SET FILL gs_fill_type	PEN gs_cont_pen	IF gs_fill_pen THEN PEN gs_fill_pen	if not(show_axis) then PROJECT2{3} 3, 270, 3+32, 4, gs_back_pen, 0, 0, 0, PARAMETERS gs_detlevel_3D_m = 2	PEN gs_cont_pen	if not(show_axis) then PROJECT2{3} 3, 270, 2, 8, 0, 0, 0, 0, PARAMETERS gs_detlevel_3D_m = 2ENDIFEND