!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDif GLOB_PREVIEW_MODE = 0 and GLOB_FEEDBACK_MODE = 0 then CALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_elIF GLOB_CONTEXT = 2 THEN res = 24MainBoxMaterial = sMatMATERIAL MainBoxMaterialPEN gs_cont_penRESOL rescall "2dmacro" parameters all, type_symb = type_symb, dirAngOnXYPlane = dirAngOnXYPlaneif show_axis then 	hotspot 0,0,0	endendif				! --- TRANSFORMATION --- !XFORM   MEP_Position[2][1], MEP_Position[3][1], dZx, MEP_Position[1][1],		MEP_Position[2][2], MEP_Position[3][2], dZy, MEP_Position[1][2],		MEP_Position[2][3], MEP_Position[3][3], dZz, MEP_Position[1][3]				! --- HOTSPOT --- !unID = 1HOTSPOT 0, 0, 0, unID: unID=unID+1HOTSPOT -A/2, -B/2, -ZZYZX/2, unID: unID=unID+1HOTSPOT -A/2, -B/2, ZZYZX/2, unID: unID=unID+1HOTSPOT A/2, -B/2, -ZZYZX/2, unID: unID=unID+1HOTSPOT A/2, -B/2, ZZYZX/2, unID: unID=unID+1HOTSPOT -A/2, B/2, -ZZYZX/2, unID: unID=unID+1HOTSPOT -A/2, B/2, ZZYZX/2, unID: unID=unID+1HOTSPOT A/2, B/2, -ZZYZX/2, unID: unID=unID+1HOTSPOT A/2, B/2, ZZYZX/2, unID: unID=unID+1				! --- DAMPER --- !DIM StraightPosition[][]StraightPosition[1][1] = -A/2StraightPosition[1][2] = 0StraightPosition[1][3] = 0StraightPosition[2][1] = 1StraightPosition[2][2] = 0StraightPosition[2][3] = 0StraightPosition[3][1] = 0StraightPosition[3][2] = 1StraightPosition[3][3] = 0CALL "MEP_m_Duct_4" PARAMETERS 	MEP_Position = StraightPosition,								gs_Connections = gs_Connections,								MEP_NumberConnections = MEP_NumberConnections,								MEP_NumConnectionData = MEP_NumConnectionData,								MEP_StrConnectionData = MEP_StrConnectionData,								MEP_StraightLength = A,								gs_AddConnections = gs_AddConnections,								duct_show = 1,								gs_DuctMat = MainBoxMaterial,								MEP_InsShow = 0,								MEP_cline_show_3D = 0,								gs_detlevel_3D_m = gs_detlevel_3D_m,								gs_cont_pen = gs_cont_penROTz -90ADD -B/2, -A/2, -ZZYZX/2ROTx 90POLY_ 5,	B, ZZYZX, 1,	B, 0, 1,	0, 0, 1,	0, ZZYZX, 1,	B, ZZYZX, -1ADDz -APOLY_ 5,	B, ZZYZX, 1,	B, 0, 1,	0, 0, 1,	0, ZZYZX, 1,	B, ZZYZX, -1DEL 4END