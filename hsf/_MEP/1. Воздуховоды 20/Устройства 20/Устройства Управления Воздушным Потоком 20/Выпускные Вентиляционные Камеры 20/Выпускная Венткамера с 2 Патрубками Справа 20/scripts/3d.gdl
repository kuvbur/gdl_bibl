!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_el! Outlet Plenum Right WyeIF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFIF gs_detlevel_3D_m = 0 THEN ENDres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! SimpleIF useSysMat THEN MainBoxMaterial = sMatMATERIAL MainBoxMaterialPEN gs_cont_penRESOL res				! --- HOTSPOTS --- !unID = 1HOTSPOT A/2, B/2, ZZYZX, unID: unID=unID+1HOTSPOT -A/2, B/2, ZZYZX, unID: unID=unID+1HOTSPOT -A/2, -B/2, ZZYZX, unID: unID=unID+1HOTSPOT A/2, -B/2, ZZYZX, unID: unID=unID+1HOTSPOT A/2, B/2, 0, unID: unID=unID+1HOTSPOT -A/2, B/2, 0, unID: unID=unID+1HOTSPOT -A/2, -B/2, 0, unID: unID=unID+1HOTSPOT A/2, -B/2, 0, unID: unID=unID+1				! --- MAIN BOX --- !ADD -A/2, -B/2, 0BLOCK A, B, ZZYZXDEL 1				! --- CONNECTIONS --- !CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	ui_current_con = ui_current_con,	MEP_NumberConnections = MEP_NumberConnections,	MEP_NumConnectionData = gs_Connections,	gs_AddConnections = gs_AddConnections,	cShow3D = 1,	gs_ConMat = MainBoxMaterial,	MEP_InsShow = 0,	MEP_cline_show_3D = 0,	gs_cont_pen = gs_cont_pen,	ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 1,	ConPosX_2=ConPosX_2, ConPosY_2=ConPosY_2, ConPosZ_2=ConPosZ_2, ConLength_2=ConLength_2, ConEdit_2 = 1,	ConPosX_3=ConPosX_3, ConPosY_3=ConPosY_3, ConPosZ_3=ConPosZ_3, ConLength_3=ConLength_3, ConEdit_3 = 1, MEP_StrConnectionData = MEP_StrConnectionDataEND