!-----------2d---------------------if show_el < EPS then endIF gs_detlevel_2D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, gs_fill_type, gs_fill_pen, gs_back_pen, MEP_cline_type, MEP_cline_pen, show_elPEN gs_cont_penSET FILL gs_fill_typecall "2dmacro" parameters all, type_symb = type_symbif show_axis then 	hotspot 0,0,0	endendifHptsUnIDBeginForConnEnds = 30000hceID_1 = HptsUnIDBeginForConnEnds+100unID = 1IF ABS(dZz-1) < EPS THEN	ADD2 MEP_Position[1][1], MEP_Position[1][2]	ROT2 dirAngOnXYPlane	capHalfWidth = ConWidth_1/2+ConWallThickness_1	capThick = EPS	IF ABS(gs_AddConnections[1][1] - 2) < EPS THEN		capHalfWidth = capHalfWidth + ConConnectorWidth_1		capThick = MAX(EPS, ConWallThickness_1)	ENDIF	HOTSPOT2 0, 0, unID: unID=unID+1	HOTSPOT2 capHalfWidth, 0, unID: unID=unID+1	HOTSPOT2 -capHalfWidth, 0, unID: unID=unID+1	PUT -capHalfWidth, 0, 1,		-capHalfWidth, -capThick, 1,		capHalfWidth, -capThick, 1,		capHalfWidth, 0, 1,		-capHalfWidth, 0, -1	POLY2_B NSP/3, 1+2+4+64, gs_fill_pen, gs_back_pen,		GET (NSP)	DEL 2ELSE	SET FILL gs_fill_type	PEN gs_cont_pen	HOTSPOT2 outCoords[1][1], outCoords[1][2], unID: unID=unID+1	IF ABS(MEP_NumConnectionData[1][21] - 1) < EPS THEN		HOTSPOT2 outCoords[4][1], outCoords[4][2], unID: unID=unID+1		HOTSPOT2 outCoords[5][1], outCoords[5][2], unID: unID=unID+1		HOTSPOT2 outCoords[6][1], outCoords[6][2], unID: unID=unID+1		HOTSPOT2 outCoords[7][1], outCoords[7][2], unID: unID=unID+1	ENDIF	IF ABS(MEP_NumConnectionData[1][21] - 2) < EPS THEN		HOTSPOT2 outCoords[8][1], outCoords[8][2], unID: unID=unID+1		HOTSPOT2 outCoords[9][1], outCoords[9][2], unID: unID=unID+1		HOTSPOT2 outCoords[10][1], outCoords[10][2], unID: unID=unID+1		HOTSPOT2 outCoords[11][1], outCoords[11][2], unID: unID=unID+1	ENDIF	IF ABS(MEP_NumConnectionData[1][21] - 3) < EPS THEN		HOTSPOT2 outCoords[8][1], outCoords[8][2], unID: unID=unID+1		HOTSPOT2 outCoords[9][1], outCoords[9][2], unID: unID=unID+1		HOTSPOT2 outCoords[12][1], outCoords[12][2], unID: unID=unID+1		HOTSPOT2 outCoords[13][1], outCoords[13][2], unID: unID=unID+1	ENDIF	IF gs_fill_pen THEN PEN gs_fill_pen	PROJECT2{3} 3, 270, 3+32, 4, gs_back_pen, 0, 0, 0, PARAMETERS MEP_InsShow = 0, gs_detlevel_3D_m = 2	PEN gs_cont_pen	PROJECT2{3} 3, 270, 2, 8, 0, 0, 0, 0, PARAMETERS MEP_InsShow = 0, gs_detlevel_3D_m = 2ENDIFEND