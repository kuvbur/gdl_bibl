
! Cabinet Unit Heater


PEN gs_cont_pen
SET FILL gs_fill_type

unID = 1
HOTSPOT2 0, 0, unID: unID=unID+1


			! --- MAIN BOX --- !

ADD2 -A/2, -B/2

HOTSPOT2 0, 0, unID: unID=unID+1
HOTSPOT2 0, B, unID: unID=unID+1
HOTSPOT2 A, B, unID: unID=unID+1
HOTSPOT2 A, 0, unID: unID=unID+1

PUT 0, 0, 1,
	0, B, 1,
	A, B, 1,
	A, 0, 1,
	0, 0, -1

POLY2_B NSP/3, 1+2+4+64, gs_fill_pen, gs_back_pen,

	GET (NSP)

DEL 1

END
