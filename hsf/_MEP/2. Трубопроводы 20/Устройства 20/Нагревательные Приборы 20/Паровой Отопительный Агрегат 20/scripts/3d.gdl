!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_el! Steam Unit HeaterIF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFIF gs_detlevel_3D_m = 0 THEN ENDres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! SimpleMainBoxMaterial = sMatMATERIAL MainBoxMaterialPEN gs_cont_penRESOL resaddStatus = 0IF GLOB_CONTEXT > 40 AND GLOB_CONTEXT < 50 THEN	addStatus = 3ENDIF				! --- HOTSPOTS --- !unID = 1HOTSPOT A/2, dTDepth/2, 0, unID: unID=unID+1HOTSPOT -A/2, dTDepth/2, 0, unID: unID=unID+1HOTSPOT -A/2, -dTDepth/2, 0, unID: unID=unID+1HOTSPOT A/2, -dTDepth/2, 0, unID: unID=unID+1HOTSPOT A/2, dTDepth/2, ZZYZX, unID: unID=unID+1HOTSPOT -A/2, dTDepth/2, ZZYZX, unID: unID=unID+1HOTSPOT -A/2, -dTDepth/2, ZZYZX, unID: unID=unID+1HOTSPOT A/2, -dTDepth/2, ZZYZX, unID: unID=unID+1				! --- MAIN BOX --- !IF gs_detlevel_3D_m = 2 THEN	ROTx 90	ADDz -dTDepth/2	EXTRUDE 5, 0, 0, dTDepth, addStatus+16+32,		-A/2, 0, 0,		A/2, 0, 0,		A/2, ZZYZX, 0,		-A/2, ZZYZX, 0,		-A/2, 0, -1	POLY_ 7,		-A/2, 0, 1,		A/2, 0, 1,		A/2, ZZYZX, 1,		-A/2, ZZYZX, 1,		-A/2, 0, -1,		0, ZZYZX/2, 901,		rr, 360, 4001	ADDz dTDepth	POLY_ 10,		-A/2, 0, 1,		A/2, 0, 1,		A/2, ZZYZX, 1,		-A/2, ZZYZX, 1,		-A/2, 0, -1,		-holeWidth/2, ZZYZX/2-holeHeight/2, 1,		holeWidth/2, ZZYZX/2-holeHeight/2, 1,		holeWidth/2, ZZYZX/2+holeHeight/2, 1,		-holeWidth/2, ZZYZX/2+holeHeight/2, 1,		-holeWidth/2, ZZYZX/2-holeHeight/2, -1	DEL 3	FOR i = 1 TO num			! --- Louver ---	ADD 0, -dTDepth/2, ZZYZX/2+holeHeight/2-0.052*(i-1)	PLANE 4,		-holeWidth/2, 0, 0,		holeWidth/2, 0, 0,		holeWidth/2, -0.021, -0.028,		-holeWidth/2, -0.021, -0.028	PLANE 4,		-holeWidth/2, -0.021, -0.028,		holeWidth/2, -0.021, -0.028,		holeWidth/2, -0.025, -0.04,		-holeWidth/2, -0.025, -0.04	DEL 1	NEXT i	ADD 0, dTDepth/2, ZZYZX/2		! --- Engine ---	MUL A/0.42, (B-dTDepth-0.025)/0.37, A/0.42	ROTx -90	FOR j = 1 TO 4	ROTz 90*j	ADD 0, 0.01,  0.08	ROTx -105		POLY_ 6,			0.015, 0, 1,			0.155, 0, 1,			0.155, 0.06, 1,			0.135, 0.08, 1001,			0.015, 0.08, 1,			0.015, 0, -1	DEL 3	NEXT j	CYLIND 0.09, 0.025	ADDz 0.09	CYLIND 0.23, 0.085	ADD -0.11, 0.085, 0	BLOCK 0.22, 0.005, 0.23	DEL 1	ADDz 0.23	CONE 0.05, 0.085, 0.035, 90, 90	DEL 2	FOR k = 1 TO 2	MULx 1-2*(k-1)	ADDx 0.11	ROTz 90	ROTx 90	PRISM_ 18, 0.01,		-0.17, 0, 15,		-0.16, 0, 15,		-0.16, 0.09, 15,		0.16, 0.09, 15,		0.16, 0, 15,		0.17, 0, 15,		0.17, 0.264, 15,		0.09, 0.32, 15,		-0.09, 0.32, 15,		-0.17, 0.264, 15,		-0.17, 0, -1,		-0.08685, 0.31, 15,		0.08685, 0.31, 15,		0.16, 0.25878, 15,		0.16, 0.1, 15,		-0.16, 0.1, 15,		-0.16, 0.25878, 15,		-0.08685, 0.31, -1	DEL 4	NEXT k	DEL 3ELSE	ADD -A/2, -dTDepth/2, 0	BLOCK A, dTDepth, ZZYZX	DEL 1	ADD -holeWidth/2, -dTDepth/2-0.025, ZZYZX/2-holeHeight/2	BLOCK holeWidth, 0.025, holeHeight		! --- Louver ---	DEL 1	ADD 0, dTDepth/2, ZZYZX/2		! --- Engine ---	MUL A/0.42, (B-dTDepth-0.025)/0.37, A/0.42	ROTx -90	ADDz 0.32	CONE 0.05, 0.085, 0.035, 90, 90	DEL 1	ROTz 90	ROTx 90	ADDz -0.12	PRISM_ 7, 0.24,		-0.17, 0, 15,		0.17, 0, 15,		0.17, 0.264, 15,		0.09, 0.32, 15,		-0.09, 0.32, 15,		-0.17, 0.264, 15,		-0.17, 0, -1	DEL 6ENDIF				! --- CONNECTIONS --- !CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	ui_current_con = ui_current_con,	MEP_NumberConnections = MEP_NumberConnections,	MEP_NumConnectionData = gs_Connections,	gs_AddConnections = gs_AddConnections,	cShow3D = 1,	gs_ConMat = MainBoxMaterial,	MEP_InsShow = 0,	MEP_cline_show_3D = 0,	gs_cont_pen = gs_cont_pen,	ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 15,	ConPosX_2=ConPosX_2, ConPosY_2=ConPosY_2, ConPosZ_2=ConPosZ_2, ConLength_2=ConLength_2, ConEdit_2 = 15, MEP_StrConnectionData = MEP_StrConnectionDataEND