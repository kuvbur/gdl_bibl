!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_el! 0.25-2.5 LPS Water SoftenerIF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFIF gs_detlevel_3D_m = 0 THEN ENDres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! SimpleIF useSysMat THEN	MainBoxMaterial = sMat	operatorMaterial = sMatENDIFMATERIAL MainBoxMaterialPEN gs_cont_penRESOL resDEFINE MATERIAL	"Shaft Black Plastic" 20,	0.147059, 0.135294, 0.103922,	0.9, 0.6, 0.2, 0, 10, 0,	1, 0.978103, 0.968383,	0, 0, 0,	0, 0, 1,	0				! --- WATER SOFTENER --- !unID = 1HOTSPOT  0, 0, 0, unID: unID=unID+1HOTSPOT A - (mineralTankDiam + brineTankDiam) / 2,0, 0, unID: unID=unID+1HOTSPOT -mineralTankDiam/2,  0, 0, unID: unID=unID+1HOTSPOT 0,  mineralTankDiam/2, 0, unID: unID=unID+1HOTSPOT 0, -mineralTankDiam/2, 0, unID: unID=unID+1HOTSPOT A - mineralTankDiam / 2, 0, 0, unID: unID=unID+1HOTSPOT A - (mineralTankDiam + brineTankDiam) / 2,  brineTankDiam/2, 0, unID: unID=unID+1HOTSPOT A - (mineralTankDiam + brineTankDiam) / 2, -brineTankDiam/2, 0, unID: unID=unID+1HOTSPOT  0, 0, ZZYZX, unID: unID=unID+1HOTSPOT A - (mineralTankDiam + brineTankDiam) / 2,0, brineTankHeight, unID: unID=unID+1HOTSPOT -mineralTankDiam/2,  0, mineralTankHeight, unID: unID=unID+1HOTSPOT 0,  mineralTankDiam/2, mineralTankHeight, unID: unID=unID+1HOTSPOT 0, -mineralTankDiam/2, mineralTankHeight, unID: unID=unID+1HOTSPOT A - mineralTankDiam / 2, 0, brineTankHeight, unID: unID=unID+1HOTSPOT A - (mineralTankDiam + brineTankDiam) / 2,  brineTankDiam/2, brineTankHeight, unID: unID=unID+1HOTSPOT A - (mineralTankDiam + brineTankDiam) / 2, -brineTankDiam/2, brineTankHeight, unID: unID=unID+1IF gs_detlevel_3D_m = 2 THEN	! Detailed	ROTy -90	MATERIAL "Shaft Black Plastic"	IF useSysMat THEN MATERIAL sMat	REVOLVE 2, 360, 1+16+32,		0, mineralTankDiam/2 + 0.003, 2,		0.2 * mineralTankHeight, mineralTankDiam/2 + 0.003, 1	MATERIAL MainBoxMaterial	IF mineralTankHeight - mineralTankDiam/2 > 0.0003+EPS THEN		REVOLVE 3, 360, 1+2+16+32,			0.003, mineralTankDiam/2, 2,			mineralTankHeight - mineralTankDiam/2, mineralTankDiam/2, 1,			mineralTankHeight, actD/2, 1002	ELSE		REVOLVE 3, 360, 1+2+16+32,			0.003, mineralTankDiam/2, 1,			1, 0, 801,			mineralTankHeight, actD/2, 1002	ENDIF	DEL 1	ADDx A - (mineralTankDiam + brineTankDiam) / 2	ROTy -90	TOLER 0.01	REVOLVE 5, 360, 1+2+16+32,		0, 0.9*brineTankDiam/2, 2,		0.02, 0.9*brineTankDiam/2, 2,		0.02, brineTankDiam/2, 2,		brineTankHeight - dR, brineTankDiam/2, 1,		brineTankHeight, brineTankDiam/2 - dR, 1002	DEL 2	MATERIAL operatorMaterial	TOLER 0.05	ADDz mineralTankHeight	CYLIND 0.01, actD/2	ADDz 0.01	ROTx 90	ADDz -valveDepth/2	PRISM_ 9, valveDepth,		-valveWidth/2, dR,				79,		-valveWidth/2, valveHeight-dR,	79,		-valveWidth/2+dR, valveHeight,	1079,		 valveWidth/2-dR, valveHeight,	79,		 valveWidth/2, valveHeight-dR,	1079,		 valveWidth/2, dR,				79,		 valveWidth/2-dR, 0,			1079,		-valveWidth/2+dR, 0,			79,		-valveWidth/2, dR,				1079	DEL 4	MATERIAL MainBoxMaterial	R = 0.02	pR = ConWidth_3/2	hX2 = (A - mineralTankDiam - brineTankDiam) / 2	hX1 = hX2 + (mineralTankDiam - valveWidth) / 2	cL = mineralTankHeight - brineTankHeight + valveHeight	ADD valveWidth/2, 0, ZZYZX - valveHeight/2	REVOLVE 2, 360, 1+2,		0,	 pR, 1,		hX1 - R, pR, 1	ROTy 90	ROTz 90	ADD 0, -R, hX1 - R	REVOLVE 2, 90, 1+2,		0,  R,    901,		pR, 360, 4001	ROTz -90	ADDz R	REVOLVE 2, 360, 1+2,		0,	 pR, 1,		cL, pR, 1	ADDx cL	ROTy 90	ROTz -90	ADDy -R	REVOLVE 2, 90, 1+2,		0,  R,    901,		pR, 360, 4001	ADDz R	ROTz -90	REVOLVE 2, 360, 1+2,		0,	 pR, 1,		hX2-R, pR, 1	DEL 12ENDIFIF gs_detlevel_3D_m = 1 THEN	! Simple	ROTy -90	IF mineralTankHeight - mineralTankDiam/2 > 0.0003+EPS THEN		REVOLVE 3, 360, 1+2+16+32,			0, mineralTankDiam/2, 2,			mineralTankHeight - mineralTankDiam/2, mineralTankDiam/2, 1,			mineralTankHeight, actD/2, 1002	ELSE		REVOLVE 3, 360, 1+2+16+32,			0.003, mineralTankDiam/2, 1,			1, 0, 801,			mineralTankHeight, actD/2, 1002	ENDIF	DEL 1	ADDx A - (mineralTankDiam + brineTankDiam) / 2	PRISM_ 2, brineTankHeight,		0,0, 979,		brineTankDiam/2,360,4079	DEL 1	MATERIAL operatorMaterial	ADDz mineralTankHeight + 0.01	ROTx 90	ADDz -valveDepth/2	PRISM 4, valveDepth,		-valveWidth/2, -0.01,		-valveWidth/2, valveHeight,		 valveWidth/2, valveHeight,		 valveWidth/2, -0.01	DEL 3	MATERIAL MainBoxMaterial	R = 0.02	pR = ConWidth_3/2	hX2 = (A - mineralTankDiam - brineTankDiam) / 2	hX1 = hX2 + (mineralTankDiam - valveWidth) / 2	cL = mineralTankHeight - brineTankHeight + valveHeight	ADD valveWidth/2, 0, ZZYZX - valveHeight/2	REVOLVE 2, 360, 1+2,		0,	 pR, 1,		hX1 - R, pR, 1	ROTy 90	ROTz 90	ADD 0, -R, hX1 - R	REVOLVE 2, 90, 1+2,		0,  R,    901,		pR, 360, 4001	ROTz -90	ADDz R	REVOLVE 2, 360, 1+2,		0,	 pR, 1,		cL, pR, 1	ADDx cL	ROTy 90	ROTz -90	ADDy -R	REVOLVE 2, 90, 1+2,		0,  R,    901,		pR, 360, 4001	ADDz R	ROTz -90	REVOLVE 2, 360, 1+2,		0,	 pR, 1,		hX2-R, pR, 1	DEL 12ENDIFDEL TOPRESOL res				! --- CONNECTIONS --- !CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	ui_current_con = ui_current_con,	MEP_NumberConnections = MEP_NumberConnections,	MEP_NumConnectionData = gs_Connections,	gs_AddConnections = gs_AddConnections,	cShow3D = 1,	gs_ConMat = MainBoxMaterial,	MEP_InsShow = 0,	MEP_cline_show_3D = 0,	gs_cont_pen = gs_cont_pen,	ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 12,	ConPosX_2=ConPosX_2, ConPosY_2=ConPosY_2, ConPosZ_2=ConPosZ_2, ConLength_2=ConLength_2, ConEdit_2 = 12,	ConPosX_3=ConPosX_3, ConPosY_3=ConPosY_3, ConPosZ_3=ConPosZ_3, ConLength_3=ConLength_3, ConEdit_3 = 14,	ConPosX_4=ConPosX_4, ConPosY_4=ConPosY_4, ConPosZ_4=ConPosZ_4, ConLength_4=ConLength_4, ConEdit_4 = 14, MEP_StrConnectionData = MEP_StrConnectionDataEND