!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_el! Rack-Mounted Water FilterIF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFIF gs_detlevel_3D_m = 0 THEN ENDres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! SimpleIF useSysMat THEN MainBoxMaterial = sMatMATERIAL MainBoxMaterialPEN gs_cont_penRESOL res				! --- HOTSPOTS --- !unID = 1HOTSPOT -dTWidth/2, dTDepth/2, 0, unID: unID=unID+1HOTSPOT -dTWidth/2, -dTDepth/2, 0, unID: unID=unID+1HOTSPOT dTWidth/2, -dTDepth/2, 0, unID: unID=unID+1HOTSPOT dTWidth/2, dTDepth/2, 0, unID: unID=unID+1HOTSPOT -dTWidth/2, dTDepth/2, dTHeight, unID: unID=unID+1HOTSPOT -dTWidth/2, -dTDepth/2, dTHeight, unID: unID=unID+1HOTSPOT dTWidth/2, -dTDepth/2, dTHeight, unID: unID=unID+1HOTSPOT dTWidth/2, dTDepth/2, dTHeight, unID: unID=unID+1				! --- MAIN BOX --- !ROTx 90ADDz -dTDepth/2PRISM_ 16, dTDepth,	-dTWidth/2, 0, 15,	-dTWidth/2, frameThick, 15,	-dTWidth/2+frameThick, frameThick, 15,	-dTWidth/2+frameThick, dTHeight, 15,	dTWidth/2-frameThick, dTHeight, 15,	dTWidth/2-frameThick, frameThick, 15,	dTWidth/2, frameThick, 15,	dTWidth/2, 0, 15,	dTWidth/2-legThick, 0, 15,	dTWidth/2-legThick, frameThick, 15,	dTWidth/2-frameThick*2, frameThick, 15,	dTWidth/2-frameThick*2, dTHeight-frameThick, 15,	-dTWidth/2+frameThick*2, dTHeight-frameThick, 15,	-dTWidth/2+frameThick*2, frameThick, 15,	-dTWidth/2+legThick, frameThick, 15,	-dTWidth/2+legThick, 0, 15PRISM_ 4, 0.35*DRate,	-dTWidth/2+frameThick*2, frameThick+0.305*HRate, 15,	-dTWidth/2+frameThick*2, frameThick*2+0.305*HRate, 15,	dTWidth/2-frameThick*2, frameThick*2+0.305*HRate, 15,	dTWidth/2-frameThick*2, frameThick+0.305*HRate, 15DEL 2IF gs_detlevel_3D_m = 2 THEN	FOR i=1 TO 4		ADD -dTWidth/2+frameThick*2+cRad+0.055*WRate+(cRad*2+0.01*WRate)*(i-1), -0.35*DRate+dTDepth/2+cRad+0.025, frameThick*2+0.305*HRate			CYLIND dTHeight-frameThick*3-0.385*HRate, cRad		DEL 1	NEXT i	ADD 0, -0.35*DRate+dTDepth/2+cRad+0.025, dTHeight-frameThick-0.08*HRate	PRISM_ 4, 0.08*HRate,		-dTWidth/2+frameThick*2, -cRad-0.01*DRate, 15,		-dTWidth/2+frameThick*2, cRad+0.01*DRate, 15,		dTWidth/2-frameThick*2, cRad+0.01*DRate, 15,		dTWidth/2-frameThick*2, -cRad-0.01*DRate, 15	DEL 1ELSE	ADD 0, -0.35*DRate+dTDepth/2+cRad+0.025, frameThick*2+0.305*HRate	PRISM_ 4, dTHeight-frameThick*3-0.305*HRate,		-dTWidth/2+frameThick*2, -cRad-0.01*DRate, 15,		-dTWidth/2+frameThick*2, cRad+0.01*DRate, 15,		dTWidth/2-frameThick*2, cRad+0.01*DRate, 15,		dTWidth/2-frameThick*2, -cRad-0.01*DRate, 15	DEL 1ENDIFADD -dTWidth/2+0.12+frameThick, -dTDepth/2+0.02, dTHeight	BLOCK 0.15, 0.30, 0.065ADDx 0.20	BLOCK 0.15, 0.30, 0.065DEL 2				! --- CONNECTIONS --- !CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	ui_current_con = ui_current_con,	MEP_NumberConnections = MEP_NumberConnections,	MEP_NumConnectionData = gs_Connections,	gs_AddConnections = gs_AddConnections,	cShow3D = 1,	gs_ConMat = MainBoxMaterial,	MEP_InsShow = 0,	MEP_cline_show_3D = 0,	gs_cont_pen = gs_cont_pen,	ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 1,	ConPosX_2=ConPosX_2, ConPosY_2=ConPosY_2, ConPosZ_2=ConPosZ_2, ConLength_2=ConLength_2, ConEdit_2 = 1,	ConPosX_3=ConPosX_3, ConPosY_3=ConPosY_3, ConPosZ_3=ConPosZ_3, ConLength_3=ConLength_3, ConEdit_3 = 1,	ConPosX_4=ConPosX_4, ConPosY_4=ConPosY_4, ConPosZ_4=ConPosZ_4, ConLength_4=ConLength_4, ConEdit_4 = 1,	ConPosX_5=ConPosX_5, ConPosY_5=ConPosY_5, ConPosZ_5=ConPosZ_5, ConLength_5=ConLength_5, ConEdit_5 = 1, MEP_StrConnectionData = MEP_StrConnectionDataEND