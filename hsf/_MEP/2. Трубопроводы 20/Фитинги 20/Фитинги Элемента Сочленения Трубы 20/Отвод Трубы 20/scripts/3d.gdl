!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_elIF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFIF gs_detlevel_3D_m = 0 THEN ENDres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! Simplegs_PipeMat = sMatMATERIAL gs_PipeMatpipe_show = pipe_show*not(show_axis) PEN gs_cont_penRESOL resgs_PipeMat = sMatDIM MEP_InclinedPartPosition[][]MEP_InclinedPartPosition[1][1] = 0MEP_InclinedPartPosition[1][2] = 0MEP_InclinedPartPosition[1][3] = 0MEP_InclinedPartPosition[2][1] = 1MEP_InclinedPartPosition[2][2] = 0MEP_InclinedPartPosition[2][3] = 0MEP_InclinedPartPosition[3][1] = 0MEP_InclinedPartPosition[3][2] = 1MEP_InclinedPartPosition[3][3] = 0unID = 1HOTSPOT 0, 0, 0, unID: unID=unID+1IF ABS(MEP_NumConnectionData[1][18]) < EPS AND ABS(MEP_NumConnectionData[2][18] - 1) < EPS THEN		! ****** rotation ****** !	XFORM   defaultMatrix[1][1], defaultMatrix[2][1], defaultMatrix[3][1], MEP_Position[1][1],			defaultMatrix[1][2], defaultMatrix[2][2], defaultMatrix[3][2], MEP_Position[1][2],			defaultMatrix[1][3], defaultMatrix[2][3], defaultMatrix[3][3], MEP_Position[1][3]	dist_y = ConPosY_1+ConLength_1/2	HOTSPOT 0, 0, 0, unID, Rotation, 6: unID=unID+1	HOTSPOT 0, dist_y, 0, unID, Rotation, 4+128: unID=unID+1	HOTSPOT 0, dist_y*COS(Rotation), dist_y*SIN(Rotation), unID, Rotation, 5: unID=unID+1	HOTSPOT 1, 0, 0, unID, Rotation, 7: unID=unID+1	DEL 1ENDIFXFORM   MEP_Position[2][1], MEP_Position[3][1], dZx, MEP_Position[1][1],		MEP_Position[2][2], MEP_Position[3][2], dZy, MEP_Position[1][2],		MEP_Position[2][3], MEP_Position[3][3], dZz, MEP_Position[1][3]IF pipe_show and not(show_axis)  THEN				! -----> pipe+connection <----- !	ROTy 90!!ADDZ -0.5!!PRISM_ 2, 1,!!	0, 0, 915,!!	DuctWidth/2+DuctWallThickness, 360, 4015!!DEL 1	CUTPOLYA 2, 2, 0,		0, 0, 913,		PipeOuterDiameter/2, 360, 4013	DEL 1	IF ABS(gs_Connections[1][12] - gs_Connections[1][15]) < EPS THEN gs_Connections[1][15] = gs_Connections[1][15] + EPS/10	CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,			MEP_NumberConnections = MEP_NumberConnections,			MEP_NumConnectionData = gs_Connections,			gs_AddConnections = gs_AddConnections,			cShow3D = 1,			gs_ConMat = gs_PipeMat,			MEP_InsShow = 0,			MEP_cline_show_3D = 0,			gs_cont_pen = gs_cont_pen,			ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 1, MEP_StrConnectionData = MEP_StrConnectionData	CUTENDENDIFIF (MEP_cline_show and show_axis_and) or show_axis THEN				! -----> center line <----- !	CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,			MEP_NumberConnections = MEP_NumberConnections,			MEP_NumConnectionData = gs_Connections,			gs_AddConnections = gs_AddConnections,			cShow3D = 0,			gs_ConMat = gs_PipeMat,			MEP_InsShow = 0,			MEP_cline_show_3D = 1,			gs_cont_pen = gs_cont_pen,			ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 0, MEP_StrConnectionData = MEP_StrConnectionDataENDIFIF MEP_InsShow and show_isol and not(show_axis) THEN				! -----> insulation <----- !	ROTy 90	CUTPOLYA 2, 2, 0,		0, 0, 913,		PipeOuterDiameter/2+MEP_InsThickness, 360, 4013	DEL 1	CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,			MEP_NumberConnections = MEP_NumberConnections,			MEP_NumConnectionData = gs_Connections,			gs_AddConnections = gs_AddConnections,			cShow3D = 0,			MEP_InsShow = 1,			MEP_InsThickness = MEP_InsThickness,			gs_InsMat = gs_InsMat,			MEP_cline_show_3D = 0,			gs_cont_pen = gs_cont_pen,			ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 1, MEP_StrConnectionData = MEP_StrConnectionData	CUTENDENDIFDEL 1