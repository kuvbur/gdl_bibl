! Cable Tray! Zsolt Feke/2008IF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFIF gs_detlevel_3D_m = 0 THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL returned_parameters gs_cont_pen, sMat, gs_InsMatIF useSysMat AND MEP_StrConnectionData[1][2] <> "0" THEN gs_CableTrayMat = sMatPEN gs_cont_penMATERIAL gs_CableTrayMatXFORM   MEP_Position[2][1], MEP_Position[3][1], dZx, MEP_Position[1][1],		MEP_Position[2][2], MEP_Position[3][2], dZy, MEP_Position[1][2],		MEP_Position[2][3], MEP_Position[3][3], dZz, MEP_Position[1][3]IF GLOB_CONTEXT > 40 AND GLOB_CONTEXT < 50 THEN	ROTy 90	PRISM_ 5, MEP_StraightLength,		cableTrayHalfHeight, cableTrayHalfWidth, 15,		-cableTrayHalfHeight, cableTrayHalfWidth, 15,		-cableTrayHalfHeight, -cableTrayHalfWidth, 15,		cableTrayHalfHeight, -cableTrayHalfWidth, 15,		cableTrayHalfHeight, cableTrayHalfWidth, -1	DEL 1ELSE	IF connectedA + connectedB < 1 + EPS THEN		HOTSPOT 0, 0, 0, hceID_1, MEP_StraightLength, 1+256*(connectedA < EPS)		HOTSPOT -1, 0, 0, HptsUnIDBeginForConnEnds, MEP_StraightLength, 3		IF ABS(connectedA) < EPS AND ABS(connectedB - 1) < EPS THEN			hID = hceID_2+1			ROTy 90			HOTSPOT -MEP_offset_z, MEP_offset_y, MEP_StraightLength, hceID_2			DEL 1		ELSE			hID = hceID_2		ENDIF		HOTSPOT MEP_StraightLength, 0, 0, hID, MEP_StraightLength, 2+128*connectedB	ELSE		ROTy 90		HOTSPOT 0, 0, 0, hceID_1		HOTSPOT 0, 0, MEP_StraightLength, hceID_2		DEL 1	ENDIF	ADD 0, -cableTrayHalfWidth, -cableTrayHalfHeight	IF MEP_CoveredCableTray OR ABS(MEP_EmptySideID - 3) > EPS THEN RECT MEP_StraightLength, cableTrayWidth	IF MEP_CoveredCableTray OR ABS(MEP_EmptySideID - 1) > EPS THEN		ADDz cableTrayHeight		RECT MEP_StraightLength, cableTrayWidth		DEL 1	ENDIF	ROTx 90	IF MEP_CoveredCableTray OR ABS(MEP_EmptySideID - 4) > EPS THEN RECT MEP_StraightLength, cableTrayHeight	ADDz -cableTrayWidth	IF MEP_CoveredCableTray OR ABS(MEP_EmptySideID - 2) > EPS THEN RECT MEP_StraightLength, cableTrayHeight	DEL 3	BODY -1	LIN_ 0, 0, 0, MEP_StraightLength, 0, 0	HOTLINE 0, 0, 0, MEP_StraightLength, 0, 0, 0ENDIFDEL TOP