! Electriccall "FM_types_MEP" parametersDIM shape_typ_to_validation[3]	shape_typ_to_validation[1] = 1		! Rectangle	shape_typ_to_validation[2] = 0		! Circle	shape_typ_to_validation[3] = 0		! OvalIF GLOB_MODPAR_NAME = "gs_detlevel_3D" OR gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1	PARAMETERS gs_detlevel_3D_m = gs_detlevel_3D_mELSE	IF gs_detlevel_3D_m = 0 THEN gs_detlevel_3D = `Откл.`	IF gs_detlevel_3D_m = 1 THEN gs_detlevel_3D = `Простой`	IF gs_detlevel_3D_m = 2 THEN gs_detlevel_3D = `Детальный`	PARAMETERS gs_detlevel_3D = gs_detlevel_3DENDIFVALUES "gs_detlevel_3D" `Детальный`,`Простой`,`Откл.`VALUES "gs_detlevel_3D_m" 2, 1, 0					! --- MAIN PARAMETERS --- !IF NOT(SetProgram) THEN	IF A < cylindDistX+cylindDia*2 THEN A = cylindDistX+cylindDia*2	VALUES "A" RANGE [cylindDistX+cylindDia*2, ]	IF B < cylindDistY+cylindDia THEN B = cylindDistY+cylindDia	VALUES "B" RANGE [cylindDistY+cylindDia, ]	VALUES "ZZYZX" RANGE (cylindHeight, ]ENDIF					! --- CONNECTIONS --- !PARAMETERS MEP_NumberConnections = 2					! --- 1st CONNECTION --- !ConID = 1ConName = `Впускное Соединение Воздуховода`ConDVecX = 0ConDVecY = 0ConDVecZ = -1ConWVecX = -1ConWVecY = 0ConWVecZ = 0IF NOT(SetProgram) THEN	VALUES "ConWidth_1" RANGE (0, A/2-ConWallThickness_1*2]	VALUES "ConDepth_1" RANGE (0, B-0.30-ConWallThickness_1*2]	VALUES "ConLength_1" RANGE [0, ]	IF ConPosX_1 < -A/2+ConWidth_1/2+ConWallThickness_1 THEN ConPosX_1 = -A/2+ConWidth_1/2+ConWallThickness_1	IF ConPosX_1 > A/2-ConWidth_1/2-ConWallThickness_1 THEN ConPosX_1 = A/2-ConWidth_1/2-ConWallThickness_1	VALUES "ConPosX_1" RANGE [-A/2+ConWidth_1/2+ConWallThickness_1, A/2-ConWidth_1/2-ConWallThickness_1]	IF ConPosY_1 < -B/2+ConDepth_1/2+ConWallThickness_1 THEN ConPosY_1 = -B/2+ConDepth_1/2+ConWallThickness_1	IF ConPosY_1 > B/2-ConDepth_1/2-ConWallThickness_1 THEN ConPosY_1 = B/2-ConDepth_1/2-ConWallThickness_1	VALUES "ConPosY_1" RANGE [-B/2+ConDepth_1/2+ConWallThickness_1, B/2-ConDepth_1/2-ConWallThickness_1]	PARAMETERS ConPosZ_1 = 0	VALUES "ConWallThickness_1" RANGE [0, MIN((A/2-ConWidth_1)/2, (B-0.30-ConDepth_1)/2)]ENDIFCALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	gs_Connections = gs_Connections,	MEP_NumConnectionData = MEP_NumConnectionData,	MEP_StrConnectionData = MEP_StrConnectionData,	MEP_NumberConnections = MEP_NumberConnections,	gs_AddConnections = gs_AddConnections,	ConName = ConName,	ConID = ConID,	ConPosX = ConPosX_1,	ConPosY = ConPosY_1,	ConPosZ = ConPosZ_1,	ConDVecX = ConDVecX,	ConDVecY = ConDVecY,	ConDVecZ = ConDVecZ,	ConWVecX = ConWVecX,	ConWVecY = ConWVecY,	ConWVecZ = ConWVecZ,	ConNominalWidth = ConNominalWidth_1,	ConNominalDepth = ConNominalDepth_1,	ConWidth = ConWidth_1,	ConDepth = ConDepth_1,	ConLength = ConLength_1,	ConWallThickness = ConWallThickness_1,	ConConnectorWidth = ConConnectorWidth_1,	ConConnectorDepth = ConConnectorDepth_1,	ConConnectorWidth2 = ConConnectorWidth2_1,	ConToolType = ConToolType_1,	ConStatus = ConStatus_1,	ConType_1 = ConType_1,	ConConnectorType_1 = ConConnectorType_1,	ConSystem_1 = ConSystem_1,	ui_page_idx = ui_page_idx,	ui_current_con = ui_current_con,	shape_typ_to_validation = shape_typ_to_validation,	MEP_enabled_geometry_mod = 1,	bUIConnectionStatusCheckbox = 1PARAMETERS MEP_ConInfo[ConID][1] = STR(ConToolType_1, 1, 0)PARAMETERS MEP_ConInfo[ConID][2] = ConNamePARAMETERS MEP_ConInfo[ConID][3] = "0"					! --- 2nd CONNECTION --- !ConID = 2ConName = `Выпускное Соединение Воздуховода`ConDVecX = 0ConDVecY = 0ConDVecZ = -1ConWVecX = -1ConWVecY = 0ConWVecZ = 0IF NOT(SetProgram) THEN	VALUES "ConWidth_2" RANGE (0, A/2-ConWallThickness_2*2]	VALUES "ConDepth_2" RANGE (0, B-0.30-ConWallThickness_2*2]	VALUES "ConLength_2" RANGE [0, ]	IF ConPosX_2 < -A/2+ConWidth_2/2+ConWallThickness_2 THEN ConPosX_2 = -A/2+ConWidth_2/2+ConWallThickness_2	IF ConPosX_2 > A/2-ConWidth_2/2-ConWallThickness_2 THEN ConPosX_2 = A/2-ConWidth_2/2-ConWallThickness_2	VALUES "ConPosX_2" RANGE [-A/2+ConWidth_2/2+ConWallThickness_2, A/2-ConWidth_2/2-ConWallThickness_2]	IF ConPosY_2 < -B/2+ConDepth_2/2+ConWallThickness_2 THEN ConPosY_2 = -B/2+ConDepth_2/2+ConWallThickness_2	IF ConPosY_2 > B/2-ConDepth_2/2-ConWallThickness_2 THEN ConPosY_2 = B/2-ConDepth_2/2-ConWallThickness_2*2	VALUES "ConPosY_2" RANGE [-B/2+ConDepth_2/2+ConWallThickness_2, B/2-ConDepth_2/2-ConWallThickness_2]	PARAMETERS ConPosZ_2 = 0	VALUES "ConWallThickness_2" RANGE [0, MIN((A/2-ConWidth_2)/2, (B-0.30-ConDepth_2)/2)]ENDIFCALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	gs_Connections = gs_Connections,	MEP_NumConnectionData = MEP_NumConnectionData,	MEP_StrConnectionData = MEP_StrConnectionData,	MEP_NumberConnections = MEP_NumberConnections,	gs_AddConnections = gs_AddConnections,	ConName = ConName,	ConID = ConID,	ConPosX = ConPosX_2,	ConPosY = ConPosY_2,	ConPosZ = ConPosZ_2,	ConDVecX = ConDVecX,	ConDVecY = ConDVecY,	ConDVecZ = ConDVecZ,	ConWVecX = ConWVecX,	ConWVecY = ConWVecY,	ConWVecZ = ConWVecZ,	ConNominalWidth = ConNominalWidth_2,	ConNominalDepth = ConNominalDepth_2,	ConWidth = ConWidth_2,	ConDepth = ConDepth_2,	ConLength = ConLength_2,	ConWallThickness = ConWallThickness_2,	ConConnectorWidth = ConConnectorWidth_2,	ConConnectorDepth = ConConnectorDepth_2,	ConConnectorWidth2 = ConConnectorWidth2_2,	ConToolType = ConToolType_2,	ConStatus = ConStatus_2,	ConType_2 = ConType_2,	ConConnectorType_2 = ConConnectorType_2,	ConSystem_2 = ConSystem_2,	shape_typ_to_validation = shape_typ_to_validation,	MEP_enabled_geometry_mod = 1,	bUIConnectionStatusCheckbox = 1PARAMETERS MEP_ConInfo[ConID][1] = STR(ConToolType_2, 1, 0)PARAMETERS MEP_ConInfo[ConID][2] = ConNamePARAMETERS MEP_ConInfo[ConID][3] = "0"IF setProgram THEN	SetProgram = 0	PARAMETERS SetProgram = SetProgramENDIFvalues "FM_HeatOutput_unit" `З`, `БТЕ/час`values "FM_RefrigeratingOutput_unit" `З`, `БТЕ/час`if FM_HeatOutput_unit = `БТЕ/час` then	hideparameter "FM_HeatOutput"else	hideparameter "FM_HeatOutput_btu"endifif FM_RefrigeratingOutput_unit = `БТЕ/час` then	hideparameter "FM_RefrigeratingOutput"else	hideparameter "FM_RefrigeratingOutput_btu"endif!1000 W = 3414,426 Btu/hourif GLOB_MODPAR_NAME = "FM_HeatOutput_btu" then	FM_HeatOutput = FM_HeatOutput_btu / 3.414426	parameters FM_HeatOutput = FM_HeatOutputelse	FM_HeatOutput_btu = FM_HeatOutput * 3.414426	parameters FM_HeatOutput_btu = FM_HeatOutput_btuendif!1000 W = 3414,426 Btu/hourif GLOB_MODPAR_NAME = "FM_RefrigeratingOutput_btu" then	FM_RefrigeratingOutput = FM_RefrigeratingOutput_btu / 3.414426	parameters FM_RefrigeratingOutput = FM_RefrigeratingOutputelse	FM_RefrigeratingOutput_btu = FM_RefrigeratingOutput * 3.414426	parameters FM_RefrigeratingOutput_btu = FM_RefrigeratingOutput_btuendif