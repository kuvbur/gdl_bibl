!------------------- 2д начало -------------------------if show_el < EPS then endIF gs_detlevel_2D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, gs_fill_type, gs_fill_pen, gs_back_pen, MEP_cline_type, MEP_cline_pen, show_elPEN gs_cont_penSET FILL gs_fill_type!-------------------- 2д ко! Horizontal Expansion TankPEN gs_cont_penSET FILL gs_fill_typeunID = 1HOTSPOT2 0, 0, unID: unID=unID+1DrawingNum = 1IF DrawingNum = 1 THEN	! Top				! --- HOTSPOTS --- !	HOTSPOT2 -dTWidth/2+endRed, -dTDepth/2, unID: unID=unID+1	HOTSPOT2 -dTWidth/2+endRed, dTDepth/2, unID: unID=unID+1	HOTSPOT2 dTWidth/2-endRed, -dTDepth/2, unID: unID=unID+1	HOTSPOT2 dTWidth/2-endRed, dTDepth/2, unID: unID=unID+1	HOTSPOT2 -dTWidth/2, 0, unID: unID=unID+1	HOTSPOT2 dTWidth/2, 0, unID: unID=unID+1				! --- Tank --- !	 rr = (dTDepth/2)/SIN(180-2*ATN((dTDepth/2)/endRed))	PUT dTWidth/2-endRed, -dTDepth/2, 1,		dTWidth/2-rr, 0, 901,		dTWidth/2-endRed, dTDepth/2, 3001,		-dTWidth/2+endRed, dTDepth/2, 1,		-dTWidth/2+rr, 0, 901,		-dTWidth/2+endRed, -dTDepth/2, 3001	POLY2_B NSP/3, 1+2+4+64, gs_fill_pen, gs_back_pen,		GET (NSP)	LINE2 dTWidth/2-endRed, -dTDepth/2, dTWidth/2-endRed, dTDepth/2	LINE2 -dTWidth/2+endRed, -dTDepth/2, -dTWidth/2+endRed, dTDepth/2				! --- SUPPORT --- !	ADD2 -dTWidth/2+endRed+0.15, 0	ROT2 90	CALL "MEP_m_Tank_support_4" PARAMETERS	supLen = 0.10, supHeight = legHeight+dTDepth/4, supThick = 0.02,											footWidth = dTDepth+0.06*dd, footThick = 0.025,											tankDia = dTDepth, tankDeepening = dTDepth/4,											gs_detlevel_3D_m = gs_detlevel_3D_m, supMat = MainBoxMaterial,											gs_cont_pen = gs_cont_pen, gs_fill_pen = gs_fill_pen,											gs_back_pen = gs_back_pen, gs_fill_type = gs_fill_type, hptsID = 100	DEL 2	ADD2 dTWidth/2-endRed-0.15, 0	ROT2 -90	CALL "MEP_m_Tank_support_4" PARAMETERS	supLen = 0.10, supHeight = legHeight+dTDepth/4, supThick = 0.02,											footWidth = dTDepth+0.06*dd, footThick = 0.025,											tankDia = dTDepth, tankDeepening = dTDepth/4,											gs_detlevel_3D_m = gs_detlevel_3D_m, supMat = MainBoxMaterial,											gs_cont_pen = gs_cont_pen, gs_fill_pen = gs_fill_pen,											gs_back_pen = gs_back_pen, gs_fill_type = gs_fill_type, hptsID = 200	DEL 2				! --- CONNECTIONS --- !	IF ConStatus_1 THEN		! --- 1st CONNECTION --- !		PUT ConPosX_1, 0, 1,			0, 1, 801,			ConPosX_1+(rr-SQR(rr^2-(ConWidth_1/2)^2)), ConPosY_1+ConWidth_1/2, 1001,			ConPosX_1, ConPosY_1+ConWidth_1/2, 0		POLY2_B NSP/3, 1+2+4+64, gs_fill_pen, gs_back_pen,			GET (NSP)		PUT ConPosX_1, 0, 1,			0, -1, 801,			ConPosX_1+(rr-SQR(rr^2-(ConWidth_1/2)^2)), ConPosY_1-ConWidth_1/2, 1001,			ConPosX_1, ConPosY_1-ConWidth_1/2, 0		POLY2_B NSP/3, 1+2+4+64, gs_fill_pen, gs_back_pen,			GET (NSP)	ENDIFENDIF				! --- OTHER VIEWS --- !ROT2 -SYMB_ROTANGLE * (DrawingNum > 1 AND NOT(SYMB_MIRRORED))+SYMB_ROTANGLE * (DrawingNum > 1 AND SYMB_MIRRORED)IF DrawingNum = 2 THEN PROJECT2 -3, 90, 2	! BottomIF DrawingNum = 3 THEN PROJECT2 4, 270, 2	! FrontIF DrawingNum = 4 THEN PROJECT2 4, 90, 2	! BackIF DrawingNum = 5 THEN PROJECT2 4, 180*(SYMB_MIRRORED), 2		! RightIF DrawingNum = 6 THEN PROJECT2 4, 180*NOT(SYMB_MIRRORED), 2	! Left				! --- CONNECTIONS --- !CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	ui_current_con = ui_current_con,	MEP_NumberConnections = MEP_NumberConnections,	MEP_NumConnectionData = gs_Connections,	gs_AddConnections = gs_AddConnections,	DrawingNum = DrawingNum,	MEP_InsShow = 0,	MEP_cline_show = 0,	gs_cont_pen = gs_cont_pen,	gs_fill_type=gs_fill_type,	gs_fill_pen=gs_fill_pen,	gs_back_pen=gs_back_pen,	ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 1, ConEdgeVisible_1 = 0, MEP_StrConnectionData = MEP_StrConnectionDataDEL 1END