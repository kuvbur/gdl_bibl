!------------------- 3д начало -------------------------if show_el < EPS then endif GLOB_SCRIPT_TYPE=2 then gs_detlevel_3D_m = gs_detlevel_2D_mIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_elMATERIAL sMatPEN gs_cont_penres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! SimpleRESOL reshotspot 0,0,0				! --- HOTSPOTS --- !unID = 1HOTSPOT 0, 0, 0, unID: unID=unID+1HOTSPOT 0, 0, legHeight+tankHeight, unID: unID=unID+1IF gs_detlevel_3D_m = 2 THEN	HOTSPOT 0, -B/2-roofExt, tankHeight+legHeight, unID: unID=unID+1	HOTSPOT 0, B/2+roofExt, tankHeight+legHeight, unID: unID=unID+1	HOTSPOT -A/2-roofExt, 0, tankHeight+legHeight, unID: unID=unID+1	HOTSPOT A/2+roofExt, 0, tankHeight+legHeight, unID: unID=unID+1	HOTSPOT 0, -B/2, tankHeight+legHeight-0.015, unID: unID=unID+1	HOTSPOT 0, B/2, tankHeight+legHeight-0.015, unID: unID=unID+1	HOTSPOT -A/2, 0, tankHeight+legHeight-0.015, unID: unID=unID+1	HOTSPOT A/2, 0, tankHeight+legHeight-0.015, unID: unID=unID+1	HOTSPOT 0, -B/2, legHeight, unID: unID=unID+1	HOTSPOT 0, B/2, legHeight, unID: unID=unID+1	HOTSPOT -A/2, 0, legHeight, unID: unID=unID+1	HOTSPOT A/2, 0, legHeight, unID: unID=unID+1ELSE	HOTSPOT 0, -B/2, tankHeight+legHeight, unID: unID=unID+1	HOTSPOT 0, B/2, tankHeight+legHeight, unID: unID=unID+1	HOTSPOT -A/2, 0, tankHeight+legHeight, unID: unID=unID+1	HOTSPOT A/2, 0, tankHeight+legHeight, unID: unID=unID+1	HOTSPOT 0, -B/2, ConPosZ_1, unID: unID=unID+1	HOTSPOT 0, B/2, ConPosZ_1, unID: unID=unID+1	HOTSPOT -A/2, 0, ConPosZ_1, unID: unID=unID+1	HOTSPOT A/2, 0, ConPosZ_1, unID: unID=unID+1ENDIF				! --- TANK --- !IF gs_detlevel_3D_m = 2 THEN	 endRed = 0.04	GROUP "cylind"		ADDz legHeight		CYLIND endRed, ConWidth_1/2		DEL 1	ENDGROUP	GROUP "tank"		ADDz legHeight+tankHeight-0.015			CUTPLANE 0: DEL 1		ADDz legHeight		CALL "MEP_m_Tank" PARAMETERS	tankLen = tankHeight+endRed, tankDia = A, endRed = endRed,										ringOn = 0, typ = 2, tankPos = 1, tankMat = MainBoxMaterial,										gs_cont_pen = gs_cont_pen, res = res		DEL 1		CUTEND	ENDGROUP	result_1=ADDGROUP("cylind","tank")	PLACEGROUP result_1	KILLGROUP "cylind"	KILLGROUP "tank"	ADDz legHeight+tankHeight-0.015		CYLIND 0.015, A/2+roofExt	DEL 1ELSE	ADDz legHeight		CYLIND tankHeight, A/2	DEL 1ENDIFCUTPOLYA 2, 2, 0,	0, 0, 913,	A/2, 360, 4013				! --- SUPPORT --- !IF gs_detlevel_3D_m = 2 THEN	RESOL res/2	FOR i = 1 TO 3		ROTz 120*(i-1)		HOTSPOT A/2+0.08, 0, 0, unID: unID=unID+1		ADDz legHeight+0.04		PRISM_ 5, 0.08,			0, 0.04, 15,			A/2+0.04, 0.04, 79,			A/2+0.04, -0.04, 1079,			0, -0.04, 15,			0, 0.04, 15		DEL 1		ADDx A/2+0.04			CYLIND legHeight+0.04, 0.03		DEL 2	NEXT i	RESOL resELSE	FOR i = 1 TO 3		ROTz 120*(i-1)		HOTSPOT A/2+0.08, 0, 0, unID: unID=unID+1		ADDz legHeight		PRISM_ 5, 0.12,			0, 0.04, 15,			A/2+0.08, 0.04, 15,			A/2+0.08, -0.04, 15,			0, -0.04, 15,			0, 0.04, 15		DEL 1		ADDx A/2+0.04			CYLIND legHeight, 0.03		DEL 2	NEXT iENDIFCUTEND				! --- CONNECTIONS --- !CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	ui_current_con = ui_current_con,	MEP_NumberConnections = MEP_NumberConnections,	MEP_NumConnectionData = gs_Connections,	gs_AddConnections = gs_AddConnections,	cShow3D = 1,	gs_ConMat = MainBoxMaterial,	MEP_InsShow = 0,	MEP_cline_show_3D = 0,	gs_cont_pen = gs_cont_pen,	ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 14END