if show_el<EPS then endIF gs_detlevel_2D_m = 0 THEN ENDPEN gs_cont_penSET FILL gs_fill_typeunID = 1DrawingNum = 1IF DrawingNum = 1 THEN	! Top				! --- SERVICE SINK --- !	HOTSPOT2 -A/2, 0, unID: unID=unID+1	HOTSPOT2  A/2, 0, unID: unID=unID+1	HOTSPOT2 -A/2, -B, unID: unID=unID+1	HOTSPOT2  A/2, -B, unID: unID=unID+1	IF GLOB_CONTEXT <> 22 THEN		POLY2_B 6, 1+2+4+64, gs_fill_pen, gs_back_pen,			-A/2,0, 1,			-A/2,-B+edgeThick+R, 1,			-A/2+edgeThick+R,-B, 1001,			 A/2-edgeThick-R,-B, 1,			 A/2,-B+edgeThick+R, 1001,			 A/2,0, 1		POLY2_B 6, 1+4, gs_fill_pen, gs_back_pen,			-A/2+edgeThick,0, 1,			-A/2+edgeThick,-B+edgeThick+R, 1,			-A/2+edgeThick+R,-B+edgeThick, 1001,			 A/2-edgeThick-R,-B+edgeThick, 1,			 A/2-edgeThick,-B+edgeThick+R, 1001,			 A/2-edgeThick,0, 1		LINE2 A/2-edgeThick,-backThick,			 -A/2+edgeThick,-backThick		CIRCLE2 ConPosX_3,ConPosY_3, ConWidth_3/2	ENDIFENDIFDEL TOP				! --- OTHER VIEWS --- !ROT2 (SYMB_MIRRORED - NOT(SYMB_MIRRORED)) * (DrawingNum > 1) * SYMB_ROTANGLEIF DrawingNum = 2 THEN PROJECT2 -3, 90, 2	! BottomIF DrawingNum = 3 THEN PROJECT2 4, 270, 2	! FrontIF DrawingNum = 4 THEN PROJECT2 4, 90, 2	! BackIF DrawingNum = 5 THEN PROJECT2 4, 180 * SYMB_MIRRORED,		 2	! RightIF DrawingNum = 6 THEN PROJECT2 4, 180 * NOT(SYMB_MIRRORED), 2	! Left				! --- CONNECTIONS --- !CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	ui_current_con = ui_current_con,	MEP_NumberConnections = MEP_NumberConnections,	MEP_NumConnectionData = gs_Connections,	gs_AddConnections = gs_AddConnections,	DrawingNum = DrawingNum,	MEP_InsShow = 0,	MEP_cline_show = 0,	gs_cont_pen = gs_cont_pen,	gs_fill_type=gs_fill_type,	gs_fill_pen=gs_fill_pen,	gs_back_pen=gs_back_pen,	ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConLength_1=ConLength_1, ConEdit_1 = 12,	ConPosX_2=ConPosX_2, ConPosY_2=ConPosY_2, ConLength_2=ConLength_2, ConEdit_2 = 12,	ConPosX_3=ConPosX_3, ConPosY_3=ConPosY_3, ConLength_3=ConLength_3, ConEdit_3 = 5DEL 1END