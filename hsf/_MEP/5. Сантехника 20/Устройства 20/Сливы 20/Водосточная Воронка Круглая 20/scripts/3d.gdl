!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_el! Round Roof DrainIF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFIF gs_detlevel_3D_m = 0 THEN ENDres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! SimpleMainBoxMaterial = sMatMATERIAL MainBoxMaterialPEN gs_cont_penRESOL resaddStatus = 0IF GLOB_CONTEXT > 40 AND GLOB_CONTEXT < 50 THEN	addStatus = 1ENDIF				! --- TRANSFORMATION --- !XFORM   MEP_Position[2][1], MEP_Position[3][1], dZx, MEP_Position[1][1],		MEP_Position[2][2], MEP_Position[3][2], dZy, MEP_Position[1][2],		MEP_Position[2][3], MEP_Position[3][3], dZz, MEP_Position[1][3]unID = 1HOTSPOT 0, 0, ZZYZX, unID: unID=unID+1HOTSPOT  0,  A/2,	0, unID: unID=unID+1HOTSPOT  0, -A/2,	0, unID: unID=unID+1HOTSPOT  A/2,	0,	0, unID: unID=unID+1HOTSPOT -A/2,	0,	0, unID: unID=unID+1IF gs_detlevel_3D_m = 2 THEN	! Detailed	POLY_ 3,		0,0, 901,		A/2,360,4001,		ConWidth_1/2,360,4001	ROTy -90	h1 = 0.05 * ZZYZX	h2 = 0.85 * ZZYZX	t = 0.08 * d2	FOR i=1 TO 3		CUTPOLY 4,			h1, t,			h1,-t,			h2,-t,			h2, t		ROTx 60	NEXT i	DEL 3	REVOLVE 2,360, addStatus+2+16+32,		0, d1/2, 2,		ZZYZX, d2/2, 2	CUTEND	CUTEND	CUTEND	DEL 1ENDIFIF gs_detlevel_3D_m = 1 THEN	! Simple	ROTy -90	REVOLVE 3,360, addStatus+2+16+32,		0, A/2, 2,		0, d1/2, 2,		ZZYZX, d2/2, 2	DEL 1ENDIF				! --- CONNECTIONS --- !CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,		ui_current_con = ui_current_con,		MEP_NumberConnections = MEP_NumberConnections,		MEP_NumConnectionData = gs_Connections,		gs_AddConnections = gs_AddConnections,		cShow3D = 1,		gs_ConMat = MainBoxMaterial,		MEP_InsShow = 0,		MEP_cline_show_3D = 0,		gs_cont_pen = gs_cont_pen,		ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 1, MEP_StrConnectionData = MEP_StrConnectionDataDEL TOPEND