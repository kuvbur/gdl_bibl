!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_el! Horiz. Disch. Sewage PumpIF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFIF gs_detlevel_3D_m = 0 THEN ENDres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! SimpleMainBoxMaterial = sMatMATERIAL MainBoxMaterialPEN gs_cont_penRESOL res				! --- SEWAGE PUMP --- !unID = 1HOTSPOT 0, 0, 0, unID: unID=unID+1HOTSPOT B/2, 0, 0, unID: unID=unID+1HOTSPOT 0, B/2, 0, unID: unID=unID+1HOTSPOT 0, -B/2, 0, unID: unID=unID+1HOTSPOT 0, 0, ZZYZX, unID: unID=unID+1footH = ConPosZ_1-bottomThk/2footTck = 0.05 * BIF gs_detlevel_3D_m = 2 THEN	! Detailed	RESOL 16	GROUP "pumpBody"		ADDz ConPosZ_1		ROTy -90		REVOLVE 3,360,1+2+16+32,			-bottomThk/2, (B-bottomThk)/2, 1,			0,1, 801,			bottomThk/2, (B-bottomThk)/2, 1001		DEL 2	ENDGROUP	PLACEGROUP "pumpBody"	ADDz ConPosZ_1	ROTy -90	REVOLVE 4,360,1+2+16+32,		bottomThk/2, motorLowerD/2, 2,		bottomThk/2+motorMiddleZ, motorLowerD/2, 2,		bottomThk/2+2*motorMiddleZ, motorUpperD/2, 2,		ZZYZX-ConPosZ_1, motorUpperD/2, 2	DEL 2	GROUP "connection"		ADD ConPosX_1, 0, ConPosZ_1!!		REVOLVE 5,360, 16+32,!!			-ConLength_1, ConWidth_1/2, 2,!!			-ConLength_1, 1.6*ConWidth_1/2, 2,!!			-ConLength_1+0.004, 1.6*ConWidth_1/2, 2,!!			-ConLength_1+0.004, ConWidth_1/2, 2,!!			ABS(ConPosX_1)-(B-bottomThk)/2, ConWidth_1/2, 2		REVOLVE 2,360, 16+32,			0, ConWidth_1/2, 2,			ABS(ConPosX_1)-(B-bottomThk)/2, ConWidth_1/2, 2		DEL 1	ENDGROUP	PLACEGROUP SUBGROUP ("connection", "pumpBody")	KILLGROUP "connection"	KILLGROUP "pumpBody"	IF footH > 0.001 THEN		TOLER 0.05		ROTy -90		FOR i=1 TO 3			ADDz (B-bottomThk)/2 - footTck			REVOLVE 3,360, 1+2+16+32,				footH, footTck, 1,				0.1 * footH, 0.5*footTck, 1,				0, 0.001, 1001			DEL 1			ROTx 120		NEXT i		DEL 4	ENDIFENDIFIF gs_detlevel_3D_m = 1 THEN	! Simple	GROUP "pumpBody2"		ADDz ConPosZ_1		ROTy -90		REVOLVE 3,360,1+2+16+32,			-bottomThk/2, (B-bottomThk)/2, 1,			0,1, 801,			bottomThk/2, (B-bottomThk)/2, 1001		DEL 2	ENDGROUP	PLACEGROUP "pumpBody2"	ADDz ConPosZ_1	ROTy -90	REVOLVE 2,360,1+2+16+32,		bottomThk/2, motorLowerD/2, 2,		ZZYZX-ConPosZ_1, motorUpperD/2, 2	DEL 2	GROUP "connection2"		ADD ConPosX_1, 0, ConPosZ_1!!		REVOLVE 3,360, 16+32,!!			-ConLength_1, 1.6*ConWidth_1/2, 2,!!			-ConLength_1, ConWidth_1/2, 2,!!			ABS(ConPosX_1)-(B-bottomThk)/2, ConWidth_1/2, 2		REVOLVE 2,360, 1+2+16+32,			0, ConWidth_1/2, 2,			ABS(ConPosX_1)-(B-bottomThk)/2, ConWidth_1/2, 1		DEL 1	ENDGROUP	PLACEGROUP SUBGROUP ("connection2", "pumpBody2")	KILLGROUP "connection2"	KILLGROUP "pumpBody2"	IF footH > 0 THEN		TOLER 0.2		ROTy -90		FOR i=1 TO 3			ADDz (B-bottomThk)/2 - footTck			REVOLVE 2,360, 1+2+16+32,				footH, footTck, 1,				0 * footH, 0.5*footTck, 1			DEL 1			ROTx 120		NEXT i		DEL 4	ENDIFENDIF				! --- CONNECTIONS --- !DEL TOPRESOL resCALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	ui_current_con = ui_current_con,	MEP_NumberConnections = MEP_NumberConnections,	MEP_NumConnectionData = gs_Connections,	gs_AddConnections = gs_AddConnections,	cShow3D = 1,	gs_ConMat = MainBoxMaterial,	MEP_InsShow = 0,	MEP_cline_show_3D = 0,	gs_cont_pen = gs_cont_pen,	ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 1, ConEdgeVisible_1 = 0, MEP_StrConnectionData = MEP_StrConnectionDataEND