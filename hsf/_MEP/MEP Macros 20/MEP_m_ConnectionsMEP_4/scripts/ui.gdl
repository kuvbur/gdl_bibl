! Zsolt Feke/2007DIM tabIDs[]DIM tabNames[]DIM tabIcons[]DIM idTab_CustomPage[]	idTab_CustomPage[1] = 0idTab_GeneralSettings = 0idTab_Visualization = 0idTab_Connections = 0cPageIdx = 1actIdx = 1page_num = VARDIM1(ui_page_idx)FOR i = 1 TO page_num	IF ui_page_idx[i] = 1 THEN		! Built-in General Settings		pageIdx = i		tabIDs[pageIdx] = pageIdx		IF ui_page_str_attr[1][1] = "" THEN			tabNames[pageIdx] = cs_ui_pag[1]			! "General Settings"		ELSE			tabNames[pageIdx] = ui_page_str_attr[1][1]	! Custom Page Name		ENDIF		tabIcons[pageIdx] = "ui_tabpg_MEP_general_MEP"		idTab_GeneralSettings = pageIdx		actIdx = actIdx + 1	ENDIF	IF ui_page_idx[i] = 2 THEN		! Built-in Visualization		pageIdx = i		tabIDs[pageIdx] = pageIdx		IF ui_page_str_attr[2][1] = "" THEN			tabNames[pageIdx] = cs_ui_pag[2]			! "Visualization"		ELSE			tabNames[pageIdx] = ui_page_str_attr[2][1]	! Custom Page Name		ENDIF		tabIcons[pageIdx] = "ui_tabpg_MEP_visual_MEP"		idTab_Visualization = pageIdx		actIdx = actIdx + 1	ENDIF	IF ui_page_idx[i] = 3 THEN		! Built-in Connections		pageIdx = i		tabIDs[pageIdx] = pageIdx		IF ui_page_str_attr[3][1] = "" THEN			tabNames[pageIdx] = cs_ui_pag[3]			! "Connections"		ELSE			tabNames[pageIdx] = ui_page_str_attr[3][1]	! Custom Page Name		ENDIF		tabIcons[pageIdx] = "ui_tabpg_MEP_connections_MEP"		idTab_Connections = pageIdx		actIdx = actIdx + 1	ENDIF	FOR j = 1 TO 20		IF ui_page_idx[i] = 3+j THEN			pageIdx = i			tabIDs[pageIdx] = pageIdx			tabNames[pageIdx] = ui_page_str_attr[3+j][1]	! Custom Page Name			tabIcons[pageIdx] = ui_page_str_attr[3+j][2]			idTab_CustomPage[cPageIdx] = pageIdx			cPageIdx = cPageIdx + 1			actIdx = actIdx + 1		ENDIF	NEXT jNEXT iFILE_DEPENDENCE "ui_tabpg_MEP_general_MEP"FILE_DEPENDENCE "ui_tabpg_MEP_visual_MEP"FILE_DEPENDENCE "ui_tabpg_MEP_connections_MEP"FOR  i = actIdx TO 20	tabIDs[i]	= 0	tabIcons[i]	= ""	tabNames[i]	= ""NEXT iUI_CURRENT_PAGE gs_ui_current_page! =============================================================================! Tabpage Selector Control + Others! =============================================================================UI_PAGE gs_ui_current_page	! Place it on every pageIF ui_con_type[1] <> 4 THEN		! calling from MEP Connection object	CALL "ui_tabcontrol" PARAMETERS controlType = 0,									gs_ui_current_page = gs_ui_current_page,									nTabs = page_num,									tabTitles = tabNames,									tabIcons = tabIcons,									drawBox = 1ENDIF!!FOR i = 1 TO VARDIM1(ui_pict_name)							! Picture on Tabpage!!	ui_cur_page = 0!!	FOR j = 1 TO page_num!!		IF ui_page_idx[j] = ui_pict_attr[i][1] THEN ui_cur_page = j!!	NEXT j!!!!	IF ABS(ui_cur_page - gs_ui_current_page) < EPS AND ui_pict_name[i] <> "" THEN!!		UI_PICT ui_pict_name[i],	ui_pict_attr[i][2],!!									ui_pict_attr[i][3],!!									ui_pict_attr[i][4],!!									ui_pict_attr[i][5], 1!!	ENDIF!!NEXT iFOR i = 1 TO VARDIM1(ui_sep_attr)							! Separator on Tabpage	ui_cur_page = 0	FOR j = 1 TO page_num		IF ui_page_idx[j] = ui_sep_attr[i][1] THEN ui_cur_page = j	NEXT j	IF ABS(ui_cur_page - gs_ui_current_page) < EPS THEN		UI_SEPARATOR	ui_sep_attr[i][2],						ui_sep_attr[i][3],						ui_sep_attr[i][4],						ui_sep_attr[i][5]	ENDIFNEXT iIF ui_set_attr[1][1] THEN 									! Fields on Tabpage	UI_STYLE 0, 0	var_num = 1	FOR i = 1 TO VARDIM1(ui_set_attr)		IF ui_set_attr[i][1] THEN			ui_cur_page = 0			FOR j = 1 TO page_num				IF ui_page_idx[j] = ui_set_attr[i][1] THEN ui_cur_page = j			NEXT j		ENDIF		IF ui_set_attr[i][1] THEN			fields 			= ui_set_attr[i][2]			distX 			= ui_set_attr[i][3]			distY 			= ui_set_attr[i][4]			textWidthOut 	= ui_set_attr[i][5]			textWidthIn		= ui_set_attr[i][6]			textHeight	 	= ui_set_attr[i][7]			adX 			= ui_set_attr[i][8]			adY	 			= ui_set_attr[i][9]			next_row 		= 0		ELSE			IF ABS(ui_cur_page - gs_ui_current_page) < EPS THEN				IF ABS(fields - 1) < EPS AND ui_set_attr[i][2] > -EPS OR ABS(fields - 3) < EPS AND ui_set_attr[i][2] > -EPS THEN					IF ui_set_attr[i][3] THEN			! A***						UI_OUTFIELD ui_set_str[var_num][1], distX+25, distY+adY*next_row+2, textWidthOut, textHeight, ui_set_attr[i][2]					ELSE						UI_OUTFIELD ui_set_str[var_num][1], distX, distY+adY*next_row, textWidthOut, textHeight, ui_set_attr[i][2]					ENDIF				ELSE					adX = 0				ENDIF				IF ABS(fields - 2) < EPS AND ui_set_attr[i][2] > -EPS OR ABS(fields - 3) < EPS AND ui_set_attr[i][2] > -EPS THEN					IF ui_set_attr[i][3] THEN						UI_INFIELD ui_set_str[var_num][2], distX, distY+adY*next_row, 20, 20					ELSE						UI_INFIELD ui_set_str[var_num][2], distX+adX, distY+adY*next_row, textWidthIn, textHeight					ENDIF				ENDIF				next_row = next_row + 1			ENDIF			var_num = var_num + 1		ENDIF	NEXT iENDIF! =============================================================================! Current Tabpages! =============================================================================IF idTab_GeneralSettings THEN	UI_PAGE idTab_GeneralSettings			! === General Settings ===	IF ABS(ui_system_var_pos - 1) < EPS THEN		UI_OUTFIELD `Система MEP:`, 220, 240, 127, 15, 0		UI_INFIELD "ConSystem_1", 348, 240, 90, 15	ENDIF	UI_OUTFIELD	`Давление`,	10,	246, 57,15	UI_INFIELD	"eq_code",	10+60, 243, 50, 20ENDIFIF idTab_Visualization THEN	UI_PAGE idTab_Visualization				! === Visualization ===	UI_SEPARATOR 222, 44, 222, 252		IF ui_centerline_vars THEN			add_Y = 26		ELSE			add_Y = 28		ENDIF		x1 = 15		x2 = x1 + 30		x3 = x2 + 165		y = 42		UI_STYLE 0,1		UI_OUTFIELD `Символ Плана`, x2, y, x3-x2,18		! Plan Symbol		UI_STYLE 0, 0		y = y + 23		cx = 40		UI_PICT 4, x1, y, 24,17, 1		! ui_frame_pen_MEP.png		UI_OUTFIELD `Перо Контура`, x2,y+3, x3-x2-cx-1,18	! Contour Pen		UI_INFIELD "gs_cont_pen",	 x3-cx,y, cx,22		y = y + add_Y		UI_PICT 3, x1, y-2, 13,17, 1		! ui_fill_type_MEP.png		UI_OUTFIELD `Тип Штриховки`, x2,y, x3-x2,15, 0			! Fill Type		UI_INFIELD "gs_fill_type",		 x2,y+16, x3-x2,25		y = y + 19 + add_Y		cx = 40		UI_PICT 2, x1, y, 24,17, 1		! ui_fill_pen_MEP.png		UI_OUTFIELD `Перо Штриховки`, x2,y+3, x3-x2-cx-1,18, 0	! Fill Pen		UI_INFIELD "gs_fill_pen",	 x3-cx,y, cx,22		y = y + add_Y		UI_PICT 1, x1, y, 24,17, 1		! ui_back_pen_MEP.png		UI_OUTFIELD `Перо Фона Штриховки`, x2,y+3, x3-x2-cx-1,18, 0	! Background Pen		UI_INFIELD "gs_back_pen",	  x3-cx,y, cx,22		y = y + add_Y		IF ui_centerline_vars THEN			UI_INFIELD{3} "MEP_cline_show", x2-15, y, x3-x2+15, 15,							7, "",							2,							1, 20, 20, 20, 20,							0, `Показ Линии Оси`, 0,							1, `Показ Линии Оси`, 1			UI_INFIELD "MEP_cline_type",		 x2,y+20, x3-x2,25			y = y + 23 + add_Y			cx = 40			IF MEP_cline_show THEN				UI_PICT 35, x1, y, 24,17, 1			ELSE				UI_PICT 34, x1, y, 24,17, 1			ENDIF			UI_OUTFIELD `Перо Линии Оси`, x2,y+3, x3-x2-cx-1,18, 4 * (not(MEP_cline_show))	! Centerline Pen			UI_INFIELD "MEP_cline_pen",	 x3-cx,y, cx,22			y = y + add_Y		ENDIF		DIM ui_vis_default_str[][]			ui_vis_default_str[1][1] = `3D-отображение`			ui_vis_default_str[2][1] = `Покрытие`			ui_vis_default_str[3][1] = `Покрытие`			ui_vis_default_str[4][1] = `Изоляция`			ui_vis_default_str[5][1] = `Покрытие`			ui_vis_default_str[6][1] = `Покрытие Системы`			ui_vis_default_str[7][1] = `Покрытие`			ui_vis_default_str[8][1] = `Покрытие`			ui_vis_default_str[1][2] = "gs_detlevel_3D"			ui_vis_default_str[2][2] = "gs_PipeMat"			ui_vis_default_str[3][2] = "gs_DuctMat"			ui_vis_default_str[4][2] = "gs_InsMat"			ui_vis_default_str[5][2] = "gs_BodyMat"			ui_vis_default_str[6][2] = "useSysMat"			ui_vis_default_str[7][2] = "MainBoxMaterial"			ui_vis_default_str[8][2] = "gs_CableTrayMat"		DIM ui_vis_var_type[]			ui_vis_var_type[1] = 1			ui_vis_var_type[2] = 0			ui_vis_var_type[3] = 0			ui_vis_var_type[4] = 0			ui_vis_var_type[5] = 0			ui_vis_var_type[6] = 2			ui_vis_var_type[7] = 0			ui_vis_var_type[8] = 0		x1 = 232		x2 = x1 + 30		x3 = x2 + 165		y = 42		UI_STYLE 0,1		UI_OUTFIELD `3D-вид`, x2, y, x3-x2,18		! 3D View		UI_STYLE 0, 0		IF ui_vis_attr[1][1] THEN			! B***			next_var = 2			next_customvar = 1			FOR i = 2 TO ui_vis_attr[1][1]+2				IF ui_vis_attr[i][1] THEN					distX 		= ui_vis_attr[i][2]					distY 		= ui_vis_attr[i][3]					distRows 	= ui_vis_attr[i][4]				ELSE					IF ui_vis_attr[1][next_var] > 0 THEN						idx = ui_vis_attr[1][next_var]						desc = ui_vis_default_str[idx][1]						var_name = ui_vis_default_str[idx][2]						var_type = ui_vis_var_type[idx]					ELSE						desc = ui_vis_str[next_customvar][1]						var_name = ui_vis_str[next_customvar][2]						var_type = ui_vis_attr[i][3]						next_customvar = next_customvar + 1					ENDIF					IF var_type = 1 THEN						adX = 114						UI_OUTFIELD desc, distX, distY, 110, 15, ui_vis_attr[i][2]						UI_INFIELD var_name, distX+adX, distY, 65, 15						distY = distY + 15 + distRows					ENDIF					IF var_type = 0 THEN						x2 = distX + 30						x3 = x2 + 150						IF ui_vis_attr[i][2] THEN							UI_PICT 5, distX, distY-2, 12,17, 1			! ui_mat_lock_MEP.png						ELSE							UI_PICT 6, distX, distY-2, 12,17, 1			! ui_mat_MEP.png						ENDIF						UI_OUTFIELD desc, x2, distY, x3-x2, 15, ui_vis_attr[i][2]	! description						UI_INFIELD var_name, x2, distY+16, x3-x2, 25 ! Variable						distY = distY + 40 + distRows					ENDIF					IF var_type = 2 THEN						adX = 114						UI_OUTFIELD desc, distX + 25, distY, 110, 15, ui_vis_attr[i][2]						UI_INFIELD var_name, distX, distY, 15, 15						distY = distY + 15 + distRows					ENDIF					next_var = next_var + 1				ENDIF			NEXT i		ENDIF			! B***ENDIFIF idTab_Connections THEN	UI_PAGE idTab_Connections				! === Connections ===	DIM conTabIDs[]	DIM conTabNames[]	DIM conTabIcons[]	DIM activeConns[]	actIdx = 1	vis_con_count = 0	UI_OUTFIELD	`Давление`,	10,	246, 57,15	UI_INFIELD	"eq_code",	10+60, 243, 50, 20	FOR i = 1 TO MEP_NumberConnections		if bUIConnectionStatusCheckbox then			GOSUB 10			vis_con_count = vis_con_count + 1		else			IF ABS(MEP_NumConnectionData[i][22] - 1) < EPS THEN				GOSUB 10				vis_con_count = vis_con_count + 1			ENDIF		endif	NEXT i	IF vis_con_count < EPS THEN GOTO 99	FOR i=actIdx TO 20		conTabIDs[i]	= 0		conTabIcons[i]	= ""		conTabNames[i]	= ""	NEXT i	UI_SEPARATOR 222, 35, 222, 256	ui_connection_ID = activeConns[ui_current_con]	IF ui_con_type[ui_connection_ID] <> 4 THEN		px = 30		py = 20		cx = px		cy = py		UI_STYLE 0,1		UI_infield{3} "ui_current_con", 15, 36, 195, cy+5,			2, "",			0, 0,			cx, cy, px, py,			conTabIcons[1],		conTabNames[1],		conTabIDs[1],			conTabIcons[2],		conTabNames[2],		conTabIDs[2],			conTabIcons[3],		conTabNames[3],		conTabIDs[3],			conTabIcons[4],		conTabNames[4],		conTabIDs[4],			conTabIcons[5],		conTabNames[5],		conTabIDs[5],			conTabIcons[6],		conTabNames[6],		conTabIDs[6],			conTabIcons[7],		conTabNames[7],		conTabIDs[7],			conTabIcons[8],		conTabNames[8],		conTabIDs[8],			conTabIcons[9],		conTabNames[9],		conTabIDs[9],			conTabIcons[10],	conTabNames[10],	conTabIDs[10],			conTabIcons[11],	conTabNames[11],	conTabIDs[11],			conTabIcons[12],	conTabNames[12],	conTabIDs[12],			conTabIcons[13],	conTabNames[13],	conTabIDs[13],			conTabIcons[14],	conTabNames[14],	conTabIDs[14],			conTabIcons[15],	conTabNames[15],	conTabIDs[15],			conTabIcons[16],	conTabNames[16],	conTabIDs[16],			conTabIcons[17],	conTabNames[17],	conTabIDs[17],			conTabIcons[18],	conTabNames[18],	conTabIDs[18],			conTabIcons[19],	conTabNames[19],	conTabIDs[19],			conTabIcons[20],	conTabNames[20],	conTabIDs[20]	ENDIF	idx = 0	distX = 32: distY = 67!!	idx_str = STR(ui_con_type[ui_connection_ID], 1, 0)!!	IF idx_str = "3" THEN idx_str = "1"!!	IF ABS(MEP_NumConnectionData[ui_connection_ID][19]-1) < EPS THEN!!		IF ABS(gs_AddConnections[ui_connection_ID][1] - 1) < EPS THEN!!			IF ABS(MEP_NumConnectionData[ui_connection_ID][21]-1) < EPS THEN pict_name = "ui_rect_"+idx_str+"_HVAC_simpleb_MEP"!!			IF ABS(MEP_NumConnectionData[ui_connection_ID][21]-2) < EPS THEN pict_name = "ui_circ_"+idx_str+"_HVAC_simpleb_MEP"!!			IF ABS(MEP_NumConnectionData[ui_connection_ID][21]-3) < EPS THEN pict_name = "ui_oval_"+idx_str+"_HVAC_simpleb_MEP"!!!!		ELSE!!			IF ABS(MEP_NumConnectionData[ui_connection_ID][21]-1) < EPS THEN pict_name = "ui_rect_"+idx_str+"_HVAC_flanged_MEP"!!			IF ABS(MEP_NumConnectionData[ui_connection_ID][21]-2) < EPS THEN pict_name = "ui_circ_"+idx_str+"_HVAC_flanged_MEP"!!			IF ABS(MEP_NumConnectionData[ui_connection_ID][21]-3) < EPS THEN pict_name = "ui_oval_"+idx_str+"_HVAC_flanged_MEP"!!		ENDIF!!		UI_PICT pict_name, distX, distY, 160, 160, 1!!		idx = 1!!	ENDIF!!	IF ABS(MEP_NumConnectionData[ui_connection_ID][19]-2) < EPS THEN!!		conStyle = 1!!		IF ABS(gs_AddConnections[ui_connection_ID][1] - 1) < EPS THEN conStyle = 0!!		IF ABS(gs_AddConnections[ui_connection_ID][1] - 3) < EPS AND ABS(MEP_NumConnectionData[ui_connection_ID][20]-1) < EPS THEN conStyle = 2!!!!		IF ABS(conStyle) < EPS THEN pict_name = "ui_circ_"+idx_str+"_Pipe_notflanged_MEP"!!		IF ABS(conStyle - 1) < EPS THEN pict_name = "ui_circ_"+idx_str+"_Pipe_flanged_MEP"!!		IF ABS(conStyle - 2) < EPS THEN pict_name = "ui_circ_"+idx_str+"_Pipe_HUB_male_MEP"!!!!		UI_PICT pict_name, distX, distY, 160, 160, 1!!		idx = 2!!	ENDIF	idx_int = ui_con_type[ui_connection_ID]	IF idx_int = 3 THEN idx_int = 1	IF idx_int = 4 THEN idx_int = 0	IF ABS(MEP_NumConnectionData[ui_connection_ID][19]-1) < EPS THEN		pict_idx = MEP_NumConnectionData[ui_connection_ID][21]*100+gs_AddConnections[ui_connection_ID][1]*10+idx_int		IF ABS(pict_idx - 110) < EPS THEN pict_idx = 16		IF ABS(pict_idx - 111) < EPS THEN pict_idx = 17		IF ABS(pict_idx - 112) < EPS THEN pict_idx = 18		IF ABS(pict_idx - 120) < EPS THEN pict_idx = 19		IF ABS(pict_idx - 121) < EPS THEN pict_idx = 20		IF ABS(pict_idx - 122) < EPS THEN pict_idx = 21		IF ABS(pict_idx - 210) < EPS THEN pict_idx = 22		IF ABS(pict_idx - 211) < EPS THEN pict_idx = 23		IF ABS(pict_idx - 212) < EPS THEN pict_idx = 24		IF ABS(pict_idx - 220) < EPS THEN pict_idx = 25		IF ABS(pict_idx - 221) < EPS THEN pict_idx = 26		IF ABS(pict_idx - 222) < EPS THEN pict_idx = 27		IF ABS(pict_idx - 310) < EPS THEN pict_idx = 28		IF ABS(pict_idx - 311) < EPS THEN pict_idx = 29		IF ABS(pict_idx - 312) < EPS THEN pict_idx = 30		IF ABS(pict_idx - 320) < EPS THEN pict_idx = 31		IF ABS(pict_idx - 321) < EPS THEN pict_idx = 32		IF ABS(pict_idx - 322) < EPS THEN pict_idx = 33		UI_PICT pict_idx, distX, distY, 160, 160, 1		idx = 1	ENDIF	IF ABS(MEP_NumConnectionData[ui_connection_ID][19]-2) < EPS THEN		conStyle = 2		IF ABS(gs_AddConnections[ui_connection_ID][1] - 1) < EPS THEN conStyle = 1		IF ABS(gs_AddConnections[ui_connection_ID][1] - 3) < EPS AND ABS(MEP_NumConnectionData[ui_connection_ID][20]-1) < EPS THEN conStyle = 3		pict_idx = 10*conStyle+idx_int		IF pict_idx = 10 THEN pict_idx = 7		IF pict_idx = 11 THEN pict_idx = 8		IF pict_idx = 12 THEN pict_idx = 9		IF pict_idx = 20 THEN pict_idx = 10		IF pict_idx = 21 THEN pict_idx = 11		IF pict_idx = 22 THEN pict_idx = 12		IF pict_idx = 30 THEN pict_idx = 13		IF pict_idx = 31 THEN pict_idx = 14		IF pict_idx = 32 THEN pict_idx = 15		UI_PICT pict_idx, distX, distY, 160, 160, 1		idx = 2	ENDIF	DIM	connections_str[][]		connections_str[1][1] = ""	DIM	connections_attr[][]		connections_attr[1][1] = 1		connections_attr[1][2] = 3		connections_attr[1][3] = 227		connections_attr[1][5] = 119		connections_attr[1][6] = 90		connections_attr[1][7] = 15		connections_attr[1][8] = 120		IF ui_con_type[ui_connection_ID] <> 4 THEN			connections_attr[1][4] = 48			connections_attr[1][9] = 21		ELSE			connections_attr[1][4] = 74			connections_attr[1][9] = 21		ENDIF		FOR jj = 2 TO 9							! *** incializalas			FOR ii = 1 TO 9				connections_attr[jj][ii] = 0			NEXT ii		NEXT jj	IF ABS(MEP_NumConnectionData[ui_connection_ID][22] - 1) < EPS THEN		outfieldFlag = 0	else		outfieldFlag = 4	endif	GOSUB 10+idx	if bUIConnectionStatusCheckbox then		gosub "connectionCheckBox"	endif	IF connections_attr[1][1] THEN 									! Fields on Tabpage		UI_STYLE 0, 0		var_num = 1		FOR i = 1 TO VARDIM1(connections_attr)			IF connections_attr[i][1] THEN				fields 			= connections_attr[i][2]				distX 			= connections_attr[i][3]				distY 			= connections_attr[i][4]				textWidthOut 	= connections_attr[i][5]				textWidthIn		= connections_attr[i][6]				textHeight	 	= connections_attr[i][7]				adX 			= connections_attr[i][8]				adY	 			= connections_attr[i][9]				next_row 		= 0			ELSE				IF connections_attr[i][3] = 1 and var_num<=VARDIM1(connections_str) and i<=VARDIM1(connections_attr) THEN					IF fields = 1 OR fields = 3 THEN						UI_OUTFIELD connections_str[var_num][1], distX, distY+adY*next_row, textWidthOut, textHeight, connections_attr[i][2]					ELSE						adX = 0					ENDIF					IF fields = 2 OR fields = 3 THEN						UI_INFIELD connections_str[var_num][2], distX+adX, distY+adY*next_row-1, textWidthIn, textHeight					ENDIF					next_row = next_row + 1				ENDIF				IF connections_attr[i][3] = 2 THEN					IF fields = 1 OR fields = 3 THEN						UI_OUTFIELD connections_str[var_num][1], distX + 25, distY+adY*next_row, textWidthOut, textHeight, connections_attr[i][2]					ELSE						adX = 0					ENDIF					IF fields = 2 OR fields = 3 THEN						UI_INFIELD connections_str[var_num][2], distX, distY+adY*next_row-1, 15, textHeight					ENDIF					next_row = next_row + 1				ENDIF				var_num = var_num + 1			ENDIF		NEXT i		IF ui_system_var_pos = 2 THEN			bFlag = 0			IF ui_con_type[ui_connection_ID] = 2 AND same_con THEN outfieldFlag = 4			UI_OUTFIELD `Система MEP:`, 227, 40+8*25, 119, 15, outfieldFlag			paramName = "ConSystem_"+STR(ui_connection_ID, 1, 0)			UI_INFIELD paramName, 347, 40+8*25-1, 90, 15		ENDIF		IF ui_system_var_pos = 3 AND ui_con_type[ui_connection_ID] = 1 THEN			