! Gen Tank! Zsolt FekePEN gs_cont_penSET FILL gs_fill_typeunID = hptsIDIF typ = 1 THEN	 chRad = endRed	 tankRad = tankDia/2	IF tankPos THEN		HOTSPOT2 0, 0, unID: unID=unID+1		POLY2_B 2, 1+2+4+64, gs_fill_pen, gs_back_pen,			0, 0, 901,			tankRad+ringThick*ringOn, 360, 4001		IF ringOn THEN			CIRCLE2 0, 0, tankRad			CIRCLE2 0, 0, tankRad-chRad		ENDIF	ELSE		HOTSPOT2 0, 0, unID: unID=unID+1		HOTSPOT2 tankLen, 0, unID: unID=unID+1			PUT 0, -(tankRad-chRad), 1			PUT 0, tankRad-chRad, 1			PUT chRad, tankRad, 1001			IF ringOn = 1 THEN				HOTSPOT2 ring_AxisPos+ringWidth/2, tankRad+ringThick, unID: unID=unID+1				LINE2 ring_AxisPos, tankRad, ring_AxisPos, -tankRad				LINE2 ring_AxisPos+ringWidth, tankRad, ring_AxisPos+ringWidth, -tankRad				PUT ring_AxisPos, tankRad, 1				PUT ring_AxisPos, tankRad+ringThick, 1				PUT ring_AxisPos+ringWidth, tankRad+ringThick, 1				PUT ring_AxisPos+ringWidth, tankRad, 1				HOTSPOT2 tankLen-ring_AxisPos-ringWidth/2, tankRad+ringThick, unID: unID=unID+1				LINE2 tankLen-ring_AxisPos-ringWidth, tankRad, tankLen-ring_AxisPos-ringWidth, -tankRad				LINE2 tankLen-ring_AxisPos, tankRad, tankLen-ring_AxisPos, -tankRad				PUT tankLen-ring_AxisPos-ringWidth, tankRad, 1				PUT tankLen-ring_AxisPos-ringWidth, tankRad+ringThick, 1				PUT tankLen-ring_AxisPos, tankRad+ringThick, 1				PUT tankLen-ring_AxisPos, tankRad, 1			ENDIF			PUT tankLen-chRad, tankRad, 1			PUT tankLen-chRad, tankRad-chRad, 901			PUT tankLen, tankRad-chRad, 3001			PUT tankLen, -(tankRad-chRad), 1			PUT tankLen-chRad, -tankRad, 1001			IF ringOn = 1 THEN				HOTSPOT2 tankLen-ring_AxisPos-ringWidth/2, -tankRad-ringThick, unID: unID=unID+1				PUT tankLen-ring_AxisPos, -tankRad, 1				PUT tankLen-ring_AxisPos, -tankRad-ringThick, 1				PUT tankLen-ring_AxisPos-ringWidth, -tankRad-ringThick, 1				PUT tankLen-ring_AxisPos-ringWidth, -tankRad, 1				HOTSPOT2 ring_AxisPos+ringWidth/2, -tankRad-ringThick, unID: unID=unID+1				PUT ring_AxisPos+ringWidth, -tankRad, 1				PUT ring_AxisPos+ringWidth, -tankRad-ringThick, 1				PUT ring_AxisPos, -tankRad-ringThick, 1				PUT ring_AxisPos, -tankRad, 1			ENDIF			PUT chRad, -tankRad, 1			PUT chRad, -(tankRad-chRad), 901			PUT 0, -(tankRad-chRad), 3001		POLY2_B NSP/3, 1+2+4+64, gs_fill_pen, gs_back_pen,			GET (NSP)	ENDIFENDIFIF typ = 2 THEN	 tankRad = tankDia/2	 alphaTankEnd = (180-2*ATN(tankRad/endRed))	 Rad = tankRad/SIN(alphaTankEnd)	IF tankPos THEN		HOTSPOT2 0, 0, unID: unID=unID+1		POLY2_B 2, 1+2+4+64, gs_fill_pen, gs_back_pen,			0, 0, 901,			tankRad, 360, 4001		IF ringOn THEN			POLY2_B 4, 1+2+4+64, gs_fill_pen, gs_back_pen,				0, 0, 901,				tankRad+ringThick, 360, 4001,				0, 0, 901,				tankRad, 360, 4001		ENDIF	ELSE		HOTSPOT2 0, 0, unID: unID=unID+1		HOTSPOT2 tankLen, 0, unID: unID=unID+1			PUT endRed, -tankRad, 1			PUT Rad, 0, 901			PUT endRed, tankRad, 3001			IF ringOn = 1 THEN				HOTSPOT2 ring_AxisPos+ringWidth/2, tankRad+ringThick, unID: unID=unID+1				LINE2 ring_AxisPos, tankRad, ring_AxisPos, -tankRad				LINE2 ring_AxisPos+ringWidth, tankRad, ring_AxisPos+ringWidth, -tankRad				PUT ring_AxisPos, tankRad, 1				PUT ring_AxisPos, tankRad+ringThick, 1				PUT ring_AxisPos+ringWidth, tankRad+ringThick, 1				PUT ring_AxisPos+ringWidth, tankRad, 1				HOTSPOT2 tankLen-ring_AxisPos-ringWidth/2, tankRad+ringThick, unID: unID=unID+1				LINE2 tankLen-ring_AxisPos-ringWidth, tankRad, tankLen-ring_AxisPos-ringWidth, -tankRad				LINE2 tankLen-ring_AxisPos, tankRad, tankLen-ring_AxisPos, -tankRad				PUT tankLen-ring_AxisPos-ringWidth, tankRad, 1				PUT tankLen-ring_AxisPos-ringWidth, tankRad+ringThick, 1				PUT tankLen-ring_AxisPos, tankRad+ringThick, 1				PUT tankLen-ring_AxisPos, tankRad, 1			ENDIF			PUT tankLen-endRed, tankRad, 1			PUT tankLen-Rad, 0, 901			PUT tankLen-endRed, -tankRad, 3001			IF ringOn = 1 THEN				HOTSPOT2 tankLen-ring_AxisPos-ringWidth/2, -tankRad-ringThick, unID: unID=unID+1				PUT tankLen-ring_AxisPos, -tankRad, 1				PUT tankLen-ring_AxisPos, -tankRad-ringThick, 1				PUT tankLen-ring_AxisPos-ringWidth, -tankRad-ringThick, 1				PUT tankLen-ring_AxisPos-ringWidth, -tankRad, 1				HOTSPOT2 ring_AxisPos+ringWidth/2, -tankRad-ringThick, unID: unID=unID+1				PUT ring_AxisPos+ringWidth, -tankRad, 1				PUT ring_AxisPos+ringWidth, -tankRad-ringThick, 1				PUT ring_AxisPos, -tankRad-ringThick, 1				PUT ring_AxisPos, -tankRad, 1			ENDIF			PUT endRed, -tankRad, -1		POLY2_B NSP/3, 1+2+4+64, gs_fill_pen, gs_back_pen,			GET (NSP)	ENDIFENDIF