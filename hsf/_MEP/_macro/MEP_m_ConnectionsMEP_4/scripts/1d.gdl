EPS = 0.001isavailable = 0isInArchiCAD = APPLICATION_QUERY ("MEPMODELER", "IsAvailable()", isavailable)DIM SystemStr[][]! ConEdit_x:!	0:	none!	1:	length!	2:	pos X!	3:	pos Y!	4:	pos Z!	5:	pos X and pos Y!	6:	pos X and pos Z!	7:	pos Y and pos Z!!	12:	length + pos X!	13:	length + pos Y!	14:	length + pos Z!	15:	length + pos X and pos Y!	16:	length + pos X and pos Z!	17:	length + pos Y and pos Z!!!!=============================== МАТЕРИАЛЫ =================================MEP_UNKNOW = 0MEP_ST3 = 1MEP_AISI = 2MEP_09G2S = 3MEP_PVC = 4MEP_GSH = 5MEP_VG = 6MEP_PE = 7MEP_PPR_1 = 8MEP_PPR_2 = 9MEP_PPR_3 = 10MEP_PPR_4 = 11MEP_PPR_HV = 12MEP_PVC_PIN = 13!!!!============================= ТИПЫ ЭЛЕМЕНТОВ ===============================MEP_TYPE_TUBE = 1MEP_TYPE_PEREH = 2MEP_TYPE_TR = 3MEP_TYPE_OTVOD = 4MEP_TYPE_FLANGE = 5MEP_TYPE_OP = 6MEP_TYPE_EQONTUBE = 7MEP_TYPE_ZAGL = 8DIM cs_ui_pag[3]	cs_ui_pag[1] = `Общие Параметры`	cs_ui_pag[2] = `Визуализация`	cs_ui_pag[3] = `Соединения`flangeout1 = 0 : flangeout2 = 0if haskey(mep_con.find.flangeout) then flangeout1 = mep_con.find.flangeoutif haskey(mep_con.find.flangeout1) then flangeout1 = mep_con.find.flangeout1if haskey(mep_con.find.flangeout2) then	flangeout2 = mep_con.find.flangeout2else	flangeout2 = flangeout1endifflangeth1 = 0.003if abs(flangeout1)>EPS and flangeout1<EPS then	flangeth1 = -flangeth1	flangeout1 = abs(flangeout1)endifflangeth2 = 0.003if abs(flangeout2)>EPS and flangeout2<EPS then	flangeth2 = -flangeth2	flangeout2 = abs(flangeout2)endifif haskey(mep_con.group) then	if mep_con.group<>MEP_PVC_PIN then		flangeth1 = 0		flangeout1 = 0		flangeth2 = 0		flangeout2 = 0	endifendif