!call "mep_get_material" parameters all mep_get_ma_name_in="" returned_parameters mep_get_ma_name, mep_get_ma_ro, mep_get_ma_group, mep_get_ma_pnMEP_UNKNOW = 0MEP_ST3 = 1MEP_AISI = 2MEP_09G2S = 3MEP_PVC = 4MEP_GSH = 5MEP_VG = 6MEP_PE = 7MEP_PPR_1 = 8MEP_PPR_2 = 9MEP_PPR_3 = 10MEP_PPR_4 = 11MEP_PPR_HV = 12MEP_PVC_PIN = 13var_t="" : txt_var = "" : ConSystemID = ""dim syst_name_t[] : syst_name_t[1]=""n_sys=0if ConID<=0 then ConID = 1if ConID>vardim1(MEP_StrConnectionData) then ConID=vardim1(MEP_StrConnectionData)mep_fill_type = 0mep_get_ma_group = MEP_UNKNOWmep_get_ma_name = ""mep_get_ma_ro = 7850mep_get_ma_pn = 0n = REQUEST ("Name_of_main", "", name)print_str = name + " : mep_get_material : " + MEP_StrConnectionData[ConID][2] + " "nprint = strlen(print_str)if MEP_StrConnectionData[ConID][2]="0" or strlen(MEP_StrConnectionData[ConID][2])=0 then	mep_get_ma_group = MEP_ST3	mep_get_ma_name = "ст. 3"	mep_get_ma_ro = 7850	goto "m_end"endifdim group_fill_inx[] : dim group_fill_name[] : dim group_fill_type[] : dim group_fill_ro[]dict mep_type_data : call "mep_common_data" parameters all, mep_set_type=1, returned_parameters mep_type_dataif haskey(mep_type_data.material) then	group_fill_inx = mep_type_data.material.group_fill_inx	group_fill_name = mep_type_data.material.group_fill_name	group_fill_type = mep_type_data.material.group_fill_type	group_fill_ro = mep_type_data.material.group_fill_roelse	print_str = print_str + "haskey(mep_type_data.material)"	goto "m_end"endifmax_pen = 0 : min_pen = 0if haskey(mep_type_data.tpn) then	min_pen = mep_type_data.tpn[1].pen	max_pen = mep_type_data.tpn[vardim1(mep_type_data.tpn)].penelse	print_str = print_str + "haskey(mep_type_data.tpn)"	goto "m_end"endifif strlen(mep_get_ma_name_in)>1 then	gosub "get_group_by_name"else	gosub "get_name_by_fill"endifgoto "m_end""get_name_by_fill":	gosub "get_syst_id_by_data"	gosub "get_syst_fill"	gosub "get_syst_pen"	if mep_fill_type > 0 then		flag_find=0		for i=1 to vardim1(group_fill_inx)			if mep_fill_type = group_fill_inx[i] then				mep_get_ma_group = group_fill_type[i]				mep_get_ma_name = group_fill_name[i]				mep_get_ma_ro = group_fill_ro[i]				flag_find = 1				i = vardim1(group_fill_inx)			endif		next i		if flag_find=0 then			mep_get_ma_group = MEP_ST3			mep_get_ma_name = "ст. 3"			mep_get_ma_ro = 7850			if flag_find = 0 then print_str = print_str + "Материал по штриховке не найден - " + str("%.0", mep_fill_type) + ConSystemID		endif	else		mep_get_ma_group = MEP_ST3		mep_get_ma_name = "ст. 3"		mep_get_ma_ro = 7850		print_str = print_str + "Штриховка не найдена " + ConSystemID	endif	if mep_pen > 0 then		flag_find=0		for i=1 to vardim1(mep_type_data.tpn)			if mep_pen = mep_type_data.tpn[i].pen then				mep_get_ma_pn = mep_type_data.tpn[i].pn				flag_find = 1				i = vardim1(group_fill_inx)			endif		next i		if flag_find=0 then			mep_get_ma_pn = 0			if flag_find = 0 then print_str = print_str + "Давление по перу не найдено - " + str("%.0", mep_pen) + ConSystemID		endif	endifreturn"get_group_by_name":	for i=1 to vardim1(group_fill_name)		if mep_get_ma_name_in = group_fill_name[i] then			mep_get_ma_group = group_fill_type[i]			mep_get_ma_name = group_fill_name[i]			mep_get_ma_ro = group_fill_ro[i]			flag_find = 1			i = vardim1(group_fill_name)		endif	next i	if flag_find = 0 then print_str = print_str + "Материал по имени не найден - " + mep_get_ma_name_in + ConSystemIDreturn"get_syst_pen":	queryStr = "GetCenterLinePen (" + ConSystemID + ")"	mep_pen = 0	n = APPLICATION_QUERY ("MEPSystem", queryStr, mep_pen)	if mep_pen>max_pen or mep_pen<min_pen then mep_pen = 0return"get_syst_fill":	queryStr = "GetFillType (" + ConSystemID + ")"	mep_fill_type = 0	n = APPLICATION_QUERY ("MEPSystem", queryStr, mep_fill_type)return"get_syst_id_by_data":	ConSystemID = MEP_StrConnectionData[ConID][2]return"get_syst_id_by_name":	!!!!Добавить поиск ID системы по имениreturn"m_end":if strlen(print_str)>nprint then print print_strend mep_get_ma_name, mep_get_ma_ro, mep_get_ma_group, mep_get_ma_pn