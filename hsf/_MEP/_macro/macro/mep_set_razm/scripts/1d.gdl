!!! call "mep_set_razm" parameters all!!!!============================= ТИПЫ ЭЛЕМЕНТОВ ===============================MEP_TYPE_TUBE = 1MEP_TYPE_PEREH = 2MEP_TYPE_TR = 3MEP_TYPE_OTVOD = 4MEP_TYPE_FLANGE = 5MEP_TYPE_OP = 6MEP_TYPE_EQONTUBE = 7EPS = 0.0001if mep_type_obor=0 then mep_type_obor = MEP_TYPE_EQONTUBEdict mep_type_data : call "mep_common_data" parameters all, mep_set_type=1 returned_parameters mep_type_datags_list_manufacturer = ""if haskey(mep_type_data.renum) then 	if haskey(mep_type_data.renum[mep_type_obor]) then		gs_list_manufacturer = mep_type_data.renum[mep_type_obor]	endifendifmep_s_in_ = 0 : mep_razm = "" : mep_razm_abs = ""if ConStatus_1=1 then	if strstr(ConType_1, "Прямоуг")>0 then		mep_razm = str("%.0mm", ConWidth_1) + "x" + str("%.0mm", ConDepth_1)		mep_razm_abs = str("%.0mm", max(ConWidth_1, ConDepth_1)) + "x" + str("%.0mm", min(ConWidth_1, ConDepth_1))		mep_s_in_ = ConWidth_1 * ConDepth_1	else		mep_razm = "ø" + str("%.0mm", ConWidth_1)		mep_razm_abs = mep_razm		mep_s_in_ = 3.14159 * ConWidth_1 * ConWidth_1 / 4	endifendifif abs(MEP_StraightLength)<EPS then 	mep_l = Aelse	mep_l = MEP_StraightLengthendifmep_vol_ = 0if is_set_vol then mep_vol_ = mep_s_in_ * mep_lmep_el_type_view = 0MEP_HORIZONTAL = 1call "mep_get_type_view" parameters MEP_Position = MEP_Position, MEP_StraightLength = MEP_StraightLength, RETURNED_PARAMETERS mep_el_type_viewif (abs(mep_el_type_view) = MEP_HORIZONTAL) then	otm_elem = SYMB_POS_Z + mep_otm_dzelse	otm_elem = SYMB_POS_Z + MEP_StraightLength*(mep_el_type_view > 0)endifotm_elem_abs = otm_elem