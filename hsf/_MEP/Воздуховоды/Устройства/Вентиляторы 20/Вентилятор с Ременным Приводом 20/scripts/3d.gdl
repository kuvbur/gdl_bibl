!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_el! Belt Drive FanIF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFIF gs_detlevel_3D_m = 0 THEN ENDres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! SimpleIF GLOB_CONTEXT = 2 THEN res = 24MainBoxMaterial = sMatMATERIAL MainBoxMaterialPEN gs_cont_penRESOL res				! --- TRANSFORMATION --- !XFORM   MEP_Position[2][1], MEP_Position[3][1], dZx, MEP_Position[1][1],		MEP_Position[2][2], MEP_Position[3][2], dZy, MEP_Position[1][2],		MEP_Position[2][3], MEP_Position[3][3], dZz, MEP_Position[1][3]				! --- HOTSPOT --- !unID = 1HOTSPOT 0, 0, 0, unID: unID=unID+1HOTSPOT -boxLen/2, -B/2, -ZZYZX/2, unID: unID=unID+1HOTSPOT -boxLen/2, -B/2, ZZYZX/2, unID: unID=unID+1HOTSPOT boxLen/2, -B/2, -ZZYZX/2, unID: unID=unID+1HOTSPOT boxLen/2, -B/2, ZZYZX/2, unID: unID=unID+1HOTSPOT -boxLen/2, B/2, -ZZYZX/2, unID: unID=unID+1HOTSPOT -boxLen/2, B/2, ZZYZX/2, unID: unID=unID+1HOTSPOT boxLen/2, B/2, -ZZYZX/2, unID: unID=unID+1HOTSPOT boxLen/2, B/2, ZZYZX/2, unID: unID=unID+1IF ABS(MEP_NumConnectionData[1][21]-1) < EPS THEN	HOTSPOT -A/2, ConWidth_1/2+ConWallThickness_1, ConDepth_1/2+ConWallThickness_1, unID: unID=unID+1	HOTSPOT -A/2, -ConWidth_1/2-ConWallThickness_1, ConDepth_1/2+ConWallThickness_1, unID: unID=unID+1	HOTSPOT A/2, -ConWidth_1/2-ConWallThickness_1, ConDepth_1/2+ConWallThickness_1, unID: unID=unID+1	HOTSPOT A/2, ConWidth_1/2+ConWallThickness_1, ConDepth_1/2+ConWallThickness_1, unID: unID=unID+1	HOTSPOT -A/2, ConWidth_1/2+ConWallThickness_1, -ConDepth_1/2-ConWallThickness_1, unID: unID=unID+1	HOTSPOT -A/2, -ConWidth_1/2-ConWallThickness_1, -ConDepth_1/2-ConWallThickness_1, unID: unID=unID+1	HOTSPOT A/2, -ConWidth_1/2-ConWallThickness_1, -ConDepth_1/2-ConWallThickness_1, unID: unID=unID+1	HOTSPOT A/2, ConWidth_1/2+ConWallThickness_1, -ConDepth_1/2-ConWallThickness_1, unID: unID=unID+1ELSE	HOTSPOT -A/2, ConWidth_1/2+ConWallThickness_1, 0, unID: unID=unID+1	HOTSPOT -A/2, -ConWidth_1/2-ConWallThickness_1, 0, unID: unID=unID+1	HOTSPOT A/2, -ConWidth_1/2-ConWallThickness_1, 0, unID: unID=unID+1	HOTSPOT A/2, ConWidth_1/2+ConWallThickness_1, 0, unID: unID=unID+1ENDIF				! --- MAIN BOX --- !ADD -boxLen/2, -B/2, -ZZYZX/2	BLOCK boxLen, B, ZZYZXDEL 1ADD boxLen/2-dist-boxDepth, -boxWidth/2, ZZYZX/2	BLOCK boxDepth, boxWidth, 0.015DEL 1ROTz -90IF gs_detlevel_3D_m = 2 THEN	ADD 0, boxLen/2-dist-boxDepth+0.01, ZZYZX/2+0.015+cylindDia/2	ROTx -90	CYLIND cylindHeight, cylindDia/2	DEL 2ELSE	ADD -cylindDia/2, boxLen/2-dist-boxDepth+0.01, ZZYZX/2+0.015		BLOCK cylindDia, cylindHeight, cylindDia	DEL 1ENDIFDEL 1CUTPOLYA 5, 2, 0,	boxLen/2, B/2, 13,	-boxLen/2, B/2, 13,	-boxLen/2, -B/2, 13,	boxLen/2, -B/2, 13,	boxLen/2, B/2, -1DIM StraightPosition[][]StraightPosition[1][1] = -A/2StraightPosition[1][2] = 0StraightPosition[1][3] = 0StraightPosition[2][1] = 1StraightPosition[2][2] = 0StraightPosition[2][3] = 0StraightPosition[3][1] = 0StraightPosition[3][2] = 1StraightPosition[3][3] = 0CALL "MEP_m_Duct_4" PARAMETERS 	MEP_Position = StraightPosition,								gs_Connections = gs_Connections,								MEP_NumberConnections = MEP_NumberConnections,								MEP_NumConnectionData = MEP_NumConnectionData,								MEP_StrConnectionData = MEP_StrConnectionData,								MEP_StraightLength = A,								gs_AddConnections = gs_AddConnections,								duct_show = 1,								gs_DuctMat = MainBoxMaterial,								MEP_InsShow = 0,								MEP_cline_show_3D = 0,								gs_detlevel_3D_m = gs_detlevel_3D_m,								gs_cont_pen = gs_cont_penCUTENDDEL 1END