!-----------2d---------------------if show_el < EPS then endIF gs_detlevel_2D_m < EPS THEN ENDif hide_conventions and show_conventions then endif GLOB_PREVIEW_MODE = 0 and GLOB_FEEDBACK_MODE = 0 then CALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, gs_fill_type, gs_fill_pen, gs_back_pen, MEP_cline_type, MEP_cline_pen, show_elPEN gs_cont_penIF gs_fill_pen THEN SET FILL gs_fill_typeDrawingNum = 1IF DrawingNum = 1 THEN	! Top				! --- HOTSPOTS --- !	unID = 1	HOTSPOT2 -footSize/2, 0, unID: unID=unID+1	HOTSPOT2 -footSize/2, footHeight, unID: unID=unID+1	HOTSPOT2 footSize/2, footHeight, unID: unID=unID+1	HOTSPOT2 footSize/2, 0, unID: unID=unID+1	HOTSPOT2 0, B, unID: unID=unID+1	HOTSPOT2 0, 0, unID: unID=unID+1				! --- MAIN BOX --- !	PUT -footSize/2, 0, 1,		-footSize/2, footHeight, 1,		footSize/2, footHeight, 1,		footSize/2, 0, 1,		-footSize/2, 0, -1	POLY2_B NSP/3, 1+2+4+64, gs_fill_pen, gs_back_pen,		GET (NSP)	ADD2 0, footHeight	MUL2 A/0.70, (B-footHeight)/0.55	HOTSPOT2 -0.35, 0.45, unID: unID=unID+1	HOTSPOT2 0.35, 0.45, unID: unID=unID+1	LINE2 -0.35, 0.45, 0.35, 0.45	LINE2 -0.22, 0.12, 0.22, 0.12	FOR i = 1 TO 2	MUL2 1-2*(i-1), 1	POLY2_B 12, 1+2+4+64, gs_fill_pen, gs_back_pen,			0,         0,      1,			-0.2,         0,      1,			-0.2,         0.10,      1,			-0.22,         0.12,      1001,			-0.2295429313288,         0.12,      1,			-0.3240130768752, 0.2049804391087,      1001,			-0.35,          0.45,      1,			-0.21,          0.45,      1,			-0.1820800847737, 0.5361650008843,      1,			-0.1630539773395,          0.55,      1001,			0,          0.55,      0,			0,         0,      1	DEL 1	NEXT i	DEL 2ENDIF				! --- OTHER VIEWS --- !ROT2 -SYMB_ROTANGLE * (DrawingNum > 1 AND NOT(SYMB_MIRRORED))+SYMB_ROTANGLE * (DrawingNum > 1 AND SYMB_MIRRORED)IF DrawingNum = 2 THEN PROJECT2 -3, 90, 2	! BottomIF DrawingNum = 3 THEN PROJECT2 4, 270, 2	! FrontIF DrawingNum = 4 THEN PROJECT2 4, 90, 2	! BackIF DrawingNum = 5 THEN PROJECT2 4, 180*(SYMB_MIRRORED), 2		! RightIF DrawingNum = 6 THEN PROJECT2 4, 180*NOT(SYMB_MIRRORED), 2	! Left					! --- CONNECTIONS --- !CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,		MEP_NumberConnections = MEP_NumberConnections,		MEP_NumConnectionData = gs_Connections,		gs_AddConnections = gs_AddConnections,		DrawingNum = DrawingNum,		MEP_InsShow = 0,		MEP_cline_show = 0,		gs_cont_pen = gs_cont_pen,		gs_fill_type=gs_fill_type,		gs_fill_pen=gs_fill_pen,		gs_back_pen=gs_back_pen,		ConPosX_1 = ConPosX_1, ConPosY_1=ConPosY_1, ConLength_1=ConLength_1, ConEdit_1 = 1, MEP_StrConnectionData = MEP_StrConnectionDataDEL 1END