!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_elIF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFIF gs_detlevel_3D_m = 0 THEN ENDres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! SimpleIF GLOB_CONTEXT = 2 THEN res = 24MainBoxMaterial = sMatMATERIAL MainBoxMaterialPEN gs_cont_penRESOL resaddStatus = 0IF GLOB_CONTEXT > 40 AND GLOB_CONTEXT < 50 THEN	addStatus = 3ENDIF				! --- TRANSFORMATION --- !XFORM   MEP_Position[2][1], MEP_Position[3][1], dZx, MEP_Position[1][1],		MEP_Position[2][2], MEP_Position[3][2], dZy, MEP_Position[1][2],		MEP_Position[2][3], MEP_Position[3][3], dZz, MEP_Position[1][3]				! --- HOTSPOT --- !unID = 1HOTSPOT 0, 0, 0, unID: unID=unID+1HOTSPOT A/2, -ConWidth_1/2-ConWallThickness_1,  0, unID: unID=unID+1HOTSPOT -A/2, -ConWidth_1/2-ConWallThickness_1,  0, unID: unID=unID+1HOTSPOT -A/2, ConWidth_1/2+ConWallThickness_1,  0, unID: unID=unID+1HOTSPOT A/2, ConWidth_1/2+ConWallThickness_1, 0, unID: unID=unID+1				! --- MAIN BOX --- !DIM StraightPosition[][]StraightPosition[1][1] = -A/2StraightPosition[1][2] = 0StraightPosition[1][3] = 0StraightPosition[2][1] = 1StraightPosition[2][2] = 0StraightPosition[2][3] = 0StraightPosition[3][1] = 0StraightPosition[3][2] = 1StraightPosition[3][3] = 0CALL "MEP_m_Duct_4" PARAMETERS 	MEP_Position = StraightPosition,								gs_Connections = gs_Connections,								MEP_NumberConnections = MEP_NumberConnections,								MEP_NumConnectionData = MEP_NumConnectionData,								MEP_StrConnectionData = MEP_StrConnectionData,								MEP_StraightLength = A,								gs_AddConnections = gs_AddConnections,								duct_show = 1,								gs_DuctMat = MainBoxMaterial,								MEP_InsShow = 0,								MEP_cline_show_3D = 0,								gs_detlevel_3D_m = gs_detlevel_3D_m,								gs_cont_pen = gs_cont_penIF gs_detlevel_3D_m = 2 THEN	MULx A/0.50	ADDx -0.25		HOTSPOT 0.04+0.12, 0.225*(B/0.45), 0, unID: unID=unID+1		HOTSPOT 0.04+0.12, -0.225*(B/0.45), 0, unID: unID=unID+1		HOTSPOT 0.04+0.35, -0.225*(B/0.45), 0, unID: unID=unID+1		HOTSPOT 0.04+0.35, 0.225*(B/0.45), 0, unID: unID=unID+1		REVOLVE 2, 360, addStatus+16,				0.04, ConWidth_1/2+ConWallThickness_1, 0,				0.16, 0.225*(B/0.45), 0		ADDx 0.16		ROTy 90		EXTRUDE 2, 0, 0, 0.23, addStatus+16+32,			0, 0, 901,			0.225*(B/0.45), 360, 4001		DEL 2		REVOLVE 4, 360, addStatus+32,				0.39, 0.225*(B/0.45), 0,				0.44, 0.17*((ConWidth_1+ConWallThickness_1*2)/0.30), 0,				0.46, 0.17*((ConWidth_1+ConWallThickness_1*2)/0.30), 0,				0.48, ConWidth_1/2+ConWallThickness_1, 0	DEL 2ELSE	MULx A/0.50	ADDx -0.25		HOTSPOT 0.04+0.12, 0.225*(B/0.45), 0, unID: unID=unID+1		HOTSPOT 0.04+0.12, -0.225*(B/0.45), 0, unID: unID=unID+1		HOTSPOT 0.04+0.35, -0.225*(B/0.45), 0, unID: unID=unID+1		HOTSPOT 0.04+0.35, 0.225*(B/0.45), 0, unID: unID=unID+1		REVOLVE 2, 360, addStatus+16,				0.04, ConWidth_1/2+ConWallThickness_1, 0,				0.16, 0.225*(B/0.45), 0		ADDx 0.16		ROTy 90		EXTRUDE 2, 0, 0, 0.23, addStatus+16+32,			0, 0, 901,			0.225*(B/0.45), 360, 4001		DEL 2		REVOLVE 2, 360, addStatus+32,				0.39, 0.225*(B/0.45), 0,				0.48, ConWidth_1/2+ConWallThickness_1, 0	DEL 2ENDIFROTz 90ADDy -A/2ROTx 90	POLY_ 2,		0, 0, 900,		ConWidth_1/2+ConWallThickness_1, 360, 4000ADDz -A	POLY_ 2,		0, 0, 900,		ConWidth_1/2+ConWallThickness_1, 360, 4000DEL 5END