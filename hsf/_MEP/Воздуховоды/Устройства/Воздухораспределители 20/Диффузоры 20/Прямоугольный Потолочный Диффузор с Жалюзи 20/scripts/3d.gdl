!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_elif not(not(show_axis) and GLOB_CONTEXT=2) then call "2dmacro" parameters all, type_symb = type_symbif show_axis then	hotspot 0,0,0	endendifduct_show = duct_show*not(show_axis)res = 24gs_DuctMat = sMatMATERIAL sMatPEN gs_cont_penRESOL res				! --- TRANSFORMATION --- !XFORM   MEP_Position[2][1], MEP_Position[3][1], dZx, MEP_Position[1][1],		MEP_Position[2][2], MEP_Position[3][2], dZy, MEP_Position[1][2],		MEP_Position[2][3], MEP_Position[3][3], dZz, MEP_Position[1][3]operatedMode = 0addStatus = 0IF GLOB_CONTEXT > 40 AND GLOB_CONTEXT < 50 THEN	operatedMode = 1	addStatus = 1ENDIF				! --- MAIN BOX + LOUVERS --- !unID = 1HOTSPOT prismHalfWidth, prismHalfDepth, 0, unID: unID=unID+1HOTSPOT -prismHalfWidth, prismHalfDepth, 0, unID: unID=unID+1HOTSPOT -prismHalfWidth, -prismHalfDepth, 0, unID: unID=unID+1HOTSPOT prismHalfWidth, -prismHalfDepth, 0, unID: unID=unID+1IF gs_detlevel_3D_m = 2 THEN	PRISM_ 10, edgeHeight,		prismHalfWidth, prismHalfDepth, 15,		-prismHalfWidth, prismHalfDepth, 15,		-prismHalfWidth, -prismHalfDepth, 15,		prismHalfWidth, -prismHalfDepth, 15,		prismHalfWidth, prismHalfDepth, -1,		prismInsideHalfWidth, prismInsideHalfDepth, 15,		-prismInsideHalfWidth, prismInsideHalfDepth, 15,		-prismInsideHalfWidth, -prismInsideHalfDepth, 15,		prismInsideHalfWidth, -prismInsideHalfDepth, 15,		prismInsideHalfWidth, prismInsideHalfDepth, -1	ADDz edgeHeight	RULED 4, addStatus+2+4+16+32,		ruledHalfWidth, ruledHalfDepth, 0,		-ruledHalfWidth, ruledHalfDepth, 0,		-ruledHalfWidth, -ruledHalfDepth, 0,		ruledHalfWidth, -ruledHalfDepth, 0,		ruledInsideHalfWidth, ruledInsideHalfDepth, ruledHeight,		-ruledInsideHalfWidth, ruledInsideHalfDepth, ruledHeight,		-ruledInsideHalfWidth, -ruledInsideHalfDepth, ruledHeight,		ruledInsideHalfWidth, -ruledInsideHalfDepth, ruledHeight	DEL 1	IF operatedMode < EPS THEN		divThick = (B-edgeThick*2)/10		louverThick = divThick-0.003		ruledLHeight = ((1/1.1)*louverThick)		IF (ruledLHeight-ZZYZX*0.85) > -EPS THEN ruledLHeight = ZZYZX*0.85		ruledLHalfWidth = A/2-edgeThick-divThick*4		ruledLHalfDepth = divThick		ruledLInsideHalfWidth = ruledLHalfWidth-divThick		RULED 4, 1+2+4+16+32,			ruledLHalfWidth, ruledLHalfDepth, 0,			-ruledLHalfWidth, ruledLHalfDepth, 0,			-ruledLHalfWidth, -ruledLHalfDepth, 0,			ruledLHalfWidth, -ruledLHalfDepth, 0,			ruledLInsideHalfWidth, 0, ruledLHeight,			-ruledLInsideHalfWidth, 0, ruledLHeight,			-ruledLInsideHalfWidth, 0, ruledLHeight,			ruledLInsideHalfWidth, 0, ruledLHeight		FOR i = 1 TO 3			ruledLHalfWidth = ruledLHalfWidth+divThick			ruledLHalfDepth = ruledLHalfDepth+divThick			ruledLInsideHalfWidth = ruledLHalfWidth-louverThick			ruledLInsideHalfDepth = ruledLHalfDepth-louverThick			RULED 4, 4+16+32,				ruledLHalfWidth, ruledLHalfDepth, 0,				-ruledLHalfWidth, ruledLHalfDepth, 0,				-ruledLHalfWidth, -ruledLHalfDepth, 0,				ruledLHalfWidth, -ruledLHalfDepth, 0,				ruledLInsideHalfWidth, ruledLInsideHalfDepth, ruledLHeight,				-ruledLInsideHalfWidth, ruledLInsideHalfDepth, ruledLHeight,				-ruledLInsideHalfWidth, -ruledLInsideHalfDepth, ruledLHeight,				ruledLInsideHalfWidth, -ruledLInsideHalfDepth, ruledLHeight		NEXT i	ENDIFELSE	PRISM_ 5, ZZYZX,		prismHalfWidth, prismHalfDepth, 15,		-prismHalfWidth, prismHalfDepth, 15,		-prismHalfWidth, -prismHalfDepth, 15,		prismHalfWidth, -prismHalfDepth, 15,		prismHalfWidth, prismHalfDepth, -1ENDIF							! --- CONNECTIONS --- !CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,		MEP_NumberConnections = MEP_NumberConnections,		MEP_NumConnectionData = gs_Connections,		gs_AddConnections = gs_AddConnections,		cShow3D = 1,		gs_ConMat = MainBoxMaterial,		MEP_InsShow = 0,		MEP_cline_show_3D = 0,		gs_cont_pen = gs_cont_pen,		ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 1, MEP_StrConnectionData = MEP_StrConnectionDataDEL TOPEND