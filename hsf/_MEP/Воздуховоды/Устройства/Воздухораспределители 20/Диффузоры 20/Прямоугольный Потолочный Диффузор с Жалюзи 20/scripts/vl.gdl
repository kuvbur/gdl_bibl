call "2dmacro" parameters all, type_symb = type_symb! Rect Louver Face Ceiling DiffuserCALL "MEP_m_RequestPlanViewGlobals" PARAMETERS ALL returned_parameters gs_symbtype_2D_mcall "FM_types_MEP" parametersDIM shape_typ_to_validation[3]	shape_typ_to_validation[1] = 1		! Rectangle	shape_typ_to_validation[2] = 1		! Circle	shape_typ_to_validation[3] = 0		! OvalIF GLOB_MODPAR_NAME = "gs_detlevel_3D" OR gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1	PARAMETERS gs_detlevel_3D_m = gs_detlevel_3D_mELSE	IF gs_detlevel_3D_m = 0 THEN gs_detlevel_3D = `Откл.`	IF gs_detlevel_3D_m = 1 THEN gs_detlevel_3D = `Простой`	IF gs_detlevel_3D_m = 2 THEN gs_detlevel_3D = `Детальный`	PARAMETERS gs_detlevel_3D = gs_detlevel_3DENDIFVALUES "gs_detlevel_3D" `Детальный`,`Простой`,`Откл.`VALUES "gs_detlevel_3D_m" 2, 1, 0IF useSysMat AND MEP_StrConnectionData[1][2] <> "0" THEN LOCK "MainBoxMaterial"IF MEP_StrConnectionData[1][2] = "0" THEN LOCK "useSysMat"		! --- MAIN PARAMETERS --- !IF NOT(SetProgram) THEN	IF ZZYZX < edgeHeight+0.001 THEN		ZZYZX = edgeHeight+0.001		PARAMETERS ZZYZX = ZZYZX	ENDIF	VALUES "ZZYZX" RANGE [edgeHeight+0.001,]	IF A < (edgeThick+ruledHeight*1.75)*2+0.001 THEN		A = (edgeThick+ruledHeight*1.75)*2+0.001		PARAMETERS A = A	ENDIF	VALUES "A" RANGE [(edgeThick+ruledHeight*1.75)*2+0.001,]	IF B < (edgeThick+ruledHeight*1.75)*2+0.001 THEN		B = (edgeThick+ruledHeight*1.75)*2+0.001		PARAMETERS B = B	ENDIF	IF B > A THEN		B = A		PARAMETERS B = B	ENDIF	VALUES "B" RANGE [(edgeThick+ruledHeight*1.75)*2+0.001, A]ENDIF					! --- CONNECTIONS --- !PARAMETERS MEP_NumberConnections = 1					! --- THE ONLY CONNECTION --- !ConID = 1ConName = `Соединение Конца Воздуховода`ConDVecX = 0ConDVecY = 0ConDVecZ = 1ConWVecX = 1ConWVecY = 0ConWVecZ = 0IF NOT(SetProgram) THEN	IF ABS(MEP_NumConnectionData[1][21] - 2) < EPS THEN		mm = MIN(A-(edgeThick+ruledHeight*1.75)*2-ConWallThickness_1*2 , B-(edgeThick+ruledHeight*1.75)*2)-ConWallThickness_1*2		IF mm < 0.001 THEN mm = 0.001		VALUES "ConWidth_1" RANGE (0, mm]		PARAMETERS ConDepth_1 = ConWidth_1		HIDEPARAMETER "ConDepth_1"	ELSE		mm = A-(edgeThick+ruledHeight*1.75)*2-ConWallThickness_1*2		IF mm < 0.001 THEN mm = 0.001		VALUES "ConWidth_1" RANGE (0, mm]		mm = B-(edgeThick+ruledHeight*1.75)*2-ConWallThickness_1*2		IF mm < 0.001 THEN mm = 0.001		VALUES "ConDepth_1" RANGE (0, mm]	ENDIF	mm = MIN((A-(edgeThick+ruledHeight*1.75)*2-ConWidth_1)/2,  (B-(edgeThick+ruledHeight*1.75)*2-ConDepth_1)/2)	IF mm < 0.001 THEN mm = 0.001	VALUES "ConWallThickness_1" RANGE [0, mm]	PARAMETERS ConPosX_1 = 0	PARAMETERS ConPosY_1 = 0	PARAMETERS ConPosZ_1 = ZZYZXENDIFCALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	gs_Connections = gs_Connections,	MEP_NumConnectionData = MEP_NumConnectionData,	MEP_StrConnectionData = MEP_StrConnectionData,	MEP_NumberConnections = MEP_NumberConnections,	gs_AddConnections = gs_AddConnections,	ConName = ConName,	ConID = ConID,	ConPosX = ConPosX_1,	ConPosY = ConPosY_1,	ConPosZ = ConPosZ_1,	ConDVecX = ConDVecX,	ConDVecY = ConDVecY,	ConDVecZ = ConDVecZ,	ConWVecX = ConWVecX,	ConWVecY = ConWVecY,	ConWVecZ = ConWVecZ,	ConNominalWidth = ConNominalWidth_1,	ConNominalDepth = ConNominalDepth_1,	ConWidth = ConWidth_1,	ConDepth = ConDepth_1,	ConLength = ConLength_1,	ConWallThickness = ConWallThickness_1,	ConConnectorWidth = ConConnectorWidth_1,	ConConnectorDepth = ConConnectorDepth_1,	ConConnectorWidth2 = ConConnectorWidth2_1,	ConToolType = ConToolType_1,	ConStatus = ConStatus_1,	ConType_1 = ConType_1,	ConConnectorType_1 = ConConnectorType_1,	ConSystem_1 = ConSystem_1,	ui_page_idx = ui_page_idx,	shape_typ_to_validation = shape_typ_to_validation,	MEP_enabled_geometry_mod = 1PARAMETERS MEP_ConInfo[ConID][1] = STR(ConToolType_1, 1, 0)PARAMETERS MEP_ConInfo[ConID][2] = ConNamePARAMETERS MEP_ConInfo[ConID][3] = "0"IF SetProgram THEN	SetProgram = 0	PARAMETERS SetProgram = SetProgramENDIFFOR k = 1 TO MEP_NumberConnections	FOR i = 1 TO 9		j = 1+INT((i-0.5)/3)		MEP_NumConnectionData[k][i] = outCoords[j+3*(k-1)][i-3*(j-1)]		PARAMETERS MEP_NumConnectionData[k][i] = MEP_NumConnectionData[k][i]	NEXT iNEXT k