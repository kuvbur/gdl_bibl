
! Sidewall Grill


UI_DIALOG `Специальные Параметры MEP`


DIM ui_set_str[5][2]
	ui_set_str[1][1] = "(A) "+`Ширина`
	ui_set_str[2][1] = "(B) "+`Длина`
	ui_set_str[3][1] = "(C) "+`Длина Соединения`
	ui_set_str[4][1] = `Тип Решетки`
	ui_set_str[5][1] = `Размер Жалюзи`

	ui_set_str[1][2] = "ConWidth_1"
	ui_set_str[2][2] = "ConDepth_1"
	ui_set_str[3][2] = "ConLength_1"
	ui_set_str[4][2] = "grilleTypeStr"
	ui_set_str[5][2] = "louverSize"

DIM ui_set_attr[6][9]
	FOR i = 2 TO 6
		FOR j = 1 TO 9
			ui_set_attr[i][j] = 0
		NEXT j
	NEXT i
	ui_set_attr[1][1] = 1
	ui_set_attr[1][2] = 3
	ui_set_attr[1][3] = 220
	ui_set_attr[1][4] = 50
	ui_set_attr[1][5] = 117
	ui_set_attr[1][6] = 100
	ui_set_attr[1][7] = 15
	ui_set_attr[1][8] = 118
	ui_set_attr[1][9] = 23

DIM ui_vis_attr[5][4]
	FOR i = 1 TO 5
		FOR j = 1 TO 4
			ui_vis_attr[i][j] = 0
		NEXT j
	NEXT i
	ui_vis_attr[1][1] = 3
	ui_vis_attr[1][2] = 1
	ui_vis_attr[1][3] = 6
	ui_vis_attr[1][4] = 7

	ui_vis_attr[2][1] = 1
	ui_vis_attr[2][2] = 237
	ui_vis_attr[2][3] = 68
	ui_vis_attr[2][4] = 14

DIM ui_con_type[1]
	ui_con_type[1] = 0

IF useSysMat AND MEP_StrConnectionData[1][2] <> "0" THEN
	ui_vis_attr[5][2] = 4
ELSE
	ui_vis_attr[5][2] = 0
ENDIF

IF MEP_StrConnectionData[1][2] = "0" THEN
	ui_vis_attr[4][2] = 4
ELSE
	ui_vis_attr[4][2] = 0
ENDIF

CALL "MEP_m_ConnectionsMEP_4" PARAMETERS ALL ui_set_str = ui_set_str,
											ui_set_attr = ui_set_attr,
											ui_vis_attr = ui_vis_attr,
											ui_centerline_vars = 0,
											ui_con_type = ui_con_type,
											ui_system_var_pos = 1


setPage = 1
ui_cur_page = 0
FOR j = 1 TO VARDIM1(ui_page_idx)
	IF ui_page_idx[j] = setPage THEN ui_cur_page = j
NEXT j

IF ABS(gs_ui_current_page - ui_cur_page) < EPS THEN
	UI_PAGE gs_ui_current_page

	UI_PICT grilleType, 22, 65, 159, 156, 1
ENDIF
