!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_elif not(not(show_axis) and GLOB_CONTEXT=2) then call "2dmacro" parameters all, type_symb = type_symbif show_axis then	hotspot 0,0,0	endendifduct_show = duct_show*not(show_axis)MainBoxMaterial = sMatMATERIAL MainBoxMaterialPEN gs_cont_pendTWidth = ConWidth_1+ConWallThickness_1*2dTDepth = ConDepth_1+ConWallThickness_1*2				! --- TRANSFORMATION --- !XFORM   MEP_Position[2][1], MEP_Position[3][1], dZx, MEP_Position[1][1],		MEP_Position[2][2], MEP_Position[3][2], dZy, MEP_Position[1][2],		MEP_Position[2][3], MEP_Position[3][3], dZz, MEP_Position[1][3]unID = 1HOTSPOT prismHalfWidth, prismHalfDepth, 0, unID: unID=unID+1HOTSPOT -prismHalfWidth, prismHalfDepth, 0, unID: unID=unID+1HOTSPOT -prismHalfWidth, -prismHalfDepth, 0, unID: unID=unID+1HOTSPOT prismHalfWidth, -prismHalfDepth, 0, unID: unID=unID+1HOTSPOT prismInsideHalfWidth, prismInsideHalfDepth, 0, unID: unID=unID+1HOTSPOT -prismInsideHalfWidth, prismInsideHalfDepth, 0, unID: unID=unID+1HOTSPOT -prismInsideHalfWidth, -prismInsideHalfDepth, 0, unID: unID=unID+1HOTSPOT prismInsideHalfWidth, -prismInsideHalfDepth, 0, unID: unID=unID+1HOTSPOT prismInsideHalfWidth, prismInsideHalfDepth, ConLength_1, unID: unID=unID+1HOTSPOT -prismInsideHalfWidth, prismInsideHalfDepth, ConLength_1, unID: unID=unID+1HOTSPOT -prismInsideHalfWidth, -prismInsideHalfDepth, ConLength_1, unID: unID=unID+1HOTSPOT prismInsideHalfWidth, -prismInsideHalfDepth, ConLength_1, unID: unID=unID+1IF gs_detlevel_3D_m = 2 THEN	ADDz -frameThick	PRISM_ 10, frameThick,		prismHalfWidth, prismHalfDepth, 15,		-prismHalfWidth, prismHalfDepth, 15,		-prismHalfWidth, -prismHalfDepth, 15,		prismHalfWidth, -prismHalfDepth, 15,		prismHalfWidth, prismHalfDepth, -1,		prismInsideHalfWidth, prismInsideHalfDepth, 15,		-prismInsideHalfWidth, prismInsideHalfDepth, 15,		-prismInsideHalfWidth, -prismInsideHalfDepth, 15,		prismInsideHalfWidth, -prismInsideHalfDepth, 15,		prismInsideHalfWidth, prismInsideHalfDepth, -1	IF MIN(ConWidth_1+ConWallThickness_1*2, ConDepth_1+ConWallThickness_1*2) > 0.03 THEN		louverDepth = MAX(frameThick, MIN(0.025, ConLength_1))		IF grilleType < 3 THEN		louverWidth = dTWidth-0.002			num = INT(dTDepth/louverSize)			IF FRA(dTDepth/louverSize)<EPS THEN num = num-1		FOR i = 1 TO num+1				ADD -louverWidth/2, num*(louverSize/2)-louverSize*(i-1), 0			ROTx 90				RECT louverWidth, louverDepth			DEL 2		NEXT i		ENDIF		IF grilleType = 1 OR grilleType = 3 THEN		louverWidth = dTDepth-0.002			num = INT(dTWidth/louverSize)			IF FRA(dTWidth/louverSize)<EPS THEN num = num-1		FOR i = 1 TO num+1				ADD num*(louverSize/2)-louverSize*(i-1), -louverWidth/2, 0			ROTz 90			ROTx 90				RECT louverWidth, louverDepth			DEL 3		NEXT i	ENDIF	ENDIF	DEL 1ELSE	ADDz -frameThick	PRISM_ 5, frameThick,		prismHalfWidth, prismHalfDepth, 15,		-prismHalfWidth, prismHalfDepth, 15,		-prismHalfWidth, -prismHalfDepth, 15,		prismHalfWidth, -prismHalfDepth, 15,		prismHalfWidth, prismHalfDepth, -1	DEL 1ENDIF				! --- CONNECTIONS --- !CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,		MEP_NumberConnections = MEP_NumberConnections,		MEP_NumConnectionData = gs_Connections,		gs_AddConnections = gs_AddConnections,		cShow3D = 1,		gs_ConMat = MainBoxMaterial,		MEP_InsShow = 0,		MEP_cline_show_3D = 0,		gs_cont_pen = gs_cont_pen,		ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 1, MEP_StrConnectionData = MEP_StrConnectionDataDEL TOPEND