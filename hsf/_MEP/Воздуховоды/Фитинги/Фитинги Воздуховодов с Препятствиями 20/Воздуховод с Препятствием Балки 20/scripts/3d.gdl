!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_elgs_DuctMat = sMatduct_show = duct_show*not(show_axis)IF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFIF gs_detlevel_3D_m = 2 THEN res = 24IF gs_detlevel_3D_m = 1 THEN res = 9IF gs_detlevel_3D_m = 0 THEN res = 12IF gs_detlevel_3D_m = 0 THEN ENDres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! SimplePEN gs_cont_penRESOL resoperatedMode = 0addStatus = 0IF GLOB_CONTEXT > 40 AND GLOB_CONTEXT < 50 THEN	operatedMode = 1	addStatus = 7ENDIFXFORM   MEP_Position[2][1], MEP_Position[3][1], dZx, MEP_Position[1][1],		MEP_Position[2][2], MEP_Position[3][2], dZy, MEP_Position[1][2],		MEP_Position[2][3], MEP_Position[3][3], dZz, MEP_Position[1][3]HptsUnIDBeginForConnEnds = 30000hceID_1 = HptsUnIDBeginForConnEnds+100hceID_2 = HptsUnIDBeginForConnEnds+200IF connectedA + connectedB < 1 + EPS THEN	unID = 1	HOTSPOT 0, 0, 0, hceID_1, MEP_StraightLength, 1+256*(connectedA < EPS)	HOTSPOT -1, 0, 0, HptsUnIDBeginForConnEnds, MEP_StraightLength, 3	IF ABS(connectedA) < EPS AND ABS(connectedB - 1) < EPS THEN		hID = hceID_2+1		HOTSPOT MEP_StraightLength, 0, 0, hceID_2	ELSE		hID = hceID_2	ENDIF	HOTSPOT MEP_StraightLength, 0, 0, hID, MEP_StraightLength, 2+128*connectedBELSE	HOTSPOT 0, 0, 0, hceID_1	HOTSPOT MEP_StraightLength, 0, 0, hceID_2ENDIFMULz -1IF ((cl_show and show_axis_and) or show_axis) AND operatedMode < EPS  THEN	pen MEP_cline_pen	LIN_ 0, 0, 0, dThLen, 0, 0	LIN_ dThLen, 0, 0, dThLen, 0, -oHeight	LIN_ dThLen, 0, -oHeight, MEP_StraightLength-dThLen, 0, -oHeight	LIN_ MEP_StraightLength-dThLen, 0, -oHeight, MEP_StraightLength-dThLen, 0, 0	LIN_ MEP_StraightLength-dThLen, 0, 0, MEP_StraightLength, 0, 0	PEN gs_cont_penENDIFIF show_axis THEN ENDMATERIAL gs_DuctMatIF flangeA AND duct_show THEN	ROTy 90	PRISM_ 10, connectorLength,		connectorHalfHeightA, connectorHalfWidthA, 15,		-connectorHalfHeightA, connectorHalfWidthA, 15,		-connectorHalfHeightA, -connectorHalfWidthA, 15,		connectorHalfHeightA, -connectorHalfWidthA, 15,		connectorHalfHeightA, connectorHalfWidthA, -1,		ductHalfHeight, ductHalfWidth, 15,		-ductHalfHeight, ductHalfWidth, 15,		-ductHalfHeight, -ductHalfWidth, 15,		ductHalfHeight, -ductHalfWidth, 15,		ductHalfHeight, ductHalfWidth, -1	DEL 1ENDIFIF flangeB AND duct_show THEN	ADDx MEP_StraightLength-connectorLength	ROTy 90	PRISM_ 10, connectorLength,		connectorHalfHeightB, connectorHalfWidthB, 15,		-connectorHalfHeightB, connectorHalfWidthB, 15,		-connectorHalfHeightB, -connectorHalfWidthB, 15,		connectorHalfHeightB, -connectorHalfWidthB, 15,		connectorHalfHeightB, connectorHalfWidthB, -1,		ductHalfHeight, ductHalfWidth, 15,		-ductHalfHeight, ductHalfWidth, 15,		-ductHalfHeight, -ductHalfWidth, 15,		ductHalfHeight, -ductHalfWidth, 15,		ductHalfHeight, ductHalfWidth, -1	DEL 2ENDIFww = ductHalfWidthhh = ductHalfHeightFOR k = 1 TO 1+MEP_InsShow*show_isol	IF NOT(duct_show) AND k = 1 THEN GOTO 10	ROTx 90	ADDz -ww	PUT shorteningA*(k = 1 AND flangeA), hh, 15	PUT dThLen+hh, hh, 15	PUT dThLen+hh, -oHeight+hh, 15	PUT MEP_StraightLength-dThLen-hh, -oHeight+hh, 15	PUT MEP_StraightLength-dThLen-hh, hh, 15	PUT MEP_StraightLength-shorteningB*(k = 1 AND flangeB), hh, 7+addStatus	PUT MEP_StraightLength-shorteningB*(k = 1 AND flangeB), -hh, 15	PUT MEP_StraightLength-dThLen+hh, -hh, 15	PUT MEP_StraightLength-dThLen+hh, -oHeight-hh, 15	PUT dThLen-hh, -oHeight-hh, 15	PUT dThLen-hh, -hh, 15	PUT shorteningA*(k = 1 AND flangeA), -hh, 7+addStatus	PUT shorteningA*(k = 1 AND flangeA), hh, -1	PRISM_ NSP/3, ww*2,		GET(NSP)	DEL 2	10:	 MATERIAL gs_InsMat	 ww  = ww + MEP_InsThickness	 hh = hh + MEP_InsThicknessNEXT kDEL 2