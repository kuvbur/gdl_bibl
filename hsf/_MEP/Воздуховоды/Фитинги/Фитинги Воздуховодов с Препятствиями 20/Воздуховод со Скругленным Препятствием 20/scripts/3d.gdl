!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_elgs_DuctMat = sMatIF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFduct_show = duct_show*not(show_axis)IF gs_detlevel_3D_m = 0 THEN ENDres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! SimplePEN gs_cont_penRESOL resoperatedMode = 0addStatus = 0IF GLOB_CONTEXT > 40 AND GLOB_CONTEXT < 50 THEN	operatedMode = 1	addStatus = 3ENDIFalpha_Res = 90/(INT(res/3))res_B = INT(rd_angle/alpha_Res)+1*(((rd_angle/alpha_Res)-INT(rd_angle/alpha_Res))>=0.5)IF NOT(res_B) THEN res_B = 1alpha_i = rd_angle/res_BXFORM   MEP_Position[2][1], MEP_Position[3][1], dZx, MEP_Position[1][1],		MEP_Position[2][2], MEP_Position[3][2], dZy, MEP_Position[1][2],		MEP_Position[2][3], MEP_Position[3][3], dZz, MEP_Position[1][3]HptsUnIDBeginForConnEnds = 30000hceID_1 = HptsUnIDBeginForConnEnds+100hceID_2 = HptsUnIDBeginForConnEnds+200HOTSPOT 0, 0, 0, hceID_1HOTSPOT MEP_StraightLength, 0, oHeight, hceID_2IF ((MEP_cline_show and show_axis_and) or show_axis) AND operatedMode < EPS THEN	pen MEP_cline_pen	MODEL WIRE	ROTx 90	alpha = 0	FOR i = 1 TO res_B+1		PUT bRadius*SIN(alpha), bRadius-bRadius*COS(alpha), 1		alpha = alpha + alpha_i	NEXT i	alpha = rd_angle	FOR i = 1 TO res_B+1		PUT MEP_StraightLength-bRadius*SIN(alpha), oHeight-bRadius+bRadius*COS(alpha), 1*(i < res_B+1)		alpha = alpha - alpha_i	NEXT i	PUT 0, oHeight, 0	POLY_ NSP/3,		GET(NSP)	DEL 1	MODEL SURFACE	PEN gs_cont_penENDIFMATERIAL gs_DuctMatIF duct_show THEN GOSUB 100IF flangeA AND duct_show THEN	ROTy 90	IF ABS(shapeType - 1) < EPS THEN		PRISM_ 10, connectorLength,			connectorHalfHeightA, connectorHalfWidthA, 15,			-connectorHalfHeightA, connectorHalfWidthA, 15,			-connectorHalfHeightA, -connectorHalfWidthA, 15,			connectorHalfHeightA, -connectorHalfWidthA, 15,			connectorHalfHeightA, connectorHalfWidthA, -1,			ductHalfHeight, ductHalfWidth, 15,			-ductHalfHeight, ductHalfWidth, 15,			-ductHalfHeight, -ductHalfWidth, 15,			ductHalfHeight, -ductHalfWidth, 15,			ductHalfHeight, ductHalfWidth, -1	ELSE		IF ABS(ductHalfWidth - ductHalfHeight) < EPS THEN		PRISM_ 4, connectorLength,				0, 0, 979,				connectorHalfWidthA, 360, 4079,				0, 0, 979,				ductHalfWidth, 360, 4079		ELSE			IF ductHalfWidth > ductHalfHeight THEN				rr = ductHalfHeight				crr = connectorHalfHeightA				PRISM_ 12, connectorLength,					-connectorHalfHeightA, connectorHalfWidthA-crr, 79,					-connectorHalfHeightA, -connectorHalfWidthA+crr, 79,					connectorHalfHeightA, -connectorHalfWidthA+crr, 1079,					connectorHalfHeightA, connectorHalfWidthA-crr, 79,					-connectorHalfHeightA, connectorHalfWidthA-crr, 1079,					-connectorHalfHeightA, connectorHalfWidthA-crr, -1,					-ductHalfHeight, ductHalfWidth-rr, 79,					-ductHalfHeight, -ductHalfWidth+rr, 79,					ductHalfHeight, -ductHalfWidth+rr, 1079,					ductHalfHeight, ductHalfWidth-rr, 79,					-ductHalfHeight, ductHalfWidth-rr, 1079,					-ductHalfHeight, ductHalfWidth-rr, -1			ELSE				rr = ductHalfWidth				crr = connectorHalfWidthA				PRISM_ 12, connectorLength,					connectorHalfHeightA-crr, connectorHalfWidthA, 79,					-connectorHalfHeightA+crr, connectorHalfWidthA, 79,					-connectorHalfHeightA+crr, -connectorHalfWidthA, 1079,					connectorHalfHeightA-crr, -connectorHalfWidthA, 79,					connectorHalfHeightA-crr, connectorHalfWidthA, 1079,					connectorHalfHeightA-crr, connectorHalfWidthA, -1,					ductHalfHeight-rr, ductHalfWidth, 79,					-ductHalfHeight+rr, ductHalfWidth, 79,					-ductHalfHeight+rr, -ductHalfWidth, 1079,					ductHalfHeight-rr, -ductHalfWidth, 79,					ductHalfHeight-rr, ductHalfWidth, 1079,					ductHalfHeight-rr, ductHalfWidth, -1			ENDIF		ENDIF	ENDIF	DEL 1ENDIFIF flangeB AND duct_show THEN	ADD MEP_StraightLength-connectorLength, 0, oHeight	ROTy 90	IF ABS(shapeType - 1) < EPS THEN		PRISM_ 10, connectorLength,			connectorHalfHeightB, connectorHalfWidthB, 15,			-connectorHalfHeightB, connectorHalfWidthB, 15,			-connectorHalfHeightB, -connectorHalfWidthB, 15,			connectorHalfHeightB, -connectorHalfWidthB, 15,			connectorHalfHeightB, connectorHalfWidthB, -1,			ductHalfHeight, ductHalfWidth, 15,			-ductHalfHeight, ductHalfWidth, 15,			-ductHalfHeight, -ductHalfWidth, 15,			ductHalfHeight, -ductHalfWidth, 15,			ductHalfHeight, ductHalfWidth, -1	ELSE		IF ABS(ductHalfWidth - ductHalfHeight) < EPS THEN		PRISM_ 4, connectorLength,				0, 0, 979,				connectorHalfWidthB, 360, 4079,				0, 0, 979,				ductHalfWidth, 360, 4079		ELSE			IF ductHalfWidth > ductHalfHeight THEN				rr = ductHalfHeight				crr = connectorHalfHeightB				PRISM_ 12, connectorLength,					-connectorHalfHeightB, connectorHalfWidthB-crr, 79,					-connectorHalfHeightB, -connectorHalfWidthB+crr, 79,					connectorHalfHeightB, -connectorHalfWidthB+crr, 1079,					connectorHalfHeightB, connectorHalfWidthB-crr, 79,					-connectorHalfHeightB, connectorHalfWidthB-crr, 1079,					-connectorHalfHeightB, connectorHalfWidthB-crr, -1,					-ductHalfHeight, ductHalfWidth-rr, 79,					-ductHalfHeight, -ductHalfWidth+rr, 79,					ductHalfHeight, -ductHalfWidth+rr, 1079,					ductHalfHeight, ductHalfWidth-rr, 79,					-ductHalfHeight, ductHalfWidth-rr, 1079,					-ductHalfHeight, ductHalfWidth-rr, -1			ELSE				rr = ductHalfWidth				crr = connectorHalfWidthB				PRISM_ 12, connectorLength,					connectorHalfHeightB-crr, connectorHalfWidthB, 79,					-connectorHalfHeightB+crr, connectorHalfWidthB, 79,					-connectorHalfHeightB+crr, -connectorHalfWidthB, 1079,					connectorHalfHeightB-crr, -connectorHalfWidthB, 79,					connectorHalfHeightB-crr, connectorHalfWidthB, 1079,					connectorHalfHeightB-crr, connectorHalfWidthB, -1,					ductHalfHeight-rr, ductHalfWidth, 79,					-ductHalfHeight+rr, ductHalfWidth, 79,					-ductHalfHeight+rr, -ductHalfWidth, 1079,					ductHalfHeight-rr, -ductHalfWidth, 79,					ductHalfHeight-rr, ductHalfWidth, 1079,					ductHalfHeight-rr, ductHalfWidth, -1			ENDIF		ENDIF	ENDIF	DEL 2ENDIFIF MEP_InsShow and show_isol and not(show_axis) THEN	MATERIAL gs_InsMat	ductHalfWidth = ductHalfWidth + MEP_InsThickness	ductHalfHeight = ductHalfHeight + MEP_InsThickness	GOSUB 100ENDIFDEL 1END100:	PUT -bRadius*SIN(alpha_i), 0, bRadius-bRadius*COS(alpha_i), 0	alpha = 0	FOR i = 1 TO res_B+1		PUT bRadius*SIN(alpha), 0, bRadius-bRadius*COS(alpha), 0		alpha = alpha + alpha_i	NEXT i	alpha = rd_angle	FOR i = 1 TO res_B+1		PUT MEP_StraightLength-bRadius*SIN(alpha), 0, oHeight-bRadius+bRadius*COS(alpha), 0		alpha = alpha - alpha_i	NEXT i	PUT MEP_StraightLength-bRadius*SIN(alpha), 0, oHeight-bRadius+bRadius*COS(alpha), 0	red = COS(alpha_i/2)/1	IF ABS(shapeType - 1) < EPS THEN		TUBE 5, NSP/4, addStatus+16+32,			-ductHalfWidth, ductHalfHeight*red, 0,			ductHalfWidth, ductHalfHeight*red, 0,			ductHalfWidth, -ductHalfHeight*red, 0,			-ductHalfWidth, -ductHalfHeight*red, 0,			-ductHalfWidth, ductHalfHeight*red, 0,			GET(NSP)	ELSE		MULy 1/red		IF ABS(ductHalfWidth - ductHalfHeight) < EPS THEN			TUBE 2, NSP/4, addStatus+16+32,				0, 0, 901,				ductHalfWidth*red, 360, 4001,				GET(NSP)		ELSE			IF ductHalfWidth > ductHalfHeight THEN				rr = ductHalfHeight				TUBE 5, NSP/4, addStatus+16+32,					(-ductHalfWidth+rr)*red, ductHalfHeight*red, 1,					(ductHalfWidth-rr)*red, ductHalfHeight*red, 1,					(ductHalfWidth-rr)*red, -ductHalfHeight*red, 1001,					(-ductHalfWidth+rr)*red, -ductHalfHeight*red, 1,					(-ductHalfWidth+rr)*red, ductHalfHeight*red, 1001,					GET(NSP)			ELSE				rr = ductHalfWidth				TUBE 5, NSP/4, addStatus+16+32,					ductHalfWidth*red, (ductHalfHeight-rr)*red, 1,					ductHalfWidth*red, (-ductHalfHeight+rr)*red, 1,					-ductHalfWidth*red, (-ductHalfHeight+rr)*red, 1001,					-ductHalfWidth*red, (ductHalfHeight-rr)*red, 1,					ductHalfWidth*red, (ductHalfHeight-rr)*red, 1001,					GET(NSP)			ENDIF		ENDIF		DEL 1	ENDIFRETURN