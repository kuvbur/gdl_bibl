UI_DIALOG `Специальные Параметры MEP`DIM ui_set_str[6][2]	ui_set_str[1][1] = "(A) "+`Длина Соединения`	ui_set_str[2][1] = `Тип Врезки`	ui_set_str[3][1] = "(B) "+`Длина Скоса`	ui_set_str[4][1] = `Изоляция`	ui_set_str[5][1] = `Толщина`	ui_set_str[1][2] = "ConLength_1"	ui_set_str[2][2] = "MEP_TakeoffType"	ui_set_str[3][2] = "InclPartLength"	ui_set_str[4][2] = "MEP_InsShow"	ui_set_str[5][2] = "MEP_InsThickness"pID = 6ui_set_str[pID][1] = `Толщина стенки`ui_set_str[pID][2] = "mep_th_elem"DIM ui_set_attr[7][9]	FOR i = 2 TO 7		FOR j = 1 TO 9			ui_set_attr[i][j] = 0		NEXT j	NEXT i	ui_set_attr[1][1] = 1	ui_set_attr[1][2] = 3	ui_set_attr[1][3] = 220	ui_set_attr[1][4] = 50	ui_set_attr[1][5] = 127	ui_set_attr[1][6] = 90	ui_set_attr[1][7] = 15	ui_set_attr[1][8] = 128	ui_set_attr[1][9] = 23DIM ui_vis_attr[6][5]	FOR i = 1 TO 6		FOR j = 1 TO 5			ui_vis_attr[i][j] = 0		NEXT j	NEXT i	ui_vis_attr[1][1] = 4	ui_vis_attr[1][2] = 1	ui_vis_attr[1][3] = 6	ui_vis_attr[1][4] = 3	ui_vis_attr[1][5] = 4	ui_vis_attr[2][1] = 1	ui_vis_attr[2][2] = 237	ui_vis_attr[2][3] = 68	ui_vis_attr[2][4] = 14DIM ui_con_type[1]	ui_con_type[1] = 0IF useSysMat AND MEP_StrConnectionData[1][2] <> "0" THEN	ui_vis_attr[5][2] = 4	ui_vis_attr[6][2] = 4ELSE	ui_vis_attr[5][2] = 0	IF MEP_InsShow THEN		ui_vis_attr[6][2] = 0	ELSE		ui_vis_attr[6][2] = 4	ENDIFENDIFIF MEP_InsShow THEN	ui_set_attr[6][2] = 0ELSE	ui_set_attr[6][2] = 4ENDIFIF MEP_StrConnectionData[1][2] = "0" THEN	ui_vis_attr[4][2] = 4ELSE	ui_vis_attr[4][2] = 0ENDIFIF NeedHtpstoInclPartLength < 0.5 THEN	ui_set_attr[4][2] = 4ELSE	ui_set_attr[4][2] = 0ENDIFIF ABS(MEP_TakeoffTypeID - 1) < EPS THEN	IF ABS(DuctShapeType - 2) < EPS OR ABS(DuctShapeType - 3) < EPS THEN	ELSE		IF ABS(MEP_NumConnectionData[1][21] - 2) < EPS THEN			Take_off_Size = MEP_NumConnectionData[1][12]+MEP_NumConnectionData[1][14]*2		ELSE			Take_off_Size = MEP_NumConnectionData[1][13]+MEP_NumConnectionData[1][14]*2		ENDIF		IF arched THEN			IF Take_off_Size < duct_radius+EPS THEN				ui_set_attr[4][2] = 0			ELSE				ui_set_attr[4][2] = 4			ENDIF		ELSE			IF ABS(DuctShapeType - 1) < EPS THEN limit_1 = (DuctWidth*reverse + DuctDepth*(reverse = 0) - Take_off_Size)/2			IF ABS(DuctShapeType - 3) < EPS THEN limit_1 = (MAX(DuctWidth, DuctDepth) - MIN(DuctWidth, DuctDepth) - Take_off_Size)/2			IF limit_1 < EPS THEN				ui_set_attr[4][2] = 4			ELSE				ui_set_attr[4][2] = 0			ENDIF		ENDIF	ENDIFELSE	ui_set_attr[4][2] = 0ENDIFMEP_NumberConnections = 1CALL "MEP_m_ConnectionsMEP_4" PARAMETERS ALL ui_set_str = ui_set_str,											ui_set_attr = ui_set_attr,											ui_vis_attr = ui_vis_attr,											ui_centerline_vars = 1,											ui_con_type = ui_con_type,											ui_system_var_pos = 1setPage = 1ui_cur_page = 0FOR j = 1 TO VARDIM1(ui_page_idx)	IF ui_page_idx[j] = setPage THEN ui_cur_page = jNEXT jIF ABS(gs_ui_current_page - ui_cur_page) < EPS THEN	UI_PAGE gs_ui_current_page	UI_PICT MEP_TakeoffTypeID, 30, 45, 156, 200, 1ENDIF