! Duct Trousers! Zsolt Feke/2008call "FM_types_MEP" parametersDIM shape_typ_to_validation[3]	shape_typ_to_validation[1] = 1		! Rectangle	shape_typ_to_validation[2] = 0		! Circle	shape_typ_to_validation[3] = 0		! OvalVALUES "branchAngle" RANGE [30, 120]VALUES "MEP_InsThickness" RANGE(0, ]LOCK "gs_DuctMat", "gs_InsMat"LOCK "useSysMat"gs_ui = 1ang = branchAngle/2					! --- CONNECTIONS --- !	PARAMETERS MEP_NumberConnections = 3					! --- 1st CONNECTION --- !	ConID = 1	ConName = `Основное`	ConDVecX = -1	ConDVecY = 0	ConDVecZ = 0	ConWVecX = 0	ConWVecY = -1	ConWVecZ = 0	IF NOT(setProgram) THEN		IF GLOB_MODPAR_NAME = "ConDepth_2" THEN			ConDepth_1 = ConDepth_2			PARAMETERS ConDepth_1 = ConDepth_1			ConDepth_3 = ConDepth_2			PARAMETERS ConDepth_3 = ConDepth_3		ENDIF		IF GLOB_MODPAR_NAME = "ConDepth_3" THEN			ConDepth_1 = ConDepth_3			PARAMETERS ConDepth_1 = ConDepth_1			ConDepth_2 = ConDepth_3			PARAMETERS ConDepth_2 = ConDepth_2		ENDIF		IF ABS(ConDepth_1 - ConDepth_2) > EPS OR ABS(ConDepth_1 - ConDepth_3) > EPS THEN			ConDepth_2 = ConDepth_1			PARAMETERS ConDepth_2 = ConDepth_2			ConDepth_3 = ConDepth_1			PARAMETERS ConDepth_3 = ConDepth_3		ENDIF		IF GLOB_MODPAR_NAME = "ConWallThickness_2" THEN			ConWallThickness_1 = ConWallThickness_2			PARAMETERS ConWallThickness_1 = ConWallThickness_1			ConWallThickness_3 = ConWallThickness_2			PARAMETERS ConWallThickness_3 = ConWallThickness_3		ENDIF		IF GLOB_MODPAR_NAME = "ConWallThickness_3" THEN			ConWallThickness_1 = ConWallThickness_3			PARAMETERS ConWallThickness_1 = ConWallThickness_1			ConWallThickness_2 = ConWallThickness_3			PARAMETERS ConWallThickness_2 = ConWallThickness_2		ENDIF		IF ABS(ConWallThickness_1 - ConWallThickness_2) > EPS OR ABS(ConWallThickness_1 - ConWallThickness_3) > EPS THEN			ConWallThickness_2 = ConWallThickness_1			PARAMETERS ConWallThickness_2 = ConWallThickness_2			ConWallThickness_3 = ConWallThickness_1			PARAMETERS ConWallThickness_3 = ConWallThickness_3		ENDIF		isConLength = 1		IF ABS(gs_AddConnections[1][1] - 1) < EPS THEN isConLength = 0		ConLength_1 = ConWallThickness_1*isConLength		PARAMETERS ConLength_1 = ConLength_1		ConPosX_1 = ConLength_1		PARAMETERS ConPosX_1 = ConPosX_1		PARAMETERS ConPosY_1 = 0		PARAMETERS ConPosZ_1 = 0	ELSE		IF ABS(MEP_NumConnectionData[2][13] - ConDepth_2) > EPS THEN			MEP_NumConnectionData[1][13] = MEP_NumConnectionData[2][13]			PARAMETERS MEP_NumConnectionData[1][13] = MEP_NumConnectionData[1][13]			MEP_NumConnectionData[3][13] = MEP_NumConnectionData[2][13]			PARAMETERS MEP_NumConnectionData[3][13] = MEP_NumConnectionData[3][13]		ENDIF		IF ABS(MEP_NumConnectionData[3][13] - ConDepth_3) > EPS THEN			MEP_NumConnectionData[1][13] = MEP_NumConnectionData[3][13]			PARAMETERS MEP_NumConnectionData[1][13] = MEP_NumConnectionData[1][13]			MEP_NumConnectionData[2][13] = MEP_NumConnectionData[3][13]			PARAMETERS MEP_NumConnectionData[2][13] = MEP_NumConnectionData[2][13]		ENDIF	ENDIF	CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,		gs_Connections = gs_Connections,		MEP_NumConnectionData = MEP_NumConnectionData,		MEP_StrConnectionData = MEP_StrConnectionData,		MEP_NumberConnections = MEP_NumberConnections,		gs_AddConnections = gs_AddConnections,		ConName = ConName,		ConID = ConID,		ConPosX = ConPosX_1,		ConPosY = ConPosY_1,		ConPosZ = ConPosZ_1,		ConDVecX = ConDVecX,		ConDVecY = ConDVecY,		ConDVecZ = ConDVecZ,		ConWVecX = ConWVecX,		ConWVecY = ConWVecY,		ConWVecZ = ConWVecZ,		ConNominalWidth = ConNominalWidth_1,		ConNominalDepth = ConNominalDepth_1,		ConWidth = ConWidth_1,		ConDepth = ConDepth_1,		ConLength = ConLength_1,		ConWallThickness = ConWallThickness_1,		ConConnectorWidth = ConConnectorWidth_1,		ConConnectorDepth = ConConnectorDepth_1,		ConConnectorWidth2 = ConConnectorWidth2_1,		ConToolType = ConToolType_1,		ConStatus = ConStatus_1,		ConType_1 = ConType_1,		ConConnectorType_1 = ConConnectorType_1,		ConSystem_1 = ConSystem_1,		MEP_bEnableInsulation = 1,		MEP_InsShow = MEP_InsShow,		gs_ui = gs_ui,		ui_page_idx = ui_page_idx,		shape_typ_to_validation = shape_typ_to_validation,		MEP_enabled_geometry_mod = 1,		MEP_cline_show = MEP_cline_showPARAMETERS MEP_ConInfo[1][1] = STR(ConToolType_1, 1, 0)PARAMETERS MEP_ConInfo[1][2] = ConNamePARAMETERS MEP_ConInfo[1][3] = "0"					! --- 2nd CONNECTION --- !	ConID = 2	ConName = `Патрубок 1`	ConDVecX = COS(ang)	ConDVecY = SIN(ang)	ConDVecZ = 0	ConWVecX = -SIN(ang)	ConWVecY = COS(ang)	ConWVecZ = 0	IF NOT(setProgram) THEN		ConSystem_2 = ConSystem_1		PARAMETERS ConSystem_2 = ConSystem_2		MEP_StrConnectionData[2][2] = MEP_StrConnectionData[1][2]		PARAMETERS MEP_StrConnectionData[2][2] = MEP_StrConnectionData[2][2]		IF GLOB_MODPAR_NAME = "ConWidth_3" THEN			ConWidth_2 = ConWidth_3			PARAMETERS ConWidth_2 = ConWidth_2		ELSE			ConWidth_3 = ConWidth_2			PARAMETERS ConWidth_3 = ConWidth_3		ENDIF		ConType_2 = ConType_1		PARAMETERS ConType_2 = ConType_2		MEP_NumConnectionData[2][21] = MEP_NumConnectionData[1][21]		PARAMETERS MEP_NumConnectionData[2][21] = MEP_NumConnectionData[2][21]		isConLength = 1		IF ABS(gs_AddConnections[2][1] - 1) < EPS THEN isConLength = 0		ConLength_2 = ConWallThickness_1*isConLength		PARAMETERS ConLength_2 = ConLength_2		ConPosX_2 = mainLength+(branchLength-ConLength_2)*COS(ang)		PARAMETERS ConPosX_2 = ConPosX_2		ConPosY_2 = (branchLength-ConLength_2)*SIN(ang)		PARAMETERS ConPosY_2 = ConPosY_2		ConPosZ_2 = 0		PARAMETERS ConPosZ_2 = ConPosZ_2	ELSE		IF ABS(MEP_NumConnectionData[3][12] - ConWidth_3) > EPS THEN			MEP_NumConnectionData[2][12] = MEP_NumConnectionData[3][12]			PARAMETERS MEP_NumConnectionData[2][12] = MEP_NumConnectionData[2][12]		ENDIF	ENDIF	CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,		gs_Connections = gs_Connections,		MEP_NumConnectionData = MEP_NumConnectionData,		MEP_StrConnectionData = MEP_StrConnectionData,		MEP_NumberConnections = MEP_NumberConnections,		gs_AddConnections = gs_AddConnections,		ConName = ConName,		ConID = ConID,		ConPosX = ConPosX_2,		ConPosY = ConPosY_2,		ConPosZ = ConPosZ_2,		ConDVecX = ConDVecX,		ConDVecY = ConDVecY,		ConDVecZ = ConDVecZ,		ConWVecX = ConWVecX,		ConWVecY = ConWVecY,		ConWVecZ = ConWVecZ,		ConNominalWidth = ConNominalWidth_2,		ConNominalDepth = ConNominalDepth_2,		ConWidth = ConWidth_2,		ConDepth = ConDepth_2,		ConLength = ConLength_2,		ConWallThickness = ConWallThickness_2,		ConConnectorWidth = ConConnectorWidth_2,		ConConnectorDepth = ConConnectorDepth_2,		ConConnectorWidth2 = ConConnectorWidth2_2,		ConToolType = ConToolType_2,		ConStatus = ConStatus_2,		ConType_2 = ConType_2,		ConConnectorType_2 = ConConnectorType_2,		ConSystem_2 = ConSystem_2,		gs_ui = gs_ui,		MEP_bEnableInsulation = 1,		MEP_InsShow = MEP_InsShow,		shape_typ_to_validation = shape_typ_to_validation,		MEP_enabled_geometry_mod = 1PARAMETERS MEP_ConInfo[2][1] = STR(ConToolType_2, 1, 0)PARAMETERS MEP_ConInfo[2][2] = ConNamePARAMETERS MEP_ConInfo[2][3] = "0"					! --- 3nd CONNECTION --- !	ConID = 3	ConName = `Патрубок 2`	ConDVecX = COS(ang)	ConDVecY = -SIN(ang)	ConDVecZ = 0	ConWVecX = SIN(ang)	ConWVecY = COS(ang)	ConWVecZ = 0	IF NOT(setProgram) THEN		ConSystem_3 = ConSystem_1		PARAMETERS ConSystem_3 = ConSystem_3		MEP_StrConnectionData[3][2] = MEP_StrConnectionData[1][2]		PARAMETERS MEP_StrConnectionData[3][2] = MEP_StrConnectionData[3][2]		ConType_3 = ConType_1		PARAMETERS ConType_3 = ConType_3		MEP_NumConnectionData[3][21] = MEP_NumConnectionData[1][21]		PARAMETERS MEP_NumConnectionData[3][21] = MEP_NumConnectionData[3][21]		isConLength = 1		IF ABS(gs_AddConnections[3][1] - 1) < EPS THEN isConLength = 0		ConLength_3 = ConWallThickness_1*isConLength		PARAMETERS ConLength_3 = ConLength_3		ConPosX_3 = mainLength+(branchLength-ConLength_3)*COS(ang)		PARAMETERS ConPosX_3 = ConPosX_3		ConPosY_3 = -(branchLength-ConLength_3)*SIN(ang)		PARAMETERS ConPosY_3 = ConPosY_3		ConPosZ_3 = 0		PARAMETERS ConPosZ_3 = ConPosZ_3		IF same_con THEN			ConConnectorType_3 = ConConnectorType_2			PARAMETERS ConConnectorType_3 = ConConnectorType_3			MEP_StrConnectionData[3][1] = MEP_StrConnectionData[2][1]			PARAMETERS MEP_StrConnectionData[3][1] = MEP_StrConnectionData[3][1]			gs_AddConnections[3][1] = gs_AddConnections[2][1]			PARAMETERS gs_AddConnections[3][1] = gs_AddConnections[3][1]			ConConnectorWidth_3 = ConConnectorWidth_2			PARAMETERS ConConnectorWidth_3 = ConConnectorWidth_3			LOCK "ConConnectorType_3", "ConConnectorWidth_3"		ENDIF	ENDIF	CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,		gs_Connections = gs_Connections,		MEP_NumConnectionData = MEP_NumConnectionData,		MEP_StrConnectionData = MEP_StrConnectionData,		MEP_NumberConnections = MEP_NumberConnections,		gs_AddConnections = gs_AddConnections,		ConName = ConName,		ConID = ConID,		ConPosX = ConPosX_3,		ConPosY = ConPosY_3,		ConPosZ = ConPosZ_3,		ConDVecX = ConDVecX,		ConDVecY = ConDVecY,		ConDVecZ = ConDVecZ,		ConWVecX = ConWVecX,		ConWVecY = ConWVecY,		ConWVecZ = ConWVecZ,		ConNominalWidth = ConNominalWidth_3,		ConNominalDepth = ConNominalDepth_3,		ConWidth = ConWidth_3,		ConDepth = ConDepth_3,		ConLength = ConLength_3,		ConWallThickness = ConWallThickness_3,		ConConnectorWidth = ConConnectorWidth_3,		ConConnectorDepth = ConConnectorDepth_3,		ConConnectorWidth2 = ConConnectorWidth2_3,		ConToolType = ConToolType_3,		ConStatus = ConStatus_3,		ConType_3 = ConType_3,		ConConnectorType_3 = ConConnectorType_3,		ConSystem_3 = ConSystem_3,		gs_ui = gs_ui,		MEP_bEnableInsulation = 1,		MEP_InsShow = MEP_InsShow,		shape_typ_to_validation = shape_typ_to_validation,		MEP_enabled_geometry_mod = 1PARAMETERS MEP_ConInfo[3][1] = STR(ConToolType_3, 1, 0)PARAMETERS MEP_ConInfo[3][2] = ConNamePARAMETERS MEP_ConInfo[3][3] = "0"! --- uploading of MEP_NumConnectionDataFOR k = 1 TO MEP_NumberConnections	FOR i = 1 TO 9		j = 1+INT((i-0.5)/3)		ad = 0		IF i < 4 THEN ad = MEP_Position[1][i]		MEP_NumConnectionData[k][i] = outCoords[j+3*(k-1)][i-3*(j-1)]+ad		PARAMETERS MEP_NumConnectionData[k][i] = MEP_NumConnectionData[k][i]	NEXT iNEXT kIF setProgram THEN	SetProgram = 0	PARAMETERS SetProgram = SetProgramELSEductHalfWidthA = MEP_NumConnectionData[1][12]/2+MEP_NumConnectionData[1][14]ductHalfWidthB = MEP_NumConnectionData[2][12]/2+MEP_NumConnectionData[1][14]minLength = ductHalfWidthB/TAN(ang)+0.001VALUES "branchLength" RANGE (minLength, ]minLength = MAX(0, -ductHalfWidthA/TAN(ang)+ductHalfWidthB/SIN(ang))VALUES "mainLength" RANGE [minLength+ConWallThickness_1, ]							! ------------------- !							! --- calculation --- !							! ------------------- !	gs_list_area = (ConWidth_1 + ConDepth_1)*2*mainLength + (ConWidth_2 + ConDepth_2)*4*branchLength	PARAMETERS gs_list_area = gs_list_areaENDIF