!-----------2d---------------------if show_el < EPS then endIF gs_detlevel_2D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, gs_fill_type, gs_fill_pen, gs_back_pen, MEP_cline_type, MEP_cline_pen, show_elPEN gs_cont_penSET FILL gs_fill_typeIF ABS(MEP_Position[3][3]) < EPS OR ABS(MEP_Position[2][3]) < EPS THEN	ADD2 MEP_Position[1][1], MEP_Position[1][2]	ROT2 dirAng	IF connectedA + connectedB < 1 + EPS THEN		unID = 1		HOTSPOT2 0, 0, unID, MEP_StraightLength, 1+256*(connectedA < EPS): unID=unID+1		HOTSPOT2 -1, 0, unID, MEP_StraightLength, 3: unID=unID+1		HOTSPOT2 MEP_StraightLength, 0, unID, MEP_StraightLength, 2+128*connectedB: unID=unID+1	ENDIF	DEL 2ENDIFHptsUnIDBeginForConnEnds = 30000hceID_1 = HptsUnIDBeginForConnEnds+100hceID_2 = HptsUnIDBeginForConnEnds+200hceID_3 = HptsUnIDBeginForConnEnds+300hceID_4 = HptsUnIDBeginForConnEnds+400ADD2 MEP_Position[1][1], MEP_Position[1][2]HOTSPOT2 outCoords[1][1], outCoords[1][2], hceID_1HOTSPOT2 outCoords[4][1], outCoords[4][2], hceID_2HOTSPOT2 outCoords[7][1], outCoords[7][2], hceID_3DEL 1IF NOT(typeID = CONNTYPE_ID_1) AND NOT(typeID = CONNTYPE_ID_2) THEN HOTSPOT2 outCoords[10][1], outCoords[10][2], hceID_4IF gs_fill_pen THEN PEN gs_fill_penif not(show_axis) then PROJECT2{3} 3, 270, 3+32, 4, gs_back_pen, 0, 0, 0, PARAMETERS duct_show = 1, MEP_InsShow = 0, gs_detlevel_3D_m = 2PEN gs_cont_penif not(show_axis) then PROJECT2{3} 3, 270, 2+32, 2+8, 0, 0, 0, 0, PARAMETERS duct_show = 1, MEP_InsShow = 0, gs_detlevel_3D_m = 2IF MEP_InsShow and show_isol and not(show_axis) THEN PROJECT2{3} 3, 270, 2+32, 8, 0, 0, 0, 0, PARAMETERS duct_show = 0, MEP_InsShow = 1, MEP_cline_show = 0, gs_detlevel_3D_m = 2