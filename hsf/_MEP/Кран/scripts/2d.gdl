paper_scale = GLOB_SCALE/1000if GLOB_CONTEXT=6 then	pen SYMB_SECT_PEN	MUL2 A_/1000, A_/1000 	k=5	line2 -k/2,-k/2,k/2,-k/2	line2 -k/2,k/2,k/2,k/2	line2 0,-k/2,0,k/2	endendifhotspot2 0,0if crane_type = CRANE_BEAM_PODV_MUN or crane_type = CRANE_BEAM_PODV_EL then 	hotspot2 A,B	hotspot2 0,Bendifishow_zone_num = 1success = LIBRARYGLOBAL ("Libraryflobals-mep", "show_crane_num", ishow_zone_num)success = LIBRARYGLOBAL ("Libraryflobals-mep", "show_plosh", show_plosh)success = LIBRARYGLOBAL ("Libraryflobals-mep", "lin_pl", lin_pl)success = LIBRARYGLOBAL ("Libraryflobals-mep", "pen_pl", pen_pl)success = LIBRARYGLOBAL ("Libraryflobals-mep", "fill_pl", fill_pl)success = LIBRARYGLOBAL ("Libraryflobals-mep", "show_gruz", show_gruz)success = LIBRARYGLOBAL ("Libraryflobals-mep", "show_work_zone", show_work_zone)success = LIBRARYGLOBAL ("Libraryflobals-mep", "pen_work", pen_work)success = LIBRARYGLOBAL ("Libraryflobals-mep", "line_work", line_work)success = LIBRARYGLOBAL ("Libraryflobals-mep", "mat_work", mat_work)success = LIBRARYGLOBAL ("Libraryflobals-mep", "show_save_zone", show_save_zone)success = LIBRARYGLOBAL ("Libraryflobals-mep", "pen_save", pen_save)success = LIBRARYGLOBAL ("Libraryflobals-mep", "line_save", line_save)success = LIBRARYGLOBAL ("Libraryflobals-mep", "mat_save", mat_save)success = LIBRARYGLOBAL ("Libraryflobals-mep", "pen_crane", pen_crane)success = LIBRARYGLOBAL ("Libraryflobals-mep", "line_crane", line_crane)success = LIBRARYGLOBAL ("Libraryflobals-mep", "mat_crane", mat_crane)if crane_type = CRANE_MONO_MUN or crane_type = CRANE_MONO_EL then 	HOTSPOT2 0, 0, unID, pos_xy[ishow_zone_num][1], 1+128: unID=unID+1	HOTSPOT2 pos_xy[ishow_zone_num][1], 0, unID, pos_xy[ishow_zone_num][1], 2: unID=unID+1	HOTSPOT2 -1, 0, unID, pos_xy[ishow_zone_num][1], 3: unID=unID+1endifif crane_type = CRANE_BEAM_PODV_MUN or crane_type = CRANE_BEAM_PODV_EL then	HOTSPOT2 pos_xy[ishow_zone_num][1], 0, unID, pos_xy[ishow_zone_num][2], 1+128: unID=unID+1	HOTSPOT2 pos_xy[ishow_zone_num][1], pos_xy[ishow_zone_num][2], unID, pos_xy[ishow_zone_num][2], 2: unID=unID+1	HOTSPOT2 pos_xy[ishow_zone_num][1], -1, unID, pos_xy[ishow_zone_num][2], 3: unID=unID+1	HOTSPOT2 0, pos_xy[ishow_zone_num][2], unID, pos_xy[ishow_zone_num][1], 1+128: unID=unID+1	HOTSPOT2 pos_xy[ishow_zone_num][1], pos_xy[ishow_zone_num][2], unID, pos_xy[ishow_zone_num][1], 2: unID=unID+1	HOTSPOT2 -1, pos_xy[ishow_zone_num][2], unID, pos_xy[ishow_zone_num][1], 3: unID=unID+1endifDRAWINDEX 10if show_work_zone then gosub "zone_work"if show_save_zone then gosub "zone_save"gosub "putt"add2 pos_xy[ishow_zone_num][1], 0gosub "teleg"del 1for i=1 to crane_nprol	add2 0, B/crane_nprol	gosub "putt"	add2 pos_xy[ishow_zone_num][1], 0	gosub "teleg"	del 1next idel topadd2 pos_xy[ishow_zone_num][1], 0hotspot2 0,Bhotspot2 0,0if crane_type = CRANE_BEAM_PODV_MUN or crane_type = CRANE_BEAM_PODV_EL then gosub "balk"add2 0, pos_xy[ishow_zone_num][2]if show_gruz then gosub "car"del 2if pl_rasp = "Вконце" then	add2 A,B	rot2 180endifif show_plosh and pl_rasp<>"Без площадки" then gosub "plosh"end"car":	gosub "set_crane_"	LINE_PROPERTY 1	DRAWINDEX 1	rect2 -gruz_A/2, -gruz_B/2, gruz_A/2, gruz_B/2	line2 -gruz_A/2, -gruz_B/2, gruz_A/2, gruz_B/2	line2 gruz_A/2, -gruz_B/2, -gruz_A/2, gruz_B/2return"teleg":	bleteg = 0.3	if crane_type = CRANE_MONO_MUN or crane_type = CRANE_MONO_EL then crane_B = bleteg	gosub "set_crane_"	LINE_TYPE 1	DRAWINDEX 40	put -crane_B/2, -bleteg/2, 1	put -crane_B/2, bleteg/2, 1	put crane_B/2, bleteg/2, 1	put crane_B/2, -bleteg/2, 1	POLY2_B NSP/3, 1+2+4, pen_crane, SYMB_FBGD_PEN, GET(NSP)return"balk":	bbalk = 0.3	gosub "set_crane_"	LINE_TYPE 1	DRAWINDEX 20	if crane_type = CRANE_BEAM_PODV_MUN or crane_type = CRANE_BEAM_PODV_EL then DRAWINDEX 50	put -bbalk/2, -crane_l, 1	put bbalk/2, -crane_l, 1	put bbalk/2, B+crane_l, 1	put -bbalk/2, B+crane_l, 1	POLY2_B NSP/3, 1+2+4, pen_crane, SYMB_FBGD_PEN, GET(NSP)	HOTSPOT2 -bbalk/2, 0, unID, crane_l, 1+128: unID=unID+1	HOTSPOT2 -bbalk/2, -crane_l, unID, crane_l, 2: unID=unID+1	HOTSPOT2 -bbalk/2, 1, unID, crane_l, 3: unID=unID+1	HOTSPOT2 -bbalk/2, B, unID, crane_l, 1+128: unID=unID+1	HOTSPOT2 -bbalk/2, B+crane_l, unID, crane_l, 2: unID=unID+1	HOTSPOT2 -bbalk/2, B-1, unID, crane_l, 3: unID=unID+1return"putt":	hotspot2 0,0	hotspot2 A,0	zas_mm = 5 * paper_scale	DRAWINDEX 10	gosub "set_crane_"	LINE_TYPE 1	if (crane_type=CRANE_MONO_MUN or crane_type=CRANE_MONO_EL) and tal_n>2 then		for k=1 to tal_n-2			HOTSPOT2 tal_xy[k][1], 0, unID, tal_xy[k][2], 1+128: unID=unID+1			HOTSPOT2 tal_xy[k][1], tal_xy[k][2], unID, tal_xy[k][2], 2: unID=unID+1			HOTSPOT2 tal_xy[k][1], -1, unID, tal_xy[k][2], 3: unID=unID+1			HOTSPOT2 0, tal_xy[k][2], unID, tal_xy[k][1], 1+128: unID=unID+1			HOTSPOT2 tal_xy[k][1], tal_xy[k][2], unID, tal_xy[k][1], 2: unID=unID+1			HOTSPOT2 -1, tal_xy[k][2], unID, tal_xy[k][1], 3: unID=unID+1		next k		if tal_n=3 then			put 0,0,1			put tal_xy[1][1]-tal_r, 0, 1			put tal_xy[1][1]-tal_r, tal_r, 901			put tal_xy[1][1], tal_r, 3001			put tal_xy[1][1], tal_xy[1][2], 1			LINE_TYPE 1			line2 0, -zas_mm/2, 0, zas_mm/2			line2 tal_xy[1][1]-zas_mm/2, tal_xy[1][2], tal_xy[1][1]+zas_mm/2, tal_xy[1][2]		endif		if tal_n=4 then			put 0,0,1			put tal_xy[1][1]-tal_r, 0, 1			put tal_xy[1][1]-tal_r, tal_r, 901			put tal_xy[1][1], tal_r, 3001			put tal_xy[1][1], tal_xy[1][2]-tal_r, 1			put tal_xy[1][1]-tal_r, tal_xy[1][2]-tal_r, 901			put tal_xy[1][1]-tal_r, tal_xy[1][2], 3001			put tal_xy[2][1], tal_xy[1][2], 1			LINE_TYPE 1			line2 0, -zas_mm/2, 0, zas_mm/2			line2 tal_xy[2][1], tal_xy[1][2]-zas_mm/2, tal_xy[2][1], tal_xy[1][2]+zas_mm/2		endif		LINE_TYPE line_crane		if NSP>3 then POLY2_ NSP/3, 1, get(NSP)	else		LINE_TYPE 1		line2 0, -zas_mm/2, 0, zas_mm/2		line2 A, -zas_mm/2, A, zas_mm/2		LINE_TYPE line_crane		line2 0, 0, A, 0	endif	LINE_TYPE 1return"zone_work":	gosub "set_work_"	DRAWINDEX 3	put crane_B/2, -(crane_l-crane_l1l2), 1	put crane_B/2, B+crane_l-crane_l1l2, 1	put A-crane_B/2, B+crane_l-crane_l1l2, 1	put A-crane_B/2, -(crane_l-crane_l1l2), 1	HOTSPOT2 crane_B/2, -(crane_l-crane_l1l2), unIDr : unIDr = unIDr + 1	HOTSPOT2 crane_B/2, B+crane_l-crane_l1l2, unIDr : unIDr = unIDr + 1	HOTSPOT2 A-crane_B/2, B+crane_l-crane_l1l2, unIDr : unIDr = unIDr + 1	HOTSPOT2 A-crane_B/2, -(crane_l-crane_l1l2), unIDr : unIDr = unIDr + 1	POLY2_B NSP/3, 1+4, pen_work, SYMB_FBGD_PEN, GET(NSP)return"zone_save":	gosub "set_save_"	DRAWINDEX 3	put -l_save, -crane_l-l_save, 1	put -l_save, B+crane_l+l_save, 1	put A+l_save, B+crane_l+l_save, 1	put A+l_save, -crane_l-l_save, 1	HOTSPOT2 -l_save, -crane_l-l_save, unIDr : unIDr = unIDr + 1	HOTSPOT2 -l_save, B+crane_l+l_save, unIDr : unIDr = unIDr + 1	HOTSPOT2 A+l_save, B+crane_l+l_save, unIDr : unIDr = unIDr + 1	HOTSPOT2 A+l_save, -crane_l-l_save, unIDr : unIDr = unIDr + 1	POLY2_B NSP/3, 1+4, pen_save, SYMB_FBGD_PEN, GET(NSP)return"set_crane_":	fill GLOB_FILL_INDEX_BACKGROUND	LINE_PROPERTY 2	pen pen_crane	LINE_TYPE line_cranereturn"set_work_":	fill GLOB_FILL_INDEX_BACKGROUND	LINE_PROPERTY 1	pen pen_crane	LINE_TYPE line_workreturn"set_save_":	fill GLOB_FILL_INDEX_BACKGROUND	LINE_PROPERTY 1	pen pen_save	LINE_TYPE line_savereturn"plosh":	pen pen_pl	fill fill_pl	LINE_PROPERTY 1	LINE_TYPE lin_pl	DRAWINDEX 1	put -pl_b, -pl_l_b, 1	put -pl_b, B+pl_l_b, 1	put pl_L, B+pl_l_b, 1	put pl_L, B-pl_d, 1	put 0, B-pl_d, 1	put 0, pl_d, 1	put pl_L, pl_d, 1	put pl_L, -pl_l_b, 1	hotspot2 -pl_b, -pl_l_b!, unIDr : unIDr = unIDr + 1	hotspot2 -pl_b, B+pl_l_b, unIDr : unIDr = unIDr + 1	hotspot2 pl_L, B+pl_l_b, unIDr : unIDr = unIDr + 1	hotspot2 pl_L, B-pl_d, unIDr : unIDr = unIDr + 1	hotspot2 0, B-pl_d, unIDr : unIDr = unIDr + 1	hotspot2 0, pl_d, unIDr : unIDr = unIDr + 1	hotspot2 pl_L, pl_d, unIDr : unIDr = unIDr + 1	hotspot2 pl_L, -pl_l_b, unIDr : unIDr = unIDr + 1	POLY2_B NSP/3, 1+4+2, pen_pl, SYMB_FBGD_PEN, GET(NSP)	if pl_rasp_l = "Вверху" then add2 0,B+pl_l_b	line2 pl_L,-pl_l_b, pl_L+pl_l_l ,-pl_l_b	line2 pl_L,0, pl_L+pl_l_l ,0	hotspot2 pl_L+pl_l_l, -pl_l_b, unIDr : unIDr = unIDr + 1	hotspot2 pl_L+pl_l_l, 0, unIDr : unIDr = unIDr + 1	LINE_TYPE 1	line2 pl_L+pl_l_l ,-pl_l_b, pl_L+pl_l_l ,0	for kkk=pl_L to pl_L+pl_l_l step pl_l_l/int(pl_l_l/0.2)		line2 kkk ,-pl_l_b,kkk,0	next kkk	if pl_rasp_l = "Вверху" then del 1	add2 -pl_b/2,B/2	CALL "Отметка на плане" PARAMETERS ALL, cz = SYMB_POS_Z+pl_otm, ang2 = 0	del 1return