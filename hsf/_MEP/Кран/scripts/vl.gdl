CALL "Общий шаблон элемента" parameters allvalues "A" RANGE[krat_txt * 2,200] STEP 0,krat_txtvalues "crane_l" RANGE[krat_txt * 2,B/2] STEP 0,krat_txtcrane_lp = crane_l * 2 + Bparameters crane_lp = crane_lpvalues{2} "crane_type" icrane_type, tcrane_typevalues{2} "crane_razm" icrane_razm, tcrane_razmvalues "gruz_A" RANGE[0.1, 100]values "gruz_B" RANGE[0.1, 100]values "gruz_H" RANGE[0.1, 100]values "pl_rasp" "Вначале", "Вконце", "Без площадки"values "pl_rasp_l" "Внизу", "Вверху", "Без лестницы"gruz_strop = sqr((gruz_A/2)*(gruz_A/2) + (gruz_B/2)*(gruz_B/2))parameters gruz_strop = round_int(gruz_strop*5)/5if crane_type = CRANE_BEAM_PODV_EL then	values "crane_grp" 1, 2, 3.2, 5, 8, 10, CUSTOM	if abs(crane_grp-10)<EPS then		values "crane_hp" 6, 12, 18, 24, 36, CUSTOM	else		if abs(crane_grp - 8)<EPS then			values "crane_hp" 12, 24, 36, CUSTOM		else			values "crane_hp" 6, 12, 18, 24, 36, CUSTOM		endif	endifendifif crane_type = CRANE_BEAM_PODV_MUN then	values "crane_grp" 1, 2, 3.2, 5, CUSTOM	values "crane_hp" 3, 6, 9, 12, CUSTOMendifif crane_type = CRANE_MONO_MUN or crane_type = CRANE_MONO_EL then 	crane_nprol = 0	parameters crane_nprol = crane_nprol	values "tal_n" 2,3,4,5	values "tal_r" range[0.5,]else	values "crane_nprol" 1, 2	if crane_razm = CRANE_GOST then		values "B" 3, 4.2, 6, 9, 12, 15	else		values "B" RANGE[krat_txt ,200] STEP 0,krat_txt	ENDIF	LOCK "tal_n", "tal_r"endifdim read_file[]dim mrf2d_koeff[][]!!Заполняем имена файловread_file[1] = "кран_7890-93"!!Заполняем коэфф (перевод мм в м)for i=1 to 15	mrf2d_koeff[1][i] = 1next imrf2d_koeff[1][4]=1/1000mrf2d_koeff[1][5]=1/1000mrf2d_koeff[1][6]=1/1000mrf2d_koeff[1][7]=1/1000mrf2d_koeff[1][8]=1/1000dict mrf2d_readed_paramdict datcall "macro_read_file2dict" parameters all mrf2d_read_file = read_file, mrf2d_koeff = mrf2d_koeff returned_parameters mrf2d_readed_paramif haskey(mrf2d_readed_param.is_read) then	if mrf2d_readed_param.is_read then		if haskey(mrf2d_readed_param.file) then			if haskey(mrf2d_readed_param.file[1].is_read) then				if mrf2d_readed_param.file[1].is_read>0 then					dat = mrf2d_readed_param.file[1]					parameters dat = dat				endif			endif		endif	endifendifif crane_razm = CRANE_GOST then	crane_l1l2 = 0.66	if abs(crane_grp - 1)<EPS then crane_l1l2 = 0.66	if abs(crane_grp - 2)<EPS then crane_l1l2 = 0.71	if abs(crane_grp - 3.2)<EPS then crane_l1l2 = 0.75	if abs(crane_grp - 5)<EPS then crane_l1l2 = 0.9	if abs(crane_grp - 8)<EPS then crane_l1l2 = 0.91	parameters crane_l1l2 = crane_l1l2endiffor i=1 to vardim1(pos_xy)	if (pos_xy[i][1]-crane_B/2)<EPS then parameters pos_xy[i][1] = crane_B/2	if (A - pos_xy[i][1]+crane_B/2)<EPS then parameters pos_xy[i][1] = A - crane_B/2	if (pos_xy[i][2]+crane_l1l2-crane_l)<EPS then parameters pos_xy[i][2] = crane_l1l2-crane_l	if (B - pos_xy[i][2]-crane_l1l2+crane_l)<EPS then parameters pos_xy[i][2] = B - crane_l1l2+crane_lnext iotm_niz_p = SYMB_POS_Zotm_niz_c = SYMB_POS_Z - ZZYZXparameters otm_niz_p = otm_niz_p, otm_niz_c = otm_niz_c PARAMETERS ac_toplevel 		= ZZYZXPARAMETERS ac_bottomlevel 	= 0dim label_otm_[]dim label_otm_num_[] : i=0i=i+1 : label_otm_[i] = "Отм. низа пути" : label_otm_num_[i] = otm_niz_pi=i+1 : label_otm_[i] = "Отм. низа крюка" : label_otm_num_[i] = otm_niz_cPARAMETERS label_otm = label_otm_, label_otm_num = label_otm_num_CRANE_MONO_MUN = 1CRANE_MONO_EL = 2CRANE_BEAM_PODV_MUN = 3CRANE_BEAM_PODV_EL = 4CRANE_BEAM_OP = 5if crane_type = CRANE_MONO_MUN then	naen = "Таль ручная подвесная"endifif crane_type = CRANE_MONO_EL then	naen = "Таль электрическая подвесная"endifif crane_type = CRANE_BEAM_PODV_MUN then	naen = "Кран ручной подвесной"endifif crane_type = CRANE_BEAM_PODV_EL then	naen = "Кран электрический подвесной"endifif crane_type = CRANE_BEAM_PODV_MUN or crane_type = CRANE_BEAM_PODV_EL then	if crane_nprol = 1 then naen = naen + ", однопролетный"	if crane_nprol = 2 then naen = naen + ", двухпролетный"endifnaen = naen + ", грузоподъемность " + str("%.1", crane_grp) + " т"naen = naen + ", высота подъёма крана " + str("%.1", crane_hp) + " м"komment = " низ пути"if crane_type = CRANE_BEAM_PODV_MUN or crane_type = CRANE_BEAM_PODV_EL then	naen = naen + ", пролет " + str("%.1", B) + " м"	naen = naen + ", консоль " + str("%.1", crane_l) + " м"	naen = naen + ", полная длина " + str("%.1", crane_lp) + " м"endifPARAMETERS naen = naen, komment = komment