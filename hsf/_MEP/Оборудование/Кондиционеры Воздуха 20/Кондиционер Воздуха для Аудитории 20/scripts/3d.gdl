!-----------3d---------------------if show_el < EPS and GLOB_VIEW_TYPE<>2 then endif GLOB_VIEW_TYPE=2 then gs_detlevel_3D_m = gs_detlevel_2D_mIF gs_detlevel_3D_m < EPS THEN ENDif GLOB_PREVIEW_MODE = 0 and GLOB_FEEDBACK_MODE = 0 then CALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_elMATERIAL sMatPEN gs_cont_penRESOL res				! --- MAIN BOX --- !ADD A/2, -B/2, 0unID = 1HOTSPOT 0, 0, 0, unID: unID=unID+1HOTSPOT 0, B, 0, unID: unID=unID+1HOTSPOT -A, B, 0, unID: unID=unID+1HOTSPOT -A, 0, 0, unID: unID=unID+1HOTSPOT 0, 0, ZZYZX, unID: unID=unID+1HOTSPOT 0, B, ZZYZX, unID: unID=unID+1HOTSPOT -A, B, ZZYZX, unID: unID=unID+1HOTSPOT -A, 0, ZZYZX, unID: unID=unID+1IF gs_detlevel_3D_m = 2 THEN	GROUP "box"		ROTy -90		PRISM_ 7, A,			0, B, 15,			0, toeDepth, 15,			toeHeight, toeDepth, 15,			toeHeight, 0, 15,			ZZYZX, 0, 15,			ZZYZX, B, 15,			0, B, -1		DEL 1	ENDGROUP	GROUP "UGrill"		ADD -A+0.025, 0.025, ZZYZX-0.025		BLOCK grillWidth, grillHeight, grillDepth		DEL 1	ENDGROUP	GROUP "LGrill"		ADD -A+0.025, 0, toeHeight+0.025		BLOCK grillWidth, grillDepth, grillHeight		DEL 1	ENDGROUP	result_1=SUBGROUP("box","UGrill")	result_2=SUBGROUP(result_1,"LGrill")	PLACEGROUP result_2	KILLGROUP "box"	KILLGROUP "UGrill"	KILLGROUP "LGrill"	 numV = 5	 numH = 6	ADD -A+0.025, 0.025, ZZYZX	ROTx -90		FOR i=1 TO numV-1			ADDz (grillHeight/numV)*i			RECT grillWidth, grillDepth			DEL 1		NEXT i	DEL 2	ADD -A+0.025, 0, toeHeight+0.025		FOR i=1 TO numV-1			ADDz (grillHeight/numV)*i			RECT grillWidth, grillDepth			DEL 1		NEXT i		FOR i=1 TO numH-1			ADDx (grillWidth/numH)*i			ROTy -90			RECT grillHeight, grillDepth			DEL 2		NEXT i	DEL 1ELSE	ROTy -90	PRISM_ 7, A,		0, B, 15,		0, toeDepth, 15,		toeHeight, toeDepth, 15,		toeHeight, 0, 15,		ZZYZX, 0, 15,		ZZYZX, B, 15,		0, B, -1	DEL 1ENDIFDEL 1				! --- CONNECTIONS --- !CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	ui_current_con = ui_current_con,	MEP_NumberConnections = MEP_NumberConnections,	MEP_NumConnectionData = gs_Connections,	gs_AddConnections = gs_AddConnections,	cShow3D = 1,	gs_ConMat = MainBoxMaterial,	MEP_InsShow = 0,	MEP_cline_show_3D = 0,	gs_cont_pen = gs_cont_pen,	ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 17,	ConPosX_2=ConPosX_2, ConPosY_2=ConPosY_2, ConPosZ_2=ConPosZ_2, ConLength_2=ConLength_2, ConEdit_2 = 17END