! Hot Water Boilercall "FM_types_MEP" parametersDIM shape_typ_to_validation[3]	shape_typ_to_validation[1] = 0		! Rectangle	shape_typ_to_validation[2] = 1		! Circle	shape_typ_to_validation[3] = 0		! OvalIF GLOB_MODPAR_NAME = "gs_detlevel_3D" OR gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1	PARAMETERS gs_detlevel_3D_m = gs_detlevel_3D_mELSE	IF gs_detlevel_3D_m = 0 THEN gs_detlevel_3D = `Откл.`	IF gs_detlevel_3D_m = 1 THEN gs_detlevel_3D = `Простой`	IF gs_detlevel_3D_m = 2 THEN gs_detlevel_3D = `Детальный`	PARAMETERS gs_detlevel_3D = gs_detlevel_3DENDIFVALUES "gs_detlevel_3D" `Детальный`,`Простой`,`Откл.`VALUES "gs_detlevel_3D_m" 2, 1, 0					! --- MAIN PARAMETERS --- !IF NOT(SetProgram) THEN	VALUES "dTWidth" RANGE [MAX(ConWidth_4+chSize*2, upperBoxWidth+chSize*2),]	VALUES "dTHeight" RANGE [legHeight+upperBoxHeight+lowerBoxHeight+upperBoxPosition+lowerBoxPosition+chSize,]ENDIFdTDepth = dTWidthPARAMETERS dTDepth = dTDepthPARAMETERS A = dTWidth+boxThickPARAMETERS B = dTDepthPARAMETERS zzyzx = dTHeightLOCK "A", "B", "ZZYZX"					! --- CONNECTIONS --- !PARAMETERS MEP_NumberConnections = 4					! --- 1st CONNECTION --- !ConID = 1ConName = `Соединение Трубы - Холодная Вода`ConDVecX = 0ConDVecY = -1ConDVecZ = 0ConWVecX = 1ConWVecY = 0ConWVecZ = 0IF NOT(SetProgram) THEN	VALUES "ConWidth_1" RANGE (0, MIN(dTWidth, dTHeight-legHeight))	PARAMETERS ConDepth_1 = ConWidth_1	HIDEPARAMETER "ConDepth_1"	IF ConStatus_2 THEN		VALUES "ColdWaterConHeight" RANGE [ConWidth_1/2, HotWaterConHeight-ConWidth_1/2-ConWidth_2/2]	ELSE		VALUES "ColdWaterConHeight" RANGE [ConWidth_1/2, dTHeight-legHeight-chSize-ConWidth_1/2]	ENDIF	VALUES "ConLength_1" RANGE [0, ]!	PARAMETERS ConPosX_1 = 0!	PARAMETERS ConPosY_1 = -dTDepth/2!	PARAMETERS ConPosZ_1 = legHeight+ColdWaterConHeightELSE	PARAMETERS ColdWaterConHeight = ConPosZ_1-legHeightENDIFCALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	gs_Connections = gs_Connections,	MEP_NumConnectionData = MEP_NumConnectionData,	MEP_StrConnectionData = MEP_StrConnectionData,	MEP_NumberConnections = MEP_NumberConnections,	gs_AddConnections = gs_AddConnections,	ConName = ConName,	ConID = ConID,	ConPosX = ConPosX_1,	ConPosY = ConPosY_1,	ConPosZ = ConPosZ_1,	ConDVecX = ConDVecX,	ConDVecY = ConDVecY,	ConDVecZ = ConDVecZ,	ConWVecX = ConWVecX,	ConWVecY = ConWVecY,	ConWVecZ = ConWVecZ,	ConNominalWidth = ConNominalWidth_1,	ConNominalDepth = ConNominalDepth_1,	ConWidth = ConWidth_1,	ConDepth = ConDepth_1,	ConLength = ConLength_1,	ConWallThickness = ConWallThickness_1,	ConConnectorWidth = ConConnectorWidth_1,	ConConnectorDepth = ConConnectorDepth_1,	ConConnectorWidth2 = ConConnectorWidth2_1,	ConToolType = ConToolType_1,	ConStatus = ConStatus_1,	ConType_1 = ConType_1,	ConConnectorType_1 = ConConnectorType_1,	ConSystem_1 = ConSystem_1,	ui_page_idx = ui_page_idx,	ui_current_con = ui_current_con,	shape_typ_to_validation = shape_typ_to_validation,	MEP_enabled_geometry_mod = 1,	bUIConnectionStatusCheckbox = 1PARAMETERS MEP_ConInfo[ConID][1] = STR(ConToolType_1, 1, 0)PARAMETERS MEP_ConInfo[ConID][2] = ConNamePARAMETERS MEP_ConInfo[ConID][3] = "0"					! --- 2nd CONNECTION --- !ConID = 2ConName = `Соединение Трубы - Горячая Вода`ConDVecX = 0ConDVecY = -1ConDVecZ = 0ConWVecX = 1ConWVecY = 0ConWVecZ = 0IF NOT(SetProgram) THEN	VALUES "ConWidth_2" RANGE (0, MIN(dTWidth, dTHeight-legHeight))	PARAMETERS ConDepth_2 = ConWidth_2	HIDEPARAMETER "ConDepth_2"	IF ConStatus_1 THEN		VALUES "HotWaterConHeight" RANGE [ColdWaterConHeight+ConWidth_1/2+ConWidth_2/2, dTHeight-legHeight-chSize-ConWidth_2/2]	ELSE		VALUES "HotWaterConHeight" RANGE [ConWidth_2/2, dTHeight-legHeight-chSize-ConWidth_2/2]	ENDIF	VALUES "ConLength_2" RANGE [0, ]!	PARAMETERS ConPosX_2 = 0!	PARAMETERS ConPosY_2 = -dTDepth/2!	PARAMETERS ConPosZ_2 = legHeight+HotWaterConHeightELSE	PARAMETERS HotWaterConHeight = ConPosZ_2-legHeightENDIFCALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	gs_Connections = gs_Connections,	MEP_NumConnectionData = MEP_NumConnectionData,	MEP_StrConnectionData = MEP_StrConnectionData,	MEP_NumberConnections = MEP_NumberConnections,	gs_AddConnections = gs_AddConnections,	ConName = ConName,	ConID = ConID,	ConPosX = ConPosX_2,	ConPosY = ConPosY_2,	ConPosZ = ConPosZ_2,	ConDVecX = ConDVecX,	ConDVecY = ConDVecY,	ConDVecZ = ConDVecZ,	ConWVecX = ConWVecX,	ConWVecY = ConWVecY,	ConWVecZ = ConWVecZ,	ConNominalWidth = ConNominalWidth_2,	ConNominalDepth = ConNominalDepth_2,	ConWidth = ConWidth_2,	ConDepth = ConDepth_2,	ConLength = ConLength_2,	ConWallThickness = ConWallThickness_2,	ConConnectorWidth = ConConnectorWidth_2,	ConConnectorDepth = ConConnectorDepth_2,	ConConnectorWidth2 = ConConnectorWidth2_2,	ConToolType = ConToolType_2,	ConStatus = ConStatus_2,	ConType_2 = ConType_2,	ConConnectorType_2 = ConConnectorType_2,	ConSystem_2 = ConSystem_2,	shape_typ_to_validation = shape_typ_to_validation,	MEP_enabled_geometry_mod = 1,	bUIConnectionStatusCheckbox = 1PARAMETERS MEP_ConInfo[ConID][1] = STR(ConToolType_2, 1, 0)PARAMETERS MEP_ConInfo[ConID][2] = ConNamePARAMETERS MEP_ConInfo[ConID][3] = "0"					! --- 3rd CONNECTION --- !ConID = 3ConName = `Соединение Трубы - Кран Подачи`ConDVecX = -1ConDVecY = 0ConDVecZ = 0ConWVecX = 0ConWVecY = -1ConWVecZ = 0IF NOT(SetProgram) THEN	VALUES "ConWidth_3" RANGE (0, MIN(lowerBoxWidth, lowerBoxHeight)]	PARAMETERS ConDepth_3 = ConWidth_3	HIDEPARAMETER "ConDepth_3"	VALUES "ConLength_3" RANGE [0, ]!	PARAMETERS ConPosX_3 = -dTWidth/2-boxThick!	PARAMETERS ConPosY_3 = 0!	PARAMETERS ConPosZ_3 = legHeight+lowerBoxPosition+lowerBoxHeight/2ENDIFCALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	gs_Connections = gs_Connections,	MEP_NumConnectionData = MEP_NumConnectionData,	MEP_StrConnectionData = MEP_StrConnectionData,	MEP_NumberConnections = MEP_NumberConnections,	gs_AddConnections = gs_AddConnections,	ConName = ConName,	ConID = ConID,	ConPosX = ConPosX_3,	ConPosY = ConPosY_3,	ConPosZ = ConPosZ_3,	ConDVecX = ConDVecX,	ConDVecY = ConDVecY,	ConDVecZ = ConDVecZ,	ConWVecX = ConWVecX,	ConWVecY = ConWVecY,	ConWVecZ = ConWVecZ,	ConNominalWidth = ConNominalWidth_3,	ConNominalDepth = ConNominalDepth_3,	ConWidth = ConWidth_3,	ConDepth = ConDepth_3,	ConLength = ConLength_3,	ConWallThickness = ConWallThickness_3,	ConConnectorWidth = ConConnectorWidth_3,	ConConnectorDepth = ConConnectorDepth_3,	ConConnectorWidth2 = ConConnectorWidth2_3,	ConToolType = ConToolType_3,	ConStatus = ConStatus_3,	ConType_3 = ConType_3,	ConConnectorType_3 = ConConnectorType_3,	ConSystem_3 = ConSystem_3,	shape_typ_to_validation = shape_typ_to_validation,	MEP_enabled_geometry_mod = 1,	bUIConnectionStatusCheckbox = 1PARAMETERS MEP_ConInfo[ConID][1] = STR(ConToolType_3, 1, 0)PARAMETERS MEP_ConInfo[ConID][2] = ConNamePARAMETERS MEP_ConInfo[ConID][3] = "0"					! --- 4th CONNECTION --- !ConID = 4ConName = `Подключение Дымохода`ConDVecX = 0ConDVecY = 0ConDVecZ = 1ConWVecX = 1ConWVecY = 0ConWVecZ = 0IF NOT(SetProgram) THEN	VALUES "ConWidth_4" RANGE (0, dTWidth-chSize*2-ConWallThickness_4*2]	PARAMETERS ConDepth_4 = ConWidth_4	HIDEPARAMETER "ConDepth_4"	VALUES "ConLength_4" RANGE [0, ]!	PARAMETERS ConPosX_4 = 0!	PARAMETERS ConPosY_4 = 0!	PARAMETERS ConPosZ_4 = dTHeightENDIFCALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	gs_Connections = gs_Connections,	MEP_NumConnectionData = MEP_NumConnectionData,	MEP_StrConnectionData = MEP_StrConnectionData,	MEP_NumberConnections = MEP_NumberConnections,	gs_AddConnections = gs_AddConnections,	ConName = ConName,	ConID = ConID,	ConPosX = ConPosX_4,	ConPosY = ConPosY_4,	ConPosZ = ConPosZ_4,	ConDVecX = ConDVecX,	ConDVecY = ConDVecY,	ConDVecZ = ConDVecZ,	ConWVecX = ConWVecX,	ConWVecY = ConWVecY,	ConWVecZ = ConWVecZ,	ConNominalWidth = ConNominalWidth_4,	ConNominalDepth = ConNominalDepth_4,	ConWidth = ConWidth_4,	ConDepth = ConDepth_4,	ConLength = ConLength_4,	ConWallThickness = ConWallThickness_4,	ConConnectorWidth = ConConnectorWidth_4,	ConConnectorDepth = ConConnectorDepth_4,	ConConnectorWidth2 = ConConnectorWidth2_4,	ConToolType = ConToolType_4,	ConStatus = ConStatus_4,	ConType_4 = ConType_4,	ConConnectorType_4 = ConConnectorType_4,	ConSystem_4 = ConSystem_4,	shape_typ_to_validation = shape_typ_to_validation,	MEP_enabled_geometry_mod = 1,	bUIConnectionStatusCheckbox = 1PARAMETERS MEP_ConInfo[ConID][1] = STR(ConToolType_4, 1, 0)PARAMETERS MEP_ConInfo[ConID][2] = ConNamePARAMETERS MEP_ConInfo[ConID][3] = "0"IF setProgram THEN	SetProgram = 0	PARAMETERS SetProgram = SetProgramENDIFvalues "FM_HeatOutput_unit" `З`, `БТЕ/час`values "FM_RefrigeratingOutput_unit" `З`, `БТЕ/час`if FM_HeatOutput_unit = `БТЕ/час` then	hideparameter "FM_HeatOutput"else	hideparameter "FM_HeatOutput_btu"endifif FM_RefrigeratingOutput_unit = `БТЕ/час` then	hideparameter "FM_RefrigeratingOutput"else	hideparameter "FM_RefrigeratingOutput_btu"endif!1000 W = 3414,426 Btu/hourif GLOB_MODPAR_NAME = "FM_HeatOutput_btu" then	FM_HeatOutput = FM_HeatOutput_btu / 3.414426	parameters FM_HeatOutput = FM_HeatOutputelse	FM_HeatOutput_btu = FM_HeatOutput * 3.414426	parameters FM_HeatOutput_btu = FM_HeatOutput_btuendif!1000 W = 3414,426 Btu/hourif GLOB_MODPAR_NAME = "FM_RefrigeratingOutput_btu" then	FM_RefrigeratingOutput = FM_RefrigeratingOutput_btu / 3.414426	parameters FM_RefrigeratingOutput = FM_RefrigeratingOutputelse	FM_RefrigeratingOutput_btu = FM_RefrigeratingOutput * 3.414426	parameters FM_RefrigeratingOutput_btu = FM_RefrigeratingOutput_btuendif