!-----------2d---------------------if show_el < EPS then endIF gs_detlevel_2D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, gs_fill_type, gs_fill_pen, gs_back_pen, MEP_cline_type, MEP_cline_pen, show_elPEN gs_cont_penSET FILL gs_fill_typeHptsUnIDBeginForConnEnds = 30000hceID_1 = HptsUnIDBeginForConnEnds+100unID = 1IF ABS(dZz-1) < EPS THEN	ADD2 MEP_Position[1][1], MEP_Position[1][2]	ROT2 dirAngOnXYPlane			! --- DRY SPRINKLER --- !	HOTSPOT2 0, 0, unID: unID=unID+1	IF sprinklerType = 1 THEN	! Horizontal Sidewall Dry Sprinkler		dTWidth = 0.10		dTDepth = 0.08		MUL2 A/dTWidth, B/dTDepth		thk = 0.05*dTDepth		diam = 0.5*dTDepth		HOTSPOT2 -dTWidth,0, unID: unID=unID+1		POLY2_B 8, 1+2+4+64, gs_fill_pen, gs_back_pen,			 0, dTDepth/2, 1,			-thk, dTDepth/2, 0,			-thk, diam/2, 1,			-0.25 * dTWidth,diam/2,  1,			-0.25 * dTWidth,-diam/2,  1,			-thk, -diam/2, 0,			-thk, -dTDepth/2, 1,			 0, -dTDepth/2, 1		LINE2 -thk, dTDepth/2, -thk, -dTDepth/2		ADD2 -dTWidth+0.25 * dTWidth, 0		POLY2_B 4, 1+2+4+64, gs_fill_pen, gs_back_pen,			0, 0.25 * dTWidth, 1,			0,-0.25 * dTWidth, 1,			-1, 0, 801,			0, 0.25 * dTWidth, 1001		DEL 1		ADD2 -0.25 * dTWidth,0		ROT2 180		forkW = diam		forkL = 0.5 * dTWidth		GOSUB 100		DEL 3	ENDIF	IF sprinklerType = 2 THEN	! Pendent Dry Sprinkler		IF gs_symbtype_2D_m = 1 OR gs_symbtype_2D_m = 2 THEN		! Realistic, Electric			POLY2_B 2, 1+2+4+64, gs_fill_pen, gs_back_pen,				0,0, 901,				A/2,360,4001		ENDIF		IF gs_symbtype_2D_m = 3 THEN		! RCP			HOTSPOT2 0, 0, unID: unID=unID+1			CIRCLE2 0, 0, A/2			END		ENDIF	ENDIF	IF sprinklerType = 3 THEN	! Recessed Horizontal Sidewall Dry Sprinkler		dTWidth = 0.075		dTDepth = 0.08		MUL2 A/dTWidth, B/dTDepth		thk = 0.05*dTDepth		diam = 0.5*dTDepth		HOTSPOT2 -dTWidth,0, unID: unID=unID+1		POLY2_B 8, 1+2+4+64, gs_fill_pen, gs_back_pen,			-0.25 * dTWidth,dTDepth/2,  1,			-0.25 * dTWidth+thk,dTDepth/2, 0,			-0.25 * dTWidth+thk,diam/2, 1,			 0, diam/2, 1,			 0, -diam/2, 1,			-0.25 * dTWidth+thk,-diam/2, 0,			-0.25 * dTWidth+thk,-dTDepth/2, 1,			-0.25 * dTWidth,-dTDepth/2,  1		LINE2 -0.25 * dTWidth+thk,dTDepth/2, -0.25 * dTWidth+thk,-dTDepth/2		ADD2 -dTWidth+0.25 * dTWidth, 0		POLY2_B 4, 1+2+4+64, gs_fill_pen, gs_back_pen,			0, 0.25 * dTWidth, 1,			0,-0.25 * dTWidth, 1,			-1, 0, 801,			0, 0.25 * dTWidth, 1001		DEL 1		ADD2 -0.25 * dTWidth,0		ROT2 180		forkW = diam		forkL = 0.5 * dTWidth		GOSUB 100		DEL 3	ENDIF	IF sprinklerType = 4 THEN	! Recessed Pendent Dry Sprinkler		IF gs_symbtype_2D_m = 1 OR gs_symbtype_2D_m = 2 THEN		! Realistic, Electric			diam = 0.5*B			POLY2_B 2, 1+2+4+64, gs_fill_pen, gs_back_pen,				0,0, 901,				A/2,360,4001			CIRCLE2 0,0, diam/2		ENDIF		IF gs_symbtype_2D_m = 3 THEN		! RCP			HOTSPOT2 0, 0, unID: unID=unID+1			CIRCLE2 0, 0, A/2			END		ENDIF	ENDIF	IF sprinklerType = 5 THEN	! Upright Dry Sprinkler		POLY2_B 2, 1+2+4+64, gs_fill_pen, gs_back_pen,			0,0, 901,			A/2,360,4001	ENDIF			! --- CONNECTIONS --- !	CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,			ui_current_con = ui_current_con,			MEP_NumberConnections = MEP_NumberConnections,			MEP_NumConnectionData = gs_Connections,			gs_AddConnections = gs_AddConnections,			DrawingNum = 1,			MEP_InsShow = 0,			MEP_cline_show = 0,			gs_cont_pen = gs_cont_pen,			gs_fill_type=gs_fill_type,			gs_fill_pen=gs_fill_pen,			gs_back_pen=gs_back_pen,			ConPosX_1 = ConPosX_1, ConPosY_1=ConPosY_1, ConLength_1=ConLength_1, ConEdit_1 = 0, MEP_StrConnectionData = MEP_StrConnectionData	DEL 2ELSE	HOTSPOT2 outCoords[1][1], outCoords[1][2], hceID_1	HOTSPOT2 outCoords[4][1], outCoords[4][2], unID: unID=unID+1	HOTSPOT2 outCoords[5][1], outCoords[5][2], unID: unID=unID+1	SET FILL gs_fill_type	PEN gs_cont_pen	IF gs_fill_pen THEN PEN gs_fill_pen	PROJECT2{3} 3, 270, 3+32, 4, gs_back_pen, 0, 0, 0, PARAMETERS gs_detlevel_3D_m = 2	PEN gs_cont_pen	PROJECT2{3} 3, 270, 2, 8, 0, 0, 0, 0, PARAMETERS gs_detlevel_3D_m = 2ENDIFEND100:	POLY2_B 8, 1+2+4+64, gs_fill_pen, gs_back_pen,		0,			forkW/9, 1,		0.8*forkL,	forkW/9, 0,		0.8*forkL,	forkW/7, 1,		forkL,		forkW/7, 1,		forkL,		-forkW/7, 1,		0.8*forkL,	-forkW/7, 0,		0.8*forkL,	-forkW/9, 1,		0,			-forkW/9, 1	LINE2 0.8*forkL,-forkW/7, 0.8*forkL,forkW/7	alpha = ATN((forkW/2 - forkW/7) / (0.2*forkL))	POLY2_B 6, 1+2+4+64, gs_fill_pen, gs_back_pen,		0,			forkW/2, 1,		0.8*forkL,	forkW/2, 1,		forkL,		forkW/7, 1,		0.8*forkL,	forkW/7, 1,		0.8*forkL-(forkW/6)/TAN(alpha),	forkW/3, 1,		0,			forkW/3, 1	POLY2_B 6, 1+2+4+64, gs_fill_pen, gs_back_pen,		0,			-forkW/2, 1,		0.8*forkL,	-forkW/2, 1,		forkL,		-forkW/7, 1,		0.8*forkL,	-forkW/7, 1,		0.8*forkL-(forkW/6)/TAN(alpha),	-forkW/3, 1,		0,			-forkW/3, 1RETURN