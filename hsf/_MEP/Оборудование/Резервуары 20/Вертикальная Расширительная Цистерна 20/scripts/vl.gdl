! Vertical Expansion Tankcall "FM_types_MEP" parametersDIM shape_typ_to_validation[3]	shape_typ_to_validation[1] = 0		! Rectangle	shape_typ_to_validation[2] = 1		! Circle	shape_typ_to_validation[3] = 0		! Oval					! --- MAIN PARAMETERS --- !IF NOT(SetProgram) THEN	IF A < 0.10 THEN A = 0.10	VALUES "A" RANGE [0.10, ]	endRed = A/10	ww = A/0.40	Height2 = 0.09*ww	IF ZZYZX < Height2+endRed+0.05 THEN ZZYZX = Height2+endRed+0.05	VALUES "ZZYZX" RANGE [Height2+endRed+0.05,]	B = A	PARAMETERS B = B	LOCK "B"ENDIF					! --- CONNECTIONS --- !PARAMETERS MEP_NumberConnections = 1					! --- 1st CONNECTION --- !ConID = 1ConName = `Труба Впускного Соединения `ConDVecX = 0ConDVecY = 0ConDVecZ = 1ConWVecX = 1ConWVecY = 0ConWVecZ = 0IF NOT(SetProgram) THEN	IF ConWidth_1 > A/4 THEN ConWidth_1 = A/4	VALUES "ConWidth_1" RANGE (0, A/4]	PARAMETERS ConDepth_1 = ConWidth_1	HIDEPARAMETER "ConDepth_1"	VALUES "ConLength_1" RANGE [0, ]	PARAMETERS ConPosX_1 = 0	PARAMETERS ConPosY_1 = 0	PARAMETERS ConPosZ_1 = ZZYZXENDIFCALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	gs_Connections = gs_Connections,	MEP_NumConnectionData = MEP_NumConnectionData,	MEP_StrConnectionData = MEP_StrConnectionData,	MEP_NumberConnections = MEP_NumberConnections,	gs_AddConnections = gs_AddConnections,	ConName = ConName,	ConID = ConID,	ConPosX = ConPosX_1,	ConPosY = ConPosY_1,	ConPosZ = ConPosZ_1,	ConDVecX = ConDVecX,	ConDVecY = ConDVecY,	ConDVecZ = ConDVecZ,	ConWVecX = ConWVecX,	ConWVecY = ConWVecY,	ConWVecZ = ConWVecZ,	ConNominalWidth = ConNominalWidth_1,	ConNominalDepth = ConNominalDepth_1,	ConWidth = ConWidth_1,	ConDepth = ConDepth_1,	ConLength = ConLength_1,	ConWallThickness = ConWallThickness_1,	ConConnectorWidth = ConConnectorWidth_1,	ConConnectorDepth = ConConnectorDepth_1,	ConConnectorWidth2 = ConConnectorWidth2_1,	ConToolType = ConToolType_1,	ConStatus = ConStatus_1,	ConType_1 = ConType_1,	ConConnectorType_1 = ConConnectorType_1,	ConSystem_1 = ConSystem_1,	ui_page_idx = ui_page_idx,	ui_current_con = ui_current_con,	shape_typ_to_validation = shape_typ_to_validation,	MEP_enabled_geometry_mod = 1,	bUIConnectionStatusCheckbox = 1PARAMETERS MEP_ConInfo[ConID][1] = STR(ConToolType_1, 1, 0)PARAMETERS MEP_ConInfo[ConID][2] = ConNamePARAMETERS MEP_ConInfo[ConID][3] = "0"IF setProgram THEN	SetProgram = 0	PARAMETERS SetProgram = SetProgramENDIF