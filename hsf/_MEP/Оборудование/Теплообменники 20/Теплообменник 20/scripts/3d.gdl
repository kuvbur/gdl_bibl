!-----------3d---------------------if show_el < EPS and GLOB_VIEW_TYPE<>2 then endif GLOB_VIEW_TYPE=2 then gs_detlevel_3D_m = gs_detlevel_2D_mIF gs_detlevel_3D_m < EPS THEN ENDif GLOB_PREVIEW_MODE = 0 and GLOB_FEEDBACK_MODE = 0 then CALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_elMainBoxMaterial = sMatMATERIAL sMatPEN gs_cont_penSHADOW ON : if gs_detlevel_3D_m =1 THEN SHADOW OFFRESOL res				! --- frame of CONNECTION --- !unID = 1HOTSPOT	   0, 0, 0, unID: unID=unID+1HOTSPOT	   0, totalhossz, 0, unID: unID=unID+1IF gs_detlevel_3D_m = 2 THEN	GROUP "1"		ROTx -90			CYLIND totalhossz-atm1, atm1		DEL 1		ADDy totalhossz-atm1		ROTx -90			ELLIPS atm1, atm1		DEL 2	ENDGROUP	GROUP "2"		ADDy 0.002		ROTx -90			CYLIND 0.0154*totalhossz, atm2		DEL 2		ADDy 3*0.0154*totalhossz		ROTx -90			CYLIND 0.0154*totalhossz, atm2		DEL 2		ADDy conpos1			CYLIND atm1, conatm1		DEL 1		ADDy conpos2		ROTx 180			CYLIND atm1, conatm2		DEL 2		ADDz conpos3		ROTx 90			CYLIND conwidth34, conatm3		DEL 2		ADDz conpos4		ROTx 90			CYLIND conwidth34, conatm4		DEL 2	ENDGROUP	result_01=ADDGROUP ("1", "2")	PLACEGROUP result_01	KILLGROUP "1"	KILLGROUP "2"	KILLGROUP result_01ELSE	GROUP "3"		ROTx -90			CYLIND totalhossz, atm1		DEL 1	ENDGROUP	GROUP "4"		ADDy 0.002		ROTx -90			CYLIND 0.065, atm2		DEL 2		ADDy conpos1			CYLIND atm2, conatm1		DEL 1		ADDy conpos2		ROTx 180			CYLIND atm2, conatm2		DEL 2		ADDz conpos3		ROTx 90			CYLIND conwidth34, conatm3		DEL 2		ADDz conpos4		ROTx 90			CYLIND conwidth34, conatm4		DEL 2	ENDGROUP	result_02=ADDGROUP ("4", "3")	PLACEGROUP result_02	KILLGROUP "4"	KILLGROUP "3"	KILLGROUP result_02ENDIF				! --- CONNECTIONS --- !CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	ui_current_con = ui_current_con,	MEP_NumberConnections = MEP_NumberConnections,	MEP_NumConnectionData = gs_Connections,	gs_AddConnections = gs_AddConnections,	cShow3D = 1,	gs_ConMat = MainBoxMaterial,	MEP_InsShow = 0,	MEP_cline_show_3D = 0,	gs_cont_pen = gs_cont_pen,	ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 13,	ConPosX_2=ConPosX_2, ConPosY_2=ConPosY_2, ConPosZ_2=ConPosZ_2, ConLength_2=ConLength_2, ConEdit_2 = 13,	ConPosX_3=ConPosX_3, ConPosY_3=ConPosY_3, ConPosZ_3=ConPosZ_3, ConLength_3=ConLength_3, ConEdit_3 = 14,	ConPosX_4=ConPosX_4, ConPosY_4=ConPosY_4, ConPosZ_4=ConPosZ_4, ConLength_4=ConLength_4, ConEdit_4 = 14, MEP_StrConnectionData = MEP_StrConnectionData, mep_con = mep_con, mep_data = mep_dataEND