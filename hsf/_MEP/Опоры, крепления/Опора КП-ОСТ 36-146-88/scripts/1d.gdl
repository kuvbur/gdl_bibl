e_)=1 then op_type_ = str("%.0", op_type_)		tDN_type[j] = str("%.0", DN) + op_type_		j = j + 1	endif	i = i + 1endwhileclose ch1if vartype(op_DN)<>1 then op_DN = 108/1000if op_DN<EPS then op_DN = 108/1000inx = 0DN_type = str("%.0", op_DN*1000) + op_typefor i=1 to vardim1(tDN)	if DN_type = tDN_type[i] then		inx = i+1		i = vardim1(tDN_type)	endifnext iflag_custom_dn = 1if inx > EPS then flag_custom_dn = 0if GLOB_MODPAR_NAME="op_type" or GLOB_MODPAR_NAME="op_DN" or op_ves<EPS or SetProgram = 1 then	if inx > EPS then		ch1 = open("text", file_name, "separator='\t', mode=ro, LIBRARY")		n = input (ch1, inx, 1,op_DN_, op_type_, op_h_, op_h1_, op_h2_, op_L_, op_B_, op_S_, op_B1_, op_lp_, op_mass_, op_P1_, op_P2_, op_P3_)		if vartype(op_h_)=1 then op_h = op_h_/1000		if vartype(op_h1_)=1 then op_h1 = op_h1_/1000		if vartype(op_h2_)=1 then op_h2 = op_h2_/1000		if vartype(op_L_)=1 then op_L = op_L_/1000		if vartype(op_B_)=1 then op_B = op_B_/1000		if vartype(op_S_)=1 then op_S = op_S_/1000		if vartype(op_B1_)=1 then op_B1 = op_B1_/1000		if vartype(op_lp_)=1 then op_lp = op_lp_/1000		if vartype(op_mass_)=1 then op_ves = op_mass_		if vartype(op_P1_)=1 then op_Pz = op_P1_		close ch1	endifendifop_rs = 0.004op_type_num = STRSUB(op_type, STRLEN(op_type)-1, 3)op_type_txt = STRSUB(op_type, 1, 1)dz = 0 : if op_type_txt = "Б" then dz = op_rsop_r = op_DN / 2