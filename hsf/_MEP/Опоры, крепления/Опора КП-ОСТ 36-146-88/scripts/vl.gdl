values "op_type" "А11", "А21", "А22", "А13", "А23", "Б12", "Б22", "Б13",  "Б23"values "op_DN" tDNparameters op_naen = "Опора " + str("%.0mm", op_DN) + "-КП-"+op_typeparameters op_obozn = "ОСТ 36-146-88"if flag_custom_dn = 0 then	parameters op_L = op_L, op_B = op_B, op_h = op_h, op_S = op_S, op_B1 = op_B1, op_lp = op_lp, op_ves = op_ves, op_Pz = op_Pz, op_h1 = op_h1, op_h2 = op_h2	lock "op_L", "op_B", "op_h", "op_S", "op_B1", "op_lp", "op_ves", "op_Pz", "op_h1", "op_h2"endifFMProductName = "Монтаж опоры " + str("%.0mm", op_DN) + "-ВП-"+op_type + " (" + op_obozn + ")"PARAMETERS FM_ProductName = FMProductNamemep_type_obor=MEP_TYPE_OPdict mep_type_data : call "mep_common_data" parameters mep_set_type=1 returned_parameters mep_type_datags_list_manufacturer = ""if haskey(mep_type_data.renum) then 	if haskey(mep_type_data.renum[mep_type_obor]) then		gs_list_manufacturer = mep_type_data.renum[mep_type_obor]	endifendifPARAMETERS gs_list_manufacturer = gs_list_manufacturerif SetProgram = 0 then	PARAMETERS naen = op_naen	PARAMETERS obozn = op_obozn	PARAMETERS ves = op_ves	PARAMETERS komment = "шт"endifmep_s_pokr_ = (op_L*op_h1)*2+op_L*op_B+(op_B-2*op_S)*op_h2PARAMETERS mep_s_pokr = mep_s_pokr_*1.3, mep_s_el = mep_s_pokr_*1.3PARAMETERS mep_s_ins = 0PARAMETERS mep_l = 0