!!!============================= ТИПЫ ЭЛЕМЕНТОВ ===============================MEP_TYPE_TUBE = 1MEP_TYPE_PEREH = 2MEP_TYPE_TR = 3MEP_TYPE_OTVOD = 4MEP_TYPE_FLANGE = 5MEP_TYPE_OP = 6MEP_TYPE_EQONTUBE = 7!-----------Основной---------------------EPS = 0.00001success = LIBRARYGLOBAL ("Libraryflobals-mep", "show_axis", show_axis)success = LIBRARYGLOBAL ("Libraryflobals-mep", "show_axis_and", show_axis_and)success = LIBRARYGLOBAL ("Libraryflobals-mep", "show_isol", show_isol)success = LIBRARYGLOBAL ("Libraryflobals-mep", "show_op", show_op)success = LIBRARYGLOBAL ("Libraryflobals-mep", "show_conventions", show_conventions)success = LIBRARYGLOBAL ("Libraryflobals-mep", "show_size", show_size)success = LIBRARYGLOBAL ("Libraryflobals-mep", "gs_detlevel_2D_m",gs_detlevel_2D_m)success = LIBRARYGLOBAL ("Libraryflobals-mep", "gs_detlevel_3D_m", gs_detlevel_3D_m)IF gs_detlevel_3D_m = 2 THEN res = 24IF gs_detlevel_3D_m = 1 THEN res = 12IF gs_detlevel_3D_m = 0 THEN res = 12n = REQUEST ("Name_of_main", "", name)is_call = 0 : if name<>"" then is_call = 1if SetProgram = 0 then	call "Показ_систем" PARAMETERS All isMep=0, isEqp=1 RETURNED_PARAMETERS show_elelse	call "Показ_систем" PARAMETERS All isMep=1, isEqp=1 RETURNED_PARAMETERS show_el, syst_nameendifif GLOB_PREVIEW_MODE = 0 then	otm_elem = SYMB_POS_Z	txt2 = ""	aaz = SYMB_POS_Z	if abs(aaz)<EPS then		az = "0.000"		znak1 = ""	 else		az = str("%.3m", abs(aaz))		if aaz < EPS then 			znak1 = "-"		else			znak1 = "+"		endif	endif	parameters otm_niz = znak1 + azendiffile_name = "Опора_ТО.txt"if vartype(op_DN)<>1 then op_DN = 108/1000if op_DN<EPS then op_DN = 108/1000dim tDN[]dim tDN_type[]ch1 = open("text", file_name, "separator='\t', mode=ro, LIBRARY")i = 2 : j = 1 : DN = 0 : DN1 = 0n = 1while n <> -1 and i<1000 do	n = input (ch1, i, 1, DN, op_type_)	if vartype(DN)=1 then		if DN>EPS then			DN1 = DN			tDN[j] = DN/1000			if vartype(op_type_)=1 then op_type_ = str("%.0", op_type_)			tDN_type[j] = str("%.0", DN) + op_type_			j = j + 1		endif	endif	i = i + 1endwhileclose ch1inx = 0DN_type = str("%.0", op_DN*1000) + op_typefor i=1 to vardim1(tDN)	if DN_type = tDN_type[i] then		inx = i+1		i = vardim1(tDN_type)	endifnext iflag_custom_dn = 1if inx > EPS then flag_custom_dn = 0if GLOB_MODPAR_NAME="op_type" or GLOB_MODPAR_NAME="op_DN" or op_ves<EPS or SetProgram = 1 then	if inx > EPS then		ch1 = open("text", file_name, "separator='\t', mode=ro, LIBRARY")		n = input (ch1, inx, 1,op_DN_, op_type_,op_h_, op_h1_, op_dnpatr_, op_S_, op_B_, op_S1_, op_e_, op_d_, op_mass_, op_P_)		if vartype(op_h_)=1 then op_h = op_h_/1000		if vartype(op_h1_)=1 then op_h1 = op_h1_/1000		if vartype(op_dnpatr_)=1 then op_dnpatr = op_dnpatr_/1000		if vartype(op_S_)=1 then op_S = op_S_/1000		if vartype(op_B_)=1 then op_B = op_B_/1000		if vartype(op_S1_)=1 then op_S1 = op_S1_/1000		if vartype(op_d_)=1 then op_d= op_d_/1000		if vartype(op_mass_)=1 then op_ves = op_mass_		if vartype(op_P_)=1 then op_Pz = op_P_		close ch1	endifendifif op_dnpatr<EPS then op_dnpatr = op_DNop_R = op_DN/2op_W=op_B-op_e