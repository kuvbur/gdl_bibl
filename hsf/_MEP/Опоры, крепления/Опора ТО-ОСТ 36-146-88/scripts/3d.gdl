!-----------3d---------------------if show_el < EPS then endif show_op<EPS then endif GLOB_VIEW_TYPE=2 then gs_detlevel_3D_m = gs_detlevel_2D_mif (GLOB_PREVIEW_MODE or GLOB_FEEDBACK_MODE or GLOB_SEO_TOOL_MODE) then gs_detlevel_3D_m = 1CALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_elshadow offMEP_cline_pen = min(max(MEP_cline_pen,1),254)gs_cont_pen = min(max(gs_cont_pen,1),254)if show_axis then	pen MEP_cline_penelse	pen gs_cont_penendifmaterial sMatresol resGROUP "Плита"	BLOCK op_B, op_B, op_S1ENDGROUPGROUP "Болтовой цилиндр"	ADD op_W/2, op_W/2, 0		CYLIND op_S1, op_d/2	DEL 1	ADD op_W/2, op_W/2+op_e, 0		CYLIND op_S1, op_d/2	DEL 1	ADD op_W/2+op_e, op_W/2+op_e, 0		CYLIND op_S1, op_d/2	DEL 1	ADD op_W/2+op_e, op_W/2, 0		CYLIND op_S1, op_d/2	DEL 1ENDGROUPGROUP "Патрубок"	mulz -1	sc=12	if NSP>0 then tt = max(get(NSP))	RESOL 36		PUT op_B/2,op_B/2,900+sc		PUT op_dnpatr/2,360,4000+sc		PUT op_dnpatr/2-op_S,360,4000+sc	if NSP>0 then PRISM_ NSP/3, op_h1-op_S1, GET(NSP)	DEL 1ENDGROUPGROUP "Отвод"	ADD op_B/2, op_B/2, 0	ROTx 180		ELBOW op_F, op_angle, op_dn/2+0.003	DEL 1ENDGROUPRESULT1=SUBGROUP ("Плита","Болтовой цилиндр")RESULT2=SUBGROUP ("Патрубок","Отвод")hotspot 0,0,0HOTSPOT	0,	0,	0,	unID,	op_h1,	1+128+512: unID=unID+1HOTSPOT	0,	0,	op_h1,	unID,	op_h1,	2+512: unID=unID+1HOTSPOT	0,	0,	op_h1-1,	unID,	op_h1,	3+512: unID=unID+1add -op_B/2, -op_B/2, 0PLACEGROUP RESULT1add 0, 0, op_h1PLACEGROUP RESULT2!PLACEGROUP "Отвод"KILLGROUP "Патрубок"KILLGROUP "Отвод"KILLGROUP RESULT1KILLGROUP RESULT2KILLGROUP "Плита"KILLGROUP "Болтовой цилиндр"