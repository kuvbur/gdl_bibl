!-----------3d---------------------if show_el < EPS or show_sal<EPS then endmodel wireCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_elgs_PipeMat = sMatMATERIAL gs_PipeMatpipe_show = pipe_show*not(show_axis) PEN gs_cont_penRESOL 12gs_PipeMat = sMatDIM MEP_InclinedPartPosition[][]MEP_InclinedPartPosition[1][1] = 0MEP_InclinedPartPosition[1][2] = 0MEP_InclinedPartPosition[1][3] = 0MEP_InclinedPartPosition[2][1] = 1MEP_InclinedPartPosition[2][2] = 0MEP_InclinedPartPosition[2][3] = 0MEP_InclinedPartPosition[3][1] = 0MEP_InclinedPartPosition[3][2] = 1MEP_InclinedPartPosition[3][3] = 0unID = 1HOTSPOT 0, 0, 0, unID: unID=unID+1XFORM   MEP_Position[2][1], MEP_Position[3][1], dZx, MEP_Position[1][1],		MEP_Position[2][2], MEP_Position[3][2], dZy, MEP_Position[1][2],		MEP_Position[2][3], MEP_Position[3][3], dZz, MEP_Position[1][3]roty 90jt=15put 0,0,900+jtput (dns/2)+0.001*GLOB_SCALE,360,4000+jtput dns/2,360,4000+jtPRISM_ NSP/3, 0.0001, get(NSP)if strstr(type_sal, "Сальник")>0 then	addz dlin_sal/2	put 0,0,900+jt	put (dns/2)+0.0005*GLOB_SCALE,360,4000+jt	put dns/2,360,4000+jt	PRISM_ NSP/3, 0.0001, get(NSP)	del 1endifaddz dlin_salput 0,0,900+jtput (dns/2)+0.001*GLOB_SCALE,360,4000+jtput dns/2,360,4000+jtPRISM_ NSP/3, 0.0001, get(NSP)del 1if type_sal<>"Нет" then	HOTSPOT dns/2, 0,0, unID: unID=unID+1	HOTSPOT -dns/2, 0,0, unID: unID=unID+1	HOTSPOT 0, dns/2,0, unID: unID=unID+1	HOTSPOT 0, -dns/2,0, unID: unID=unID+1	HOTSPOT 0, 0,0, unID: unID=unID+1	HOTSPOT dns/2, 0,ZZYZX, unID: unID=unID+1	HOTSPOT -dns/2, 0,ZZYZX, unID: unID=unID+1	HOTSPOT 0, dns/2,ZZYZX, unID: unID=unID+1	HOTSPOT 0, -dns/2,ZZYZX, unID: unID=unID+1	HOTSPOT 0, 0,ZZYZX, unID: unID=unID+1	jt = 15	put 0,0,900+jt	put dns/2,360,4000+jt	put dns/2-tst,360,4000+jt	PRISM_ NSP/3, dlin_sal, get(NSP)endif