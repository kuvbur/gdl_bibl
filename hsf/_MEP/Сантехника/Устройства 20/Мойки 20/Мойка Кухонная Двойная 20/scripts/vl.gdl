! Double Kitchen Sinkcall "FM_types_MEP" parametersDIM shape_typ_to_validation[3]	shape_typ_to_validation[1] = 0		! Rectangle	shape_typ_to_validation[2] = 1		! Circle	shape_typ_to_validation[3] = 0		! OvalIF GLOB_MODPAR_NAME = "gs_detlevel_3D" OR gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1	PARAMETERS gs_detlevel_3D_m = gs_detlevel_3D_mELSE	IF gs_detlevel_3D_m = 0 THEN gs_detlevel_3D = `Откл.`	IF gs_detlevel_3D_m = 1 THEN gs_detlevel_3D = `Простой`	IF gs_detlevel_3D_m = 2 THEN gs_detlevel_3D = `Детальный`	PARAMETERS gs_detlevel_3D = gs_detlevel_3DENDIFVALUES "gs_detlevel_3D" `Детальный`,`Простой`,`Откл.`VALUES "gs_detlevel_3D_m" 2, 1, 0					! --- MAIN PARAMETERS --- !IF NOT(SetProgram) THEN	minW = 0.3	IF A<minW THEN		A=minW		PARAMETERS A=minW	ENDIF	VALUES "A" RANGE [minW,]	minD = 0.3	IF B<minD THEN		B=minD		PARAMETERS B=minD	ENDIF	VALUES "B" RANGE [minD,]	IF ZZYZX<0.1 THEN		ZZYZX=0.1		PARAMETERS ZZYZX=ZZYZX	ENDIF	VALUES "ZZYZX" RANGE [0.1,]ENDIFDIM stDirHCWaterConn[]	stDirHCWaterConn[1] = `Прямой`	stDirHCWaterConn[2] = `Вертикальный`VALUES "DirHCWaterConn" stDirHCWaterConnVALUES "iDirHCWaterConn" 1, 2IF GLOB_MODPAR_NAME = "DirHCWaterConn" THEN	IF DirHCWaterConn = stDirHCWaterConn[1] THEN iDirHCWaterConn = 1	IF DirHCWaterConn = stDirHCWaterConn[2] THEN iDirHCWaterConn = 2	PARAMETERS iDirHCWaterConn = iDirHCWaterConnELSE	IF iDirHCWaterConn = 1 THEN DirHCWaterConn = stDirHCWaterConn[1]	IF iDirHCWaterConn = 2 THEN DirHCWaterConn = stDirHCWaterConn[2]	PARAMETERS DirHCWaterConn = DirHCWaterConnENDIF					! --- OBJECT SPECIFIC PARAMETERS --- !IF NOT(SetProgram) THEN	epsSpec = MAX(EPS, R-ConWidth_3/2)	minX = edgeThick+ConWidth_3/2+epsSpec	maxX = A/2-edgeThick/2-ConWidth_3/2-epsSpec	minY = edgeThick+ConWidth_3/2+epsSpec	maxY = B-edgeThickTop-ConWidth_3/2-epsSpec	VALUES "ConPosX_3" RANGE [minX, maxX]	VALUES "ConPosY_3" RANGE [minY, maxY]	IF ConPosX_3<minX THEN		ConPosX_3=minX		PARAMETERS ConPosX_3=minX	ENDIF	IF ConPosX_3>maxX THEN		ConPosX_3=maxX		PARAMETERS ConPosX_3=maxX	ENDIF	IF ConPosY_3<minY THEN		ConPosY_3=minY		PARAMETERS ConPosY_3=minY	ENDIF	IF ConPosY_3>maxY THEN		ConPosY_3=maxY		PARAMETERS ConPosY_3=maxY	ENDIF	epsSpec = MAX(EPS, R-ConWidth_4/2)	minX = A/2+edgeThick/2+ConWidth_4/2+epsSpec	maxX = A-edgeThick-ConWidth_4/2-epsSpec	minY = edgeThick+ConWidth_4/2+epsSpec	maxY = B-edgeThickTop-ConWidth_4/2-epsSpec	VALUES "ConPosX_4" RANGE [minX, maxX]	VALUES "ConPosY_4" RANGE [minY, maxY]	IF ConPosX_4<minX THEN		ConPosX_4=minX		PARAMETERS ConPosX_4=minX	ENDIF	IF ConPosX_4>maxX THEN		ConPosX_4=maxX		PARAMETERS ConPosX_4=maxX	ENDIF	IF ConPosY_4<minY THEN		ConPosY_4=minY		PARAMETERS ConPosY_4=minY	ENDIF	IF ConPosY_4>maxY THEN		ConPosY_4=maxY		PARAMETERS ConPosY_4=maxY	ENDIF	PARAMETERS ConPosZ_3 = -ZZYZX	PARAMETERS ConPosZ_4 = -ZZYZXENDIFIF GLOB_MODPAR_NAME = "ConPosY_1" THEN	ConPosY_2 = ConPosY_1	PARAMETERS ConPosY_2 = ConPosY_2ENDIFIF GLOB_MODPAR_NAME = "ConPosY_2" THEN	ConPosY_1 = ConPosY_2	PARAMETERS ConPosY_1 = ConPosY_1ENDIFIF iDirHCWaterConn = 2 THEN	ConPosZ_1 = 0	PARAMETERS ConPosZ_1 = ConPosZ_1	ConPosZ_2 = 0	PARAMETERS ConPosZ_2 = ConPosZ_2ENDIF					! --- CONNECTIONS --- !PARAMETERS MEP_NumberConnections = 4IF GLOB_MODPAR_NAME = "ZZYZX" THEN	ConPosZ_3=-ZZYZX	PARAMETERS ConPosZ_3=ConPosZ_3	ConPosZ_4=-ZZYZX	PARAMETERS ConPosZ_4=ConPosZ_4ENDIF					! --- COLD WATER CONNECTION --- !ConID = 1ConName = `Соединение Трубы - Холодная Вода`ConDVecX = 0IF iDirHCWaterConn = 1 THEN	ConDVecY = 1	ConDVecZ = 0ELSE	ConDVecY = 0	ConDVecZ = -1ENDIFConWVecX = 1ConWVecY = 0ConWVecZ = 0IF NOT(SetProgram) THEN	maxCW = A/4	VALUES "ConLength_1" RANGE [0,]	VALUES "ConWidth_1" RANGE (0, maxCW]	VALUES "ConDepth_1" RANGE (0, maxCW]	LOCK "ConType_1"	IF ConWidth_1<EPS THEN		ConWidth_1=EPS		PARAMETERS ConWidth_1=EPS	ENDIF	IF ConWidth_1>maxCW THEN		ConWidth_1=maxCW		PARAMETERS ConWidth_1=maxCW	ENDIF	VALUES "ConWallThickness_1" RANGE [0,ConWidth_1/2]ENDIFCALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	gs_Connections = gs_Connections,	MEP_NumConnectionData = MEP_NumConnectionData,	MEP_StrConnectionData = MEP_StrConnectionData,	MEP_NumberConnections = MEP_NumberConnections,	gs_AddConnections = gs_AddConnections,	ConName = ConName,	ConID = ConID,	ConPosX = ConPosX_1,	ConPosY = ConPosY_1,	ConPosZ = ConPosZ_1,	ConDVecX = ConDVecX,	ConDVecY = ConDVecY,	ConDVecZ = ConDVecZ,	ConWVecX = ConWVecX,	ConWVecY = ConWVecY,	ConWVecZ = ConWVecZ,	ConNominalWidth = ConNominalWidth_1,	ConNominalDepth = ConNominalDepth_1,	ConWidth = ConWidth_1,	ConDepth = ConDepth_1,	ConLength = ConLength_1,	ConWallThickness = ConWallThickness_1,	ConConnectorWidth = ConConnectorWidth_1,	ConConnectorDepth = ConConnectorDepth_1,	ConConnectorWidth2 = ConConnectorWidth2_1,	ConToolType = ConToolType_1,	ConStatus = ConStatus_1,	ConType_1 = ConType_1,	ConConnectorType_1 = ConConnectorType_1,	ConSystem_1 = ConSystem_1,	ui_page_idx = ui_page_idx,	ui_current_con = ui_current_con,	shape_typ_to_validation = shape_typ_to_validation,	MEP_enabled_geometry_mod = 1,	bUIConnectionStatusCheckbox = 1PARAMETERS MEP_ConInfo[ConID][1] = STR(ConToolType_1, 1, 0)PARAMETERS MEP_ConInfo[ConID][2] = ConNamePARAMETERS MEP_ConInfo[ConID][3] = "0"					! --- HOT WATER CONNECTION --- !ConID = 2ConName = `Соединение Трубы - Горячая Вода`ConDVecX = 0IF iDirHCWaterConn = 1 THEN	ConDVecY = 1	ConDVecZ = 0ELSE	ConDVecY = 0	ConDVecZ = -1ENDIFConWVecX = 1ConWVecY = 0ConWVecZ = 0IF NOT(SetProgram) THEN	maxCW = A/4	VALUES "ConLength_2" RANGE [0,]	VALUES "ConWidth_2" RANGE (0, maxCW]	VALUES "ConDepth_2" RANGE (0, maxCW]	LOCK "ConType_2"	IF ConWidth_2<EPS THEN		ConWidth_2=EPS		PARAMETERS ConWidth_2=EPS	E