! Temperature GaugeUI_DIALOG `Специальные Параметры MEP`DIM ui_vis_attr[5][4]	FOR i = 1 TO 5		FOR j = 1 TO 4			ui_vis_attr[i][j] = 0		NEXT j	NEXT i	ui_vis_attr[1][1] = 3	ui_vis_attr[1][2] = 1	ui_vis_attr[1][3] = 6	ui_vis_attr[1][4] = 7	ui_vis_attr[2][1] = 1	ui_vis_attr[2][2] = 237	ui_vis_attr[2][3] = 68	ui_vis_attr[2][4] = 14DIM ui_con_type[1]	ui_con_type[1] = 0IF useSysMat AND MEP_StrConnectionData[1][2] <> "0" THEN	ui_vis_attr[5][2] = 4ELSE	ui_vis_attr[5][2] = 0ENDIFIF MEP_StrConnectionData[1][2] = "0" THEN	ui_vis_attr[4][2] = 4ELSE	ui_vis_attr[4][2] = 0ENDIFCALL "MEP_m_ConnectionsMEP_4" PARAMETERS ALL ui_vis_attr = ui_vis_attr,											ui_centerline_vars = 0,											ui_con_type = ui_con_type,											ui_system_var_pos = 2