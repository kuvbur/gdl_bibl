!-----------2d---------------------if show_el < EPS then endIF gs_detlevel_2D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, gs_fill_type, gs_fill_pen, gs_back_pen, MEP_cline_type, MEP_cline_pen, show_el! Electric Wall HeaterPEN gs_cont_penSET FILL gs_fill_type				! --- MAIN BOX --- !POLY2_B 4, 1+2+4+64, gs_fill_pen, gs_back_pen,	-A/2, -frontTh, 1,	A/2, -frontTh, 1,	A/2, 0, 1,	-A/2, 0, 1unID = 1HOTSPOT2 -A/2, -frontTh, unID: unID=unID+1HOTSPOT2 A/2, -frontTh, unID: unID=unID+1HOTSPOT2 -A/2, 0, unID: unID=unID+1HOTSPOT2 A/2, 0, unID: unID=unID+1POLY2_B 4, 1+2+4+64, gs_fill_pen, gs_back_pen,	-boxX/2, 0, 1,	-boxX/2, boxY, 1,	boxX/2, boxY, 1,	boxX/2, 0, 1HOTSPOT2 0, 0, unID: unID=unID+1HOTSPOT2 -boxX/2, 0, unID: unID=unID+1HOTSPOT2 boxX/2, 0, unID: unID=unID+1HOTSPOT2 -boxX/2, boxY, unID: unID=unID+1HOTSPOT2 boxX/2, boxY, unID: unID=unID+1END