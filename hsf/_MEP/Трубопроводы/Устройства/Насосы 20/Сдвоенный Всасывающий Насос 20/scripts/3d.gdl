!-----------3d---------------------if show_el < EPS and GLOB_VIEW_TYPE<>2 then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_el! Close Coupled End Suct PumpIF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFIF gs_detlevel_3D_m = 0 THEN ENDres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! SimpleMainBoxMaterial = sMatMATERIAL MainBoxMaterialPEN gs_cont_penRESOL res				! --- CLOSE COUPLED END SUCTION PUMP --- !unID = 1HOTSPOT 0, 0, 0, unID: unID=unID+1HOTSPOT A, 0, 0, unID: unID=unID+1IF gs_detlevel_3D_m = 2 THEN	! Detailed	ADDx houseThk/2	REVOLVE 5,360,1+2+4+8+16+32,		-houseThk/2, houseR-hR, 1,		0,1, 801,		-houseThk/2+hR,houseR, 1001,		 houseThk/2-hR,houseR, 1,		 houseThk/2, houseR-hR, 1001	ROTy 90	CUTPOLYA 2,1,0,		0,0,913,		houseR,360,4013	DEL 1	ADD ConPosX_2-houseThk/2, ConPosY_2, ConPosZ_2	ROTy 90	REVOLVE 2,360,1+2+4+8+16+32,			0,			ConWidth_2/2, 2,			ABS(ConPosZ_2),	ConWidth_2/2, 2	DEL 2	CUTEND	ADDx houseThk/2	FOR i=1 TO 2		CUTPOLYA 4,1,0,			0.20*motorNeckL, motorR/4, 15,			0.20*motorNeckL,-motorR/4, 15,			0.85*motorNeckL,-motorR/3, 15,			0.85*motorNeckL, motorR/3, 15		ROTx 90	NEXT i	DEL 2	MULx motorL/(motorL+motorR/8)	! --- Motor	REVOLVE 8,360, 1+2+4+8+16+32,		0, 0.7*motorR, 2,		motorNeckL, motorR, 2,		motorL-motorBackL, motorR, 2,		motorL-motorBackL, 1.05*motorR, 2,		motorL-mR, 1.05*motorR, 1,		motorL, 1.05*motorR-mR, 1001,		motorL, motorR/5, 2,		motorL+motorR/8, motorR/5, 2	DEL 3	CUTEND	CUTENDENDIFIF gs_detlevel_3D_m = 1 THEN	! Simple	ADDx houseThk/2	REVOLVE 5,360,1+2+4+8+16+32,		-houseThk/2, houseR-hR, 1,		0,1, 801,		-houseThk/2+hR,houseR, 1,		 houseThk/2-hR,houseR, 1,		 houseThk/2, houseR-hR, 1	ROTy 90	CUTPOLYA 2,1,0,		0,0,913,		houseR,360,4013	DEL 1	ADD ConPosX_2-houseThk/2, ConPosY_2, ConPosZ_2	ROTy 90	REVOLVE 2,360,1+2+4+8+16+32,			0,		ConWidth_2/2, 2,			ABS(ConPosZ_2),	ConWidth_2/2, 2	DEL 2	CUTEND	ADDx houseThk/2	FOR i=1 TO 2		CUTPOLYA 4,1,0,			0.20*motorNeckL, motorR/4, 15,			0.20*motorNeckL,-motorR/4, 15,			0.85*motorNeckL,-motorR/3, 15,			0.85*motorNeckL, motorR/3, 15		ROTx 90	NEXT i	DEL 2	! --- Motor	REVOLVE 4,360, 1+2+4+8+16+32,		0, 0.7*motorR, 2,		motorNeckL, motorR, 2,		motorL-mR, motorR, 2,		motorL, motorR-mR, 1	DEL 2	CUTEND	CUTENDENDIFDEL TOPRESOL res				! --- CONNECTIONS --- !CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	ui_current_con = ui_current_con,	MEP_NumberConnections = MEP_NumberConnections,	MEP_NumConnectionData = gs_Connections,	gs_AddConnections = gs_AddConnections,	cShow3D = 1,	gs_ConMat = MainBoxMaterial,	MEP_InsShow = 0,	MEP_cline_show_3D = 0,	gs_cont_pen = gs_cont_pen,	ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 1,	ConPosX_2=ConPosX_2, ConPosY_2=ConPosY_2, ConPosZ_2=ConPosZ_2, ConLength_2=ConLength_2, ConEdit_2 = 13, MEP_StrConnectionData = MEP_StrConnectionDataEND