!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_el! 5-20 LPS Water SoftenerIF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFIF gs_detlevel_3D_m = 0 THEN ENDres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! SimpleIF useSysMat THEN MainBoxMaterial = sMatIF useSysMat THEN operatorMaterial = sMatMATERIAL MainBoxMaterialPEN gs_cont_penRESOL res				! --- WATER SOFTENER --- !unID = 1HOTSPOT  0, 0, 0, unID: unID=unID+1HOTSPOT  mineralTankPos, 0, 0, unID: unID=unID+1HOTSPOT  brineTankPos, 0, 0, unID: unID=unID+1HOTSPOT  0, 0, mineralTankHeight, unID: unID=unID+1HOTSPOT  mineralTankPos, 0, mineralTankHeight, unID: unID=unID+1HOTSPOT  brineTankPos, 0, brineTankHeight, unID: unID=unID+1HOTSPOT -mineralTankDiam/2, -mineralTankDiam/2, 0, unID: unID=unID+1HOTSPOT -mineralTankDiam/2,  mineralTankDiam/2, 0, unID: unID=unID+1HOTSPOT  mineralTankPos+mineralTankDiam/2,  mineralTankDiam/2, 0, unID: unID=unID+1HOTSPOT  mineralTankPos+mineralTankDiam/2, -mineralTankDiam/2, 0, unID: unID=unID+1IF GLOB_CONTEXT <> 23 THEN	PRISM 4, 0.05,		-mineralTankDiam/2, -mineralTankDiam/2,		-mineralTankDiam/2,  mineralTankDiam/2,		 mineralTankPos+mineralTankDiam/2,  mineralTankDiam/2,		 mineralTankPos+mineralTankDiam/2, -mineralTankDiam/2ENDIFIF gs_detlevel_3D_m = 2 AND GLOB_CONTEXT <> 23 THEN	! Detailed	GOSUB 200	ADDx mineralTankPos	GOSUB 200	DEL 1	ADDx brineTankPos	PRISM_ 2, brineTankHeight,		0,0, 979,		brineTankDiam/2,360,4079	DEL 1	MATERIAL operatorMaterial	dR = 0.04	CUTPOLYA 2,1,0,		0,0, 979,		mineralTankDiam/2,360,4079	RESOL 12	ADD mineralTankDiam/2, valveDepth/2, ConPosZ_8	ROTx 90	PRISM_ 6, valveDepth,		-mineralTankDiam/2,-valveHeight/2,	15,		-mineralTankDiam/2, valveHeight/2,	15,		 valveWidth-dR, valveHeight/2,	79,		 valveWidth, valveHeight/2-dR,	1079,		 valveWidth, -valveHeight/2+dR,	79,		 valveWidth-dR, -valveHeight/2,	1079	DEL 2	CUTEND	GOSUB 100ENDIFIF gs_detlevel_3D_m = 1 AND GLOB_CONTEXT <> 23 THEN	! Simple	GOSUB 210	ADDx mineralTankPos	GOSUB 210	DEL 1	ADDx brineTankPos	PRISM_ 2, brineTankHeight,		0,0, 979,		brineTankDiam/2,360,4079	DEL 1	MATERIAL operatorMaterial	CUTPOLYA 2,1,0,		0,0, 979,		mineralTankDiam/2,360,4079	ADD mineralTankDiam/2, valveDepth/2, ConPosZ_8	ROTx 90	PRISM 4, valveDepth,		-mineralTankDiam/2,-valveHeight/2,		-mineralTankDiam/2, valveHeight/2,		 valveWidth, valveHeight/2,		 valveWidth, -valveHeight/2	DEL 2	CUTEND	GOSUB 100ENDIFDEL TOPRESOL res				! --- CONNECTIONS --- !CALL "MEP_m_ConnectionsMEP_4" PARAMETERS SetProgram = SetProgram,	ui_current_con = ui_current_con,	MEP_NumberConnections = MEP_NumberConnections,	MEP_NumConnectionData = gs_Connections,	gs_AddConnections = gs_AddConnections,	cShow3D = 1,	gs_ConMat = MainBoxMaterial,	MEP_InsShow = 0,	MEP_cline_show_3D = 0,	gs_cont_pen = gs_cont_pen,	ConPosX_1=ConPosX_1, ConPosY_1=ConPosY_1, ConPosZ_1=ConPosZ_1, ConLength_1=ConLength_1, ConEdit_1 = 14,	ConPosX_2=ConPosX_2, ConPosY_2=ConPosY_2, ConPosZ_2=ConPosZ_2, ConLength_2=ConLength_2, ConEdit_2 = 14,	ConPosX_3=ConPosX_3, ConPosY_3=ConPosY_3, ConPosZ_3=ConPosZ_3, ConLength_3=ConLength_3, ConEdit_3 = 14,	ConPosX_4=ConPosX_4, ConPosY_4=ConPosY_4, ConPosZ_4=ConPosZ_4, ConLength_4=ConLength_4, ConEdit_4 = 14,	ConPosX_5=ConPosX_5, ConPosY_5=ConPosY_5, ConPosZ_5=ConPosZ_5, ConLength_5=ConLength_5, ConEdit_5 = 14,	ConPosX_6=ConPosX_6, ConPosY_6=ConPosY_6, ConPosZ_6=ConPosZ_6, ConLength_6=ConLength_6, ConEdit_6 = 14,	ConPosX_7=ConPosX_7, ConPosY_7=ConPosY_7, ConPosZ_7=ConPosZ_7, ConLength_7=ConLength_7, ConEdit_7 = 14,	ConPosX_8=ConPosX_8, ConPosY_8=ConPosY_8, ConPosZ_8=ConPosZ_8, ConLength_8=ConLength_8, ConEdit_8 = 16, MEP_StrConnectionData = MEP_StrConnectionDataEND100:	! Upper Pipe	MATERIAL MainBoxMaterial	R = MAX(0.06, 2*pR)	difiH = mineralTankHeight - brineTankHeight	IF difiH > 0 THEN		pipeH1 = 0.2		pipeH2 = difiH + 0.2	ELSE		pipeH1 = 0.2 - difiH		pipeH2 = 0.2	ENDIF	ADDz mineralTankHeight	ROTy -90	REVOLVE 2, 360, 1+2,		0,		pR, 1,		pipeH1,	pR, 1	ADD pipeH1, 0,-R	ROTy 90	ROTz 90	REVOLVE 2, 90, 1+2,		0,  R,    901,		pR, 360, 4001	ROTy 90	ROTz -90	ADDy -R	REVOLVE 2, 360, 1+2,		0,		pR, 1,		brineTankPos-2*R,	pR, 1	DEL 8	ADD mineralTankPos, 0, mineralTankHeight+pipeH1+R	ROTy 90	ARMC pR, pR, pipeH1+R, 0,0,90	DEL 2	ADD brineTankPos, 0, brineTankHeight	ROTy -90	REVOLVE 2, 360, 1+2,		0,		pR, 1,		pipeH2,	pR, 1	ADD pipeH2, 0,R	ROTy 90	ROTz -90	REVOLVE 2, 90, 1+2,		0,  R,    901,		pR, 360, 4001	DEL 5RETURN200:	RESOL 12	fZ = footH-0.05	FOR i=1 TO 4		ADD mineralTankDiam/3, 0, 0.05		CYLIND fZ/2, 0.04		ADDz fZ/2		CYLIND fZ/2, 0.06		DEL 2		ROTz 90	NEXT i	DEL 4	dhX = mineralTankHeight-hX	RESOL res	ROTy -90	REVOLVE 5, 360, 1+2+16+32,		footH, mineralTankDiam/2, 2,		hX-0.03, mineralTankDiam/2, 2,		hX, mineralTankDiam/2-0.03, 1,		mineralTankHeight-dhX/4, 0.2*mineralTankDiam, 1,		mineralTankHeight, 0.002, 1	ADDx hX-0.03	ROTy 90	MODEL WIRE	CIRCLE mineralTankDiam/2	MODEL SOLID	DEL 3RETURN210:	RESOL 6	FOR i=1 TO 4		ADD mineralTankDiam/3, 0, 0.05		CYLIND footH-0.05, 0.04		DEL 1		ROTz 90	NEXT i	DEL 4	RESOL res	ROTy -90	REVOLVE 4, 360, 1+2+16+32,		footH, mineralTankDiam/2, 2,		hX, mineralTankDiam/2, 1,		0, 0, 901,		mineralTankHeight, 0.002, 3001	ADDx hX	ROTy 90	MODEL WIRE	CIRCLE mineralTankDiam/2	MODEL SOLID	DEL 3RETURN