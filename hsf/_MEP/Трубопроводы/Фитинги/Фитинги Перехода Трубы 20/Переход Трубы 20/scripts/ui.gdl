UI_DIALOG `Специальные Параметры MEP`						! ---------- !						! --- UI --- !						! ---------- !DIM ui_set_str[][2]pID = 1ui_set_str[pID][1] = "(A) "+`Длина`ui_set_str[pID][2] = "MEP_StraightLength"pID = pID + 1ui_set_str[pID][1] = `Толщина стенки`ui_set_str[pID][2] = "mep_th_elem"pID = pID + 1ang_pID = pIDui_set_str[pID][1] = `Угол`ui_set_str[pID][2] = "MEP_TransformerAngle"pID = pID + 1offsety_pID = pIDui_set_str[pID][1] = "(B) "+`Смещение`ui_set_str[pID][2] = "MEP_offset_y"pID = pID + 1offsetz_pID = pIDui_set_str[pID][1] = "(C) "+`Смещение`ui_set_str[pID][2] = "MEP_offset_z"pID = pID + 1ui_set_str[pID][1] = `Изоляция`ui_set_str[pID][2] = "MEP_InsShow"pID = pID + 1ins_pID = pIDui_set_str[pID][1] = `Толщина`ui_set_str[pID][2] = "MEP_InsThickness"DIM ui_set_attr[][9]	FOR i = 2 TO pID+1		FOR j = 1 TO 9			ui_set_attr[i][j] = 0		NEXT j	NEXT i	ui_set_attr[1][1] = 1	ui_set_attr[1][2] = 3	ui_set_attr[1][3] = 220	ui_set_attr[1][4] = 50	ui_set_attr[1][5] = 127	ui_set_attr[1][6] = 90	ui_set_attr[1][7] = 15	ui_set_attr[1][8] = 128	ui_set_attr[1][9] = 23IF ABS(MEP_NumConnectionData[1][21] - 2) < EPS AND ABS(MEP_NumConnectionData[2][21] - 2) < EPS THEN	ui_set_attr[ang_pID+1][2] = 4	IF NOT(MEP_offset_y) AND NOT(MEP_offset_z) THEN		ui_set_attr[ang_pID+1][2] = 0	ENDIFENDIFui_set_attr[offsety_pID+1][2] = 0ui_set_attr[offsetz_pID+1][2] = 0ui_set_attr[ins_pID][3] = 1DIM ui_vis_attr[6][5]	FOR i = 1 TO 6		FOR j = 1 TO 5			ui_vis_attr[i][j] = 0		NEXT j	NEXT i	ui_vis_attr[1][1] = 4	ui_vis_attr[1][2] = 1	ui_vis_attr[1][3] = 6	ui_vis_attr[1][4] = 2	ui_vis_attr[1][5] = 4	ui_vis_attr[2][1] = 1	ui_vis_attr[2][2] = 237	ui_vis_attr[2][3] = 68	ui_vis_attr[2][4] = 14DIM ui_con_type[2]	ui_con_type[1] = 1	ui_con_type[2] = 3ui_set_attr[ang_pID+1][2] = 4IF NOT(MEP_offset_y) AND NOT(MEP_offset_z) THEN	ui_set_attr[ang_pID+1][2] = 0ENDIFui_set_attr[offsety_pID+1][2] = 0ui_set_attr[offsetz_pID+1][2] = 0ui_set_attr[ins_pID][3] = 1IF useSysMat AND MEP_StrConnectionData[1][2] <> "0" THEN	ui_vis_attr[5][2] = 4	ui_vis_attr[6][2] = 4ELSE	ui_vis_attr[5][2] = 0	IF MEP_InsShow THEN		ui_vis_attr[6][2] = 0	ELSE		ui_vis_attr[6][2] = 4	ENDIFENDIFIF MEP_InsShow THEN	ui_set_attr[ins_pID+1][2] = 0ELSE	ui_set_attr[ins_pID+1][2] = 4ENDIFIF MEP_StrConnectionData[1][2] = "0" THEN	ui_vis_attr[4][2] = 4ELSE	ui_vis_attr[4][2] = 0ENDIFCALL "MEP_m_ConnectionsMEP_4" PARAMETERS ALL ui_set_str = ui_set_str,										ui_set_attr = ui_set_attr,										ui_vis_attr = ui_vis_attr,										ui_centerline_vars = 1,										ui_con_type = ui_con_type,										ui_system_var_pos = 1setPage = 1ui_cur_page = 0FOR j = 1 TO VARDIM1(ui_page_idx)	IF ui_page_idx[j] = setPage THEN ui_cur_page = jNEXT jIF ABS(gs_ui_current_page - ui_cur_page) < EPS THEN	UI_PAGE gs_ui_current_page	UI_PICT 1, 30, 45, 156, 200, 1ENDIF