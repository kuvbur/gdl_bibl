!-----------2d---------------------if show_el < EPS then endIF gs_detlevel_2D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, gs_fill_type, gs_fill_pen, gs_back_pen, MEP_cline_type, MEP_cline_pencall "error_sphere" parameters is_error = is_errorPEN gs_cont_penSET FILL gs_fill_typehotspot2 0,0PEN gs_cont_pen_pTM = GLOB_SCALE / 1000if show_conventions and show_axis then end!	flange_d = 1.5 * _pTM!	flange_t = 0.5 * _pTM!	flange_t1 = 0!	flange_db2 = 0!	flange_full_t = flange_t!endifif SetProgram then	!!!================= Настройки видимости по диапазону ======================	!!!По умолчанию - вид не в сечении, элемент видим	is_sech = 0	is_show = 1	!!!Абсолютные значения границ видимости	rx1 = MEP_Position[1][1]	ry1 = MEP_Position[1][2]	rz1 = MEP_Position[1][3]	rx2 = rx1+MEP_Position[2][1]*A	ry2 = ry1+MEP_Position[2][2]*A	rz2 = rz1+MEP_Position[2][3]*A	dx = rx2-rx1	dy = ry2-ry1	dz = rz2-rz1	flag = 0	SYMB_POS_Z_temp = SYMB_POS_Z	IF SGN(dz) = 0 then		_MEP_StraightLength = 0	endif	IF SGN(dx) = 0 AND SGN(dy) = 0 and flag = 0 THEN		_MEP_StraightLength = A		IF rz1 > rz2 THEN			SYMB_POS_Z_temp = SYMB_POS_Z			SYMB_POS_Z = SYMB_POS_Z-_MEP_StraightLength		ENDIF		flag = 1	endif	if flag = 0 then		_MEP_StraightLength = A		Length=SQR(dx^2+dy^2)		ar = ABS(dz)/_MEP_StraightLength		br = Length/_MEP_StraightLength		cr = ABS(dz)/Length		IF rz1 > rz2 THEN			SYMB_POS_Z_temp = SYMB_POS_Z			SYMB_POS_Z = SYMB_POS_Z-_MEP_StraightLength		ENDIF		dz_temp = ABS(dz)		dz = ABS(dz)-cutbackA*ar-cutbackB*ar		SYMB_POS_Z_temp = SYMB_POS_Z		SYMB_POS_Z = SYMB_POS_Z+cutbackA*ar		flag = 1	endif	down_level = GLOB_CSTORY_ELEV + GLOB_CUTPLANES_INFO[3]+SYMB_POS_Z	up_level = GLOB_CSTORY_ELEV + GLOB_CUTPLANES_INFO[2]+SYMB_POS_Z	!!Ниже границы видимости	IF down_level - SYMB_POS_Z > EPS then		if (down_level - (SYMB_POS_Z+_MEP_StraightLength)) > EPS THEN !!Закончился до границы			is_show = 0		else				is_sech = 1		endif	endif	!!Выше границы видимости	IF up_level - SYMB_POS_Z < EPS then		if up_level - (SYMB_POS_Z+_MEP_StraightLength) < EPS THEN !!Закончился до границы			is_show = 0		else			is_sech = 1		endif	endif	if is_sech then is_show = 1	SYMB_POS_Z = SYMB_POS_Z_temp	!!!================= Настройки видимости по диапазону ======================	if AC_AutoOnStoryVisibility and is_show=0 THEN end	rx = rx1+MEP_Position[2][1]*flange_full_t+MEP_Position[3][1]*MEP_offset_y+dZx*MEP_offset_z	ry = ry1+MEP_Position[3][2]*MEP_offset_y+MEP_Position[2][2]*flange_full_t+dZy*MEP_offset_z	HOTSPOT2 rx1, ry1, hceID_1	HOTSPOT2 rx, ry, hceID_2endifIF SGN(dz) = 0 THEN		! -----> the pipe in the xy-plane <----- !	if show_axis and show_conventions then end	ADD2 rx1, ry1	ROT2 dirAngOnXYPlane	if flange_mir then rot2 180	hotspot2 0,-flange_d/2	hotspot2 0,flange_d/2	hotspot2 flange_full_t,-flange_d/2	hotspot2 flange_full_t,flange_d/2	if flange_type = 1 then		put 0,-flange_d/2,1		put 0,flange_d/2,1		put flange_t,flange_d/2,1		put flange_t,-flange_d/2,1	endif	if flange_type = 11 then		put 0,-flange_d/2,1		put flange_t,-flange_d/2,1		put flange_t,-flange_db1/2,1		put flange_t+flange_t1,-flange_db2/2,1		put flange_t+flange_t1+flange_t2,-flange_db2/2,1		put flange_t+flange_t1+flange_t2,flange_db2/2,1		put flange_t+flange_t1,flange_db2/2,1		put flange_t,flange_db1/2,1		put flange_t,flange_d/2,1		put 0,flange_d/2,1		line2 flange_t,-flange_d/2, flange_t,flange_d/2		hotspot2 flange_full_t,-flange_db2/2		hotspot2 flange_full_t,flange_db2/2		hotspot2 flange_full_t,0	endif	if flange_type = 2 then		put 0,-flange_d/2,1		put 0,-flange_db1/2,1		put -flange_t1,-flange_db1/2,1		put -flange_t1,flange_db1/2,1		put 0,flange_db1/2,1		put 0,flange_d/2,1		put flange_t,flange_d/2,1		put flange_t,-flange_d/2,1		line2 0,-flange_db1/2, 0,flange_db1/2		hotspot2 -flange_t1,-flange_db1/2		hotspot2 -flange_t1,flange_db1/2		hotspot2 -flange_t1,0	endif	if flange_type = 14 then		if flange_subtype=2 then			put flange_t1,-flange_db2/2,1			put 0,-flange_db2/2,1			put 0,flange_db2/2,1			put flange_t1,flange_db2/2,1			put flange_t1,flange_d/2,1			put flange_t+flange_t1,flange_d/2,1			put flange_t+flange_t1,flange_db1/2,1			put flange_t+flange_t2+flange_t1,flange_db1/2,1			put flange_t+flange_t2+flange_t1,-flange_db1/2,1			put flange_t+flange_t1,-flange_db1/2,1			put flange_t+flange_t1,-flange_d/2,1			put flange_t1,-flange_d/2,1			put flange_t1,-flange_db2/2,1			put 0,-flange_db2/2,1			line2 flange_t1,-flange_d/2, flange_t1,flange_d/2			line2 flange_t1+flange_t,-flange_d/2, flange_t1+flange_t,flange_d/2			hotspot2 flange_t+flange_t2+flange_t1,-flange_db1/2			hotspot2 flange_t+flange_t2+flange_t1,flange_db1/2			hotspot2 flange_t+flange_t1,-flange_db1/2			hotspot2 flange_t+flange_t1,flange_db1/2!			line2 0,flange_dy/2,flange_full_t,flange_dy/2!			line2 0,-flange_dy/2,flange_full_t,-flange_dy/2		else			put 0,-flange_d/2,1			put 0,flange_d/2,1			put flange_t,flange_d/2,1			put flange_t,-flange_d/2,1		endif	endif	if show_axis then		gs_back_pen = 19		gs_fill_pen = 19	endif	if NSP>3 then POLY2_B NSP/3, 1*(show_axis=0)+2+4, gs_fill_pen, gs_back_pen, GET(NSP)	if show_axis then		line2 flange_t*0.9,-flange_d/2, flange_t*0.9,flange_d/2	endif	del 2	endendifIF SGN(dx) = 0 AND SGN(dy) = 0 and (show_axis=0) THEN		! -----> the pipe on the xy-plane <----- !	ADD2 rx1, ry1	ROT2 dirAngOnXYPlane	hotspot2 0,-flange_d/2	hotspot2 0,flange_d/2	PUT 0,0,900+1	PUT flange_d/2,360,4000+1	PUT flange_dnf/2,360,4000+1	POLY2_B NSP/3, 1*(show_axis=0)+2+4, gs_fill_pen, gs_back_pen, GET(NSP)	del 2	endendifIF gs_fill_pen THEN PEN gs_fill_penPROJECT2{3} 3, 270, 3+32, 4, gs_back_pen, 0, 0, 0PEN gs_cont_penPROJECT2{3} 3, 270, 2, 8, 0, 0, 0, 0