!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, bodyMaterial, gs_InsMat, MEP_cline_pen, show_el! Small Junction BoxIF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFIF gs_detlevel_3D_m = 0 THEN ENDres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! SimpleMATERIAL bodyMaterialPEN gs_cont_penRESOL res				! --- JUNCTION BOX --- !unID = 1ADDz -ZZYZX/2HOTSPOT 0, 0, 0, unID: unID=unID+1HOTSPOT  A/2, 0, 0, unID: unID=unID+1HOTSPOT -A/2, 0, 0, unID: unID=unID+1HOTSPOT  A/2, -B, 0, unID: unID=unID+1HOTSPOT -A/2, -B, 0, unID: unID=unID+1HOTSPOT  A/2, 0, ZZYZX, unID: unID=unID+1HOTSPOT -A/2, 0, ZZYZX, unID: unID=unID+1HOTSPOT  A/2, -B, ZZYZX, unID: unID=unID+1HOTSPOT -A/2, -B, ZZYZX, unID: unID=unID+1DEL 1gx = A/2gy = ZZYZX!IF gs_detlevel_3D_m = 2 AND GLOB_CONTEXT <> 23 THEN	! Detailed	ADDz -ZZYZX/2	ROTx 90	IF boxShapeID = 1 THEN		PRISM_ 9,B,				-gx,	gy-R,	79,				-gx,	R,		79,				-gx+R,	0,		1079,				 gx-R,	0,		79,				 gx, 	R,		1079,				 gx, 	gy-R,	79,				 gx-R,	gy,		1079,				-gx+R,	gy,		79,				-gx,	gy-R,	1079	ENDIF	IF boxShapeID = 2 THEN		alpha = (16 - 2) * 180 / 16		sideL = A / TAN(alpha/2)		sa = sideL * SIN(alpha)		ca = sideL * COS(alpha)		dL = 0.04 * A		PRISM_ 16,B,			-sideL/2-dL,		0,		15,			-sideL/2-dL + ca,	sa,		15,			-A/2 + sa, A/2 - sideL/2 - dL + ca, 15,			-A/2, A/2 - sideL/2 - dL, 15,			-A/2, A/2 + sideL/2 + dL, 15,			-A/2 + sa, A/2 + sideL/2 + dL - ca, 15,			-sideL/2 - dL + ca,	A - sa,		15,			-sideL/2 - dL,		A,		15,			 sideL/2 + dL,		A,		15,			 sideL/2 + dL - ca,	A - sa,		15,			 A/2 - sa, A/2 + sideL/2 + dL - ca, 15,			 A/2, A/2 + sideL/2 + dL, 15,			 A/2, A/2 - sideL/2 - dL, 15,			 A/2 - sa, A/2 - sideL/2 - dL + ca, 15,			 sideL/2 + dL - ca,	sa,		15,			 sideL/2 + dL,		0,		15	ENDIF	DEL 2!ENDIF!IF gs_detlevel_3D_m = 1 AND GLOB_CONTEXT <> 23  THEN	! Simple!ENDIF