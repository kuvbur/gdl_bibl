!-----------3d---------------------if show_el < EPS then endIF gs_detlevel_3D_m < EPS THEN ENDCALL "MEP_m_RequestSystemGlobals" PARAMETERS ALL one_ConID=0 returned_parameters gs_cont_pen, sMat, gs_InsMat, MEP_cline_pen, show_el! Metering SwitchboardIF gs_detlevel_3D_m = -1 THEN	gs_detlevel_3D_m = 2	! Detailed	IF gs_detlevel_3D = `Откл.` THEN gs_detlevel_3D_m = 0	IF gs_detlevel_3D = `Простой` THEN gs_detlevel_3D_m = 1ENDIFIF gs_detlevel_3D_m = 0 THEN ENDres = 24	! DetailedIF gs_detlevel_3D_m = 1 THEN res = 8	! SimpleMATERIAL bodyMaterialPEN gs_cont_penRESOL resDEFINE MATERIAL	"Shaft Black Plastic" 20,	0.147059, 0.135294, 0.103922,	0.9, 0.6, 0.2, 0, 10, 0,	1, 0.978103, 0.968383,	0, 0, 0,	0, 0, 1,	0DEFINE MATERIAL	"Shaft White Plastic" 20,	1, 1, 1,	0.98, 0.8, 0.25, 0, 3, 0,	1, 1, 1,	0.654902, 0.654902, 0.654902,	100, 0, -1,	0DEFINE MATERIAL	"Shaft Aluminum" 20,	0.854902, 0.913725, 0.905882,	0.98, 0.7, 0.5, 0, 7, 0,	0.907897, 0.907897, 0.907897,	0, 0, 0,	0, 0, 1,	0				! --- SWITCHBOARD  --- !unID = 1HOTSPOT 0, 0, 0, unID: unID=unID+1HOTSPOT A, 0, 0, unID: unID=unID+1HOTSPOT 0, -B, 0, unID: unID=unID+1HOTSPOT A, -B, 0, unID: unID=unID+1HOTSPOT 0, 0, ZZYZX, unID: unID=unID+1HOTSPOT A, 0, ZZYZX, unID: unID=unID+1HOTSPOT 0, -B, ZZYZX, unID: unID=unID+1HOTSPOT A, -B, ZZYZX, unID: unID=unID+1ADDy -BADDy 0.01BLOCK A, B-0.01, ZZYZXDEL 1ROTx 90ADDz -0.01FPRISM_ bodyMaterial,bodyMaterial,bodyMaterial,bodyMaterial,	4, 0.01, 45, 0.01,	0.005, footH-0.005, 15,	A-0.005, footH-0.005, 15,	A-0.005, 0, 15,	0.005, 0, 15DEL 2ROTx 90ADD 0, ZZYZX-headH, -0.01FPRISM_ bodyMaterial,bodyMaterial,bodyMaterial,bodyMaterial,	4, 0.01, 45, 0.01,	0.005, headH-0.005, 15,	A-0.005, headH-0.005, 15,	A-0.005, 0, 15,	0.005, 0, 15DEL 2yy = (ZZYZX-footH-headH) / numUnitsROTx 90ADD 0, footH, -0.01!FPRISM_ bodyMaterial,bodyMaterial,bodyMaterial,bodyMaterial,!	4, 0.01, 45, 0.01,!	A-sideW+0.005, ZZYZX-footH-headH-0.005, 15,!	A-0.005, ZZYZX-footH-headH-0.005, 15,!	A-0.005, 0, 15,!	A-sideW+0.005, 0, 15!DEL 2numSwitchs = int((A-sideW) / 0.15) - 1IF gs_detlevel_3D_m = 2 AND GLOB_CONTEXT <> 23 THEN	! Detailed	RESOL 12	FOR i=1 TO numUnits		FPRISM_ bodyMaterial,bodyMaterial,bodyMaterial,bodyMaterial,			4, 0.01, 45, 0.01,			0.005, yy/2-0.005, 15,			A-sideW, yy/2-0.005, 15,			A-sideW, 0, 15,			0.005, 0, 15		FPRISM_ bodyMaterial,bodyMaterial,bodyMaterial,bodyMaterial,			4, 0.01, 45, 0.01,			0.005, yy-0.005, 15,			A-sideW, yy-0.005, 15,			A-sideW, yy/2, 15,			0.005, yy/2, 15		ADD 0.08, yy/4-0.05, 0.01		MATERIAL	"Shaft Aluminum"		RESOL 6		ROTy -90		FOR j=1 TO 2			TUBE 2, 8, 1+2+16+32,				0,0,901,				0.01,360,4001,				-1,0,0, 0,				 0,0,0, 0,				 0.032,0,0, 0,				 0.040,0.008,0, 0,				 0.040,0.092,0, 0,				 0.032,0.100,0, 0,				 0,0.100,0, 0,				-1,0.100,0, 0			ADDz -(A-sideW-0.08*2)		NEXT j		DEL 4		ADD (A-sideW-(numSwitchs-1)*0.15)/2,yy/2+0.12,0.011		FOR j=1 TO numSwitchs			MATERIAL	"Shaft Black Plastic"			POLY_ 10,				-0.060, 0.050, 1,				-0.060,-0.050, 1,				 0.060,-0.050, 1,				 0.060, 0.050, 1,				-0.060, 0.050, -1,				-0.050, 0.040, 1,				-0.050,-0.020, 1,				 0.050,-0.020, 1,				 0.050, 0.040, 1,				-0.050, 0.040, -1			ROTz 30			POLY 4,				-0.001, 0.030,				-0.001,-0.020,				 0.001,-0.020,				 0.001, 0.030			DEL 1			MATERIAL "Shaft White Plastic"			POLY_ 4,				-0.050, 0.040, 1,				-0.050,-0.020, 1,				 0.050,-0.020, 1,				 0.050, 0.040, 1			ADDx 0.15		NEXT j		DEL numSwitchs + 1		ADDy yy	NEXT i	DEL numUnits + 2ENDIFIF gs_detlevel_3D_m = 1 AND GLOB_CONTEXT <> 23  THEN	! Simple	FOR i=1 TO numUnits		FPRISM_ bodyMaterial,bodyMaterial,bodyMaterial,bodyMaterial,			4, 0.01, 45, 0.01,			0.005, yy/2-0.005, 15,			A-sideW, yy/2-0.005, 15,			A-sideW, 0, 15,			0.005, 0, 15		FPRISM_ bodyMaterial,bodyMaterial,bodyMaterial,bodyMaterial,			4, 0.01, 45, 0.01,			0.005, yy-0.005, 15,			A-sideW, yy-0.005, 15,			A-sideW, yy/2, 15,			0.005, yy/2, 15		ADD 0.08, yy/4-0.05, 0.01		MATERIAL	"Shaft Aluminum"		RESOL 6		ROTy -90		FOR j=1 TO 2			ADDz -0.008			PRISM 8, 0.02,				0, -0.008,				0.048, -0.008,				0.048,  0.108,				0, 0.108,				0, 0.092,				0.032, 0.092,				0.032, 0.008,				0,0.008			DEL 1			ADDz -(A-sideW-0.08*2)		NEXT j		DEL 4		ADD (A-sideW-(numSwitchs-1)*0.15)/2,yy/2+0.12,0.011		FOR j=1 TO numSwitchs			MATERIAL	"Shaft Black Plastic"			POLY_ 10,				-0.060, 0.050, 1,				-0.060,-0.050, 1,				 0.060,-0.050, 1,				 0.060, 0.050, 1,				-0.060, 0.050, -1,				-0.050, 0.040, 1,				-0.050,-0.020, 1,				 0.050,-0.020, 1,				 0.050, 0.040, 1,				-0.050, 0.040, -1			MATERIAL "Shaft White Plastic"			POLY_ 4,				-0.050, 0.040, 1,				-0.050,-0.020, 1,				 0.050,-0.020, 1,				 0.050, 0.040, 1			ADDx 0.15		NEXT j		DEL numSwitchs + 1		ADDy yy	NEXT i	DEL numUnits + 2ENDIFDEL 1