

UI_DIALOG `Специальные Параметры MEP`


DIM ui_set_str[][2]

pID = 1
ui_set_str[pID][1] = "(A) "+`Длина`
ui_set_str[pID][2] = "MEP_StraightLength"

pID = pID + 1
ui_set_str[pID][1] = "(B) "+`Длина Соединения`
ui_set_str[pID][2] = "ConLength_3"

pID = pID + 1
ui_set_str[pID][1] = "(C) "+`Смещение Врезки`
ui_set_str[pID][2] = "MEP_BranchOffset"

pID = pID + 1
ui_set_str[pID][1] = "(D) "+`Угол Врезки`
ui_set_str[pID][2] = "MEP_BranchAngle"

pID = pID + 1
ui_set_str[pID][1] = "(E) "+`Основная Ширина`
ui_set_str[pID][2] = "ConWidth_1"

pID = pID + 1
ui_set_str[pID][1] = "(F) "+`Ширина Отвода`
