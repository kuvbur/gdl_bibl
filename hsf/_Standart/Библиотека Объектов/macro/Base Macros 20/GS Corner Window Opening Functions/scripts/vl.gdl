				iParapetBottom = i
				i = vardim1(stParapetBottom)
			endif
		next i
		parameters iParapetBottom = iParapetBottom
	else
		gs_parapet_bottom_st = stParapetBottom[min(max(1,iParapetBottom),vardim1(stParapetBottom))]
