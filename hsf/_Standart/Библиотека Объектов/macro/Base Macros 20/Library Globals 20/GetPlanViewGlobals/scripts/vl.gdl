

DIM st2Dsymbol[4]
	st2Dsymbol[1] = `Реалистичный`
	st2Dsymbol[2] = `Электрические Данные`
	st2Dsymbol[3] = `План Отраженного Потолка`
	st2Dsymbol[4] = `по ПМВ`

values "gs_symbtype_2D_m" PLANSYMB_REALISTIC, PLANSYMB_ELECTRIC, PLANSYMB_RCP, PLANSYMB_BY_MVO
values "gs_symbtype_2D" st2Dsymbol

if GLOB_MODPAR_NAME = "gs_symbtype_2D" then			!setting string to intiger and vice versa 2D symbol
	for iSymbType = 1 to 4
		if st2Dsymbol[iSymbType] = gs_symbtype_2D then
			gs_symbtype_2D_m = iSymbType
		endif
	next iSymbType
	parameters gs_symbtype_2D_m = gs_symbtype_2D_m
else
	parameters gs_symbtype_2D = st2Dsymbol[gs_symbtype_2D_m]
endif

if gs_symbtype_2D_m = PLANSYMB_BY_MVO then
	reqILSymbolTemp = 0
	success = LIBRARYGLOBAL ("LibraryGlobals13", "iLSymbol", reqILSymbolTemp)
	if success > 0 then
		gs_symbtype_2D_m = reqILSymbolTemp
	else
		gs_symbtype_2D_m = PLANSYMB_RCP
	endif
endif
end gs_symbtype_2D_m

