
if bPartOfLayout then
	call "completeTable_m" parameters all	iTableTopType		= TTOP_STRAIGHT,
											bCalledAsExtension	= 1,
											bMirroredCabPos		= 0,
											bKnobNoneType		= bKnobNoneType,
											zzyzx				= tableTopHeight

	call "officeCabinet02_m" parameters all	a					= extCabinetWidth,
											b					= extCabinetDepth,
											zzyzx				= extCabinetHeight,
											bKnobNoneType		= bKnobNoneType,
											bPartOfLayout		= 1

	values "extCabinetWidth" range [0.2, )
	values "extCabinetDepth" range [0.2, )
	values "extCabinetHeight" range [tableTopHeight, )

	if NOT(bShowExtDeskChairParams) then
		lock			"extraDeskChairDistX",
						"extraDeskChairDistY"
		hideparameter	"extraDeskChairDistX",
						"extraDeskChairDistY"
	endif
	if NOT(bShowExtCabParams) then
		lock			"extCabinetWidth",
						"extCabinetDepth",
						"extCabinetHeight"
	endif
	if NOT(bShowExtDeskParams) then
		lock			"extraDeskWidth",
						"extraDeskDepth"
	endif
	if NOT(bShowCabParams) or (bShowCabParams and NOT(bShowExtDeskParams)) then
		lock			"extraDeskCab",
						"iExtraDeskCab"
	endif
endif

call "partitions_m" parameters all

