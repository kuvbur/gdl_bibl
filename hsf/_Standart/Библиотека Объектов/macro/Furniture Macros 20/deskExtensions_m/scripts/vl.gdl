
! ======================================================================
! TableTop
! ======================================================================

call "tableTops_m" parameters all	enableTableTopTypes	= 8 + 16 + 32 + 64 + 128 + 256 + 512 + 1024 + 2048,
									bPartOfLayout		= 0,
									bCalledAsExtension	= 1

! ======================================================================
! Leg
! ======================================================================

call "fa_desklegtyp" parameters all	enableLegTypes			= enableLegTypes,
									bEnableLegbaseSurface	= 1

if gs_detlevel_3D_m = 1 then
	lock			"gs_resol"
	hideparameter	"gs_resol"
endif
if gs_detlevel_3D_m = 0 then
	lock			"gs_resol",
					"gs_shadow"
	hideparameter	"gs_resol",
					"gs_shadow"
endif

if enableLegTypes = 128 then
	lock			"gs_leg_type",
					"gs_leg_type_m"
	hideparameter	"gs_leg_type",
					"gs_leg_type_m"
endif
if iTableTopType = TTOP_RECT then
	lock			"MSFront"
	hideparameter	"MSFront"
else
	lock			"MSFront2",
					"MSRear"

	hideparameter	"MSFront2",
					"MSRear"
endif

! ======================================================================
! UI
! ======================================================================

values "gs_ui_current_page" 1

