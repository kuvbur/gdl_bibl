!!! ***************************************************** Comb. Mic-Oven
30:

IF ApplStyle_m=1 THEN

		!!! *** BASE Panels
		Addy b
		ROTX 90
		CPRISM_ BaseMat,BaseMat,PanelMat,
				15,b,
				0,0,15,
				a,0,15,
				a,ZZYZX,15,
				0,ZZYZX,15,
				0,0,-1,
				A*1/6+0.01,ZZYZX*1/8,15,
				A*5/6-0.01,ZZYZX*1/8,15,
				A*5/6-0.01,ZZYZX*3/8,15,
				A*1/6+0.01,ZZYZX*3/8,15,
				A*1/6+0.01,ZZYZX*1/8,-1,
				A*1/6+0.01,ZZYZX*5/8,15,
				A*5/6-0.01,ZZYZX*5/8,15,
				A*5/6-0.01,ZZYZX*6/8,15,
				A*1/6+0.01,ZZYZX*6/8,15,
				A*1/6+0.01,ZZYZX*5/8,-1

		gosub 1000

			Addz b/4
			MATERIAL GlassMat
			PRISM_ 5,b/2,
					A*1/6+0.01,ZZYZX*1/8,15,
					A*5/6-0.01,ZZYZX*1/8,15,
					A*5/6-0.01,ZZYZX*3/8,15,
					A*1/6+0.01,ZZYZX*3/8,15,
					A*1/6+0.01,ZZYZX*1/8,-1

		gosub 1000

			PRISM_ 5,b/2,
					A*1/6+0.01,ZZYZX*5/8,15,
					A*5/6-0.01,ZZYZX*5/8,15,
					A*5/6-0.01,ZZYZX*6/8,15,
					A*1/6+0.01,ZZYZX*6/8,15,
					A*1/6+0.01,ZZYZX*5/8,-1

		gosub 1000

			DEL 1
		DEL 2

		!!! *** Vet. Panels
		Add 0,-0.01,0
			FOR n=1 to 2
					MATERIAL PanelMat
					PRISM_ 5,ZZYZX*4/8-0.01,
							0,0,15,
							a/6,0,15,
							a/6,0.01,15,
							0,0.01,15,
							0,0,-1

					gosub 1000

			ADDx A*5/6
			NEXT n
			DEL 2

			ADDz ZZYZX*9/16
				FOR nn=1 to 2
						MATERIAL PanelMat
						PRISM_ 5,ZZYZX*2/8-0.01,
								0,0,15,
								a/6,0,15,
								a/6,0.01,15,
								0,0.01,15,
								0,0,-1

						gosub 1000

				ADDx A*5/6
				NEXT nn
				DEL 2
			DEL 1

		DEL 1
		!!! *** Control Panel
		ADDz ZZYZX-ZZYZX*1/8
		ROTX 90
				PRISM_ 9,0.01,
						0,0,15,
						0,ZZYZX*1/8,15,
						a,ZZYZX*1/8,15,
						a,0,15,
						A*5/6,0,15,
						A*5/6,ZZYZX*1/8*4/5,15,
						A*1/6,ZZYZX*1/8*4/5,15,
						A*1/6,0,15,
						0,0,-1

				gosub 1000

		DEL 2


		!!! *** Ventilation
		Addy -0.01
		FOR tt=1 to 2
			ADDZ ZZYZX*4/8
				FOR t=1 to 3
					PRISM_ 5,(ZZYZX*1/16)/6,
							0,0,15,
							a,0,15,
							a,0.01,15,
							0,0.01,15,
							0,0,-1

					gosub 1000

					ADDz (ZZYZX*1/16)/6*2
				NEXT t
				DEL 3
			DEL 1
			ADDZ ZZYZX*5/16
		NEXT tt
		DEL 2
		DEL 1


		!!! *** HANDLE
		N=A*4/6
		PP=1/8

			ADDZ ZZYZX*3/8+ZZYZX*1/8*1/3
			GOSUB 100
			DEL 1
ENDIF
IF ApplStyle_m=2 THEN

		!!! *** BASE Panels
		Addy b
		ROTX 90
		CPRISM_ BaseMat,BaseMat,PanelMat,
				15,b,
				0,0,15,
				a,0,15,
				a,ZZYZX,15,
				0,ZZYZX,15,
				0,0,-1,
				A*1/5+0.01,ZZYZX*4/8*1/4,15,
				A*4/5-0.01,ZZYZX*4/8*1/4,15,
				A*4/5-0.01,ZZYZX*4/8*3/4,15,
				A*1/5+0.01,ZZYZX*4/8*3/4,15,
				A*1/5+0.01,ZZYZX*4/8*1/4,-1,
				A*1/5+0.01,ZZYZX*4/8*5/4-0.005,15,
				A*4/5-0.01,ZZYZX*4/8*5/4-0.005,15,
				A*4/5-0.01,ZZYZX*4/8*6/4-0.005,15,
				A*1/5+0.01,ZZYZX*4/8*6/4-0.005,15,
				A*1/5+0.01,ZZYZX*4/8*5/4-0.005,-1

		gosub 1000

			Addz b/4
			MATERIAL GlassMat
			PRISM_ 5,b/2,
				A*1/5+0.01,ZZYZX*4/8*1/4,15,
				A*4/5-0.01,ZZYZX*4/8*1/4,15,
				A*4/5-0.01,ZZYZX*4/8*3/4,15,
				A*1/5+0.01,ZZYZX*4/8*3/4,15,
				A*1/5+0.01,ZZYZX*4/8*1/4,-1

		gosub 1000

			PRISM_ 5,b/2,
				A*1/5+0.01,ZZYZX*4/8*5/4-0.005,15,
				A*4/5-0.01,ZZYZX*4/8*5/4-0.005,15,
				A*4/5-0.01,ZZYZX*4/8*6/4-0.005,15,
				A*1/5+0.01,ZZYZX*4/8*6/4-0.005,15,
				A*1/5+0.01,ZZYZX*4/8*5/4-0.005,-1

		gosub 1000

			DEL 1
		DEL 2

		!!! *** Oven Doors


			ROTX 90
			MATERIAL PanelMat
			PRISM_ 10,0.01,
					0.01,0.01,15,
					a-0.01,0.01,15,
					a-0.01,ZZYZX*4/8-0.005,15,
					0.01,ZZYZX*4/8-0.005,15,
					0.01,0.01,-1,
					A*1/8,ZZYZX*4/8*1/4-0.01,15,
					A*7/8,ZZYZX*4/8*1/4-0.01,15,
					A*7/8,ZZYZX*4/8*3/4+0.01,15,
					A*1/8,ZZYZX*4/8*3/4+0.01,15,
					A*1/8,ZZYZX*4/8*1/4-0.01,-1

			gosub 1000

			ADDy ZZYZX*4/8-0.005
			PRISM_ 10,0.01,
					0.01,0.01,15,
					a-0.01,0.01,15,
					a-0.01,ZZYZX*3/8-0.005,15,
					0.01,ZZYZX*3/8-0.005,15,
					0.01,0.01,-1,
					A*1/8,ZZYZX*4/8*1/4-0.01,15,
					A*7/8,ZZYZX*4/8*1/4-0.01,15,
					A*7/8,ZZYZX*3/8-ZZYZX*4/8*1/4+0.01,15,
					A*1/8,ZZYZX*3/8-ZZYZX*4/8*1/4+0.01,15,
					A*1/8,ZZYZX*4/8*1/4-0.01,-1

			gosub 1000

			DEL 1
			DEL 1


		!!! *** Control Panel

			ADDz ZZYZX*7/8
			ROTX 90
			MATERIAL PanelMat
			PRISM_ 10,0.01,
					0,0,15,
					a,0,15,
					a,ZZYZX*1/8,15,
					0,ZZYZX*1/8,15,
					0,0,-1,
					A*1/8,ZZYZX*1/8*2/3,15,
					A*2/8,ZZYZX*1/8*2/3,15,
					A*2/8,ZZYZX*1/8*1/3,15,
					A*1/8,ZZYZX*1/8*1/3,15,
					A*1/8,ZZYZX*1/8*2/3,-1

			gosub 1000

			DEL 2


		!!! *** HANDLE
		N=A-0.02
		PP=1/8
		ADDy -0.01
			FOR nn=1 to 2
					ADDZ ZZYZX*4/8-ZZYZX*PP*1/3-0.005
					GOSUB 100
					DEL 1
			ADDz ZZYZX*3/8-0.005
			NEXT nn
			DEL 2

		!!! *** SWITCHES
		if gs_detlevel_3D_m = 2 then
			ADD A-A*1/8,0, ZZYZX*15/16
				GOSUB 200
			ADDx -A*3/32
				GOSUB 200
			ADDx -A*3/32
				GOSUB 200
	
			ADDx -A*6/32
				GOSUB 200
			ADDx -A*3/32
				GOSUB 200
	
			DEL 6
		endif

ENDIF

END

