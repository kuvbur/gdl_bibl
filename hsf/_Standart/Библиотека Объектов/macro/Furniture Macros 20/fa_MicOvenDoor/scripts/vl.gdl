	ENDIF

	IF ApplType_m=3 THEN
		PARAMETERS A=0.6 , ZZYZX=0.8
		A=0.6
		ZZYZX=0.8
	ENDIF

	IF ApplType_m=4 THEN
		PARAMETERS A=0.6 , ZZYZX=0.9
		A=0.6
		ZZYZX=0.9
	ENDIF

ENDIF

VALUES "ApplStyle_m" 1,2

