	vert 0, 0, 0
	vert 0, 1, 0
	vert -1, 0, 0
	vert 0, 0, -1
	
	coor 2, -1, -2, -3, -4
	body 1
	
	base
			PRISM_	4,fr_th1,
					-door_w2/2+frw,door_h2,13,
					door_w2/2-frw,door_h2,8,
					door_w2/2-frw,door_h2-frw,14,
