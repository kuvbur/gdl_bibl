! --- Simple: one handle -------------------------------------------------------
"one handle":

	TempLine=0.025
	TempR=0.025

	POLY2_b 2, 1+2+64, gs_fill_pen,gs_back_pen,
			0,0,901,
			TempR,360,4001


	LINE2 TempLine*0.5,0,TempLine*1.5,0
	LINE2 -TempLine*0.5,0,-TempLine*1.5,0
	LINE2 0,TempLine*0.5,0,TempLine*1.5
	LINE2 0,-TempLine*0.5,0,-TempLine*1.5

RETURN


! --- Simple: two handles ------------------------------------------------------
"two handles":

	TempLine=0.025
	TempR=0.025

	FOR k=1 to 2
		ADD2 TempHandleDist,0
		POLY2_b 2, 1+2+64,gs_fill_pen,gs_back_pen,
			0,0,901,
			TempR,360,4001

		LINE2 TempLine*0.5,0,TempLine*1.5,0
		LINE2 -TempLine*0.5,0,-TempLine*1.5,0
		LINE2 0,TempLine*0.5,0,TempLine*1.5
		LINE2 0,-TempLine*0.5,0,-TempLine*1.5

		HOTSPOT2 0,0,unID : unID=unID+1
		HOTSPOT2 0,0,unID : unID=unID+1

		DEL 1

		ADD2 -TempHandleDist*2,0
	NEXT k
	DEL 2

	if bEnableCenterCircle then
		POLY2_b 2, 1+2+64,gs_fill_pen,gs_back_pen,
			0,0,901,
			TempR-0.002,360,4001
	endif

RETURN


! --- Simple: two handles ------------------------------------------------------
"two half handles":

	TempLine=0.025
	TempR=0.025

	FOR k=1 to 2
		ADD2 TempHandleDist,0
		POLY2_b 4, 1+2+64, gs_fill_pen, gs_back_pen,
			TempR,0,1,
			0, -1, 801,
			-TempR, 0, 1001,
			TempR, 0, -1

		LINE2 TempLine*0.5,0,TempLine*1.5,0
		LINE2 -TempLine*0.5,0,-TempLine*1.5,0
		LINE2 0,-TempLine*0.5,0,-TempLine*1.5

		HOTSPOT2 0,0,unID : unID=unID+1

		DEL 1

		ADD2 -TempHandleDist*2,0
	NEXT k
	DEL 2

	POLY2_b 4, 1+2+64, gs_fill_pen, gs_back_pen,
		TempR-0.002,0,1,
		0, -1, 801,
		-TempR+0.002, 0, 1001,
		TempR-0.002, 0, -1

RETURN


! ==============================================================================
! Simple Tap Styles
! ==============================================================================
"simple tap style":

if TapSymbol_m = TAPSYMBOL_DETAILED then
	if bSinkTap then
		gosub "simple sink faucet"
	else
		if iTapGroup = TAPGROUP_SINGLE then			gosub "simple single level mixer"
		if iTapGroup = TAPGROUP_TWOHANDLE then		gosub "simple two handle mixer"
		if iTapGroup = TAPGROUP_TRADITIONAL then	gosub "simple traditional basin pillar tap"
		if iTapGroup = TAPGROUP_SENSOR then			gosub "simple sensor faucet"
		if iTapGroup = TAPGROUP_WALLMOUNTED then	gosub "simple wall mounted"
	endif
else
	if bSinkTap then
		gosub "one handle"
	else
		if iTapGroup = TAPGROUP_SINGLE then
			gosub "one handle"
		endif
		if iTapGroup = TAPGROUP_TWOHANDLE then
			TempHandleDist		= 0.1
			bEnableCenterCircle	= 1
			gosub "two handles"
		endif
		if iTapGroup = TAPGROUP_TRADITIONAL then
			TempHandleDist		= 0.05
			bEnableCenterCircle	= 0
			gosub "two handles"
		endif
		if iTapGroup = TAPGROUP_SENSOR then
			gosub "one handle"
		endif
		if iTapGroup = TAPGROUP_WALLMOUNTED then
			TempHandleDist		= 0.062
			gosub "two half handles"
		endif
	endif
endif

return


