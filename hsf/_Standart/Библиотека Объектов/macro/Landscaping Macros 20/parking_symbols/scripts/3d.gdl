
if isymboltype = SYMBOL_WHEELCHAIR then
	gosub "wheelchair points"
else
	if isymboltype = SYMBOL_FAMILY then
		gosub "family points"
	else
		if isymboltype = SYMBOL_EMPTY_DET then
			gosub "detailed empty points"
		else
			if isymboltype = SYMBOL_EMPTY_SIMP then
				gosub "simple empty points"
			else
				gosub "wheelchair_bound points"
			endif
		endif
	endif
endif

mul A/get(1), B/get(1), 1

while NSP > 0 do
	nPolyPoints = get(1)
	prism_ nPolyPoints , ZZYZX,
			get (nPolyPoints *3)
endwhile
