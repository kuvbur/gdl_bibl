

!================================================================================
! WC User Interface
!================================================================================

! --- Squatting Toilet ---

if iWCType = WC_SQUATTING then
	call "ui_plumbingfixtures" parameters all 	iObjectType 		= 40,
												iWCType 			= iWCType,
												lock_conn_param 	= (not(ConStatus_1) and not(ConStatus_2))
endif


! --- WC and Corner-WC ---

if iWCType = WC_NORMAL or iWCType = WC_CORNER then
	dim matparnames[]
	matparnames[1] = ""

	if bDisabledWC then
		dim gs_ui_cust_tab_txt[3]
			gs_ui_cust_tab_txt[1] = `Стиль`
			gs_ui_cust_tab_txt[2] = `Размеры - Прибор и Бачок`
			gs_ui_cust_tab_txt[3] = `Размеры - Поручни и Бумагодержатель`

		dim gs_ui_cust_tab_ico[3]
			gs_ui_cust_tab_ico[1] = "uiTab_Style_flat(1)"
			gs_ui_cust_tab_ico[2] = "uiTab_Dimension_flat(1)"
			gs_ui_cust_tab_ico[3] = "uiTab_Dimension_flat(1)"

		file_dependence  "uiTab_Style_flat(1)"
		file_dependence  "uiTab_Dimension_flat(1)"

		i = 1
		matparnames[i] = "gs_bowl_mat"			: i = i + 1
		matparnames[i] = "matSeatCover"			: i = i + 1
		if iFlushType = CISTERN then
			matparnames[i] = "gs_tank_mat"		: i = i + 1
			matparnames[i] = "flushHandleMat"	: i = i + 1
		endif
		if iFlushType = IN_WALL_CARRIER then
			matparnames[i] = "matFlushButton"	: i = i + 1
		endif
		if isMEPEnabled & (ConStatus_1 | ConStatus_2) then
			matparnames[i] = "gs_con_mat"		: i = i + 1
		endif
		if bToiletPaper then
			matparnames[i] = "matPaper"			: i = i + 1
		endif
		if bToiletPaper and bCorner then
			matparnames[i] = "matDispenser"		: i = i + 1
		endif
		if reargb | sgb | vertgb | (iGrabBars = GRAB_PARALLEL) then
			matparnames[i] = "matGrabBar"		: i = i + 1
		endif
		if iGrabBars = GRAB_PARALLEL & (iPositionGrabBars = GRAB_TOILET | iPositionGrabBars = GRAB_FLOOR) then
			matparnames[i] = "matGrip"			: i = i + 1
		endif

	else
		dim gs_ui_cust_tab_txt[1]
			gs_ui_cust_tab_txt[1] = `Унитаз, Слив`

		dim gs_ui_cust_tab_ico[1]
			gs_ui_cust_tab_ico[1] = "uiTab_Style_flat(1)"

		file_dependence "uiTab_Style_flat(1)"
	endif

	call "ui_plumbingfixtures" parameters all 	iObjectType 		= 40 + bDisabledWC,
												iWCType 			= iWCType,
												matparnames			= matparnames,
												iFlushType			= iFlushType,
												iFlushometerType	= iFlushometerType,
												gs_ui_cust_tab_txt 	= gs_ui_cust_tab_txt,
												gs_ui_cust_tab_ico	= gs_ui_cust_tab_ico,
												lock_conn_param 	= (not(ConStatus_1) and not(ConStatus_2))

	if bDisabledWC then
		TABID_ROOT				= -1
		TABID = 1 : gosub "StylePage"
		TABID = 2 : gosub "dimBowlPage"
		TABID = 3 : gosub "dimGrabPage"
		TABID_LISTING = 7
		listTabname = `Характеристики`
	else
		gosub "BOWL_AND_FLUSH_PAGE"
		TABID_LISTING = 6
		listTabname = `Параметры Спецификаций`
	endif
	
	gosub "parameter_listing_page"
	
endif


!================================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end
