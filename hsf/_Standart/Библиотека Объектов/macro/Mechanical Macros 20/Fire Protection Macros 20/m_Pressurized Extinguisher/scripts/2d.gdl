
PEN gs_cont_pen


HOTSPOT2 0, 0
HOTSPOT2 b/2+0.04, -b/2
HOTSPOT2 b/2+0.04,  b/2

RECT2 -0.025, -0.025, 0.025, 0.025
RECT2 -0.14, -0.015, -0.025, 0.015

POLY2_ 4, 1,
		0.025, -0.012, 1,
		ra/2+0.04-0.012, -0.012, 1,
		ra/2+0.04-0.012,  0.012, 1001,
		0.025, 0.012, 1

IF ra/2<=SQR(0.015*0.015+0.14*0.14) THEN
	ARC2 0, 0, ra/2, ASN(0.012/(ra/2)), 180-ASN(0.015/(ra/2))
	ARC2 0, 0, ra/2, 180+ASN(0.015/(ra/2)), 360-ASN(0.012/(ra/2))

	HOTSPOT2 -0.14, -ra/2
	HOTSPOT2 -0.14,  ra/2
else
	ARC2 0, 0, ra/2, ASN(0.012/(ra/2)), 360-ASN(0.012/(ra/2))
	HOTSPOT2 -ra/2, -ra/2
	HOTSPOT2 -ra/2,  ra/2
ENDIF


FILL gs_fill_type
POLY2_B 4, 2+64, gs_fill_pen, gs_back_pen,
	   ra/2, 0, 1,
	   0, 1, 800,
	  -ra/2, 0, 1001,
	   ra/2, 0, 1001

POLY2_B 13, 2+64, gs_fill_pen, gs_back_pen,
	   -0.14, -0.015, 1,
	   -0.025, -0.015, 1,
	   -0.025, -0.025, 1,
		0.025, -0.025, 1,
		0.025, -0.012, 1,
		ra/2+0.04-0.012, -0.012, 1,
		ra/2+0.04-0.012,  0.012, 1001,
		0.025, 0.012, 1,
		0.025, 0.025, 1,
	   -0.025, 0.025, 1,
	   -0.025, 0.015, 1,
	   -0.14,  0.015, 1,
	   -0.14, -0.015, -1
