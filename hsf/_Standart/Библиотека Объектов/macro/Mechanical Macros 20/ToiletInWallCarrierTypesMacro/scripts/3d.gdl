
if gs_detlevel_3d_m = DETLEV_3D_OFF then end


!================================================================================
! Attributes
!================================================================================

pen gs_cont_pen
material matFlushButton


!================================================================================
! In-Wall Carriers
!================================================================================

resol 4

if bShowInWallCarrier3D then

	if iWCType = WC_NORMAL then

		if bShowHotspots then
			hotspot  carrierWidth/2,	 0, 			0, unID, carrierDepth, 1 + 256 	: unID = unID + 1
			hotspot  carrierWidth/2,	-1, 			0, unID, carrierDepth, 3		: unID = unID + 1
			hotspot  carrierWidth/2,	 carrierDepth, 	0, unID, carrierDepth, 2 		: unID = unID + 1

			hotspot -carrierWidth/2,	 0, 			0, unID, carrierDepth, 1 + 256 	: unID = unID + 1
			hotspot -carrierWidth/2,	-1, 			0, unID, carrierDepth, 3		: unID = unID + 1
			hotspot -carrierWidth/2,	 carrierDepth, 	0, unID, carrierDepth, 2 		: unID = unID + 1


			hotspot  carrierWidth/2, 0,  0, 			unID, carrierHeight, 1 + 128 	: unID = unID + 1
			hotspot  carrierWidth/2, 0, -1, 			unID, carrierHeight, 3			: unID = unID + 1
			hotspot  carrierWidth/2, 0,  carrierHeight, unID, carrierHeight, 2 			: unID = unID + 1

			hotspot -carrierWidth/2, 0,  0, 			unID, carrierHeight, 1 + 128 	: unID = unID + 1
			hotspot -carrierWidth/2, 0, -1, 			unID, carrierHeight, 3			: unID = unID + 1
			hotspot -carrierWidth/2, 0,  carrierHeight, unID, carrierHeight, 2 			: unID = unID + 1

			hotspot  carrierWidth/2, carrierDepth,  0, 				unID, carrierHeight, 1 + 128 	: unID = unID + 1
			hotspot  carrierWidth/2, carrierDepth, -1, 				unID, carrierHeight, 3			: unID = unID + 1
			hotspot  carrierWidth/2, carrierDepth,  carrierHeight, 	unID, carrierHeight, 2 			: unID = unID + 1

			hotspot -carrierWidth/2, carrierDepth,  0, 				unID, carrierHeight, 1 + 128 	: unID = unID + 1
			hotspot -carrierWidth/2, carrierDepth, -1, 				unID, carrierHeight, 3			: unID = unID + 1
			hotspot -carrierWidth/2, carrierDepth,  carrierHeight, 	unID, carrierHeight, 2 			: unID = unID + 1
		endif


		if GLOB_CONTEXT = 1 or GLOB_CONTEXT = 5 then
			for i=1 to 2
				for j=1 to 2
					addx -carrierWidth/2
					cylind carrierHeight, rPreview
					del 1

					addy carrierDepth
				next j
				del 2

				for j=1 to 2
					addx -carrierWidth/2
					rotx -90
					cylind carrierDepth, rPreview
					del 2

					addz carrierHeight
				next j
				del 2

				addx carrierWidth
			next i
			del 2

			for i=1 to 2
				for j=1 to 2
					addx -carrierWidth/2
					roty 90
					cylind carrierWidth, rPreview
					del 2

					addy carrierDepth
				next j
				del 2

				addz carrierHeight
			next i
			del 2
		else
			model wire

			prism_ 4, carrierHeight,
				-carrierWidth/2, 0, 			15,
				 carrierWidth/2, 0, 			15,
				 carrierWidth/2, carrierDepth, 	15,
				-carrierWidth/2, carrierDepth, 	15

			model solid
		endif

	endif


	if iWCType = WC_CORNER then

		if bShowHotspots then
			hotspot  0,  0,  0, 				unID, carrierHeight, 1 	: unID = unID + 1
			hotspot  0,  0, -1, 				unID, carrierHeight, 3	: unID = unID + 1
			hotspot  0,  0,  carrierHeight, 	unID, carrierHeight, 2 	: unID = unID + 1

			hotspot  carrierWidth/2, -carrierWidth/2,  0, 				unID, carrierHeight, 1 	: unID = unID + 1
			hotspot  carrierWidth/2, -carrierWidth/2, -1, 				unID, carrierHeight, 3	: unID = unID + 1
			hotspot  carrierWidth/2, -carrierWidth/2,  carrierHeight, 	unID, carrierHeight, 2 	: unID = unID + 1

			hotspot -carrierWidth/2, -carrierWidth/2,  0, 				unID, carrierHeight, 1 	: unID = unID + 1
			hotspot -carrierWidth/2, -carrierWidth/2, -1, 				unID, carrierHeight, 3	: unID = unID + 1
			hotspot -carrierWidth/2, -carrierWidth/2,  carrierHeight, 	unID, carrierHeight, 2 	: unID = unID + 1

			hotspot  0,	 0, 			0, unID, carrierDepth, 1 + 128 	: unID = unID + 1
			hotspot  0,	 1, 			0, unID, carrierDepth, 3		: unID = unID + 1
			hotspot  0,	-carrierDepth, 	0, unID, carrierDepth, 2 		: unID = unID + 1
		endif


		if GLOB_CONTEXT = 1 or GLOB_CONTEXT = 5 then
			cylind carrierHeight, rPreview

			for i=1 to 2
				add -carrierWidth/2, -carrierWidth/2, 0
				cylind carrierHeight, rPreview
				del 1

				for j=1 to 2
					rotz 45
					rotx 90
					cylind carrierWidth*SQR(2)/2, rPreview
					del 2

					addz carrierHeight
				next j
				del 2

				mulx -1
			next i
			del 2

			for i=1 to 2
				add -carrierWidth/2, -carrierWidth/2, 0
				roty 90
				cylind carrierWidth, rPreview
				del 2

				addz carrierHeight
			next i
			del 2
		else
			model wire

			prism_ 3, carrierHeight,
				-carrierWidth/2, 	-carrierWidth/2, 	15,
				 0, 				 0, 				15,
				 carrierWidth/2, 	-carrierWidth/2, 	15

			model solid
		endif

	endif

endif


! --- Button --------------------------------------------------------------------

resol gs_resol

if iWCType = WC_CORNER then addy -carrierWidth/2

if iButtonPosition = BUTTON_TOP then
	addy 0.01
	addz carrierHeight
	rotx -90
endif

if iButtonPosition = BUTTON_FRONT then
	addz carrierHeight-buttonHeight
endif

if gs_detlevel_3d_m = DETLEV_3D_DRAFT then
	addx -buttonWidth/2
	block buttonWidth, 0.005, buttonHeight
	del 1
else
	if iInWallCarrierButtonType = BUTTON_TYPE_1 then gosub "BUTTON_1"
	if iInWallCarrierButtonType = BUTTON_TYPE_2 then gosub "BUTTON_2"
	if iInWallCarrierButtonType = BUTTON_TYPE_3 then gosub "BUTTON_3"
	if iInWallCarrierButtonType = BUTTON_TYPE_4 then gosub "BUTTON_4"
	if iInWallCarrierButtonType = BUTTON_TYPE_5 then gosub "BUTTON_5"
endif

if iButtonPosition = BUTTON_TOP   then del 3
if iButtonPosition = BUTTON_FRONT then del 1

if iWCType = WC_CORNER then del 1


