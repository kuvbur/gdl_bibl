	prism_ 5, 0.005,
			-0.081,        	0.032,      s1,
			 0.017,        	0.032,      s1,
			 0.017,        	0.132,      s1,
			-0.081,        	0.132,      s1,
			-0.081,        	0.032,      s1

	prism_ 5, 0.005,
			0.019,        	0.132,      s1,
			0.019,        	0.032,      s1,
			0.081,        	0.032,      s1,
			0.081,        	0.132,      s1,
			0.019,        	0.132,      s1

	del 3

return



