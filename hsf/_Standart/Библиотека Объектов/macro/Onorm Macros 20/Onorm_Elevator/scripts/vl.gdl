
values "gs_onorm_name_value" `Имя Библиотечного Элемента`,`Специальный`
values "gs_onorm_baseheight_value" `Автоматически`,`Специальный`

gs_onorm_id = GLOB_ID
parameters gs_onorm_id = GLOB_ID
Lock "gs_onorm_id"

width = STR("%.2cm", A)
depth = STR("%.2cm", B)
height = STR("%.2cm", zzyzx)
gs_onorm_dimensions = width +  "/" + depth +  "/" + height
parameters gs_onorm_dimensions = gs_onorm_dimensions
lock "gs_onorm_dimensions"

if gs_onorm_name_value = `Имя Библиотечного Элемента` then
	gs_onorm_name = sLibpartName
	parameters gs_onorm_name = sLibpartName

	lock "gs_onorm_name"
endif

if gs_onorm_baseheight_value = `Автоматически` then
	gs_onorm_baseheight = str("%.2cm",GLOB_ELEVATION)
	parameters gs_onorm_baseheight = gs_onorm_baseheight
	lock "gs_onorm_baseheight"
endif

if ABS(gs_onorm_objectweight) < 0.00001 or GLOB_MODPAR_NAME = "FM_ObjectWeight" then
	gs_onorm_objectweight = FM_ObjectWeight
	parameters gs_onorm_objectweight = FM_ObjectWeight
endif
if ABS(FM_ObjectWeight) < 0.00001 or GLOB_MODPAR_NAME = "gs_onorm_objectweight" then
	FM_ObjectWeight = gs_onorm_objectweight
	parameters FM_ObjectWeight = gs_onorm_objectweight
endif

if gs_onorm_manufacturer = "" or GLOB_MODPAR_NAME = "gs_list_manufacturer" then
	gs_onorm_manufacturer = gs_list_manufacturer
	parameters gs_onorm_manufacturer = gs_list_manufacturer
endif
if gs_list_manufacturer = "" or GLOB_MODPAR_NAME = "gs_onorm_manufacturer" then
	gs_list_manufacturer = gs_onorm_manufacturer
	parameters gs_list_manufacturer = gs_onorm_manufacturer
endif

if gs_onorm_maxpersons = "" | GLOB_MODPAR_NAME = "FM_TransportablePersons" then
	gs_onorm_maxpersons = str(FM_TransportablePersons, 1, 0)
	parameters gs_onorm_maxpersons = gs_onorm_maxpersons
endif
if FM_TransportablePersons = 0 | GLOB_MODPAR_NAME = "gs_onorm_maxpersons" then
	numPersons = 0
	isPersons = split(gs_onorm_maxpersons, "%n", numPersons)
	FM_TransportablePersons = numPersons
	parameters FM_TransportablePersons = FM_TransportablePersons
endif
