
values "gs_onorm_name_value" `Имя Библиотечного Элемента`,`Специальный`

gs_onorm_id = GLOB_ID
parameters gs_onorm_id = GLOB_ID
Lock "gs_onorm_id"

gs_onorm_height = STR("%.2cm", zzyzx)
parameters gs_onorm_height = gs_onorm_height
lock "gs_onorm_height"

if gs_onorm_name_value = `Имя Библиотечного Элемента` then
	gs_onorm_name = sLibpartName
	parameters gs_onorm_name = sLibpartName

	lock "gs_onorm_name"
endif

if ABS(gs_onorm_powercons) < 0.00001 or GLOB_MODPAR_NAME = "FM_PowerConsumption" then
	gs_onorm_powercons = FM_PowerConsumption
	parameters gs_onorm_powercons = FM_PowerConsumption
endif
if ABS(FM_PowerConsumption) < 0.00001 or GLOB_MODPAR_NAME = "gs_onorm_powercons" then
	FM_PowerConsumptiong = gs_onorm_powercons
	parameters FM_PowerConsumption = gs_onorm_powercons
endif
