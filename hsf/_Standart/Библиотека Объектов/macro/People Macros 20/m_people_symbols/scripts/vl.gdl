
call "FM_types" parameters all

if	iSymbol_type = SYMBOL_MAN_BICYCLE_2		or \
	iSymbol_type = SYMBOL_MAN_SITTING_1		or \
	iSymbol_type = SYMBOL_MAN_SITTING_2		or \
	iSymbol_type = SYMBOL_WOMAN_SITTING_3	or \
	iSymbol_type = SYMBOL_WOMAN_BICYCLE		then

	values "view_type" `Линейный Вид Сверху`, `Вид спереди`, `Вид Сбоку`, `Общий Вид`
	values "iView_type" TYPE_TOP, TYPE_FRONT, TYPE_SIDE, TYPE_GENERAL

	if GLOB_MODPAR_NAME = "view_type" then
		iView_type = TYPE_TOP
		if view_type = `Вид спереди`		then			iView_type = TYPE_FRONT
		if view_type = `Вид Сбоку`		then			iView_type = TYPE_SIDE
		if view_type = `Общий Вид`	then			iView_type = TYPE_GENERAL
		parameters iView_type = iView_type
	else
		view_type = `Линейный Вид Сверху`
		if iView_type = TYPE_FRONT		then			view_type = `Вид спереди`
		if iView_type = TYPE_SIDE		then			view_type = `Вид Сбоку`
		if iView_type = TYPE_GENERAL	then			view_type = `Общий Вид`
		parameters view_type = view_type
	endif

	if iSymbol_type = SYMBOL_MAN_SITTING_1 then
		all_B_size = 1.35
		side_A_size = 0.85
		front_A_size = 0.59
		gen_A_size = 0.81
	endif
	if iSymbol_type = SYMBOL_MAN_SITTING_2 then
		all_B_size = 1.35
		side_A_size = 0.88
		front_A_size = 0.54
		gen_A_size = 0.85
	endif
	if iSymbol_type = SYMBOL_MAN_BICYCLE_2 then
		all_B_size	= 1.75
		side_A_size	= 1.79
		front_A_size = 0.57
		gen_A_size = 1.28
	endif
	if iSymbol_type = SYMBOL_WOMAN_SITTING_3 then
		all_B_size = 1.40
		side_A_size = 0.77
		front_A_size = 0.52
		gen_A_size = 0.70
	endif
	if iSymbol_type = SYMBOL_WOMAN_BICYCLE then
		all_B_size = 1.75
		side_A_size = 1.79
		front_A_size = 0.56
		gen_A_size = 1.28
	endif

	IF GLOB_MODPAR_NAME = "iView_type" or GLOB_MODPAR_NAME = "view_type" then
		B = all_B_size
		if iView_type = TYPE_SIDE or iView_type = TYPE_AUTOMATIC then
			A = side_A_size
		endif
		if iView_type = TYPE_FRONT then
			A = front_A_size
		endif
		if iView_type = TYPE_GENERAL then
			A = gen_A_size
		endif
		parameters A=A, B=B
	endif
else
	values "view_type" `Линейный Вид Сверху`, `Вид спереди`
	values "iView_type" TYPE_TOP, TYPE_FRONT

	if GLOB_MODPAR_NAME = "view_type" then
		iView_type = TYPE_TOP
		if view_type = `Вид спереди`		then			iView_type = TYPE_FRONT
		parameters iView_type = iView_type
	else
		view_type = `Линейный Вид Сверху`
		if iView_type = TYPE_FRONT		then			view_type = `Вид спереди`
		parameters view_type = view_type
	endif
endif


if GLOB_MODPAR_NAME = "ZZYZX" then
	b = ZZYZX
	parameters b = b
else
	ZZYZX = b
	parameters ZZYZX = ZZYZX
endif

ac_toplevel = ZZYZX
ac_bottomlevel = 0
parameters	ac_toplevel		= ac_toplevel,
			ac_bottomlevel	= ac_bottomlevel

