IF gs_gutter_ds_profile_num=3 THEN								!!CORRUGATED profile!!
	PUT 				-AA-dshw,	 BB,						gs_mask,
						-AA-pt,		 BB,						gs_mask,
						-AA-pt,		-BB+r-pt - (dis_wall - BB),	gs_mask,
						 0,			-1,							800,
						-AA+r,		-BB-pt - (dis_wall - BB),	1000+gs_mask,
						 AA-r,		-BB-pt - (dis_wall - BB),	gs_mask,
						 1,			 0,							800,
						 AA+pt,		-BB+r-pt - (dis_wall - BB),	1000+gs_mask,
						 AA+pt,		 BB,						gs_mask,
						 AA+dshw,	 BB,						gs_mask,
						 AA+dshw,	 BB-sth,					gs_mask,
						 AA+sth,	 BB-sth,					gs_mask,
						 AA+sth,	-BB-sth+r - (dis_wall - BB),gs_mask,
						 0,			-1,							800,
						 AA+sth-r,	-BB-sth - (dis_wall - BB),	1000+gs_mask,
						-AA-sth+r,	-BB-sth - (dis_wall - BB),	gs_mask,
						-1,			 0,							800,
						-AA-sth,	-BB-sth+r - (dis_wall - BB),1000+gs_mask,
