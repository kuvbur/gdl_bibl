IF gs_gutter_ds_profile_num=2 THEN								!!RECTANGULAR profile!!
	PUT 				-AA-dshw,	 BB,						gs_mask,
						-AA-pt,		 BB,						gs_mask,
						-AA-pt,		-BB-pt - (dis_wall - BB),	gs_mask,
						 AA+pt,		-BB-pt - (dis_wall - BB),	gs_mask,
						 AA+pt,		 BB,						gs_mask,
						 AA+dshw,	 BB,						gs_mask,
						 AA+dshw,	 BB-sth,					gs_mask,
						 AA+sth,	 BB-sth,					gs_mask,
						 AA+sth,	-BB-sth - (dis_wall - BB),	gs_mask,
						-AA-sth,	-BB-sth - (dis_wall - BB),	gs_mask,
						-AA-sth,	 BB-sth,					gs_mask,
						-AA-dshw,	 BB-sth,					gs_mask
