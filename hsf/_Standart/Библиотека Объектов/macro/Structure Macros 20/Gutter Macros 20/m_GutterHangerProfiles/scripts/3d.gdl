
!!GUTTER HANGER!!

IF gs_gutter_p_num = 1 THEN										!!Round profile!!
!!	PUT		-gs_gutter_p_r,				  0.0 * 0.0254,			601,
!!			 0,							 -1,					801,
!!			 gs_gutter_p_r,				  0.0 * 0.0254,			1001,
!!			 gs_gutter_p_r + sth,		  0.0 * 0.0254,			1,
!!			 0,							 -1,					801,
!!			-gs_gutter_p_r - sth,		  0.0 * 0.0254,			1001,
!!			-gs_gutter_p_r,				  0.0 * 0.0254,			-1

!!	PUT		-gs_gutter_p_r,				  0.0 * 0.0254,			601,
!!			 0,							 -1,					801,
!!			 gs_gutter_p_r,				  0.0 * 0.0254,			1001

	PUT		gs_gutter_p_r + sth,		  0.0 * 0.0254,			1,
			 0,							 -1,					801,
			-gs_gutter_p_r - sth,		  0.0 * 0.0254,			1001

ENDIF

IF gs_gutter_p_num = 2 THEN										!!"K" profile!!
!!	PUT		 gs_gutter_p_w/2,			 0.00 * 0.0254,			1,
!!			 gs_gutter_p_w/40,			-gs_gutter_p_h,			1,
!!			-gs_gutter_p_w/2,			-gs_gutter_p_h,			1,
!!			-gs_gutter_p_w/2,			 0.00 * 0.0254,			1,
!!
!!			-gs_gutter_p_w/2 - sth,		 0.00 * 0.0254,			1,
!!			-gs_gutter_p_w/2 - sth,		-gs_gutter_p_h - sth,	1,
!!			 gs_gutter_p_w/40 + sth,	-gs_gutter_p_h - sth,	1,
!!			 gs_gutter_p_w/2 + sth,		 0.00 * 0.0254,			1,
!!			 gs_gutter_p_w/2,			 0.00 * 0.0254,			-1

	PUT		-gs_gutter_p_w/2 - sth,		 0.00 * 0.0254,			1,
			 0,	-gs_gutter_p_h - sth,	1,
			gs_gutter_p_w/2 + sth,		-gs_gutter_p_h - sth,	1,
			 gs_gutter_p_w/2 + sth,		 0.00 * 0.0254,			1

ENDIF

IF gs_gutter_p_num = 3 THEN										!!"U" profile!!
!!	PUT		 gs_gutter_p_w/2,			 0.0 * 0.0254,			1,
!!			 gs_gutter_p_w/2,			-gs_gutter_p_h,			1,
!!			-gs_gutter_p_w/2,			-gs_gutter_p_h,			1,
!!			-gs_gutter_p_w/2,			 0.0 * 0.0254,			1,
!!			-gs_gutter_p_w/2 - sth,		 0.0 * 0.0254,			1,
!!			-gs_gutter_p_w/2 - sth,		-gs_gutter_p_h - sth,	1,
!!			 gs_gutter_p_w/2 + sth,		-gs_gutter_p_h - sth,	1,
!!			 gs_gutter_p_w/2 + sth,		 0.0 * 0.0254,			1,
!!			 gs_gutter_p_w/2,			 0.0 * 0.0254,			-1


		PUT -gs_gutter_p_w/2 - sth,		 0.0 * 0.0254,			1,
			-gs_gutter_p_w/2 - sth,		-gs_gutter_p_h - sth,	1,
			 gs_gutter_p_w/2 + sth,		-gs_gutter_p_h - sth,	1,
			 gs_gutter_p_w/2 + sth,		 0.0 * 0.0254,			1

ENDIF
