


MATERIAL gs_ibeam_mat

ROTy rot_p

	GROUP "plate"
	IF ABS(dp-0)>0.00001 THEN
		ROTx 90
		ADDz -a/2
			PRISM_ 5, a,
				0, 0, 15,
				dp, dp*TAN(cut1), 15,
				dp, b+dp*TAN(cut2), 15,
				0, b, 15,
				0, 0, -1
		DEL 2
	ELSE
		ROTz 90
		ROTx 90
			PRISM_ 5, dp,
				-a/2, 0, 15,
				-a/2, b, 15,
				a/2, b, 15,
				a/2, 0, 15,
				-a/2, 0, -1
		DEL 2
	ENDIF


fhx=SQR((x0)*(x0)+(y0)*(y0))
	IF ABS(dp-0)>0.00001 THEN
		ADDy dw/2
		ROTx 90
		ADDz -a/2+dw/2
			PRISM_ 5, a,
				y1, x1, 15,
				y2, b+x2, 15,
				y2-y22, b+x2-x22, 15,
				y1-y11, x1-x11, 15,
				y1, x1, -1
		DEL 3
	ELSE

IF ang<0 THEN
	beta=-ang-90
ELSE
	beta=-ang+90
ENDIF
		ADD y1, 0, x1
		ROTy  beta  !!!!!-(ang+90)*(h_1>h_2)-(ang+270)*(h_1<h_2)-(ang+270)*(ABS(h_1-h_2)<0.0001)*(cut2>cut1)-(ang+90)*(ABS(h_1-h_2)<0.0001)*(cut2<cut1)
		ROTz 90
		ROTx 90
			PRISM_ 5, dp,
				-a/2, 0, 15,
				-a/2, fhx, 15,
				a/2, fhx, 15,
				a/2, 0, 15,
				-a/2, 0, -1
		DEL 4
	ENDIF


	ENDGROUP

	GROUP "web"
		ADDy dw/2
		ROTx 90
			PRISM_ 5, dw,
				0, 0, 15,
				y1, x1, 15,
				y2, b+x2, 15,
				0, b, 15,
				0, 0, -1
		DEL 2

	IF sp THEN
		IF ABS(dp-0)<0.001 THEN dp=0.00001
		ROTx 90
		ADDz -a/2
			PRISM_ 5, a,
				0, 0, 15,
				y1, x1, 15,
				y1-dp*COS(ang)/COS(90+ang-cut1), x1-dp*SIN(ang)/COS(90+ang-cut1), 15,
				0, dp/COS(cut1), 15,
				0, 0, -1
		DEL 2
	ENDIF

	IF ep THEN
		IF ABS(dp-0)<0.001 THEN dp=0.00001
		ROTx 90
		ADDy b
		ADDz -a/2
			PRISM_ 5, a,
				0, 0, 15,
				y2, x2, 15,
				y2+dp*COS(ang)/COS(90+ang-cut2), x2+dp*SIN(ang)/COS(90+ang-cut2), 15,
				0, -dp/COS(cut2), 15,
				0, 0, -1
		DEL 3
	ENDIF

	ENDGROUP

	result_0=ADDGROUP ("plate", "web")
	PLACEGROUP result_0


	KILLGROUP result_0
	KILLGROUP "plate"
	KILLGROUP "web"


DEL 1












