"dimensionPage":
	pageStartY = 0
	if iObjectType = RAIL_TUBE then
		xCenter	= 222
		yCenter	= pageStartY + 85

		xLeftStr1	= 0
		Strlength1	= 110
		xLeftStr2	= xLeftStr1 + Strlength1 + 12
		Strlength2	= 90
		yLeftStr1	= pageStartY + 2
		yCurrent	= yLeftStr1

		ui_infield{3} "sb", xLeftStr1, yCurrent - 2, Strlength1 + 90, infieldHeight,
			7, "",0, 0,
			0, 0, 0, 0,
			"", `Дополнительный Поручень`, 0,
			"", `Дополнительный Поручень`, 1

		yCurrent = yCurrent + rowDistance + textElevInfield
		ui_outfield `Диаметр Трубы`, xLeftStr1, yCurrent, Strlength1, outfieldHeight
		yCurrent = yCurrent - textElevInfield
		ui_infield "hrd", xLeftStr2, yCurrent, Strlength2, infieldHeight
		yCurrent = yCurrent + rowDistance + textElevInfield

		xLeftStr1	= 10 + 222
		Strlength1	= 110
		xLeftStr2	= xLeftStr1 + Strlength1 + 12
		Strlength2	= 90
		yLeftStr1	= pageStartY + 2
		yCurrent	= yLeftStr1

		ui_infield{3} "bSupport", xLeftStr1, yCurrent - 2, xLeftStr2 - xLeftStr1 - 22, infieldHeight,
			7, "",0, 0,
			0, 0, 0, 0,
			"", `Основание`, 0,
			"", `Основание`, 1

		ui_infield{3} "iSupportType", xLeftStr2-20, yCurrent - 2, Strlength1, infieldHeight,
					8, "", 0, 0, 
					0, 0, 0, 0,
					"", `Стойки`, SUPPORT_POST,
					"", `Настенные Кронштейны`, SUPPORT_WALLBRACKET

		yCurrent = yCurrent + rowDistance + 2

		ui_outfield `Кол-во Креплений`, xLeftStr1, yCurrent, xLeftStr2+10 - xLeftStr1 - 2 , outfieldHeight, 4 * NOT(bSupport)
		yCurrent = yCurrent - textElevInfield
		ui_infield "np", xLeftStr2+10, yCurrent, Strlength2-10, infieldHeight
		yCurrent = yCurrent + rowDistance + textElevInfield

		ui_outfield `Вынос Опор`, xLeftStr1, yCurrent, Strlength1-20, outfieldHeight, 4 * (NOT(bSupport) | iSupportType = 1)
		yCurrent = yCurrent - textElevInfield
		ui_infield "wabh", xLeftStr2-20, yCurrent, Strlength2+20, infieldHeight
		yCurrent = yCurrent + rowDistance + textElevInfield

		ui_separator 0, yCurrent , xCenter * 2, yCurrent 

		xCenter	= 222
		yCenter	= pageStartY + 145

		xPict	= 143
		yPict	= 96

		if bSupport & iSupportType = SUPPORT_POST then
			if sb then
				ui_pict 12, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_rails_tube_post_rail.png
			else
				ui_pict 13, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_rails_tube_post.png
			endif
		else
			if sb then
				ui_pict 14, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_rails_tube_rail.png
			else
				ui_pict 15, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_rails_tube.png
			endif
		endif

		xCurrent = xCenter - xPict / 2 - 75
		yCurrent = yCenter + yPict / 2 - 50
		ui_infield "hr", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Высота Ограждения`

		xCurrent = xCenter - xPict / 2 - 35
		yCurrent = yCenter + yPict / 2 + 2
		ui_infield "hro", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Выступ Поручня`

		xCurrent = xCenter - 30
		yCurrent = yCenter + yPict / 2 + 2
		ui_infield "lra", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Длина Оси Ограждения`

		xCurrent = xCenter + xPict / 2 - 25
		yCurrent = yCenter + yPict / 2 + 4
		ui_outfield STR(workingLengthUnit, hro), xCurrent, yCurrent, 50, outfieldHeight

		xCurrent = xCenter + xPict / 2 + 5
		yCurrent = yCenter + yPict / 2 - 35
		ui_infield "tvd", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Общее Смещение по Вертикали`

		if sb then
			xCurrent = xCenter + xPict / 2 + 5
			yCurrent = yCenter - yPict / 2
			ui_infield "sbh", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Высота Дополнительного Поручня`
		endif
	else
		xLeftStr1	= 0
		Strlength1	= 215
		xLeftStr2	= xLeftStr1 + Strlength1 + 12
		Strlength2	= 205 + 12
		yLeftStr1	= pageStartY 
		yCurrent	= yLeftStr1

		ui_outfield `Тип и Параметры Ограждения`, xLeftStr1, yCurrent, Strlength1, outfieldHeight
		yCurrent = yCurrent - textElevInfield
		ui_infield{3} "ptyp_m", xLeftStr2, yCurrent, Strlength2, infieldHeight,
			8, "",
			3,
			1, 90, 20, 90, 20,
			"", `Прямое`, 1,
			"", `Длина Дуги и Радиус`, 2,
			"", `Угол и Радиус`, 3
		yCurrent = yCurrent + 25

		xCenter	= 222
		yCenter	= yCurrent
		ui_separator xLeftStr1, yCenter, xCenter * 2, yCenter



		xLeftStr1	= 0
		Strlength1	= 90
		xLeftStr2	= xLeftStr1 + Strlength1 + 12
		Strlength2	= 58
		yLeftStr1	= pageStartY + 45
		yCurrent	= yLeftStr1

		ui_outfield `Наклон`, xLeftStr1, yCurrent, Strlength1, outfieldHeight
		yCurrent = yCurrent - 20
		ui_infield{3} "iInclinationMethod", xLeftStr2, yCurrent, Strlength2, 50,
			2, 1, 3, 1,		!ui_dim_inclinationType.png
			58, 30, 46, 30,
			1, `Без Наклона`, 1,
			2, `Вертикальное`, 2,
			3, `Перпендикулярно`, 3
		yCurrent = yCurrent + 54

		xCenter	= 222
		yCenter	= yCurrent
		ui_separator xLeftStr1, yCenter, xCenter * 2, yCenter

		xLeftStr1	= 10 + 222
		Strlength1	= 90
		xLeftStr2	= xLeftStr1 + Strlength1 + 12
		Strlength2	= 58
		yLeftStr1	= pageStartY + 45
		yCurrent	= yLeftStr1

		if ptyp_m = 1 and iInclinationMethod = 2 then
			ui_outfield `Измерение`, xLeftStr1, yCurrent, Strlength1, outfieldHeight, 4 * (ABS(ani) < EPS)
			yCurrent = yCurrent - 20
			ui_infield{3} "rmm_m", xLeftStr2, yCurrent, Strlength2, 50,
				2, 2, 2, 1,		!ui_dim_measureType.png
				58, 30, 46, 30,
				1, `Вертикальное`, 1,
				2, `Перпендикулярно Наклону`, 2
		endif



		if ptyp_m = 1 then
			xCenter	= 222
			yCenter	= pageStartY + 142

			xPict	= 141
			yPict	= 76

			if iObjectType = 6 or iObjectType = 13 then
				if iInclinationMethod = 1 then ui_pict 40, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1	!ui_dim_Elev_notIncl2.png
				if iInclinationMethod = 2 then
					if rmm_m = 1 then ui_pict 41, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1				!ui_dim_Elev_vertvert2.png
					if rmm_m = 2 then ui_pict 42, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1				!ui_dim_Elev_vertperp2.png
				endif
				if iInclinationMethod = 3 then ui_pict 43, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1	!ui_dim_Elev_perp2.png
			else
				if iInclinationMethod = 1 then ui_pict 3, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_dim_Elev_notIncl.png
				if iInclinationMethod = 2 then
					if rmm_m = 1 then ui_pict 4, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1				!ui_dim_Elev_vertvert.png
					if rmm_m = 2 then ui_pict 5, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1				!ui_dim_Elev_vertperp.png
				endif
				if iInclinationMethod = 3 then ui_pict 6, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_dim_Elev_perp.png
			endif

			xCurrent = xCenter - 40
			yCurrent = yCenter - yPict / 2 - 21
			ui_infield "lrah", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Длина Ограждения`

			xCurrent = xCenter - xPict / 2 - 75
			yCurrent = yCenter + yPict / 2 - 16 - 26 - 15 * (iInclinationMethod = 1)
			ui_infield "hr", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Высота Ограждения`

			xCurrent = xCenter - xPict / 2 - 75
			yCurrent = yCenter + yPict / 2 - 16 - 15 * (iInclinationMethod = 1)
			if iObjectType <> 6 and iObjectType <> 13 then
				ui_infield "ds", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Расстояние от Перекрытия/Лестницы`
			endif

			if iInclinationMethod <> 1 then
				xCurrent = xCenter + xPict / 2 + 5
				yCurrent = yCenter + yPict / 2 - 16 - 26
				ui_infield "ani", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Угол Наклона`
			endif

			xCenter	= 222
			yCenter	= yCenter + yPict / 2 + 14

			xPict	= 141
			yPict	= 26

			if iInclinationMethod = 3 then
				ui_pict 7, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_dim_plan_perp.png
			else
				ui_pict 8, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_dim_plan_vert.png
			endif

			if iInclinationMethod <> 3 then
				xCurrent = xCenter - xPict / 2 - 75
				yCurrent = yCenter - yPict / 2 + 4
				ui_infield "gs_AngleL", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Угол Среза Левого Конца`

				xCurrent = xCenter + xPict / 2 + 5
				yCurrent = yCenter - yPict / 2 - 17
				ui_infield "gs_AngleR", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Угол Среза Правого Конца`
			endif

			xCurrent = xCenter + xPict / 2 + 5
			yCurrent = yCenter - yPict / 2 + 4
			if	iObjectType = RAIL_BALUSTRADE or\
				iObjectType = RAIL_SOLID_FRAME or iObjectType = RAIL_SOLID_FRAME_FILLED then
				if gs_frameStyle_m = 1 then
					ui_infield "fth", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Толщина`
				else
					ui_infield "gs_frameDiameter", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Толщина`
				endif
			endif
			if	iObjectType = RAIL_SOLID or iObjectType = RAIL_PARALLEL_BARS then
				ui_infield "fth", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Толщина`
			endif
			if	iObjectType = RAIL_SIMPLE_BAR then
				if btyp_m = 1 then
					ui_infield "fth", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Толщина`
				else
					ui_infield "bd", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Толщина`
				endif
			endif
			if	iObjectType = RAIL_WIRED or iObjectType = RAIL_WIRED_02 or\
				iObjectType = RAIL_TWISTED or iObjectType = RAIL_WOODEN_BALUSTRADE or\
				iObjectType = RAIL_POST_WITH_BALUSTERS or iObjectType = RAIL_POST_WITH_BARS or\
				iObjectType = RAIL_POST_WITH_PANELS or\
				iObjectType = RAILING_HORIZONTAL or iObjectType = RAILING_VERTICAL then
				if hrt_m = 1 then
					ui_infield "hrw", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Толщина`
				else
					ui_infield "hrd", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Толщина`
				endif
			endif


			xCurrent = xCenter - 40
			yCurrent = yCenter + yPict / 2 + 1
			ui_infield "lra", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Горизонтальная Длина Ограждения`
		else
			xCenter	= 105 + 52
			yCenter	= pageStartY + 162 + 7

			xPict	= 44
			yPict	= 67

			ui_pict 39, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_dim_plan_angle2.png


			xCenter	= 105
			yCenter	= pageStartY + 162

			xPict	= 62
			yPict	= 113

			ui_pict 9, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_dim_plan_angle1.png



			xCurrent = 0
			yCurrent = yCenter - yPict / 2 + 35
			ui_infield "rb", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Радиус Скругления`

			xCurrent = xCenter + xPict / 2 + 2
			yCurrent = yCenter - 45
			if	iObjectType = RAIL_BALUSTRADE or\
				iObjectType = RAIL_SOLID_FRAME or iObjectType = RAIL_SOLID_FRAME_FILLED then
				if gs_frameStyle_m = 1 then
					ui_infield "fth", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Толщина`
				else
					ui_infield "gs_frameDiameter", xCurrent, yCurrent, 70, infieldHeight	ui_tooltip `Толщина`
