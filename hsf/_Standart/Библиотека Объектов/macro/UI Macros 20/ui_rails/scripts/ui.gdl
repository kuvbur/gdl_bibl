					stSurfaceParamNames[iParam][1] = "pmat"		: stSurfaceParamNames[iParam][2] = "ui_list_surface_flat(1)"	: iParam = iParam + 1
			endif
			if	iObjectType # RAILING_VERTICAL &\
				iObjectType # RAIL_WOODEN_BALUSTRADE then
					stSurfaceParamNames[iParam][1] = "bmat"		: stSurfaceParamNames[iParam][2] = "ui_list_surface_flat(1)"	: iParam = iParam + 1
			endif
			if	iObjectType # RAILING_VERTICAL &\
				iObjectType # RAIL_TWISTED &\
				iObjectType # RAIL_POST_WITH_BARS &\
				iObjectType # RAIL_WIRED_02 &\
				iObjectType # RAIL_WIRED &\
				iObjectType # RAIL_WOODEN_BALUSTRADE then
					stSurfaceParamNames[iParam][1] = "gs_railMat" : stSurfaceParamNames[iParam][2] = "ui_list_surface_flat(1)"	: iParam = iParam + 1
			endif
		endif

	call "ui_railing_Representation" parameters all scriptType = 2,
										listfieldID = TABID_3DATTRIBUTES,
										stDescHeader = "",
										stValueHeader = "",
										stSurfaceParamNames = stSurfaceParamNames,
										bEnableCrossSectionRes = bEnableCrossSectionRes,
										bEnableRailArcRes = bEnableRailArcRes
return


