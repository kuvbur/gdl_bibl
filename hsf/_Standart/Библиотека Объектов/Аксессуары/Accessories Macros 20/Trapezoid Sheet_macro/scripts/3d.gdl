
row_num=ceil(a/wawe_l)
if ac_pitch>=90 or ac_pitch<=-90 then ac_pitch=0

length=b/cos(ac_pitch)
rotx -90+ac_pitch

material sheet_mat

for i=1 to row_num
	extrude 5,0,0,length,32+16,
			0, 0, 0,
			wawe_l/3, 0, 0,
			wawe_l/3+wawe_l/6, -wawe_h, 0,
			2*wawe_l/3+wawe_l/6, -wawe_h, 0,
			wawe_l, 0, 0

	addx wawe_l
next i
del row_num

end

