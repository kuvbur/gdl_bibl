
call "FM_types_Accessories" parameters

if GLOB_MODPAR_NAME="A" then
	parameters ac_rooflength=A/cos(ac_pitch)
	parameters zzyzx=A*tan(ac_pitch)
endif
if GLOB_MODPAR_NAME="ZZYZX" then
	parameters ac_rooflength=sqr(A^2+zzyzx^2)
	parameters ac_pitch=atn(zzyzx/A)
endif
if GLOB_MODPAR_NAME="ac_pitch" then
	parameters zzyzx=tan(ac_pitch)*A
	parameters ac_rooflength=A/cos(ac_pitch)
endif
if GLOB_MODPAR_NAME="ac_rooflength" then
	parameters A=ac_rooflength*cos(ac_pitch)
	parameters zzyzx=ac_rooflength*sin(ac_pitch)
endif


!!!!if not(ac_accessory_debug) then hideparameter "gs_accessory_addon"

values "gs_detlevel_2D" `Детальный`,`Простой`
values "gs_detlevel_3D" `Детальный`,`Простой`,`Откл.`

if abs(ac_pitch)>EPS then
	parameters show_ref=0
	lock "show_ref"
endif

values "wain_type" `Панельная Обшивка 1`,`Панельная Обшивка 2`,`Панельная Обшивка 3`,`Панельная Обшивка 4`

