
EPS=0.00001

! plate_type_m:
PLATE_NONE		= 1
PLATE_SINGLE	= 2
PLATE_DOUBLE	= 3

dim stPlateTypeForMigration[3]
	stPlateTypeForMigration[1] = `Нет`
	stPlateTypeForMigration[2] = `Одиночный`
	stPlateTypeForMigration[3] = `Двойная`

if GLOB_SCRIPT_TYPE = 7 | GLOB_SCRIPT_TYPE = 8 then goto "MasterEnd"

! --- String Resources -----------------------------------------------------

dim stPlateType[3]
	stPlateType[1] = `Нет`
	stPlateType[2] = `Одиночный`
	stPlateType[3] = `Двойная`

! -----------------------
! Scan the input polygons
! -----------------------

cp_num=0
cp_all_num=vardim1(ac_coords)
for i=1 to vardim1(ac_coords)
	if ac_coords[i][1]>1 1/2" then cp_num=cp_num+1
next i

if gs_gdl_debug then print "cp_num=",cp_num

!------------------
! Scan the polygons
!------------------

exam=1
crd_end = 0
dim crd_begend[][]
for i=1 to cp_num
	crd_num=0
	crd_beg=crd_end+1
	r=crd_beg

	while ac_coords[r][1]>eps and ac_coords[r][1]<1 1/2" and exam<>0 do
		crd_num=crd_num+1
		r=r+1
		if r=vardim1(ac_coords) then
			exam=0
		endif
	endwhile
	crd_end=crd_beg+crd_num

	crd_begend[i][1]=crd_beg
	crd_begend[i][2]=crd_end
next i

open_polygon=0
if cp_num=1 and (abs(ac_coords[vardim1(ac_coords)][2]-ac_coords[1][2])>eps Or abs(ac_coords[vardim1(ac_coords)][3]-ac_coords[1][3])>eps) then
	open_polygon=1
endif

if gs_gdl_debug then
	print "crd_begend"
	for i=1 to cp_num
		print crd_begend[i]
	next i
endif

"MasterEnd":

