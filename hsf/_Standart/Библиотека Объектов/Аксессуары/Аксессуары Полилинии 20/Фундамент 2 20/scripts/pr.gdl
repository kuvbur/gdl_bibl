
if plate_type_m <> PLATE_NONE then
	dim wood_pack[][]
	dim empty[1][2]
		empty[1][1] = 0
		empty[1][2] = 0

	if plate_type_m = PLATE_SINGLE then
		plate_num=1
	else
		plate_num=2
	endif

	!!---- Plate ----!!

	p=1
	for i=1 to cp_num
		for j=crd_begend[i][1] to crd_begend[i][2]-1
			Length=sqr((ac_coords[j+1][2]-ac_coords[j][2])^2+(ac_coords[j+1][3]-ac_coords[j][3])^2)

			for k=1 to plate_num
				wood_pack[p][1]=plate_thick
				wood_pack[p][2]=plate_width
				wood_pack[p][3]=Length

				p=p+1
			next k
		next j
	next i

	call "LumberComponents_11" Parameters op_met=1, wood_pack=wood_pack, pack_type=pack_type

	wood_pack=empty
endif
