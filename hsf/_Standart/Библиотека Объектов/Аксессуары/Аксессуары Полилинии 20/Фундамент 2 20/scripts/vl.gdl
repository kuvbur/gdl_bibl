
call "FM_types_Accessories" parameters

!if not(ac_accessory_debug) then hideparameter "gs_accessory_addon"

if GLOB_MODPAR_NAME = "plate_type" then
	plate_type_m = PLATE_NONE
	if plate_type = stPlateType[2] then plate_type_m = PLATE_SINGLE
	if plate_type = stPlateType[3] then plate_type_m = PLATE_DOUBLE
	parameters plate_type_m = plate_type_m
else
	plate_type = stPlateType[plate_type_m]
	parameters plate_type = plate_type
endif
values "plate_type" stPlateType
values "plate_type_m" 1, 2, 3

if plate_type_m = PLATE_NONE then lock "plate_width","plate_thick"

!!---- Pack Type : 1 - USA, 2 - INT
values "pack_type" 1,2

values "StemH" range[0,)
values "StemT" range[0,)
values "Fheight" range[0,)
values "Fthick" range[0,)

gs_list_length=0
For i=1 to cp_num
	For j=crd_begend[i][1] to crd_begend[i][2]-1
		gs_list_length=gs_list_length+sqr((ac_coords[j+1][2]-ac_coords[j][2])^2+(ac_coords[j+1][3]-ac_coords[j][3])^2)
	Next j
Next i
parameters gs_list_length=gs_list_length
