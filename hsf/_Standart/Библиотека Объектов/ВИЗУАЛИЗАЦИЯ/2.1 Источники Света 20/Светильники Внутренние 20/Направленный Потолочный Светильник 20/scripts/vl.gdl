
hideparameter all

hideparameter "ZZYZX"

! --- Bottom, Top Level Calculation  ---

dim xDistFromCenter[]
dim zDistFromCenter[]
dim spotRad[]
dim alpha[]

xDistFromCenter[1]	= 0.083
zDistFromCenter[1]	= 0.0355
xDistFromCenter[2]	= 0.08
zDistFromCenter[2]	= 0.048
xDistFromCenter[3]	= 0.0799
zDistFromCenter[3]	= 0.0481
xDistFromCenter[4]	= 0.055
zDistFromCenter[4]	= 0.05
xDistFromCenter[5]	= -0.055
zDistFromCenter[5]	= 0.0246

for i = 1 to 5
	spotRad[i]		= SQR(xDistFromCenter[i]**2 + zDistFromCenter[i]**2)
	alpha[i]		= ACS(xDistFromCenter[i] / spotRad[i])
	put -spotRad[i] * SIN(alpha[i] - rx) - 0.155
next i

ac_bottomlevel	= MIN(GET(nsp))
ac_toplevel		= 0
parameters	ac_bottomlevel	= ac_bottomlevel,
			ac_toplevel		= ac_toplevel
