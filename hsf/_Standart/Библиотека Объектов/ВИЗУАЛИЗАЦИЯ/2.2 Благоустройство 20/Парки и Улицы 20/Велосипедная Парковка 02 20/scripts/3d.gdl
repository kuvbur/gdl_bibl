
!! Bycicle Holder 02.gsm

if gs_detlevel_3D_m = DLEV3D_OFF then end
if gs_detlevel_3D_m = DLEV3D_SIMPLE then gs_resol = 4

IF gs_shadow=0 THEN SHADOW OFF


pen gs_cont_pen

resol gs_resol

ADDx -a/2 	!!Drag to Origo

r=0.04

addx 0.10
for k=1 to s_num
	for i=1 to iBiCycleNum
		material pmat
		cylind zzyzx, 0.02

		material hmat

		rotx 90
		roty -90
		addy zzyzx-0.44
		for j=1 to 2
			if j=1 then
				addz -0.03
				roty 4
			else
				addz 0.03
				roty -4
			endif
			addz -0.01
			prism_      34,      0.02,
						r,     0.05,      79,
						r+0.05,     0.05,    900,
						0.0,      90.0,   4001,
						r+0.05,     0.00,      79,
						r+0.24,     0.00,      79,
						r+0.24,     0.05,    900,
						0.0,      90.0,   4001,
						r+0.29,     0.05,      79,
						r+0.29,     0.39,      79,
						r+0.24,     0.39,    900,
						0.0,      90.0,   4001,
						r+0.24,     0.44,      79,
						r+0.05,     0.44,      79,
						r+0.05,     0.39,    900,
						0.0,      90.0,   4001,
						r,     0.39,      79,
						r,     0.05,     -1,
						r+0.24,     0.42,      79,
						r+0.24,     0.39,    900,
						0.0,     -90.0,   4001,
						r+0.27,     0.39,      79,
						r+0.27,     0.05,      79,
						r+0.24,     0.05,    900,
						0.0,     -90.0,   4001,
						r+0.24,     0.02,      79,
						r+0.05,     0.02,      79,
						r+0.05,     0.05,    900,
						0.0,     -90.0,   4001,
						r+0.02,     0.05,      79,
						r+0.02,     0.39,      79,
						r+0.05,     0.39,    900,
						0.0,     -90.0,   4001,
						r+0.05,     0.42,      79,
						r+0.24,     0.39,     -1
			del 3
		next j
		del 3

		addx (a-0.20)/(iBiCycleNum - 1)
	next i
	del iBiCycleNum

	muly -1
next k
del s_num

del 1

material rmat

addz zzyzx-0.10
for j=1 to s_num
	for i=1 to 2
		prism_  5, 0.02,
				0.00, -0.02, 15,
				0.00, -0.04, 15,
				a, -0.04, 15,
				a, -0.02, 15,
				0.00, -0.02, -1
		addz -0.26
	next i
	del 2
	muly -1
next j
del s_num
del 2
