
! Trash Holder

pen gs_cont_pen
fill gs_fill_type

! --- Hotspots ---
unID=1

HOTSPOT2 0,0,unID :	unID=unID+1

FOR NN=1 TO 4
	HOTSPOT2 0, 0, unID, temp_RR, 1+128, A:	unID=unID+1
	HOTSPOT2 -1, 0, unID, temp_RR, 3+128, A: unID=unID+1
	HOTSPOT2 temp_RR, 0, unID, temp_RR, 2, A: unID=unID+1

	ROT2 90
NEXT NN
DEL 4

IF GLOB_CONTEXT>20 and GLOB_CONTEXT<40 THEN

	! --- Feedback of 2D Hotspot Editing ---
	CIRCLE2 0,0,temp_RR

ELSE

	! --- 2D Symbol ---

	mul2 a/0.4614,b/0.4614

	circle2 0, 0, 0.191
	POLY2_B      5, 1+2+64,    gs_fill_pen,     gs_back_pen,
			-0.2, 0,      1,
			 0,   0,    900,
			   0,          180,   4001,
			 0,   0,    900,
			   0,          180,   4001


	for i = 1 to 21
		POLY2_B      5, 1+2+64,    gs_fill_pen,     gs_back_pen,
				-0.229773,    -0.027004,      1,
				-0.199775,    -0.026743,      1,
				-0.200000,     0.023256,      1,
				-0.230209,     0.022994,      1,
				-0.229773,    -0.027004,      -1
		rot2 360/21
	next i
	DEL 21+1
ENDIF
