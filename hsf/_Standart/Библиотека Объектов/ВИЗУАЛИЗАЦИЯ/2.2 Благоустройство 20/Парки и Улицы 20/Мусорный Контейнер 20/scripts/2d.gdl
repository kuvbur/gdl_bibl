
b=rad+rad_abst

pen gs_cont_pen
set fill gs_fill_type

set line_type lin_typ

poly2_b   5, 1+2+64,  gs_fill_pen, gs_back_pen,
		-a/2, -b/2,      1,
		-a/2, b/2,      1,
		a/2, b/2,      1,
		a/2, -b/2,      1,
	   -a/2, -b/2,     -1


set line_type lin_kont

poly2_b   5, 1+2+64,  gs_fill_pen, gs_back_pen,
		a/2, b/2,      1,
		a/2, b/2+sp,      1,
	   -a/2, b/2+sp,      1,
	   -a/2, b/2,     1,
		a/2, b/2,      -1





