
call "FM_types" parameters all

parameters ac_bottomlevel = 0
parameters ac_toplevel = ZZYZX

values{2} "gs_detlevel_3D_m" 	DLEV3D_DETAILED, `Детальный`,
								DLEV3D_OFF, `Откл.`

values{2} "iFrameVineStyle" 	FRAMEVINESTYLE_DOUBLE, `Двойная`,
								FRAMEVINESTYLE_SINGLESUPP, `Консоль`,
								FRAMEVINESTYLE_SINGLE, `Одиночная`

values{2} "iPostSize" 	POSTSIZE_1, "5 x 10",
						POSTSIZE_2, "5 x 15",
						POSTSIZE_3, "5 x 20",
						POSTSIZE_4, "10 x 10",
						POSTSIZE_5, "10 x 15",
						POSTSIZE_6, "10 x 20",
						POSTSIZE_CUSTOM, `Специальный`

if not(_bPostSizeCustom) then
	hideparameter "gs_width_post", "gs_height_post"
endif

values{2} "iBeamSize" 	BEAMSIZE_1, "5 x 10",
						BEAMSIZE_2, "5 x 15",
						BEAMSIZE_3, "5 x 20",
						BEAMSIZE_4, "10 x 10",
						BEAMSIZE_5, "10 x 15",
						BEAMSIZE_6, "10 x 20",
						BEAMSIZE_CUSTOM, `Специальный`

if not(_bBeamSizeCustom) then
	hideparameter "gs_width_beam", "gs_height_beam"
endif

values{2} "iBraceSize"	BRACESIZE_1, "5 x 10",
						BRACESIZE_2, "5 x 15",
						BRACESIZE_3, "5 x 20",
						BRACESIZE_4, "10 x 10",
						BRACESIZE_5, "10 x 15",
						BRACESIZE_6, "10 x 20",
						BRACESIZE_CUSTOM, `Специальный`

if not(_bBraceSizeCustom) then
	hideparameter "gs_width_braces", "gs_height_braces"
endif

if gs_cont_pen=0 then parameters gs_cont_pen=2
if gs_fill_pen=0 then parameters gs_fill_pen=91

values "maxspace" RANGE [0.05,)
values "beam_ohl" RANGE (0,)
values "beam_ohr" RANGE (0,)
values "brace_oh" RANGE (0,)
values "post_num" RANGE (0,)

VALUES "floorPlanDisplayType" st_floorPlanDisplayTypes
VALUES "iFloorPlanDisplayType" DISPLAY_OVERHEAD_PROJ, DISPLAY_PROJECTED, DISPLAY_SYMBOLIC, DISPLAY_OUTLINES, DISPLAY_OVERHEAD_ALL

if GLOB_MODPAR_NAME = "floorPlanDisplayType" then
	iFloorPlanDisplayType = DISPLAY_OVERHEAD_PROJ
	if floorPlanDisplayType = st_floorPlanDisplayTypes[1] 	then iFloorPlanDisplayType = DISPLAY_PROJECTED
	if floorPlanDisplayType = st_floorPlanDisplayTypes[3] 	then iFloorPlanDisplayType = DISPLAY_SYMBOLIC
	if floorPlanDisplayType = st_floorPlanDisplayTypes[4] 	then iFloorPlanDisplayType = DISPLAY_OUTLINES
	if floorPlanDisplayType = st_floorPlanDisplayTypes[5] 	then iFloorPlanDisplayType = DISPLAY_OVERHEAD_ALL
	parameters iFloorPlanDisplayType = iFloorPlanDisplayType
else
	floorPlanDisplayType = st_floorPlanDisplayTypes[2]
	if iFloorPlanDisplayType = DISPLAY_PROJECTED	then floorPlanDisplayType = st_floorPlanDisplayTypes[1]
	if iFloorPlanDisplayType = DISPLAY_SYMBOLIC		then floorPlanDisplayType = st_floorPlanDisplayTypes[3]
	if iFloorPlanDisplayType = DISPLAY_OUTLINES		then floorPlanDisplayType = st_floorPlanDisplayTypes[4]
	if iFloorPlanDisplayType = DISPLAY_OVERHEAD_ALL	then floorPlanDisplayType = st_floorPlanDisplayTypes[5]
	parameters floorPlanDisplayType = floorPlanDisplayType
endif

if iFloorPlanDisplayType = DISPLAY_PROJECTED then
	hideparameter "overhead_Line_Type"
endif
if iFloorPlanDisplayType = DISPLAY_OUTLINES then
	hideparameter "cut_pen", "overhead_Line_Type"
endif
if iFloorPlanDisplayType = DISPLAY_OVERHEAD_ALL then
	hideparameter "cut_pen", "contour_line_type"
endif

