! style4_3d
! ------------------------------------------------------------------------------
"style4_3d":
	
	addy -B
	
	! --- Bearing

	material gs_stand_mat
	for i=1 to 2
		prism_ 4, 0.05,
			0.015,	0.015,		15,
			0.135,	0.015,		15,
			0.135,	B - 0.015,	15,
			0.015,	B - 0.015,	15
	
		addz 0.05
		prism_ 4, ZZYZX - 0.09,
			0,		0,	11,
			0.15,	0,	11,
			0.15,	B,	11,
			0,		B,	11
	
		addz ZZYZX - 0.09
		prism_ 8, 0.04,
			0,		0,			14,
			0.15,	0,			14,
			0.15,	0.025,		15,
			0.085,	0.025,		15,
			0.085,	B - 0.025,	15,
			0.15,	B - 0.025,	14,
			0.15,	B,			14,
			0,		B,			14
	
		lin_ 0.15, 0.025, 0, 0.15, B - 0.025, 0
		del 2
	
		addx a
		mulx -1
	next i
	del 4
	! --- End of bearing
	
	! --- Boards
	
	material gs_seat_mat
	addy 0.04
	addz ZZYZX - 0.04
	for i=1 to 4
		prism_  4, 0.04,
			0.10,	 	0,				15,
			A - 0.10,	0,				15,
			A - 0.10,	(B - 0.11)/4,	15,
			0.10,		(B - 0.11)/4,	15

		addy (B - 0.11)/4+0.01
	next i
	del 6
	! --- End of boards
	
	del 1

return

