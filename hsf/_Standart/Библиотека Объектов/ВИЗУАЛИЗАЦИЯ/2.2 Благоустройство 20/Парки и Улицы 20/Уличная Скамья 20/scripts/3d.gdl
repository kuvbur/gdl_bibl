"style1_hotspots":	

	! --- 3D Hotspots
	unID = 1
	
	hotspot	A/2,	0,		ZZYZX, unID, B,	1+256	: unID = unID + 1
	hotspot	A/2,	-B,		ZZYZX, unID, B,	2+128	: unID = unID + 1
	hotspot	A/2,	+1,		ZZYZX, unID, B,	3		: unID = unID + 1
	
	hotspot	A/2,	0,		0, unID, B,	1+256	: unID = unID + 1
	hotspot	A/2,	-B,		0, unID, B,	2+128	: unID = unID + 1
	hotspot	A/2,	+1,		0, unID, B,	3		: unID = unID + 1
	
	hotspot	A/2,	0,		seatHeight, unID, B,	1+128	: unID = unID + 1
	hotspot	A/2,	-B,		seatHeight, unID, B,	2		: unID = unID + 1
	hotspot	A/2,	+1,		seatHeight, unID, B,	3		: unID = unID + 1
	
	hotspot	0,	-B,		0,			unID, seatHeight,	1+128	: unID = unID + 1
	hotspot	0,	-B,		seatHeight, unID, seatHeight,	2		: unID = unID + 1
	hotspot	0,	-B,		-1,			unID, seatHeight,	3		: unID = unID + 1
	
	hotspot	A,	-B,		0,			unID, seatHeight,	1+128	: unID = unID + 1
	hotspot	A,	-B,		seatHeight, unID, seatHeight,	2		: unID = unID + 1
	hotspot	A,	-B,		-1,			unID, seatHeight,	3		: unID = unID + 1
	
	add 0, - 0.25926 * B, _seatBearingHeight + 0.83333 * (ZZYZX - _seatBearingHeight)
	hotspot	0,		0,		0, unID, A,	1+256	: unID = unID + 1
	hotspot	A,		0,		0, unID, A,	2		: unID = unID + 1
	hotspot	-1,		0,		0, unID, A,	3		: unID = unID + 1
	del 1

	addx A / 2
	hotspot	-0.348958 * A,	0,		0,			unID, ZZYZX,	1+128	: unID = unID + 1
	hotspot	-0.348958 * A,	0,		ZZYZX,		unID, ZZYZX,	2		: unID = unID + 1
	hotspot	-0.348958 * A,	0,		-1,			unID, ZZYZX,	3		: unID = unID + 1
	
	hotspot	0.348958 * A,	0,		0,			unID, ZZYZX,	1+128	: unID = unID + 1
	hotspot	0.348958 * A,	0,		ZZYZX,		unID, ZZYZX,	2		: unID = unID + 1
	hotspot	0.348958 * A,	0,		-1,			unID, ZZYZX,	3		: unID = unID + 1
	del 1
	! --- End of 3D Hotspots

return

