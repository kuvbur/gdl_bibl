"style1_3d":	

	! --- Lower part

	material gs_stand_mat
	rotx 90
	roty -90

	addz -0.182278 * A
	
	prism_ 9, 0.031278 * A,
		0.370370 * B,	0,								15,
		0.555556 * B,	0,								15,
		0.555556 * B,	_seatBearingHeight - 0.10,		15,
		0.925926 * B,	_seatBearingHeight - 0.10,		15,
		0.925926 * B,	_seatBearingHeight,				15,
		0.10,			_seatBearingHeight,				8,
		0,				_seatBearingHeight,				13,
		0,				_seatBearingHeight - 0.10,		15,
		0.370370 * B,	_seatBearingHeight - 0.10,		15

	addz -0.666667 * A

	prism_ 9, 0.031278 * A,
		0.370370 * B,	0,								15,
		0.555556 * B,	0,								15,
		0.555556 * B,	_seatBearingHeight - 0.10,		15,
		0.925926 * B,	_seatBearingHeight - 0.10,		15,
		0.925926 * B,	_seatBearingHeight,				15,
		0.10,			_seatBearingHeight,				8,
		0,				_seatBearingHeight,				13,
		0,				_seatBearingHeight - 0.10,		15,
		0.370370 * B,	_seatBearingHeight - 0.10,		15
	
	del 4
	! --- End of lower part


	! --- Upper part
	addx 0.5 * A
	addz _seatBearingHeight

	! ------ Bearing
	material gs_stand_mat
	rotx 90
	roty -90
	addz 0.317722 * A
	
	prism_ 4,0.031278 * A,
		0.10,	0,								15,
		0.10,	(ZZYZX - _seatBearingHeight),	15,
		0,		(ZZYZX - _seatBearingHeight),	15,
		0,		0,								8
	
	addz -0.666667 * A

	prism_ 4,0.031278 * A,
		0.10,	0,								15,
		0.10,	(ZZYZX - _seatBearingHeight),	15,
		0,		(ZZYZX - _seatBearingHeight),	15,
		0,		0,								8
	
	del 4
	! ------ End of bearing
	
	! ------ Boards
	material gs_seat_mat
	add -0.5 * A,	-0.165, 0.5 * (ZZYZX - _seatBearingHeight)
	block A, 0.045, 0.333333 * (ZZYZX - _seatBearingHeight)
	del 1

	add -0.5 * A, -B, 0
	block A, 0.246914 * B, 0.045
	addy 0.283951 * B
	block A, 0.246914 * B, 0.045
	addy 0.283951 * B
	block A, 0.246914 * B, 0.045
	del 3
	! ------ End of boards
	
	del 1
	! --- End of upper part
	
	del 3

return

