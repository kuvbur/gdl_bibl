
actualGUID = FROM_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

	_autoID		= "E4839762-4BF3-4682-8563-ECB43DEECB0B"
	_startID	= "3190DF94-36EB-45FE-B9DF-8F6BFEF60596"
	_endID		= "2D59A3D9-148C-4D7B-9B49-8186AED3A3E2"
gosub "newparameters_FWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid actualGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! newparameters
! ==============================================================================
"newparameters_FWM":
	if actualGuid = _startID | actualGuid = _autoID then
		
		bHas = STORED_PAR_VALUE("iType", _iType)
		if bHas then
			if _iType = 4 then	! Custom
				bCustomSize = 1
				iType = 1	! NBA
			else
				bCustomSize = 0
				iType = _iType
			endif
			
			parameters	bCustomSize = bCustomSize,
						iType		= iType
		endif
		
		actualGuid = _endID
	endif
return

