
call "FM_types" parameters all

values "gs_detlevel_3D" stDetlevels
values "gs_detlevel_3D_m" DETLEV_DETAILED, DETLEV_SIMPLE, DETLEV_OFF

if GLOB_MODPAR_NAME = "gs_detlevel_3D" then
	gs_detlevel_3D_m  = DETLEV_DETAILED
	if gs_detlevel_3D = stDetlevels[2] then gs_detlevel_3D_m  = DETLEV_SIMPLE
	if gs_detlevel_3D = stDetlevels[3] then gs_detlevel_3D_m  = DETLEV_OFF
	parameters gs_detlevel_3D_m = gs_detlevel_3D_m
else
	gs_detlevel_3D = stDetlevels[1]
	if gs_detlevel_3D_m  = DETLEV_SIMPLE then gs_detlevel_3D = stDetlevels[2]
	if gs_detlevel_3D_m  = DETLEV_OFF then gs_detlevel_3D = stDetlevels[3]
	parameters gs_detlevel_3D = gs_detlevel_3D
endif

values "poolShape" stShapes
values "iPoolShape" POOL_RECT, POOL_ROUND, POOL_OVAL

if GLOB_MODPAR_NAME = "poolShape" then
	iPoolShape = POOL_RECT
	if poolShape = stShapes[2] then iPoolShape = POOL_ROUND
	if poolShape = stShapes[3] then iPoolShape = POOL_OVAL
	parameters iPoolShape = iPoolShape
else
	poolShape = stShapes[1]
	if iPoolShape = POOL_ROUND	then poolShape = stShapes[2]
	if iPoolShape = POOL_OVAL	then poolShape = stShapes[3]
	parameters poolShape = poolShape
endif

if gs_cont_pen=0 then parameters gs_cont_pen=2
if gs_fill_pen=0 then parameters gs_fill_pen=91
if deck_fill_pen=0 then parameters deck_fill_pen=91
if ladder_fill_pen=0 then parameters ladder_fill_pen=91

values "gs_resol" range [3, )
values "zzyzx" range [0.4, )
values "pldp" range [0.4, )
values "wh" range [0, pldp]
values "pdn" range [0, )

if gs_detlevel_3D_m  = DETLEV_SIMPLE then lock "gs_resol"

if ABS(wh)<EPS then
	lock "wmat"
	lock "wpn"
endif

if not(ladder) then
	lock "lmat"
	lock "lpn"
endif

parameters zzyzx=pldp+vb
lock "zzyzx"
