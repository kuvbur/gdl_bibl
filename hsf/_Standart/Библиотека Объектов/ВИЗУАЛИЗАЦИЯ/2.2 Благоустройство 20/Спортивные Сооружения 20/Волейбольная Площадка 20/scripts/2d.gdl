HOTSPOT2 A+rout_area_x,0,unID, rout_area_y, 1+128 :unID=unID+1
HOTSPOT2 A+rout_area_x,1,unID, rout_area_y, 3: unID=unID+1
HOTSPOT2 A+rout_area_x,-rout_area_y,unID, rout_area_y, 2: unID=unID+1

HOTSPOT2 A+rout_area_x,B,unID, rout_area_y, 1+128 :unID=unID+1
HOTSPOT2 A+rout_area_x,B-1,unID, rout_area_y, 3: unID=unID+1
