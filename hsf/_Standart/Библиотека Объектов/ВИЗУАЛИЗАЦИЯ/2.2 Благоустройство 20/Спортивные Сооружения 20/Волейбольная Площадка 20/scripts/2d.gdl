HOTSPOT2 -rout_area_x,B+rout_area_y,unID, rout_area_x, 2: unID=unID+1

HOTSPOT2 A,-rout_area_y,unID, rout_area_x, 1+128 :unID=unID+1
HOTSPOT2 A-1,-rout_area_y,unID, rout_area_x, 3: unID=unID+1
