! ==============================================================================
! run out area
! ==============================================================================

if iDet2D = DETLEVEL_2D_DETAILED then
	tx=0
	ty=0
else
	tx=1"
	ty=1"
endif

pen gs_cont_pen
rect2 -0'-1"-rout_area_x, -0'-1"-rout_area_y,29'-7"+rout_area_x, 59'-1"+rout_area_y

if gs_fill_type<>0 then
	fill gs_fill_type
	poly2_b 10, 2+64, runout_fill_pen, gs_back_pen,
			-0'-1"-rout_area_x, 59'-1"+rout_area_y,     15,
			29'-7"+rout_area_x, 59'-1"+rout_area_y,     15,
			29'-7"+rout_area_x, -0'-1"-rout_area_y,     15,
			-0'-1"-rout_area_x, -0'-1"-rout_area_y,     15,
			-0'-1"-rout_area_x, 59'-1"+rout_area_y,     -1,
			-0'-1"+tx, 59'-1"-ty,     15,
			29'-7"-tx, 59'-1"-ty,     15,
			29'-7"-tx, -0'-1"+ty,     15,
			-0'-1"+tx, -0'-1"+ty,     15,
			-0'-1"+tx, 59'-1"-ty,     -1
endif

