HOTSPOT2 A+rout_area_x,B/2,unID, rout_area_x, 2: unID=unID+1

! rout_area_y
HOTSPOT2 -rout_area_x,0,unID, rout_area_y, 1+128 :unID=unID+1
HOTSPOT2 -rout_area_x,1,unID, rout_area_y, 3: unID=unID+1
