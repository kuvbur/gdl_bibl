		29'-5", 39'-3",     15,
		29'-5", 29'-7",     15,
		0'-1", 29'-7",     15,
		0'-1", 39'-3",     15,
		29'-5", 39'-3",     -1

prism_  5, 0'-1",
		0'-1", 29'-5",     15,
		0'-1", 19'-9",     15,
		29'-5", 19'-9",     15,
		29'-5", 29'-5",     15,
		0'-1", 29'-5",     -1

prism_  5, 0'-1",
		0'-1", 19'-7",     15,
		0'-1", 0'-1",     15,
		29'-5", 0'-1",     15,
		29'-5", 19'-7",     15,
		0'-1", 19'-7",     -1


