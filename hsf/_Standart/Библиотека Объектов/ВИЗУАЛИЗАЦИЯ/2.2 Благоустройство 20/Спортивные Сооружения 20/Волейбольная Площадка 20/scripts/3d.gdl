		-0'-1", 59'-1",     -1


! ==============================================================================
! net
! ==============================================================================

if net then

	! --- posts ---

	material post_mat
	resol gs_resol

	prism_  7, 8'-5/8",
			32'-8", 29'-6",      79,
			32'-6", 29'-6",    979,
			0', 590'-6.6142",   4079,
			32'-4", 29'-6",      79,
			32'-6", 29'-6",    979,
			0', 590'-6.6142",   4079,
			32'-8", 29'-6",     -1

	prism_  7, 8'-5/8",
			-2'-10", 29'-6",      79,
			-3', 29'-6",    979,
			0', 590'-6.6142",   4079,
			-3'-2", 29'-6",      79,
			-3', 29'-6",    979,
			0', 590'-6.6142",   4079,
			-2'-10", 29'-6",     -1

	resol gs_resol
	add 0, 29'-6", Height-3'-3"
	cylind  5'-11",1/8"
	del 1

	add 29'-6", 29'-6", Height-3'-3"
	cylind  5'-11",1/8"
	del 1

	material net_mat

	add 0, 29'-6", 0
	rotx 90
	addz -1/8"
	prism_  10, 1/4",
		   -2'-9", Height-3'-3", 15,
		   -2'-9", Height, 15,
			32'-3", Height, 15,
			32'-3", Height-3'-3", 15,
		   -2'-9", Height-3'-3", -1,

		   -2'-7", Height-3'-1", 15,
		   -2'-7", Height-2", 15,
			32'-1", Height-2", 15,
			32'-1", Height-3'-1", 15,
		   -2'-7", Height-3'-1", -1
	del 1

	material web_mat

	plane   5,
		   -2'-7", Height-3'-1", 0,
		   -2'-7", Height-2", 0,
			32'-1", Height-2", 0,
			32'-1", Height-3'-1", 0,
		   -2'-7", Height-3'-1", 0
	del 2
endif
DEL 1
