		-0'-1"-rout_area_x, 59'-1"+rout_area_y,     15,
		29'-7"+rout_area_x, 59'-1"+rout_area_y,     15,
		29'-7"+rout_area_x, -0'-1"-rout_area_y,     15,
		-0'-1"-rout_area_x, -0'-1"-rout_area_y,     15,
