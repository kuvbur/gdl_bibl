
call "FM_types" parameters all



values{2} "iDet2D" 	DETLEVEL_2D_DETAILED, `Детальный`,
					DETLEVEL_2D_SIMPLE, `Простой`
values{2} "gs_detlevel_3D_m" 	DLEV3D_DETAILED, `Детальный`,
								DLEV3D_SIMPLE, `Простой`,
								DLEV3D_OFF, `Откл.`


if gs_cont_pen=0 then parameters gs_cont_pen=4
if line_cont_pen=0 then parameters line_cont_pen=4

if gs_fill_pen=0 then parameters gs_fill_pen=91
if runout_fill_pen=0 then parameters runout_fill_pen=91
if line_fill_pen=0 then parameters line_fill_pen=91

VALUES "A" RANGE (0,)
VALUES "B" RANGE (0,)
VALUES "rout_area_x" RANGE [0,)
VALUES "rout_area_y" RANGE [0,)

IF gs_detlevel_3D_m = DLEV3D_SIMPLE THEN
LOCK "goal","net_mat","goal_mat"
ENDIF

IF iDet2D = DETLEVEL_2D_DETAILED THEN
LOCK "gs_dashed_line"
ENDIF

IF iDet2D = DETLEVEL_2D_SIMPLE THEN
LOCK "line_fill_pen"
ENDIF

IF goal=0 THEN
LOCK "net_mat","goal_mat"
ENDIF
