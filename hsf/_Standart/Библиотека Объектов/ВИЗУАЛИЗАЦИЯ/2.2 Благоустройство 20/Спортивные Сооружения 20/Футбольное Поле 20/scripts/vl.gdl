
call "FM_types" parameters all


values "a" range [100,105]
values "b" range [60,68]
values{2} "gs_detlevel_3D_m" 	DLEV3D_DETAILED, `Детальный`,
								DLEV3D_SIMPLE, `Простой`,
								DLEV3D_OFF, `Откл.`
values "lanes" 6,8
values{2} "iDet2D" 	DETLEVEL_2D_SIMPLE, `Простой`,
					DETLEVEL_2D_DETAILED, `Детальный`
values "gs_resol" range [10, )

if gs_cont_pen=0 then Parameters gs_cont_pen=4
if gs_fill_pen=0 then Parameters gs_fill_pen=91
if track_fill_pen=0 then Parameters track_fill_pen=91
if line_fill_pen=0 then Parameters line_fill_pen=91

if gs_detlevel_3D_m = DLEV3D_SIMPLE then
	gs_resol = 12
	parameters gs_resol = gs_resol
endif
if gs_detlevel_3D_m = DLEV3D_SIMPLE then lock "gs_resol"

if goal=0 then
	lock "goalmat"
endif

if track=0 then
	lock "smat"
	lock "tpn"
	lock "lanes"
endif

if flag=0 then
	lock "flagmat"
endif
