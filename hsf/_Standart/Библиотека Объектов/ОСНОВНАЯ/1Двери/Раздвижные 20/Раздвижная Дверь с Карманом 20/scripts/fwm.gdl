"RenameMinSpaceParams_FWM":
	if actualGUID = _startID then
	    bHas =  DELETED_PAR_VALUE("ms_openingSWidth1", _ms_openingSWidth1) &\
	            DELETED_PAR_VALUE("ms_openingSWidth2", _ms_openingSWidth2) &\
	            DELETED_PAR_VALUE("ms_oppositeSWidth1", _ms_oppositeSWidth1) &\
	            DELETED_PAR_VALUE("ms_oppositeSWidth2", _ms_oppositeSWidth2)
	            
	    if bHas then
	        msOpeningSideWidth2 = _ms_openingSWidth1
	        msOpeningSideWidth1 = _ms_openingSWidth2
	        msOppositeSideWidth2 = _ms_oppositeSWidth1
	        msOppositeSideWidth1 = _ms_oppositeSWidth2
	        
	        parameters  msOpeningSideWidth2 = msOpeningSideWidth2,
	                    msOpeningSideWidth1 = msOpeningSideWidth1,
	                    msOppositeSideWidth2 = msOppositeSideWidth2,
	                    msOppositeSideWidth1 = msOppositeSideWidth1
		endif
	    
	    actualGUID = _endID
	endif
return


! ==============================================================================
! B406 - Enable Editing Leaf Sizes in Pocket Doors
! ==============================================================================
"B406_PocketDoorLeafSizes_FWM":
	if actualGuid = _startID  then
		bGetValues =	STORED_PAR_VALUE ("gs_unit_nominal",		_bUnitNominal)			& \
						STORED_PAR_VALUE ("ac_unit_width",			_currUnitWidth)			& \
						STORED_PAR_VALUE ("ac_wallhole_width",		_currWallholeWidth)		& \
						STORED_PAR_VALUE ("ac_egress_width",		_currEgressWidth)		& \
						STORED_PAR_VALUE ("ac_egress_height",		_currEgressHeight)		& \
						STORED_PAR_VALUE ("gs_tolerance_right",		_toleranceRight)		& \
						STORED_PAR_VALUE ("gs_tolerance_left",		_toleranceLeft)			& \
						STORED_PAR_VALUE ("gs_uniform_framewidth",	_bUniformFrameWidth)	& \
						STORED_PAR_VALUE ("gs_frame_width",			_frameWidthUniform)		& \
						STORED_PAR_VALUE ("gs_frame_width_right",	_frameWidthRight)		& \
						STORED_PAR_VALUE ("gs_frame_width_left",	_frameWidthLeft)		& \
						STORED_PAR_VALUE ("gs_pocket_door_frame",	_bDoorFrame)			& \
						STORED_PAR_VALUE ("gs_pocket_frame_thk",	_pocketFrameThickness)

		if _bUnitNominal then
			_posHole = 0.005

			if _bUniformFrameWidth then
				_frameWidthRight	= _frameWidthUniform
				_frameWidthLeft		= _frameWidthUniform
			endif

			if _bDoorFrame then
				_wallholeDiffSum	= _frameWidthRight + _toleranceRight + _toleranceLeft + (_frameWidthLeft - _pocketFrameThickness - _posHole) / 2
			else
				_wallholeDiffSum	= (-_toleranceRight - _pocketFrameThickness - _posHole) / 2
			endif

			ac_wallhole_width	= _currUnitWidth/2 + _wallholeDiffSum
			ac_egress_width		= ac_wallhole_width - _frameWidthRight - _frameWidthLeft - _toleranceRight - _toleranceLeft
			ac_leaf_width		= ac_egress_width
			ac_leaf_height		= _currEgressHeight
		else
			ac_wallhole_width	= _currWallholeWidth
			ac_egress_width		= _currEgressWidth
			ac_leaf_width		= _currEgressWidth
			ac_leaf_height		= _currEgressHeight
		endif

		parameters	ac_wallhole_width	= ac_wallhole_width,
					ac_egress_width		= ac_egress_width,
					ac_leaf_width		= ac_leaf_width,
					ac_leaf_height		= ac_leaf_height

		actualGuid = _endID
	endif
return
