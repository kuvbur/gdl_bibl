	itemID = 1
	
	ui_listfield iUiID_WinSettOpModAttr, 0, 5, 444, 260 
	ui_listitem itemID, iUiID_WinSettOpModAttr, "iDWDet3D",	0,	"ui_list_detailLevel3D_flat(1)"	: itemID = itemID + 1
	ui_listitem itemID, iUiID_WinSettOpModAttr, "", 			0, "", `Покрытия`	: itemID = itemID + 1
	ui_listitem itemID, iUiID_WinSettOpModAttr, "gs_glass_mat",	1,		"ui_list_surface_flat(1)"		: itemID = itemID + 1
	ui_listitem itemID, iUiID_WinSettOpModAttr, "gs_joint_mat",	1,		"ui_list_surface_flat(1)"		: itemID = itemID + 1
	ui_listitem itemID, iUiID_WinSettOpModAttr, "", 			0, "", `Перья в 3D`	: itemID = itemID + 1
	ui_listitem itemID, iUiID_WinSettOpModAttr, "gs_glass_pen",	1,		"ui_list_pencolor_flat(1)"		: itemID = itemID + 1
	ui_listitem itemID, iUiID_WinSettOpModAttr, "gs_frame_pen",	1,		"ui_list_pencolor_flat(1)"		: itemID = itemID + 1
return


