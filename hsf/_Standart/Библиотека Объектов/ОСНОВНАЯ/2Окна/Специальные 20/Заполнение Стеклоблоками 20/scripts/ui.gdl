! --------------------------------------------------------------------------------
! Glass Block Settings
! --------------------------------------------------------------------------------
"pageGlassBlockSettings":
	pageStartY = 0
	picPosX = 124
	picPosY = pageStartY + 27
	infieldWidth = 85
	infieldHeight = 19

	if iTypeDimensionInput = BLOCKNUM_WITH_BLOCKSIZE then
		ui_pict 1, picPosX, picPosY, 145, 159, 1 ! ui_glassblock_elevBlock.png
		ui_infield "blockWidth", picPosX + 10, picPosY - 6 - infieldHeight, infieldWidth, infieldHeight ui_tooltip `Ширина Блока`
		ui_infield "blockHeight", picPosX - 6 - infieldWidth, picPosY + 30, infieldWidth, infieldHeight ui_tooltip `Высота Блока`
	else
		ui_pict 2, picPosX, picPosY, 145, 159, 1 ! ui_glassblock_elevJoint.png
		ui_infield "vGapSize", picPosX + 10, picPosY - 6 - infieldHeight, infieldWidth, infieldHeight ui_tooltip `Ширина Вертикальных Соединений`
		ui_infield "hGapSize", picPosX - 6 - infieldWidth, picPosY + 43, infieldWidth, infieldHeight ui_tooltip `Ширина Горизонтальных Соединений`
	endif

	picPosX = 284
	ui_pict 3, picPosX, picPosY, 37, 159, 1 ! ui_glassblock_vertSectBlock.png
	ui_infield "jointThickness", picPosX - 25 , picPosY - 6 - infieldHeight, infieldWidth, infieldHeight ui_tooltip `Толщина Шва`
	ui_infield "blockThickness", picPosX + 38 + 6, picPosY + 80, infieldWidth, infieldHeight ui_tooltip `Толщина Блока`

	ui_separator 0, pageStartY + 192, 444, pageStartY + 192
	yCurr = pageStartY + 199 + 4
	x1 = 5
	x2 = 268
	x3 = x2 + 45 + 16
	dy = 22

	ui_outfield `Определение Размеров по`, x1, yCurr, x2-x1-1, 15
	ui_infield{3} "iTypeDimensionInput", x2, yCurr - 4, 170, infieldHeight,
				8, "",
				1, 1,
				0, 0, 0, 0,
				"", `Количество Блоков и Размеры Зазоров`, BLOCKNUM_WITH_GAPSIZE,
				"", `Количество и Размеры Блоков`, BLOCKNUM_WITH_BLOCKSIZE

	yCurr = yCurr + dy

	ui_outfield `Количество Блоков по Горизонтали`, x1, yCurr, x2-x1-1, 15
	ui_infield "horizBlock_n", x2, yCurr - 4, x3-x2, infieldHeight
	yCurr = yCurr + dy

	ui_outfield `Количество Блоков по Вертикали`, x1, yCurr, x2-x1-1, 15
	ui_infield "verticalBlock_n", x2, yCurr - 4, x3-x2, infieldHeight
return



