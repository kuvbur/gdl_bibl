
!!Architrave 01.gsm

IF gs_detlevel_3D_m = DLEV3D_OFF THEN END

IF gs_shadow=0 THEN SHADOW OFF
IF gs_shadow=1 THEN SHADOW ON

!!ADDy b/2		!!Drag to Origo

PEN gs_cont_pen
MATERIAL gs_trim_mat
C=ZZYZX

!!---- Hotspot Editing
unID=1
hotspot 0, 0, 0, unID : unID=unID+1

!!---- Width Editing
hotspot 0, 0, 0, unID, A2, 1+128, A : unID=unID+1
hotspot -1, 0, 0, unID, A2, 3, A : unID=unID+1
hotspot A2, 0, 0, unID, A2, 2, A : unID=unID+1

hotspot 0, 0, 0, unID, A2, 1+128, A : unID=unID+1
hotspot 1, 0, 0, unID, A2, 3, A : unID=unID+1
hotspot -A2, 0, 0, unID, A2, 2, A : unID=unID+1

hotspot 0, 0, zzyzx, unID, A2, 1+128, A : unID=unID+1
hotspot -1, 0, zzyzx, unID, A2, 3, A : unID=unID+1
hotspot A2, 0, zzyzx, unID, A2, 2, A : unID=unID+1

hotspot 0, 0, zzyzx, unID, A2, 1+128, A : unID=unID+1
hotspot 1, 0, zzyzx, unID, A2, 3, A : unID=unID+1
hotspot -A2, 0, zzyzx, unID, A2, 2, A : unID=unID+1

!!---- Height Editing
hotspot A2, 0, 0, unID, zzyzx, 1+128 : unID=unID+1
hotspot A2, 0, -1, unID, zzyzx, 3 : unID=unID+1
hotspot A2, 0, zzyzx, unID, zzyzx, 2 : unID=unID+1

hotspot -A2, 0, 0, unID, zzyzx, 1+128 : unID=unID+1
hotspot -A2, 0, -1, unID, zzyzx, 3 : unID=unID+1
hotspot -A2, 0, zzyzx, unID, zzyzx, 2 : unID=unID+1

!!---- Jamb Trim Width Editing
hotspot A2, 0, -ijw, unID, ijw, 1+128 : unID=unID+1
hotspot A2-1, 0, -ijw, unID, ijw, 3 : unID=unID+1
hotspot A2+ijw, 0, -ijw, unID, ijw, 2 : unID=unID+1

hotspot -A2, 0, -ijw, unID, ijw, 1+128 : unID=unID+1
hotspot -A2+1, 0, -ijw, unID, ijw, 3 : unID=unID+1
hotspot -A2-ijw, 0, -ijw, unID, ijw, 2 : unID=unID+1

hotspot A2, 0, zzyzx+ijw, unID, ijw, 1+128 : unID=unID+1
hotspot A2-1, 0, zzyzx+ijw, unID, ijw, 3 : unID=unID+1
hotspot A2+ijw, 0, zzyzx+ijw, unID, ijw, 2 : unID=unID+1

hotspot -A2, 0, zzyzx+ijw, unID, ijw, 1+128 : unID=unID+1
hotspot -A2+1, 0, zzyzx+ijw, unID, ijw, 3 : unID=unID+1
hotspot -A2-ijw, 0, zzyzx+ijw, unID, ijw, 2 : unID=unID+1

!!---- Sill Trim Width Editing
hotspot A2+ijw, 0, 0, unID, ijw, 1+128 : unID=unID+1
hotspot A2+ijw, 0, 1, unID, ijw, 3 : unID=unID+1
hotspot A2+ijw, 0, -ijw, unID, ijw, 2 : unID=unID+1

hotspot -A2-ijw, 0, 0, unID, ijw, 1+128 : unID=unID+1
hotspot -A2-ijw, 0, 1, unID, ijw, 3 : unID=unID+1
hotspot -A2-ijw, 0, -ijw, unID, ijw, 2 : unID=unID+1

!!---- Head Trim Width Editing
hotspot A2+ijw, 0, zzyzx, unID, ijw, 1+128 : unID=unID+1
hotspot A2+ijw, 0, zzyzx-1, unID, ijw, 3 : unID=unID+1
hotspot A2+ijw, 0, zzyzx+ijw, unID, ijw, 2 : unID=unID+1

hotspot -A2-ijw, 0, zzyzx, unID, ijw, 1+128 : unID=unID+1
hotspot -A2-ijw, 0, zzyzx-1, unID, ijw, 3 : unID=unID+1
hotspot -A2-ijw, 0, zzyzx+ijw, unID, ijw, 2 : unID=unID+1


ROTX 90

ADDX -(A/2+ijw)
ADDY -ijw
S_dim=A+ijw*2
IF iProfileType = TYPEPROFILE_MOLDED then
GOSUB 1
endif

IF iProfileType = TYPEPROFILE_SQUARE then
GOSUB 2
endif

ADDX S_dim
ROTZ 90
S_dim=C+ijw*2
IF iProfileType = TYPEPROFILE_MOLDED then
GOSUB 1
endif

IF iProfileType = TYPEPROFILE_SQUARE then
GOSUB 2
endif

ADDX S_dim
ROTZ 90
S_dim=A+ijw*2
IF iProfileType = TYPEPROFILE_MOLDED then
GOSUB 1
endif

IF iProfileType = TYPEPROFILE_SQUARE then
GOSUB 2
endif


ADDX S_dim
ROTZ 90
S_dim=C+ijw*2
IF iProfileType = TYPEPROFILE_MOLDED then
GOSUB 1
endif

IF iProfileType = TYPEPROFILE_SQUARE then
GOSUB 2
endif

DEL TOP
END

1:
PRISM_ 5, B/2,
0, 0, 15,
S_dim, 0, 8,
(S_dim-ijw), ijw, 15,
ijw, ijw, 8,
0, 0, 15

ADDZ B/2
PRISM_ 5, B/2,
ijw/4, ijw/4, 15,
S_dim-ijw/4, ijw/4, 8,
S_dim-ijw*.75, ijw*.75, 15,
ijw*.75, ijw*.75, 8,
ijw/4, ijw/4, 15



DEL 1
return

2:
PRISM_ 5, B,
0, 0, 15,
S_dim, 0, 8,
(S_dim-ijw), ijw, 15,
ijw, ijw, 8,
0, 0, 15

return
