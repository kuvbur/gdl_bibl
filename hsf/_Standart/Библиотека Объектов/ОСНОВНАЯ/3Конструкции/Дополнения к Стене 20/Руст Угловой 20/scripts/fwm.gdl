
actualGUID = FROM_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

	_startID	= "14FE15D4-B554-45B4-8851-E0F463388ADD"	!19 version
	_endID		= "830EA254-3ADB-4C88-9869-D73E0AF02FDE"	!new version
gosub "mig20_FWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid actualGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! New and deleted parameters
! ==============================================================================
"mig20_FWM":
	if actualGUID = _startID then

		if	thc <= 0 then _endID = ""
		if	wfc <= 0 then _endID = ""
		if	hfc <= 0 then _endID = ""
		if	(iQuoinStyle = QUOINSTYLE_DOBFACE1 | iQuoinStyle = QUOINSTYLE_DOBFACE2) \
			 & wsc <= 0 then _endID = ""
		if	(iQuoinStyle = QUOINSTYLE_DOBFACE1 | iQuoinStyle = QUOINSTYLE_SINGLEFACE1) \
			 & hsc <= 0 then _endID = ""
		if	(iQuoinStyle = QUOINSTYLE_DOBFACE2 | iQuoinStyle = QUOINSTYLE_SINGLEFACE2) \
			 & spc < 0 then _endID = ""
		if	(iQuoinStyle = QUOINSTYLE_DOBFACE1 | iQuoinStyle = QUOINSTYLE_SINGLEFACE1) \
			 & mfh < 0 then _endID = ""
		if	(iQuoinStyle = QUOINSTYLE_DOBFACE1 | iQuoinStyle = QUOINSTYLE_DOBFACE2) \
			 & min(wfc, wsc) > 0 then

			if cang > 360 - 2 * atn(thc / min(wfc, wsc)) then _endID = ""
		endif
		if	(iQuoinStyle = QUOINSTYLE_SINGLEFACE1 | iQuoinStyle = QUOINSTYLE_SINGLEFACE2) & wfc > 0 then
			if cend > 180 - atn(thc / wfc) then _endID = ""
		endif

		actualGUID = _endID
	endif
return
