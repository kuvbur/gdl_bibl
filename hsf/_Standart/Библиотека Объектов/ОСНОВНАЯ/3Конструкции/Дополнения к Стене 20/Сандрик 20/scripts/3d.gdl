
!!Window Header.gsm

if gs_detlevel_3D_m = DLEV3D_OFF then end

IF gs_shadow=0 THEN SHADOW OFF
IF gs_shadow=1 THEN SHADOW ON

!!---- Hotspot Editing
unID=1

hotspot 0,0,0,unID,gs_width2,1+128,gs_width : unID=unID+1
hotspot -1,0,0,unID,gs_width2,3,gs_width : unID=unID+1
hotspot gs_width2,0,0,unID,gs_width2,2,gs_width : unID=unID+1

hotspot 0,0,0,unID,gs_width2,1+128,gs_width : unID=unID+1
hotspot 1,0,0,unID,gs_width2,3,gs_width : unID=unID+1
hotspot -gs_width2,0,0,unID,gs_width2,2,gs_width : unID=unID+1

hotspot 0,-B+toh,0,unID,gs_width2,1+128,gs_width : unID=unID+1
hotspot -1,-B+toh,0,unID,gs_width2,3,gs_width : unID=unID+1
hotspot gs_width2,-B+toh,0,unID,gs_width2,2,gs_width : unID=unID+1

hotspot 0,-B+toh,0,unID,gs_width2,1+128,gs_width : unID=unID+1
hotspot 1,-B+toh,0,unID,gs_width2,3,gs_width : unID=unID+1
hotspot -gs_width2,-B+toh,0,unID,gs_width2,2,gs_width : unID=unID+1

hotspot gs_width2,0,zzyzx,unID,toh,1+128 : unID=unID+1
hotspot gs_width2-1,0,zzyzx,unID,toh,3 : unID=unID+1
hotspot gs_width2+toh,0,zzyzx,unID,toh,2 : unID=unID+1

hotspot -gs_width2,0,zzyzx,unID,toh,1+128 : unID=unID+1
hotspot -gs_width2+1,0,zzyzx,unID,toh,3 : unID=unID+1
hotspot -gs_width2-toh,0,zzyzx,unID,toh,2 : unID=unID+1

hotspot gs_width2,-B,zzyzx,unID,toh,1+128 : unID=unID+1
hotspot gs_width2-1,-B,zzyzx,unID,toh,3 : unID=unID+1
hotspot gs_width2+toh,-B,zzyzx,unID,toh,2 : unID=unID+1

hotspot -gs_width2,-B,zzyzx,unID,toh,1+128 : unID=unID+1
hotspot -gs_width2+1,-B,zzyzx,unID,toh,3 : unID=unID+1
hotspot -gs_width2-toh,-B,zzyzx,unID,toh,2 : unID=unID+1

hotspot gs_width2+toh,0,zzyzx,unID,B,1+128 : unID=unID+1
hotspot gs_width2+toh,1,zzyzx,unID,B,3 : unID=unID+1
hotspot gs_width2+toh,-B,zzyzx,unID,B,2 : unID=unID+1

hotspot -gs_width2-toh,0,zzyzx,unID,B,1+128 : unID=unID+1
hotspot -gs_width2-toh,1,zzyzx,unID,B,3 : unID=unID+1
hotspot -gs_width2-toh,-B,zzyzx,unID,B,2 : unID=unID+1

hotspot 0,0,0,unID,zzyzx,1+256 : unID=unID+1
hotspot 0,0,-1,unID,zzyzx,3 : unID=unID+1
hotspot 0,0,zzyzx,unID,zzyzx,2 : unID=unID+1

hotspot 0,-B,0,unID,zzyzx,1+256 : unID=unID+1
hotspot 0,-B,-1,unID,zzyzx,3 : unID=unID+1
hotspot 0,-B,zzyzx,unID,zzyzx,2 : unID=unID+1
!!---- Hotspot Editing

pen gs_cont_pen
material gs_trim_mat

ADD -a/2,-b,0	!! Drag to Origo
add toh,toh,0
Block a-2*toh,b-toh,zzyzx/2
add -toh/5,-toh/5,zzyzx/2
Block a-2*toh+2*toh/5,b-toh+toh/5,zzyzx/12
add -toh/5,-toh/5,zzyzx/12
Block a-2*toh+4*toh/5,b-toh+2*toh/5,zzyzx/6
add -2*toh/5,-2*toh/5,zzyzx/6
Block a-2*toh+8*toh/5,b-toh+4*toh/5,zzyzx/6
add -toh/5,-toh/5,zzyzx/6
Block a,b,zzyzx/12
del 5
end
