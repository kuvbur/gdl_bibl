
call "FM_types" parameters all

values "gs_detlevel_3D" st3dDetLevelValues[1],
			st3dDetLevelValues[3]
values "gs_detlevel_3D_m" 2, 0

if GLOB_MODPAR_NAME = "gs_detlevel_3D" then
	gs_detlevel_3D_m = 2
	if gs_detlevel_3D = st3dDetLevelValues[3] then gs_detlevel_3D_m = 0
	parameters gs_detlevel_3D_m = gs_detlevel_3D_m
else
	gs_detlevel_3D = st3dDetLevelValues[1]
	if gs_detlevel_3D_m = 0 then gs_detlevel_3D = st3dDetLevelValues[3]
	parameters gs_detlevel_3D = gs_detlevel_3D
endif

IF gs_cont_pen=0 then parameters gs_cont_pen=2
if gs_fill_pen=0 then parameters gs_fill_pen=91

values "gs_resol" RANGE [3,)

values "Slnt" RANGE [5,175]
values "Mtre_L" RANGE (-90,90)
values "Mtre_R" RANGE (-90,90)
values "Bot_Wll" RANGE [0,)

If GLOB_MODPAR_NAME="ZZYZX" Then parameters zzyzx2=ZZYZX/sin(Slnt), B=zzyzx/tan(Slnt)
If GLOB_MODPAR_NAME="zzyzx2" Then parameters ZZYZX=zzyzx2*sin(Slnt), B=zzyzx2*cos(Slnt)
If GLOB_MODPAR_NAME="B" Then parameters ZZYZX=B*tan(Slnt), zzyzx2=B/cos(Slnt)

If GLOB_MODPAR_NAME="Mtre_L" Then parameters Ang_L=Mtre_L+90
If GLOB_MODPAR_NAME="Ang_L" Then parameters Mtre_L=Ang_L-90

If GLOB_MODPAR_NAME="Mtre_R" Then parameters Ang_R=Mtre_R+90
If GLOB_MODPAR_NAME="Ang_R" Then parameters Mtre_R=Ang_R-90
