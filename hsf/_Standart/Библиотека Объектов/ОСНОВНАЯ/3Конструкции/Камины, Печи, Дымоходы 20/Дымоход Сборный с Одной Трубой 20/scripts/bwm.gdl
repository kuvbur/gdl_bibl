
targetGUID = TO_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

gosub "NewFlueShapeAndSymbolOption_BWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid targetGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! NewFlueShapeAndSymbolOption
! ==============================================================================
"NewFlueShapeAndSymbolOption_BWM":
	if targetGUID # "" then
		if	iFlueShape	= FLUESHAPE_RECTANGLE		| \
			iSymbolType	= SYMBOL_STYLE2				| \
			(not(ventilation) & abs(A - B) > EPS)	then

			targetGUID = ""
		endif
	endif
return

