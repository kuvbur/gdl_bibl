
call "FM_types" parameters all

hideparameter all "A", "B", "ZZYZX"

values "gs_resol" range [3, )
values{2} "gs_detlevel_3D_m" 	DLEV3D_DETAILED, `Детальный`,
								DLEV3D_SIMPLE, `Простой`,
								DLEV3D_OFF, `Откл.`


values{2} "iOrnamentType" 	ORNAMTYPE_TUSCAN, `Тосканский`,
							ORNAMTYPE_DORIC, `Дорический`,
							ORNAMTYPE_IONIC, `Ионический`

values{2} "iPlacementType" 	PLACEMENT_FREESTAND, `Отдельностоящая`,
							PLACEMENT_CORNER, `Угловая`,
							PLACEMENT_HALFONWALL, `Полуколонна`,
							PLACEMENT_PARTCUT, `Частично Срезанная`

IF iPlacementType <> 4 THEN LOCK "dca"

IF dca>=coldb/2 THEN
	dca=coldb/2-0.01
	PARAMETERS dca=dca
ENDIF

if GLOB_MODPAR_NAME = "A" then
	parameters B = A
	parameters coldb = A
	coldb = A
	if coldt > coldb then parameters coldt = coldb
endif

if GLOB_MODPAR_NAME = "B" then
	parameters A = B
	parameters coldb = B
	coldb = B
	if coldt > coldb then parameters coldt = coldb
endif

if GLOB_MODPAR_NAME = "coldb" then
	parameters A = coldb
	parameters B = coldb
endif


if zzyzx < zpoc + zcoc + zboc + coldb/9 then parameters zzyzx = zpoc + zcoc + zboc + coldb/9

parameters ac_bottomlevel = 0
parameters ac_toplevel = ZZYZX

values "cannum" range [3,]
