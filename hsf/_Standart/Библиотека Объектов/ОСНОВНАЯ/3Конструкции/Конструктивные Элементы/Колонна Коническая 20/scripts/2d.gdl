
!!Column Conic.gsm


!k=max((a/0.3),(td/0.203/0.15)*0.1015)
!t=max((b/0.3),(td/0.203/0.15)*0.1015)
hLineID = 1000

PEN gs_cont_pen
LINE_TYPE gs_line_type
fill gs_fill_type
POLY2_B 2, 1+2+4+32,gs_fill_pen,gs_back_pen,
		0,0,900+32,
		a/2,360,4001+32

hotarc2 0,0,a/2,0,180, hLineID : hLineID = hLineID + 1
hotarc2 0,0,a/2,180,360, hLineID : hLineID = hLineID + 1

!!---- Hotspot Editing
unID=1
hotspot2 0,0,unID : unID=unID+1

for i=1 to 4
	hotspot2 0,0,unID,A2,1+128,A2 : unID=unID+1
	hotspot2 -1,0,unID,A2,3,A2 : unID=unID+1
	hotspot2 A2,0,unID,A2,2,A2 : unID=unID+1
	rot2 90
next i
del 4
