
call "FM_types" parameters all

hideparameter all "A", "B", "ZZYZX"

values{2} "gs_detlevel_3D_m" 	DLEV3D_DETAILED, `Детальный`,
								DLEV3D_OFF, `Откл.`

if gs_cont_pen=0 then parameters gs_cont_pen=28
if gs_fill_pen=0 then parameters gs_fill_pen=19

if a<=0 then parameters a=0.3
if b<=0 then parameters b=0.3
if ZZYZX<0.3 then parameters ZZYZX=0.3

If GLOB_MODPAR_NAME="A" Then
	If a<=0 Then parameters a=0.1, a2=0.05
	parameters A2=A/2
EndIf
If GLOB_MODPAR_NAME="A2" Then
	If a2<=0.05 Then parameters a=0.1, a2=0.05
	parameters A=A2*2
EndIf
If GLOB_MODPAR_NAME="B" Then
	If b<=0.1 Then parameters b=0.1, b2=0.05
	parameters B2=B/2
EndIf
If GLOB_MODPAR_NAME="B2" Then
	If b2<=0.05 Then parameters b=0.1, b2=0.05
	parameters B=B2*2
EndIf

parameters ac_bottomlevel = 0
parameters ac_toplevel = ZZYZX
