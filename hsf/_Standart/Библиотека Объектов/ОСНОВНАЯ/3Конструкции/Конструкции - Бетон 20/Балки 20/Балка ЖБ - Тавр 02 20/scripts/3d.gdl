HOTSPOT 0,-B/2,ZZYZX-itee_h1,unID,itee_h1,2 : unID=unID+1
HOTSPOT 0,-B/2,ZZYZX+1,unID,itee_h1,3 : unID=unID+1

HOTSPOT A,-B/2,ZZYZX,unID,itee_h1,1+128 : unID=unID+1
HOTSPOT A,-B/2,ZZYZX-itee_h1,unID,itee_h1,2 : unID=unID+1
HOTSPOT A,-B/2,ZZYZX+1,unID,itee_h1,3 : unID=unID+1


FOR w=1 TO nn*2
	PUT GET (2), MAX (-1, 15*GET (1))
NEXT w

ROTz 90
ROTx 90
PRISM_ NSP/3, a,
	GET (NSP)
DEL 2

DEL 1


999:
	FOR w=1 TO NSP
		qq= GET (1)
	NEXT w

END
