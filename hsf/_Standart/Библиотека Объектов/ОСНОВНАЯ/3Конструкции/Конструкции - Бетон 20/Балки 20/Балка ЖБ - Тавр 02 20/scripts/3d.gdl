HOTSPOT 0,b2,0,unID,B2,2, itee_w1 : unID=unID+1
HOTSPOT 0,-1,0,unID,B2,3, itee_w1 : unID=unID+1

! >>> Width (Top)
HOTSPOT A,0,ZZYZX-itee_h1,unID,w22,1+128, itee_w2 : unID=unID+1
HOTSPOT A,-w22,ZZYZX-itee_h1,unID,w22,2, itee_w2 : unID=unID+1
HOTSPOT A,1,ZZYZX-itee_h1,unID,w22,3, itee_w2 : unID=unID+1

HOTSPOT A,0,ZZYZX-itee_h1,unID,w22,1+128, itee_w2 : unID=unID+1
HOTSPOT A,w22,ZZYZX-itee_h1,unID,w22,2, itee_w2 : unID=unID+1
HOTSPOT A,-1,ZZYZX-itee_h1,unID,w22,3, itee_w2 : unID=unID+1

HOTSPOT 0,0,ZZYZX-itee_h1,unID,w22,1+128, itee_w2 : unID=unID+1
HOTSPOT 0,-w22,ZZYZX-itee_h1,unID,w22,2, itee_w2 : unID=unID+1
HOTSPOT 0,1,ZZYZX-itee_h1,unID,w22,3, itee_w2 : unID=unID+1

HOTSPOT 0,0,ZZYZX-itee_h1,unID,w22,1+128, itee_w2 : unID=unID+1
HOTSPOT 0,w22,ZZYZX-itee_h1,unID,w22,2, itee_w2 : unID=unID+1
HOTSPOT 0,-1,ZZYZX-itee_h1,unID,w22,3, itee_w2 : unID=unID+1

! >>> Height
HOTSPOT 0,-itee_w2/2,0,unID,ZZYZX,1+128 : unID=unID+1
HOTSPOT 0,-itee_w2/2,ZZYZX,unID,ZZYZX,2 : unID=unID+1
HOTSPOT 0,-itee_w2/2,-1,unID,ZZYZX,3 : unID=unID+1

HOTSPOT A,-itee_w2/2,0,unID,ZZYZX,1+128 : unID=unID+1
HOTSPOT A,-itee_w2/2,ZZYZX,unID,ZZYZX,2 : unID=unID+1
HOTSPOT A,-itee_w2/2,-1,unID,ZZYZX,3 : unID=unID+1

HOTSPOT 0,itee_w2/2,0,unID,ZZYZX,1+128 : unID=unID+1
HOTSPOT 0,itee_w2/2,ZZYZX,unID,ZZYZX,2 : unID=unID+1
HOTSPOT 0,itee_w2/2,-1,unID,ZZYZX,3 : unID=unID+1

HOTSPOT A,itee_w2/2,0,unID,ZZYZX,1+128 : unID=unID+1
HOTSPOT A,itee_w2/2,ZZYZX,unID,ZZYZX,2 : unID=unID+1
HOTSPOT A,itee_w2/2,-1,unID,ZZYZX,3 : unID=unID+1

! >>> Height (Top)
HOTSPOT 0,B/2,ZZYZX,unID,itee_h1,1+128 : unID=unID+1
HOTSPOT 0,B/2,ZZYZX-itee_h1,unID,itee_h1,2 : unID=unID+1
HOTSPOT 0,B/2,ZZYZX+1,unID,itee_h1,3 : unID=unID+1

