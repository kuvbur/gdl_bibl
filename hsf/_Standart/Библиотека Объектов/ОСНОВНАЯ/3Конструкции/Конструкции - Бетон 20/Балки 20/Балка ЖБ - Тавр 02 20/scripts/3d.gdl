
IF gs_detlevel_3D_m = DLEV3D_OFF THEN 999

PEN gs_cont_pen
MATERIAL gs_itee_mat
if not(gs_shadow) then shadow off

Unid=1

! Length

HOTSPOT 0, 0, 0, unID, A, 1+256 : unID=unID+1
HOTSPOT -1, 0, 0, unID, A, 3 : unID=unID+1
HOTSPOT A, 0, 0, unID, A, 2 : unID=unID+1

ROTx angy

! >>> Width (Bottom)
HOTSPOT A,0,0,unID,B2,1+128, itee_w1 : unID=unID+1
HOTSPOT A,-b2,0,unID,B2,2, itee_w1 : unID=unID+1
HOTSPOT A,1,0,unID,B2,3, itee_w1 : unID=unID+1

HOTSPOT A,0,0,unID,B2,1+128, itee_w1 : unID=unID+1
HOTSPOT A,b2,0,unID,B2,2, itee_w1 : unID=unID+1
HOTSPOT A,-1,0,unID,B2,3, itee_w1 : unID=unID+1

HOTSPOT 0,0,0,unID,B2,1+128, itee_w1 : unID=unID+1
HOTSPOT 0,-b2,0,unID,B2,2, itee_w1 : unID=unID+1
HOTSPOT 0,1,0,unID,B2,3, itee_w1 : unID=unID+1

