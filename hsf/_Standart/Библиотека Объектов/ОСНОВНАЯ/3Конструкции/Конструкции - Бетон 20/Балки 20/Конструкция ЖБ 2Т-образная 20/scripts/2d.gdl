
PEN gs_cont_pen
FILL gs_fill_type


Unid=1

! --- Length

HOTSPOT2 0, 0, unID, A, 1+256 : unID=unID+1
HOTSPOT2 -1, 0, unID, A, 3 : unID=unID+1
HOTSPOT2 A, 0, unID, A, 2 : unID=unID+1

HOTSPOT2 0, B, unID, A, 1+256 : unID=unID+1
HOTSPOT2 -1, B, unID, A, 3 : unID=unID+1
HOTSPOT2 A, B, unID, A, 2 : unID=unID+1

! --- Width

HOTSPOT2 A, 0, unID, B, 1+256 : unID=unID+1
HOTSPOT2 A, -1, unID, B, 3 : unID=unID+1
HOTSPOT2 A, B, unID, B, 2 : unID=unID+1

HOTSPOT2 0, 0, unID, B, 1+256 : unID=unID+1
HOTSPOT2 0, -1, unID, B, 3 : unID=unID+1
HOTSPOT2 0, B, unID, B, 2 : unID=unID+1

! --- Length/Axis

HOTSPOT2 0, B/4, unID, A, 1+256 : unID=unID+1
HOTSPOT2 -1, B/4, unID, A, 3 : unID=unID+1
HOTSPOT2 A, B/4, unID, A, 2 : unID=unID+1

HOTSPOT2 0, B*3/4, unID, A, 1+256 : unID=unID+1
HOTSPOT2 -1, B*3/4, unID, A, 3 : unID=unID+1
HOTSPOT2 A, B*3/4, unID, A, 2 : unID=unID+1

! --- Center

HOTSPOT2 A/2, B/2, unID : unID=unID+1

IF only_axis=0 THEN
	!!! *** Web Thickness
	IF iDet2D = DETLEVEL_2D_DETAILED THEN

		UnIDTmp = UnID !!! Compatibility requirement - New hotspots
		UnID = 200

		ADD2 0,B/4
		FOR i=0 TO 1
			ADD2 0,i*B/2
			!!! >>> Web Thickness(Bottom)
			HOTSPOT2 A,0,unID,dtee_thhalf,1+128, dtee_th : unID=unID+1
			HOTSPOT2 A,-dtee_thhalf,unID,dtee_thhalf,2, dtee_th : unID=unID+1
			HOTSPOT2 A,1,unID,dtee_thhalf,3, dtee_th : unID=unID+1

			HOTSPOT2 A,0,unID,dtee_thhalf,1+128, dtee_th : unID=unID+1
			HOTSPOT2 A,dtee_thhalf,unID,dtee_thhalf,2, dtee_th : unID=unID+1
			HOTSPOT2 A,-1,unID,dtee_thhalf,3, dtee_th : unID=unID+1

			HOTSPOT2 0,0,unID,dtee_thhalf,1+128, dtee_th : unID=unID+1
			HOTSPOT2 0,-dtee_thhalf,unID,dtee_thhalf,2, dtee_th : unID=unID+1
			HOTSPOT2 0,1,unID,dtee_thhalf,3, dtee_th : unID=unID+1

			HOTSPOT2 0,0,unID,dtee_thhalf,1+128, dtee_th : unID=unID+1
			HOTSPOT2 0,dtee_thhalf,unID,dtee_thhalf,2, dtee_th : unID=unID+1
			HOTSPOT2 0,-1,unID,dtee_thhalf,3, dtee_th : unID=unID+1

			!!! >>> Web Thickness(Top)
			HOTSPOT2 A,0,unID,dtee_th2half,1+128, dtee_th2 : unID=unID+1
			HOTSPOT2 A,-dtee_th2half,unID,dtee_th2half,2, dtee_th2 : unID=unID+1
			HOTSPOT2 A,1,unID,dtee_th2half,3, dtee_th2 : unID=unID+1

			HOTSPOT2 A,0,unID,dtee_th2half,1+128, dtee_th2 : unID=unID+1
			HOTSPOT2 A,dtee_th2half,unID,dtee_th2half,2, dtee_th2 : unID=unID+1
			HOTSPOT2 A,-1,unID,dtee_th2half,3, dtee_th2 : unID=unID+1

			HOTSPOT2 0,0,unID,dtee_th2half,1+128, dtee_th2 : unID=unID+1
			HOTSPOT2 0,-dtee_th2half,unID,dtee_th2half,2, dtee_th2 : unID=unID+1
			HOTSPOT2 0,1,unID,dtee_th2half,3, dtee_th2 : unID=unID+1

			HOTSPOT2 0,0,unID,dtee_th2half,1+128, dtee_th2 : unID=unID+1
			HOTSPOT2 0,dtee_th2half,unID,dtee_th2half,2, dtee_th2 : unID=unID+1
			HOTSPOT2 0,-1,unID,dtee_th2half,3, dtee_th2 : unID=unID+1
		NEXT i
		DEL 3

		UnID = UnIDTmp !!! Compatibility requirement - New hotspots
	ENDIF
ENDIF

ADD2 0,B/2

FOR I=-1 TO 1 STEP 2
	ADD2 0, I*b/4
		HOTSPOT2 0, 0
		HOTSPOT2 a, 0
	DEL 1
NEXT I

!!		HOTSPOT2 0, 0, 1, a, 1+256
!!		HOTSPOT2 -1, 0, 2, a, 3
!!		HOTSPOT2 a, 0, 3, a, 2
!!		HOTSPOT2 0, 0

if only_axis=0 then
	POLY2_B 5, 1+2+4+64, gs_fill_pen, gs_back_pen,
		0, -b/2, 1,
		0, b/2, 1,
		a, b/2, 1,
		a, -b/2, 1,
		0, -b/2, -1



!!		HOTSPOT2 0, -b/2
!!		HOTSPOT2 0, b/2
!!		HOTSPOT2 a, b/2
!!		HOTSPOT2 a, -b/2
!!		HOTSPOT2 a/2, 0

	if iDet2D = DETLEVEL_2D_DETAILED then
		LINE_TYPE DetailLineType
		for I=-1 TO 1 STEP 2
			ADD2 0, I*(b/4)
			LINE2 0,  th1/2, a,  th1/2
			LINE2 0, -th1/2, a, -th1/2
			DEL 1
		next I
	endif
endif



if showsect AND only_axis=0 then
	LINE_TYPE 1
!!		HOTSPOT2 0, 0, 21, pos_2Dx, 1+128
!!		HOTSPOT2 0, -1', 22, pos_2Dx, 3
!!		HOTSPOT2 pos_2Dx, 0, 23, pos_2Dx, 2
	ADD2 pos_2Dx, 0
	ROT2 90
	FILL cross_fill
	POLY2_ NSP/3, 7,
		GET (NSP)
	if dtee_top_th>0 then
		LINE2 -b/2, zzyzx+dtee_top_th, b/2, zzyzx+dtee_top_th
	endif
	DEL 2
endif

if axis then
	PEN axis_pen
	LINE_TYPE axis_line_type
	FOR I=-1 TO 1 STEP 2
		ADD2 0, I*b/4
		LINE2 0, 0, a, 0
		DEL 1
	NEXT I
endif

DEL 1
END
