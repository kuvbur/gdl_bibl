! ==============================================================================
! newParameters
! ==============================================================================
"newBooleanParameter_BWM":
	if targetGUID # "" then
		! Topping boolean is a new option, if it's OFF, Topping thickness will be 0.
		if not(bTopping) then
			dtee_top_th = 0.0
		else
			dtee_top_th = dtee_top_th
		endif
		parameters dtee_top_th = dtee_top_th
	endif
return

