! ==============================================================================
! newParameters
! ==============================================================================
"newBooleanParameter_FWM":
	if actualGuid = _startID | actualGuid = _autoID then
    	! Topping boolean is a new option
        _bHasParam = STORED_PAR_VALUE ("dtee_top_th", _tempTopThickness)
        if _bHasParam then
        	if _tempTopThickness > 0 then
        		bTopping = 1
        	else
       		   bTopping = 0
        	endif
           	parameters bTopping = bTopping
        endif
		actualGuid = _endID
	endif
return

