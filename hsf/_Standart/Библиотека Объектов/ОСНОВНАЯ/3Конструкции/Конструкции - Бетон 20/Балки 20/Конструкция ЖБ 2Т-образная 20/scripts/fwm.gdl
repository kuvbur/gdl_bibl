	_autoID		= "AB509278-B617-45C3-B38C-E772BDE1F66B"
	_startID	= "ADCE9E88-444B-4802-B3AD-8D29894B4001"
	_endID		= "3B72884B-C86D-4106-9723-A98E9D850456"
gosub "newBooleanParameter_FWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid actualGUID

