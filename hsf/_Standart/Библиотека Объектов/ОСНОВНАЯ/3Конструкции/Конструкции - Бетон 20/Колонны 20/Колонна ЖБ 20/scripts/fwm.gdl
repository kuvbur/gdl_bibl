
actualGUID = FROM_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

	_autoID		= "7E4DA79A-8B07-4DA3-A041-D81BDE549F9F"
	_startID	= "20871B48-5D28-4BA8-984E-0CEF4E62EE45"
	_endID		= "BAE07D92-8CCA-4BC4-85E7-FC89379AF3B6"
gosub "newParameters_FWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid actualGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! newParameters
! ==============================================================================
"newParameters_FWM":
	if actualGuid = _startID | actualGuid = _autoID then
    	_bHasParameter = STORED_PAR_VALUE ("iColumnHeadType", _iColumnHeadType)
        if _bHasParameter then
            if _iColumnHeadType = 0 then
      		    bColumnHead = 0
      		    iColumnHeadType = 1
            else
                bColumnHead = 1
            endif
            parameters bColumnHead = bColumnHead,
                    iColumnHeadType = iColumnHeadType
        endif
    	actualGUID = _endID
	endif
return

