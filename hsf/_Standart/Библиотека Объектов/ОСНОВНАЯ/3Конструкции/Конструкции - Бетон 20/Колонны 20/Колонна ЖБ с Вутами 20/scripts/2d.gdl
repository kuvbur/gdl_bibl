
PEN gs_cont_pen
FILL gs_fill_type

unID=1
hLineID = 1000

!!! *** Center
hotspot2 0,0,unID : unID=unID+1

!!! *** X, Y Dimension
hotspot2 a/2,0,unID,B2,1+128, B : unID=unID+1
hotspot2 a/2,b2,unID,B2,2, B : unID=unID+1
hotspot2 a/2,-1,unID,B2,3, B : unID=unID+1

hotspot2 -a/2,0, unID,B2,1+128, B : unID=unID+1
hotspot2 -a/2,b2, unID,B2,2, B : unID=unID+1
hotspot2 -a/2,-1, unID,B2,3, B : unID=unID+1

hotspot2 0,b2, unID,A2,1+128, A : unID=unID+1
hotspot2 a2,b2, unID,A2,2, A : unID=unID+1
hotspot2 -1,b2, unID,A2,3, A : unID=unID+1

hotspot2 0,b2, unID,A2,1+128, A : unID=unID+1
hotspot2 -a2,b2, unID,A2,2, A : unID=unID+1
hotspot2 1,b2, unID,A2,3, A : unID=unID+1

hotspot2 a/2,0, unID,B2,1+128, B : unID=unID+1
hotspot2 a/2,-b2, unID,B2,2, B : unID=unID+1
hotspot2 a/2,1, unID,B2,3, B : unID=unID+1

hotspot2 -a/2,0, unID,B2,1+128, B : unID=unID+1
hotspot2 -a/2,-b2, unID,B2,2, B : unID=unID+1
hotspot2 -a/2,1, unID,B2,3, B : unID=unID+1

hotspot2 0,-b2, unID,A2,1+128, A : unID=unID+1
hotspot2 a2,-b2, unID,A2,2, A : unID=unID+1
hotspot2 -1,-b2, unID,A2,3, A : unID=unID+1

hotspot2 0,-b2, unID,A2,1+128, A : unID=unID+1
hotspot2 -a2,-b2, unID,A2,2, A : unID=unID+1
hotspot2 1,-b2, unID,A2,3, A : unID=unID+1

LINE_TYPE linetypeofcolumn

POLY2_B 5, 1+2+4+32, gs_fill_pen, gs_back_pen,
	-a/2, -b/2, 1+32,
	-a/2, b/2, 1+32,
	 a/2, b/2, 1+32,
	 a/2, -b/2, 1+32,
	-a/2, -b/2, -1

HOTLINE2 -a/2, -b/2, a/2, -b/2, hLineID : hLineID = hLineID + 1
HOTLINE2 -a/2, b/2, a/2, b/2, hLineID : hLineID = hLineID + 1
HOTLINE2 -a/2, -b/2, -a/2, b/2, hLineID : hLineID = hLineID + 1
HOTLINE2 a/2, -b/2, a/2, b/2, hLineID : hLineID = hLineID + 1

unID=100

IF iDet2D = DETLEVEL_2D_DETAILED THEN
	LINE_TYPE linetypeofHaunch
	IF NumOfBracket_1<>0 THEN
		_rotz=-90
		cx=b
		cy=a
		GOSUB "haunch"
	ENDIF
	IF NumOfBracket_2<>0 THEN
		_rotz=0
		cx=a
		cy=b
		GOSUB "haunch"
	ENDIF
	IF NumOfBracket_3<>0 THEN
		_rotz=-270
		cx=b
		cy=a
		GOSUB "haunch"
	ENDIF
	IF NumOfBracket_4<>0 THEN
		_rotz=180
		cx=a
		cy=b
		GOSUB "haunch"
	ENDIF
ENDIF

END


"haunch":
	ROT2 _rotz

	rot2 90
	! Haunch Length

	hotspot2 cx/2,cy/2, unID,Bracket_length,1+128 : unID=unID+1
	hotspot2 cx/2+Bracket_length,cy/2, unID,Bracket_length,2 : unID=unID+1
	hotspot2 cx/2-1,cy/2, unID,Bracket_length,3 : unID=unID+1

	hotspot2 cx/2,-cy/2, unID,Bracket_length,1+128 : unID=unID+1
	hotspot2 cx/2+Bracket_length,-cy/2, unID,Bracket_length,2 : unID=unID+1
	hotspot2 cx/2-1,-cy/2, unID,Bracket_length,3 : unID=unID+1

	DEL 1

	ADD2 0, cy/2+bl/2

	RECT2 -cx/2, -bl/2, cx/2, bl/2

	DEL 2

	RETURN
