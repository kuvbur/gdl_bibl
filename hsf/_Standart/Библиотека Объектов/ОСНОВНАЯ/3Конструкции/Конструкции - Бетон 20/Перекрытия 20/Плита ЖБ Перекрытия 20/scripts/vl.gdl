
hideparameter all "A", "B", "Zzyzx" 

call "FM_types" parameters all

values{2} "gs_detlevel_3D_m" 	DLEV3D_DETAILED, `Детальный`,
								DLEV3D_SIMPLE, `Простой`,
								DLEV3D_OFF, `Откл.`

if not(showsect) then
	hideparameter "ShowHoles2D", "cross_fill"
	lock "ShowHoles2D", "cross_fill"
endif

if iSlabType = SLAB_TYPE_FLAT then
	hideparameter	"ShowHoles2D"
	lock	"ShowHoles2D"
endif

if not(bTopLayer) then
	hideparameter "slab_top_th"
	lock "slab_top_th"
endif

values "A"				range(0, )
values "B"				range[0.3, )
values "ZZYZX"			range[0.05,)
values "slab_top_th"	range(0, )
values "gs_resol"		range[3,)

values{2} "iSlabType"	SLAB_TYPE_FLAT, stSlabType[7],
						SLAB_TYPE_A, stSlabType[1],
						SLAB_TYPE_B, stSlabType[2],
						SLAB_TYPE_C, stSlabType[3],
						SLAB_TYPE_D, stSlabType[4],
						SLAB_TYPE_E, stSlabType[5],
						SLAB_TYPE_F, stSlabType[6]


values "pos_2Dx" range(zzyzx, a)

if gs_cont_pen <= 0 then parameters gs_cont_pen = 2
if gs_fill_pen <= 0 then parameters gs_fill_pen = 19

es_length = a
es_surface = a*b
es_volume = es_surface*zzyzx
PARAMETERS es_length = es_length,
			es_surface = es_surface,
			es_volume = es_volume

CALL "Precast_quantity_calc" parameters A = A,
	bMetricDimension = bMetricDimension,
	es_density		= es_density,
	es_density_us	= es_density_us,
	es_volume		= es_volume,
	es_surface		= es_surface
