! ==============================================================================
! newParameters
! ==============================================================================
"newParameters_BWM":
	if targetGUID # "" then
		newparameter "XwidthOfPlate", "Length"
		newparameter "YwidthOfPlate", "Length"
		newparameter "XwidthOfPlate2", "Length"
		newparameter "YwidthOfPlate2", "Length"

		XwidthOfPlate	= A
		YwidthOfPlate	= B
		XwidthOfPlate2	= A2
		YwidthOfPlate2	= B2
		A	= holeWidthX
		B	= holeWidthY
		A2	= holeHalfWidthX
		B2	= holeHalfWidthY

		parameters	XwidthOfPlate	= XwidthOfPlate,
					YwidthOfPlate	= YwidthOfPlate,
					XwidthOfPlate2	= XwidthOfPlate2,
					YwidthOfPlate2	= YwidthOfPlate2,
					A	= A,
					B	= B,
					A2	= A2,
					B2	= B2
	endif
return

