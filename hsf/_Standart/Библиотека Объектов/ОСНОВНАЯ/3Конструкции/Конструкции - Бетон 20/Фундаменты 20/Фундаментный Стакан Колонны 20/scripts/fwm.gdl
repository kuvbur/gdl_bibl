
actualGUID = FROM_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

	_autoID		= "CA90B8D0-D439-42AB-8D8F-305A23E0B1EF"
	_startID	= "6E7947B7-2B8F-4E89-B2A6-E09642D3EBD1"
	_endID		= "714F20D9-ABDD-4694-840B-71F16874F848"
gosub "newParameters_FWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid actualGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! newParameters
! ==============================================================================
"newParameters_FWM":
	if actualGuid = _startID | actualGuid = _autoID then
		_bHasParam	=	DELETED_PAR_VALUE("XwidthOfPlate", _XwidthOfPlate) &\
						DELETED_PAR_VALUE("YwidthOfPlate", _YwidthOfPlate) &\
						DELETED_PAR_VALUE("XwidthOfPlate2", _XwidthOfPlate2) &\
						DELETED_PAR_VALUE("YwidthOfPlate2", _YwidthOfPlate2)

		if _bHasParam then
			holeWidthX		= A
			holeWidthY		= B
			holeHalfWidthX	= A2
			holeHalfWidthY	= B2
			A	= _XwidthOfPlate
			B	= _YwidthOfPlate
			A2	= _XwidthOfPlate2
			B2	= _YwidthOfPlate2

			parameters	holeWidthX		= holeWidthX,
						holeWidthY		= holeWidthY,
						holeHalfWidthX	= holeHalfWidthX,
						holeHalfWidthY	= holeHalfWidthY,
						A	= A,
						B	= B,
						A2	= A2,
						B2	= B2
		endif
		
		actualGuid = _endID
	endif
return

