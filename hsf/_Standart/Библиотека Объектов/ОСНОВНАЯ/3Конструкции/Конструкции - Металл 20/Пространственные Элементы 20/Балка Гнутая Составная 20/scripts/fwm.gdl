
actualGUID = FROM_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

	_autoID		= "15A81DB5-4C5D-4BC4-9EBB-EF9202718D59"
	_startID	= "B8FEC32D-3A38-47B4-A54A-9D3872552C09"
	_endID		= "4C081266-8856-4166-9657-5CD087E737CA"
gosub "newHoleBoolean_FWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid actualGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! newHoleBoolean
! ==============================================================================
"newHoleBoolean_FWM":
	! new boolean and parameter name change for Hole Diameter
	if actualGUID = _startID | actualGUID = _autoID then
		_bHasParam = DELETED_PAR_VALUE("H", _tempHoleDiameter)
		if _bHasParam then
			if abs(_tempHoleDiameter) < EPS then
				bHole = 0
			else
				bHole = 1
				holeDiameter = _tempHoleDiameter
				parameters holeDiameter = holeDiameter
			endif
			parameters bHole = bHole

			actualGUID = _endID
		else
			actualGUID = ""
		endif
	endif
return

