
ax=a*cos(ang)			!!vertical A size
hx=h*cos(90-ang)

pen gs_cont_pen
fill gs_fill_type

!!---- Hotspot Editing
unID=1

If abs(ang)<eps Then
	hotspot2 0,0,unID,A2,1+256,A : unID=unID+1
	hotspot2 -1,0,unID,A2,3,A : unID=unID+1
	hotspot2 A2,0,unID,A2,2,A : unID=unID+1

	hotspot2 0,0,unID,B2,1+128,B : unID=unID+1
	hotspot2 0,-1,unID,B2,3,B : unID=unID+1
	hotspot2 0,B2,unID,B2,2,B : unID=unID+1

	hotspot2 A,0,unID,B2,1+128,B : unID=unID+1
	hotspot2 A,-1,unID,B2,3,B : unID=unID+1
	hotspot2 A,B2,unID,B2,2,B : unID=unID+1

	hotspot2 0,0,unID,B2,1+128,B : unID=unID+1
	hotspot2 0,1,unID,B2,3,B : unID=unID+1
	hotspot2 0,-B2,unID,B2,2,B : unID=unID+1

	hotspot2 A,0,unID,B2,1+128,B : unID=unID+1
	hotspot2 A,1,unID,B2,3,B : unID=unID+1
	hotspot2 A,-B2,unID,B2,2,B : unID=unID+1

	hotspot2 A2/2,0,unID : unID=unID+1

	poly2_b	5, 1+2+64, gs_fill_pen, gs_back_pen,
			0, -B2, 1,
			A, -B2, 1,
			A, B2, 1,
			0, B2, 1,
			0, -B2, -1
Else
	If abs(ang-90)<eps Then
		hotspot2 -h/2,0,unID : unID=unID+1

		hotspot2 0,0,unID,h,1+256 : unID=unID+1
		hotspot2 1,0,unID,h,3 : unID=unID+1
		hotspot2 -h,0,unID,h,2 : unID=unID+1

		poly2_b	5, 1+2+64, gs_fill_pen, gs_back_pen,
				0, Dd, 1,
				-h, Dd, 1,
				-h, -Dd, 1,
				0, -Dd, 1,
				0, Dd, -1

		poly2_b	2, 1+2+64, gs_fill_pen, gs_back_pen,
				0, 0, 901,
				B/2, 360, 4001

		poly2_b	2, 1+2+64, gs_fill_pen, gs_back_pen,
				-h, 0, 901,
				B/2, 360, 4001
	Else
		hotspot2 -hx,0,unID,A2,1+256,A : unID=unID+1
		hotspot2 -hx-1,0,unID,A2,3,A : unID=unID+1
		hotspot2 -hx+A2,0,unID,A2,2,A : unID=unID+1

		hotspot2 0,0,unID,A2,1+256,A : unID=unID+1
		hotspot2 -1,0,unID,A2,3,A : unID=unID+1
		hotspot2 A2,0,unID,A2,2,A : unID=unID+1

		hotspot2 0,0,unID,B2,1+128,B : unID=unID+1
		hotspot2 0,-1,unID,B2,3,B : unID=unID+1
		hotspot2 0,B2,unID,B2,2,B : unID=unID+1

		hotspot2 0,0,unID,B2,1+128,B : unID=unID+1
		hotspot2 0,1,unID,B2,3,B : unID=unID+1
		hotspot2 0,-B2,unID,B2,2,B : unID=unID+1

		hotspot2 A2,0,unID,B2,1+128,B : unID=unID+1
		hotspot2 A2,-1,unID,B2,3,B : unID=unID+1
		hotspot2 A2,B2,unID,B2,2,B : unID=unID+1

		hotspot2 A2,0,unID,B2,1+128,B : unID=unID+1
		hotspot2 A2,1,unID,B2,3,B : unID=unID+1
		hotspot2 A2,-B2,unID,B2,2,B : unID=unID+1

		hotspot2 -hx,0,unID,B2,1+128,B : unID=unID+1
		hotspot2 -hx,-1,unID,B2,3,B : unID=unID+1
		hotspot2 -hx,B2,unID,B2,2,B : unID=unID+1

		hotspot2 A2-hx,0,unID,B2,1+128,B : unID=unID+1
		hotspot2 A2-hx,-1,unID,B2,3,B : unID=unID+1
		hotspot2 A2-hx,B2,unID,B2,2,B : unID=unID+1

		hotspot2 -hx,0,unID,B2,1+128,B : unID=unID+1
		hotspot2 -hx,1,unID,B2,3,B : unID=unID+1
		hotspot2 -hx,-B2,unID,B2,2,B : unID=unID+1

		hotspot2 A2-hx,0,unID,B2,1+128,B : unID=unID+1
		hotspot2 A2-hx,1,unID,B2,3,B : unID=unID+1
		hotspot2 A2-hx,-B2,unID,B2,2,B : unID=unID+1

		hotspot2 A2/2,0,unID : unID=unID+1

		if ang<eps then
			poly2_b	5, 1+2+64, gs_fill_pen, gs_back_pen,
					0, -B2, 1,
					A2-hx, -B2, 0,
					A2-hx, B2, 1,
					0, B2, 0,
					0, -B2, -1

			add2 A2-hx, 0
			mul2 -1,1
			poly2_b	4, 1+2+64, gs_fill_pen, gs_back_pen,
					0, B2, 1,
					0, 0, 901,
					B2, 180, 4000,
					0, -B2, -1
			del 2

			mul2 cos(90-ang),-1
			poly2_b	2, 1+2+64, gs_fill_pen, gs_back_pen,
					0, 0, 901,
					B2, 360, 4001
			del 1

			add2 -hx, 0
			mul2 cos(90-ang),-1
			poly2_b	2, 1+2+64, gs_fill_pen, gs_back_pen,
					0, 0, 901,
					B2, 360, 4001
			del 2
		else
			poly2_b	5, 1+2+64, gs_fill_pen, gs_back_pen,
					-hx, -B2, 1,
					A2, -B2, 0,
					A2, B2, 1,
					-hx, B2, 0,
					-hx, -B2, -1

			add2 -hx, 0
			mul2 cos(90-ang),1
			poly2_b	4, 1+2+64, gs_fill_pen, gs_back_pen,
					0, B2, 1,
					0, 0, 901,
					B2, 180, 4000,
					0, -B2, -1
			del 2

			add2 A2-hx, 0
			mul2 cos(90-ang),1
			poly2_b	2, 1+2+64, gs_fill_pen, gs_back_pen,
					0, 0, 901,
					B2, 360, 4001
			del 2

			add2 A2, 0
			mul2 cos(90-ang),1
			poly2_b	2, 1+2+64, gs_fill_pen, gs_back_pen,
					0, 0, 901,
					B2, 360, 4001
			del 2

		endif
	EndIf
EndIf

!project2 3,270,2

