! Subroutines
! --------------------------------------------------------------------------------

"DimensionsPage":
	x1 = 0
	x2 = 120
	x3 = 210
	x4 = x2 + 100
	x5 = x2 + 200	!200 (Style Guide => fillTypeInfLenght)
	x6 = 360
	x7 = 408
	xEnd = 444
	
	outfieldHeight		= 15
	infieldHeight 		= 19
	
	dy 					= 24
	yEnd 				= 266
	pageStartY			= 0
	yCurrent 			= pageStartY + 1
	
	ui_style 0, 0
	
	ui_outfield	`Ширина Крепления к Стене` ,x1,    yCurrent+4, 	x2 - 2,	outfieldHeight	
	ui_infield{2} "brfu", 	 x2,   yCurrent, 	x3 - x2, infieldHeight

	ui_outfield	`Наклон Ската` ,x4,    yCurrent+4, 	x5 - x4,	outfieldHeight	
	ui_infield "neig", 	 x6,   yCurrent, 	xEnd - x6, infieldHeight

	yCurrent = yCurrent + dy


	ui_separator 0, yCurrent, xEnd, yCurrent

	pageStartY = 5
	
	xStart = 30				! start Position X Coordinate
	y1 = yCurrent + 15		! start Position Y Coordinate
	Gap = 2
	PictX1 = 130
	PictX2 = 124
	PictX = 254
	PictY1 = 143
	PictY = 206
	CellX = 70		! width of cell (numerical infield box)
	CellY = 18		! height of 1.cell (numerical infield box)
	xText = 30		! start Position of outfield text X Coordinate
	TextX = 180		! width of outfield box
	TextCellX = 120 ! width of Text infield box
	
	
	ui_pict	1,	xStart+CellX+Gap,	        y1+(PictY-PictY1),	PictX1,	PictY1, 1
	ui_pict	2,	xStart+CellX+Gap+PictX1,	y1,	PictX2,	PictY, 1
	
	ui_outfield	str("%.0", LumberSize*1000),	    xStart+CellX/2-2*Gap+PictX1,    y1+(PictY-PictY1)-CellY-Gap, CellX/2,  CellY ui_tooltip `Размер Пиломатериала`
	ui_infield	"schneid",	        xStart,	                          y1+145,                      CellX,	   CellY ui_tooltip `Угол Торца Стропила`
	ui_infield	"ueb",	            xStart,	                          y1+145+CellY+Gap,            CellX,	   CellY ui_tooltip `Свес Стропила`
	ui_infield	"edgeOverhang",	    xStart,	                          y1+145+CellY*2+Gap*2,        CellX,	   CellY ui_tooltip `Свес Торца Стропила`
	ui_infield	"h",	            xStart+CellX+Gap+PictX+Gap,       y1,                          CellX,	   CellY ui_tooltip `Высота Профиля`
	ui_infield	"b",	            xStart+CellX+Gap+PictX+Gap,	      y1+CellY+15,                 CellX-18,  CellY ui_tooltip `Толщина Профиля`
	ui_infield	"brfi",	            xStart+CellX+Gap+PictX+Gap,	      y1+CellY+58,                 CellX,	   CellY ui_tooltip `Ширина Паза Вверху`
	ui_infield	"gs_struc_length",	xStart+CellX+Gap+PictX+Gap,	      y1+CellY+133,                CellX-18,  CellY ui_tooltip `Конструктивная Длина Пролета`
	ui_outfield	str("%.0", purlinsOutDist*1000),	xStart+CellX+2*Gap+PictX+Gap,	      y1+CellY+162,                CellX-18,  CellY ui_tooltip `Внешнее Расстояние Обрешетки`
	ui_outfield	str("%.0", a*1000),	xStart+CellX+Gap+PictX/2-CellX/2, y1+PictY+Gap,                CellX/2,  CellY ui_tooltip `Вертикальная Проекция Стропила`
	!ui_infield	"schneid2",	xStart+PictX+Gap*2,	y1+CellY*2+10*2, CellX,	CellY ui_tooltip `Cutting Angle 2`
	!ui_infield	"schneid1",	xStart-CellX-Gap*2,	y1+100, CellX,	CellY ui_tooltip `Cutting Angle 1`

return


