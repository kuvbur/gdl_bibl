
! Wooden Landing


!!!!!!!!!!!!!! *** 2D Representation on Stories *** !!!!!!!!!!!!!!
!!! *** Stories down the story, where the object is placed on !!!!

IF GLOB_CH_STORY_DIST < -EPS  THEN
	gs_cont_line=gs_LineUnderHStory
	gs_fill_type=65
	gs_back_pen=0
ENDIF
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!


PEN gs_cont_pen
FILL gs_fill_type
SET LINE_TYPE gs_cont_line

unID=1

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
IF iLandingType = 3 THEN

	HOTSPOT2 0,fjl, unID, flwl, 1+256 : unID=unID+1
	HOTSPOT2 -1,fjl, unID, flwl, 3 : unID=unID+1
	HOTSPOT2 flwl,fjl, unID, flwl, 2 : unID=unID+1

	HOTSPOT2 0,fjl+fy, unID, flwl, 1+256 : unID=unID+1
	HOTSPOT2 -1,fjl+fy, unID, flwl, 3 : unID=unID+1
	HOTSPOT2 flwl,fjl+fy, unID, flwl, 2 : unID=unID+1

	HOTSPOT2 0,fjl, unID, fy, 1+256 : unID=unID+1
	HOTSPOT2 0,-1, unID, fy, 3 : unID=unID+1
	HOTSPOT2 0,fjl+fy, unID, fy, 2 : unID=unID+1

	HOTSPOT2 flwl,fjl, unID, fy, 1+256 : unID=unID+1
	HOTSPOT2 flwl,-1, unID, fy, 3 : unID=unID+1
	HOTSPOT2 flwl,fjl+fy, unID, fy, 2 : unID=unID+1


	POLY2_b	5, 1+2+64,	gs_fill_pen,  gs_back_pen,
		0,0,1,
		flwl,0,1,
		flwl,fy,1,
		0,fy,1,
		0,0,-1

	IF strings THEN
		POLY2_b	9, 1+2+64,	gs_fill_pen,  gs_back_pen,
			0,0,1,
			0,fy,1,
			flwl,fy,1,
			flwl,0,1,
			flwl+st,0,1,
			flwl+st,fy+st,1,
			-st,fy+st,1,
			-st,0,1,
			0,0,-1

		!!! *** BOTTOM / RIGHT direction
		HOTSPOT2 0,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT2 1,0, unID, st, 3 : unID=unID+1
		HOTSPOT2 -st,0, unID, st, 2 : unID=unID+1

		!!! *** BOTTOM / LEFT direction
		HOTSPOT2 flwl,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT2 flwl-1,0, unID, st, 3 : unID=unID+1
		HOTSPOT2 flwl+st,0, unID, st, 2 : unID=unID+1

		!!! *** TOP
		HOTSPOT2 flwl/2,fy, unID, st, 1+128 : unID=unID+1
		HOTSPOT2 flwl/2,fy-1, unID, st, 3 : unID=unID+1
		HOTSPOT2 flwl/2,fy+st, unID, st, 2 : unID=unID+1

		HOTSPOT2 flwl+st,fy+st, unID : unID=unID+1
		HOTSPOT2 -st,fy+st, unID : unID=unID+1
	ENDIF
ENDIF


!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
IF iLandingType = 2 THEN

	!!! *** Middle-bottom
	HOTSPOT2 0,0, unID, flwl, 1+256 : unID=unID+1
	HOTSPOT2 -1, 0, unID, flwl, 3 : unID=unID+1
	HOTSPOT2 flwl,0, unID, flwl, 2 : unID=unID+1

	!!! *** Left-top
	HOTSPOT2 0,yl, unID, fy, 1+128 : unID=unID+1
	HOTSPOT2 0, -1, unID, fy, 3 : unID=unID+1
	HOTSPOT2 0,fy+yl, unID, fy, 2 : unID=unID+1

	!!!!!!!!! MIDDLE
	HOTSPOT2 0,yl, unID, flwl, 1+128 : unID=unID+1
	HOTSPOT2 -1, yl, unID, flwl, 3 : unID=unID+1
	HOTSPOT2 flwl, yl, unID, flwl, 2 : unID=unID+1

	HOTSPOT2 flwl,0, unID, yl, 1+256 : unID=unID+1
	HOTSPOT2 flwl,-1, unID, yl, 3 : unID=unID+1
	HOTSPOT2 flwl,yl, unID, yl, 2 : unID=unID+1

	!!! *** Right-bottom
	HOTSPOT2 flwl,yl, unID, fx, 1+128 : unID=unID+1
	HOTSPOT2 flwl-1,yl, unID, fx, 3 : unID=unID+1
	HOTSPOT2 flwl+fx,yl, unID, fx, 2 : unID=unID+1

	!!! *** Right-top
	HOTSPOT2 flwl,yl+fy, unID, fx, 1+128 : unID=unID+1
	HOTSPOT2 flwl-1,yl+fy, unID, fx, 3 : unID=unID+1
	HOTSPOT2 flwl+fx,yl+fy, unID, fx, 2 : unID=unID+1

	HOTSPOT2 flwl+fx,yl, unID, fy, 1+256 : unID=unID+1
	HOTSPOT2 flwl+fx,yl-1, unID, fy, 3 : unID=unID+1
	HOTSPOT2 flwl+fx,yl+fy, unID, fy, 2 : unID=unID+1

	DRAWINDEX 20
	POLY2_b	7, 1+2+64,	gs_fill_pen,  gs_back_pen,
		0,0,1,
		0,fy+yl,1,
		flwl+fx, fy+yl,1,
		flwl+fx, yl,1,
		flwl, yl,1,
		flwl,0,1,
		0,0,-1

	IF strings THEN
	DRAWINDEX 10
		POLY2_b	7, 1+2+64,	gs_fill_pen,  gs_back_pen,
			0,0,0,
			0,yl+fy,0,
			flwl+fx,fy+yl,1,
			flwl+fx,fy+yl+st,1,
			-st, fy+yl+st,1,
			-st, 0,1,
			0,0,-1
		POLY2_b	7, 1+2+64,	gs_fill_pen,  gs_back_pen,
			flwl, 0,0,
			flwl, yl,0,
			flwl+fx, yl,1,
			flwl+fx, yl-st,1,
			flwl+st, yl-st,1,
			flwl+st, 0,1,
			flwl, 0,-1

		!!! *** BOTTOM / RIGHT direction
		HOTSPOT2 0,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT2 1,0, unID, st, 3 : unID=unID+1
		HOTSPOT2 -st,0, unID, st, 2 : unID=unID+1

		!!! *** BOTTOM / LEFT direction
		HOTSPOT2 flwl,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT2 flwl-1,0, unID, st, 3 : unID=unID+1
		HOTSPOT2 flwl+st,0, unID, st, 2 : unID=unID+1

		!!! *** LEFT FLIGHT / Top
		HOTSPOT2 (flwl+fx),fy+yl, unID, st, 1+128 : unID=unID+1
		HOTSPOT2 (flwl+fx),fy+yl-1, unID, st, 3 : unID=unID+1
		HOTSPOT2 (flwl+fx),fy+yl+st, unID, st, 2 : unID=unID+1

		!!! *** LEFT FLIGHT / Bottom
		HOTSPOT2 (flwl+fx),yl, unID, st, 1+128 : unID=unID+1
		HOTSPOT2 (flwl+fx),yl+1, unID, st, 3 : unID=unID+1
		HOTSPOT2 (flwl+fx),yl-st, unID, st, 2 : unID=unID+1

		HOTSPOT2 flwl+st,yl-st, unID : unID=unID+1
		HOTSPOT2 -st,fy+yl+st, unID : unID=unID+1
	ENDIF
ENDIF


!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
IF iLandingType = 1 THEN

	!!! *** Middle-Left-Bottom
	HOTSPOT2 0,0, unID, flwl, 1+256 : unID=unID+1
	HOTSPOT2 -1, 0, unID, flwl, 3 : unID=unID+1
	HOTSPOT2 flwl, 0, unID, flwl, 2 : unID=unID+1

	!!! *** Left-top
	HOTSPOT2 0,yl, unID, fy, 1+128 : unID=unID+1
	HOTSPOT2 0, -1, unID, fy, 3 : unID=unID+1
	HOTSPOT2 0,fy+yl, unID, fy, 2 : unID=unID+1

	!!! *** Middle-Left-Top
	HOTSPOT2 0,yl, unID, flwl, 1+128 : unID=unID+1
	HOTSPOT2 -1, yl, unID, flwl, 3 : unID=unID+1
	HOTSPOT2 flwl, yl, unID, flwl, 2 : unID=unID+1

	HOTSPOT2 flwl,0, unID, yl, 1+256 : unID=unID+1
	HOTSPOT2 flwl,-1, unID, yl, 3 : unID=unID+1
	HOTSPOT2 flwl,yl, unID, yl, 2 : unID=unID+1

	!! *** Right-Bottom
	HOTSPOT2 flwl+fx,yl-yr, unID, flwr, 1+256 : unID=unID+1
	HOTSPOT2 flwl+fx-1,yl-yr, unID, flwr, 3 : unID=unID+1
	HOTSPOT2 flwl+fx+flwr,yl-yr, unID, flwr, 2 : unID=unID+1

	HOTSPOT2 flwl+fx+flwr,yl, unID, yr, 1+128 : unID=unID+1
	HOTSPOT2 flwl+fx+flwr,yl+1, unID, yr, 3 : unID=unID+1
	HOTSPOT2 flwl+fx+flwr,yl-yr, unID, yr, 2 : unID=unID+1

	!!! *** Middle-Right-Top
	HOTSPOT2 flwl+fx,yl, unID, yr, 1+256 : unID=unID+1
	HOTSPOT2 flwl+fx,yl+1, unID, yr, 3 : unID=unID+1
	HOTSPOT2 flwl+fx,yl-yr, unID, yr, 2 : unID=unID+1

	HOTSPOT2 flwl,yl, unID, fx, 1+128 : unID=unID+1
	HOTSPOT2 flwl-1,yl, unID, fx, 3 : unID=unID+1
	HOTSPOT2 flwl+fx,yl, unID, fx, 2 : unID=unID+1

	!! *** Right-top
	HOTSPOT2 flwl+fx,yl+fy, unID, flwr, 1+128 : unID=unID+1
	HOTSPOT2 flwl+fx-1,yl+fy, unID, flwr, 3 : unID=unID+1
	HOTSPOT2 flwl+fx+flwr,yl+fy, unID, flwr, 2 : unID=unID+1

	HOTSPOT2 flwl+fx+flwr,yl, unID, fy, 1+128 : unID=unID+1
	HOTSPOT2 flwl+fx+flwr,yl-1, unID, fy, 3 : unID=unID+1
	HOTSPOT2 flwl+fx+flwr,yl+fy, unID, fy, 2 : unID=unID+1

	DRAWINDEX 20
	POLY2_b	9, 1+2+64,	gs_fill_pen,  gs_back_pen,
		0,0,1,
		0, yl+fy,1,
		flwl+fx+flwr, fy+yl, 1,
		flwl+fx+flwr, yl-yr,1,
		flwl+fx, yl-yr,1,
		flwl+fx, yl,1,
		flwl, yl,1,
		flwl, 0,1,
		0,0,-1

	DRAWINDEX 10
	IF strings THEN
		POLY2_b	9, 1+2+64,	gs_fill_pen,  gs_back_pen,
			0, 0,0,
			0, yl+fy,0,
			flwl+flwr+fx, fy+yl,0,
			flwl+flwr+fx, yl-yr,1,
			flwl+flwr+fx+st, yl-yr,1,
			flwl+flwr+fx+st, yl+fy+st,1,
			-st, fy+yl+st,1,
			-st, 0,1,
			0,0,-1
		POLY2_b	9, 1+2+64,	gs_fill_pen,  gs_back_pen,
			flwl, 0,0,
			flwl, yl,0,
			flwl+fx, yl,0,
			flwl+fx, yl-yr,1,
			flwl+fx-st, yl-yr,1,
			flwl+fx-st, yl-st,1,
			flwl+st, yl-st,1,
			flwl+st, 0,1,
			flwl, 0,-1

		!!! *** LEFT FLIGHT -BOTTOM/LEFT direction
		HOTSPOT2 0,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT2 1,0, unID, st, 3 : unID=unID+1
		HOTSPOT2 -st,0, unID, st, 2 : unID=unID+1

		!!! *** LEFT FLIGHT -BOTTOM/RIGHT direction
		HOTSPOT2 flwl,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT2 flwl-1,0, unID, st, 3 : unID=unID+1
		HOTSPOT2 flwl+st,0, unID, st, 2 : unID=unID+1

		!!! *** RIGHT FLIGHT -BOTTOM/RIGHT direction
		HOTSPOT2 flwl+fx,yl-yr, unID, st, 1+128 : unID=unID+1
		HOTSPOT2 flwl+fx+1,yl-yr, unID, st, 3 : unID=unID+1
		HOTSPOT2 flwl+fx-st,yl-yr, unID, st, 2 : unID=unID+1

		!!! *** RIGHT FLIGHT -BOTTOM/LEFT direction
		HOTSPOT2 flwl+flwr+fx,yl-yr, unID, st, 1+128 : unID=unID+1
		HOTSPOT2 flwl+flwr+fx-1,yl-yr, unID, st, 3 : unID=unID+1
		HOTSPOT2 flwl+flwr+fx+st,yl-yr, unID, st, 2 : unID=unID+1

		!!! *** TOP
		HOTSPOT2 (flwl+flwr+fx)/2,fy+yl, unID, st, 1+128 : unID=unID+1
		HOTSPOT2 (flwl+flwr+fx)/2,fy+yl, unID, st, 3 : unID=unID+1
		HOTSPOT2 (flwl+flwr+fx)/2,fy+yl+st, unID, st, 2 : unID=unID+1

		HOTSPOT2 flwl+st,yl-st, unID : unID=unID+1
		HOTSPOT2 flwl+fx-st,yl-st, unID : unID=unID+1
		HOTSPOT2 -st,fy+yl+st, unID : unID=unID+1
		HOTSPOT2 flwl+flwr+fx+st,fy+yl+st, unID : unID=unID+1

	ENDIF

ENDIF

