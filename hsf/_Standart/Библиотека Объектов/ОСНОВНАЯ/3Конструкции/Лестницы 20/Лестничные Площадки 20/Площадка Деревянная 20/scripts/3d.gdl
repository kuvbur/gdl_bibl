
! Wooden Landing

IF gs_detlevel_3D_m = 0 THEN end

IF GLOB_CONTEXT>20 and GLOB_CONTEXT<40 THEN 		!! for Simple MoDEL and in 3D Windoow
gs_detlevel_3D_m = 1
ENDIF

IF gs_shadow=0 THEN SHADOW OFF
IF gs_shadow=1 THEN SHADOW ON

PEN gs_cont_pen
unID=1


!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
IF iLandingType = 3 THEN

	HOTSPOT flwl,0,0, unID, fy, 1+256 : unID=unID+1
	HOTSPOT flwl,-1,0, unID, fy, 3 : unID=unID+1
	HOTSPOT flwl,0+fy,0, unID, fy, 2 : unID=unID+1

	HOTSPOT 0,0,0, unID, fy, 1+256 : unID=unID+1
	HOTSPOT 0,-1,0, unID, fy, 3 : unID=unID+1
	HOTSPOT 0,0+fy, 0,unID, fy, 2 : unID=unID+1

	HOTSPOT 0,0,0, unID, flwl, 1+256 : unID=unID+1
	HOTSPOT -1,0, 0,unID, flwl, 3 : unID=unID+1
	HOTSPOT flwl,0, 0,unID, flwl, 2 : unID=unID+1

	HOTSPOT 0,0+fy,0, unID, flwl, 1+256 : unID=unID+1
	HOTSPOT -1,0+fy, 0,unID, flwl, 3 : unID=unID+1
	HOTSPOT flwl,0+fy, 0,unID, flwl, 2 : unID=unID+1

	IF strings THEN
		!!! *** BOTTOM / RIGHT direction
		HOTSPOT 0,0,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT 1,0,0, unID, st, 3 : unID=unID+1
		HOTSPOT -st,0,0, unID, st, 2 : unID=unID+1

		!!! *** BOTTOM / LEFT direction
		HOTSPOT flwl,0,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT flwl-1,0,0, unID, st, 3 : unID=unID+1
		HOTSPOT flwl+st,0,0, unID, st, 2 : unID=unID+1

		!!! *** TOP
		HOTSPOT flwl/2,fy,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT flwl/2,fy-1,0, unID, st, 3 : unID=unID+1
		HOTSPOT flwl/2,fy+st,0, unID, st, 2 : unID=unID+1

		HOTSPOT flwl+st,fy+st,0, unID : unID=unID+1
		HOTSPOT -st,fy+st,0, unID : unID=unID+1

		!!! *** STRING HEIGHT
		HOTSPOT -st,0,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT -st,0,1, unID, sh, 3 : unID=unID+1
		HOTSPOT -st,0,-sh, unID, sh, 2 : unID=unID+1

		HOTSPOT flwl+st,0,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT flwl+st,0,1, unID, sh, 3 : unID=unID+1
		HOTSPOT flwl+st,0,-sh, unID, sh, 2 : unID=unID+1

		HOTSPOT flwl+st,fy+st,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT flwl+st,fy+st,1, unID, sh, 3 : unID=unID+1
		HOTSPOT flwl+st,fy+st,-sh, unID, sh, 2 : unID=unID+1

		HOTSPOT 0,fy+st,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT 0,fy+st,1, unID, sh, 3 : unID=unID+1
		HOTSPOT 0,fy+st,-sh, unID, sh, 2 : unID=unID+1
	ENDIF

	ADDz -lth
	MATERIAL matl
	if GLOB_CONTEXT = 4 then SECT_FILL typeFillTread, penBackFillTread, penFillTread, penLineTread

	!!! slab
	PRISM 5, lth,
		0,-ns,
		flwl,-ns,
		flwl,fy,
		0,fy,
		0,-ns
	DEL 1

	ADDz -sh
	IF strings THEN
		if GLOB_CONTEXT = 4 then SECT_FILL typeFillStringer, penBackFillStringer, penFillStringer, penLineStringer

		MATERIAL matstr
		PRISM 9, sh,
			0,0,
			0,fy,
			flwl,fy,
			flwl,0,
			flwl+st,0,
			flwl+st,fy+st,
			-st,fy+st,
			-st,0,
			0,0

	ADDz softh
	!!! face desk
	MATERIAL mat_riser
	if GLOB_CONTEXT = 4 then SECT_FILL typeFillRiser, penBackFillRiser, penFillRiser, penLineRiser

	PRISM 5, sh-lth-softh,
		0,0,
		flwl,0,
		flwl,lth,
		0,lth,
		0,0
	DEL 1
	ENDIF

	IF soffit THEN
	IF strings=0 THEN ADDz sh-softh-lth
		MATERIAL matsoff
		if GLOB_CONTEXT = 4 then SECT_FILL typeFillSoffit, penBackFillSoffit, penFillSoffit, penLineSoffit

		PRISM 5, softh,
			0,0,
			flwl,0,
			flwl,fy,
			0,fy,
			0,0
	IF strings=0 THEN DEL 1
	ENDIF
DEL 1
ENDIF


!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
IF iLandingType = 2 THEN

	fxx=flwl+fx

		!!! *** Middle-bottom
		HOTSPOT 0,0,0, unID, flwl, 1+256 : unID=unID+1
		HOTSPOT -1, 0,0, unID, flwl, 3 : unID=unID+1
		HOTSPOT flwl,0,0, unID, flwl, 2 : unID=unID+1

		!!! *** Left-top
		HOTSPOT 0,yl,0, unID, fy, 1+128 : unID=unID+1
		HOTSPOT 0, -1,0, unID, fy, 3 : unID=unID+1
		HOTSPOT 0,fy+yl,0, unID, fy, 2 : unID=unID+1

		!!!!!!!!! MIDDLE
		HOTSPOT 0,yl,0, unID, flwl, 1+128 : unID=unID+1
		HOTSPOT -1, yl,0, unID, flwl, 3 : unID=unID+1
		HOTSPOT flwl, yl,0, unID, flwl, 2 : unID=unID+1

		HOTSPOT flwl,0,0, unID, yl, 1+256 : unID=unID+1
		HOTSPOT flwl,-1,0, unID, yl, 3 : unID=unID+1
		HOTSPOT flwl,yl,0, unID, yl, 2 : unID=unID+1

		!!! *** Right-bottom
		HOTSPOT flwl,yl,0, unID, fx, 1+128 : unID=unID+1
		HOTSPOT flwl-1,yl,0, unID, fx, 3 : unID=unID+1
		HOTSPOT flwl+fx,yl,0, unID, fx, 2 : unID=unID+1

		!!! *** Right-top
		HOTSPOT flwl,yl+fy,0, unID, fx, 1+128 : unID=unID+1
		HOTSPOT flwl-1,yl+fy,0, unID, fx, 3 : unID=unID+1
		HOTSPOT flwl+fx,yl+fy,0, unID, fx, 2 : unID=unID+1

		HOTSPOT fxx,yl, 0,unID, fy, 1+256 : unID=unID+1
		HOTSPOT fxx,yl-1, 0,unID,fy, 3 : unID=unID+1
		HOTSPOT fxx,yl+fy, 0,unID, fy, 2 : unID=unID+1

	IF strings THEN

		!!! *** BOTTOM / RIGHT direction
		HOTSPOT 0,0,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT 1,0,0, unID, st, 3 : unID=unID+1
		HOTSPOT -st,0,0, unID, st, 2 : unID=unID+1

		!!! *** BOTTOM / LEFT direction
		HOTSPOT flwl,0,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT flwl-1,0,0, unID, st, 3 : unID=unID+1
		HOTSPOT flwl+st,0,0, unID, st, 2 : unID=unID+1

		!!! *** LEFT FLIGHT / Top
		HOTSPOT (flwl+fx),fy+yl,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT (flwl+fx),fy+yl-1,0, unID, st, 3 : unID=unID+1
		HOTSPOT (flwl+fx),fy+yl+st,0, unID, st, 2 : unID=unID+1

		!!! *** LEFT FLIGHT / Bottom
		HOTSPOT (flwl+fx),yl,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT (flwl+fx),yl+1,0, unID, st, 3 : unID=unID+1
		HOTSPOT (flwl+fx),yl-st,0, unID, st, 2 : unID=unID+1

		HOTSPOT flwl+st,yl-st,0, unID : unID=unID+1
		HOTSPOT -st,fy+yl+st,0, unID : unID=unID+1

		!!! *** STRING HEIGHT
		HOTSPOT -st,0,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT -st,0,1, unID, sh, 3 : unID=unID+1
		HOTSPOT -st,0,-sh, unID, sh, 2 : unID=unID+1

		HOTSPOT flwl+st,0,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT flwl+st,0,1, unID, sh, 3 : unID=unID+1
		HOTSPOT flwl+st,0,-sh, unID, sh, 2 : unID=unID+1

		HOTSPOT flwl+st,yl-st,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT flwl+st,yl-st,1, unID, sh, 3 : unID=unID+1
		HOTSPOT flwl+st,yl-st,-sh, unID, sh, 2 : unID=unID+1

		HOTSPOT flwl+fx,yl-st,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT flwl+fx,yl-st,1, unID, sh, 3 : unID=unID+1
		HOTSPOT flwl+fx,yl-st,-sh, unID, sh, 2 : unID=unID+1

		HOTSPOT flwl+fx,fy+yl+st,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT flwl+fx,fy+yl+st,1, unID, sh, 3 : unID=unID+1
		HOTSPOT flwl+fx,fy+yl+st,-sh, unID, sh, 2 : unID=unID+1

		HOTSPOT -st,fy+yl+st,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT -st,fy+yl+st,1, unID, sh, 3 : unID=unID+1
		HOTSPOT -st,fy+yl+st,-sh, unID, sh, 2 : unID=unID+1

	ENDIF

	ADDz -lth
	MATERIAL matl
	if GLOB_CONTEXT = 4 then SECT_FILL typeFillTread, penBackFillTread, penFillTread, penLineTread

	PRISM 7, lth,
		0,-ns,
		0,fy+yl,
		flwl+fx, fy+yl,
		flwl+fx, yl,
		flwl, yl,
		flwl,-ns,
		0,-ns
	DEL 1

	ADDz -sh
	IF strings THEN
		MATERIAL matstr
		if GLOB_CONTEXT = 4 then SECT_FILL typeFillStringer, penBackFillStringer, penFillStringer, penLineStringer

		PRISM 7, sh,
			0,0,
			0,yl+fy,
			flwl+fx,fy+yl,
			flwl+fx,fy+yl+st,
			-st, fy+yl+st,
			-st, 0,
			0,0
		PRISM 7, sh,
			flwl, 0,
			flwl, yl,
			flwl+fx, yl,
			flwl+fx, yl-st,
			flwl+st, yl-st,
			flwl+st, 0,
			flwl, 0

		ADDz softh
		!!! face desk
		MATERIAL mat_riser
		if GLOB_CONTEXT = 4 then SECT_FILL typeFillRiser, penBackFillRiser, penFillRiser, penLineRiser

		PRISM 5, sh-lth-softh,
			0,0,
			flwl,0,
			flwl,lth,
			0,lth,
			0,0

		ADD flwl+fx,yl,0
		ROTz 90
			PRISM 5, sh-lth-softh,
				0,0,
				fy,0,
				fy,lth,
				0,lth,
				0,0
		DEL 2
		DEL 1

	ENDIF

	IF soffit THEN
	IF strings=0 THEN ADDz sh-softh-lth
		MATERIAL matsoff
		if GLOB_CONTEXT = 4 then SECT_FILL typeFillSoffit, penBackFillSoffit, penFillSoffit, penLineSoffit

		PRISM 7, softh,
			0,0,
			0,fy+yl,
			flwl+fx, fy+yl,
			flwl+fx, yl,
			flwl, yl,
			flwl,0,
			0,0
	IF strings=0 THEN DEL 1
	ENDIF
DEL 1
ENDIF


!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
IF iLandingType = 1 THEN
		fxx=flwl+fx

		!!! *** Middle-Left-Bottom
		HOTSPOT 0,0,0, unID, flwl, 1+256 : unID=unID+1
		HOTSPOT -1, 0,0, unID, flwl, 3 : unID=unID+1
		HOTSPOT flwl, 0,0, unID, flwl, 2 : unID=unID+1

		!!! *** Left-top
		HOTSPOT 0,yl,0, unID, fy, 1+128 : unID=unID+1
		HOTSPOT 0, -1,0, unID, fy, 3 : unID=unID+1
		HOTSPOT 0,fy+yl,0, unID, fy, 2 : unID=unID+1

		!!! *** Middle-Left-Top
		HOTSPOT 0,yl,0, unID, flwl, 1+128 : unID=unID+1
		HOTSPOT -1, yl,0, unID, flwl, 3 : unID=unID+1
		HOTSPOT flwl, yl,0, unID, flwl, 2 : unID=unID+1

		HOTSPOT flwl,0,0, unID, yl, 1+256 : unID=unID+1
		HOTSPOT flwl,-1,0, unID, yl, 3 : unID=unID+1
		HOTSPOT flwl,yl,0, unID, yl, 2 : unID=unID+1

		!!! *** Right-Bottom
		HOTSPOT fxx,yl-yr,0, unID, flwr, 1+256 : unID=unID+1
		HOTSPOT fxx-1,yl-yr,0, unID, flwr, 3 : unID=unID+1
		HOTSPOT fxx+flwr,yl-yr,0, unID, flwr, 2 : unID=unID+1

		HOTSPOT fxx+flwr,yl,0, unID, yr, 1+128 : unID=unID+1
		HOTSPOT fxx+flwr,yl+1,0, unID, yr, 3 : unID=unID+1
		HOTSPOT fxx+flwr,yl-yr,0, unID, yr, 2 : unID=unID+1

		!!! *** Middle-Right-Top
		HOTSPOT fxx,yl,0, unID, yr, 1+256 : unID=unID+1
		HOTSPOT fxx,yl+1,0, unID, yr, 3 : unID=unID+1
		HOTSPOT fxx,yl-yr,0, unID, yr, 2 : unID=unID+1

		HOTSPOT flwl,yl,0, unID, fx, 1+128 : unID=unID+1
		HOTSPOT flwl-1,yl,0, unID, fx, 3 : unID=unID+1
		HOTSPOT flwl+fx,yl,0, unID, fx, 2 : unID=unID+1

		!! *** Right-top
		HOTSPOT fxx,yl+fy,0, unID, flwr, 1+128 : unID=unID+1
		HOTSPOT fxx-1,yl+fy,0, unID, flwr, 3 : unID=unID+1
		HOTSPOT fxx+flwr,yl+fy,0, unID, flwr, 2 : unID=unID+1

		HOTSPOT fxx+flwr,yl,0, unID, fy, 1+128 : unID=unID+1
		HOTSPOT fxx+flwr,yl-1,0, unID, fy, 3 : unID=unID+1
		HOTSPOT fxx+flwr,yl+fy,0, unID, fy, 2 : unID=unID+1

	IF strings THEN

		!!! *** LEFT FLIGHT -BOTTOM/LEFT direction
		HOTSPOT 0,0,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT 1,0,0, unID, st, 3 : unID=unID+1
		HOTSPOT -st,0,0, unID, st, 2 : unID=unID+1

		!!! *** LEFT FLIGHT -BOTTOM/RIGHT direction
		HOTSPOT flwl,0,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT flwl-1,0,0, unID, st, 3 : unID=unID+1
		HOTSPOT flwl+st,0,0, unID, st, 2 : unID=unID+1

		!!! *** RIGHT FLIGHT -BOTTOM/RIGHT direction
		HOTSPOT flwl+fx,yl-yr,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT flwl+fx+1,yl-yr,0, unID, st, 3 : unID=unID+1
		HOTSPOT flwl+fx-st,yl-yr,0, unID, st, 2 : unID=unID+1

		!!! *** RIGHT FLIGHT -BOTTOM/LEFT direction
		HOTSPOT flwl+flwr+fx,yl-yr,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT flwl+flwr+fx-1,yl-yr,0, unID, st, 3 : unID=unID+1
		HOTSPOT flwl+flwr+fx+st,yl-yr,0, unID, st, 2 : unID=unID+1

		!!! *** TOP
		HOTSPOT (flwl+flwr+fx)/2,fy+yl,0, unID, st, 1+128 : unID=unID+1
		HOTSPOT (flwl+flwr+fx)/2,fy+yl,0, unID, st, 3 : unID=unID+1
		HOTSPOT (flwl+flwr+fx)/2,fy+yl+st,0, unID, st, 2 : unID=unID+1

		HOTSPOT flwl+st,yl-st,0, unID : unID=unID+1
		HOTSPOT flwl+fx-st,yl-st,0, unID : unID=unID+1
		HOTSPOT -st,fy+yl+st,0, unID : unID=unID+1
		HOTSPOT flwl+flwr+fx+st,fy+yl+st,0, unID : unID=unID+1

		!!! *** STRING HEIGHT
		HOTSPOT -st,0,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT -st,0,1, unID, sh, 3 : unID=unID+1
		HOTSPOT -st,0,-sh, unID, sh, 2 : unID=unID+1

		HOTSPOT flwl+st,0,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT flwl+st,0,1, unID, sh, 3 : unID=unID+1
		HOTSPOT flwl+st,0,-sh, unID, sh, 2 : unID=unID+1

		HOTSPOT flwl+st,yl-st,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT flwl+st,yl-st,1, unID, sh, 3 : unID=unID+1
		HOTSPOT flwl+st,yl-st,-sh, unID, sh, 2 : unID=unID+1

		HOTSPOT flwl+fx-st,yl-st,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT flwl+fx-st,yl-st,1, unID, sh, 3 : unID=unID+1
		HOTSPOT flwl+fx-st,yl-st,-sh, unID, sh, 2 : unID=unID+1

		HOTSPOT flwl+fx-st,yl-yr,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT flwl+fx-st,yl-yr,1, unID, sh, 3 : unID=unID+1
		HOTSPOT flwl+fx-st,yl-yr,-sh, unID, sh, 2 : unID=unID+1

		HOTSPOT flwl+fx+flwr+st,yl-yr,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT flwl+fx+flwr+st,yl-yr,1, unID, sh, 3 : unID=unID+1
		HOTSPOT flwl+fx+flwr+st,yl-yr,-sh, unID, sh, 2 : unID=unID+1

		HOTSPOT flwl+fx+flwr+st,fy+yl+st,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT flwl+fx+flwr+st,fy+yl+st,1, unID, sh, 3 : unID=unID+1
		HOTSPOT flwl+fx+flwr+st,fy+yl+st,-sh, unID, sh, 2 : unID=unID+1

		HOTSPOT -st,fy+yl+st,0, unID, sh, 1+128 : unID=unID+1
		HOTSPOT -st,fy+yl+st,1, unID, sh, 3 : unID=unID+1
		HOTSPOT -st,fy+yl+st,-sh, unID, sh, 2 : unID=unID+1

	ENDIF

	ADDz -lth
		MATERIAL matl
		if GLOB_CONTEXT = 4 then SECT_FILL typeFillTread, penBackFillTread, penFillTread, penLineTread

		PRISM 9, lth,
			0,-ns,
			0, yl+fy,
			flwl+fx+flwr, fy+yl,
			flwl+fx+flwr, yl-yr,
			flwl+fx, yl-yr,
			flwl+fx, yl,
			flwl, yl,
			flwl, -ns,
			0,-ns
	DEL 1

	ADDz -sh
	IF strings THEN
		MATERIAL matstr
		if GLOB_CONTEXT = 4 then SECT_FILL typeFillStringer, penBackFillStringer, penFillStringer, penLineStringer

		PRISM 9, sh,
			0, 0,
			0, yl+fy,
			flwl+flwr+fx, fy+yl,
			flwl+flwr+fx, yl-yr,
			flwl+flwr+fx+st, yl-yr,
			flwl+flwr+fx+st, yl+fy+st,
			-st, fy+yl+st,
			-st, 0,
			0,0
		PRISM 9, sh,
			flwl, 0,
			flwl, yl,
			flwl+fx, yl,
			flwl+fx, yl-yr,
			flwl+fx-st, yl-yr,
			flwl+fx-st, yl-st,
			flwl+st, yl-st,
			flwl+st, 0,
			flwl, 0

		ADDz softh
		!!! face desk
		MATERIAL mat_riser
		if GLOB_CONTEXT = 4 then SECT_FILL typeFillRiser, penBackFillRiser, penFillRiser, penLineRiser

		PRISM 5, sh-lth-softh,
			0,0,
			flwl,0,
			flwl,lth,
			0,lth,
			0,0
		ADD flwl+fx,yl-yr,0
			PRISM 5, sh-lth-softh,
				0,0,
				flwr,0,
				flwr,lth,
				0,lth,
				0,0
		DEL 1
		DEL 1
	ENDIF

	IF soffit THEN
	IF strings=0 THEN ADDz sh-softh-lth
		MATERIAL matsoff
		if GLOB_CONTEXT = 4 then SECT_FILL typeFillSoffit, penBackFillSoffit, penFillSoffit, penLineSoffit

		PRISM 9, softh,
			0,0,
			0, yl+fy,
			flwl+fx+flwr, fy+yl,
			flwl+fx+flwr, yl-yr,
			flwl+fx, yl-yr,
			flwl+fx, yl,
			flwl, yl,
			flwl, 0,
			0,0
	IF strings=0 THEN DEL 1
	ENDIF
DEL 1
ENDIF

