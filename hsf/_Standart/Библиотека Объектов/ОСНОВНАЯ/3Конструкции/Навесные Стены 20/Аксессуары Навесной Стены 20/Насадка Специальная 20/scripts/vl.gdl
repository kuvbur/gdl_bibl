
call "FM_types" parameters all

values "gs_detlevel_3d" `Детальный`, `Простой`
values "gs_detlevel_3d_m" 2, 1
values "gs_resol" range [4,)

if GLOB_MODPAR_NAME = "gs_detlevel_3d" then
	if gs_detlevel_3d = `Простой` then gs_detlevel_3d_m = 1
	if gs_detlevel_3d = `Детальный` then gs_detlevel_3d_m = 2
	parameters gs_detlevel_3d_m = gs_detlevel_3d_m
else
	if gs_detlevel_3d_m = 1 then gs_detlevel_3d = `Простой`
	if gs_detlevel_3d_m = 2 then gs_detlevel_3d = `Детальный`
	parameters gs_detlevel_3d = gs_detlevel_3d
endif

if gs_detlevel_3d_m < 2 then
	hideparameter "gs_resol"
	lock "gs_resol"
endif

if GLOB_MODPAR_NAME = "radCap" then
	AC_height = sqr (AC_width * radCap - AC_width * AC_width/ 4)
	parameters AC_height = AC_height
else
	radCap = (AC_height * AC_height+ AC_width * AC_width/ 4) / AC_width
	parameters radCap = radCap
endif

values "AC_width" range (0, )
values "AC_height" range (0, )


if bSameWidth then
	AC_width = AC_frameWidthRight + AC_frameWidthLeft
	parameters AC_width = AC_width
	lock "AC_width"
endif
