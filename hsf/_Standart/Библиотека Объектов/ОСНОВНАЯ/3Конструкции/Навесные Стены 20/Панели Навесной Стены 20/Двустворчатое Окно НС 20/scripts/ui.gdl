									gs_handle_type_win_m		= gs_handle_type_win_m,
									gs_cust_handle_win			= gs_cust_handle_win,
									gs_handle_win_mat			= gs_handle_win_mat,
									gs_handle_win_pos			= gs_handle_win_pos,
									iHandleWinCategoryEnable	= 1,
									bWindow						= 1,
									bCurtainWall				= 1

