
!!!!========= Parameters from AC
!	PARAMETERS AC_ClampVector[1][1] = 1,			AC_ClampVector[1][2] = 0
!	PARAMETERS AC_ClampVector[2][1] = 0,		AC_ClampVector[2][2] = 0
!	PARAMETERS AC_ClampVector[3][1] = -1,			AC_ClampVector[3][2] = 0
!	PARAMETERS AC_ClampVector[4][1] = 0,		AC_ClampVector[4][2] = 0
!!!!========= Parameters from AC

call "FM_types" parameters all

values{2} "gs_detlevel_3D_m" 	DLEV3D_DETAILED, `Детальный`,
								DLEV3D_OFF, `Откл.`

!!!=========Cutplanes
values{2} "iCutDirTop" 	CUTMODE_ALONG_X, stCutModeColValues[1],
						CUTMODE_ALONG_Y, stCutModeColValues[2]
values{2} "iCutDirBottom" 	CUTMODE_ALONG_X, stCutModeColValues[1],
							CUTMODE_ALONG_Y, stCutModeColValues[2]
values "gs_cutdir_top" RANGE [-90,90]
values "gs_cutdir_bot" RANGE [-90,90]
values "gs_cutang1" RANGE [-45,45]
values "gs_cutang2" RANGE [-45,45]
values "gs_cutang3" RANGE [-45,45]
values "gs_cutang4" RANGE [-45,45]
values "st_frameType"  `Тип 3`
values "gs_frameType" 2

IF iCutDirTop = CUTMODE_ALONG_Y Then
	parameters gs_cutdir_top = 0.00
				gs_cutdir_top = 0.00
Else
	parameters gs_cutdir_top = 90.00
				gs_cutdir_top = 90.00
Endif

IF iCutDirBottom = CUTMODE_ALONG_Y Then
	parameters gs_cutdir_bot = 0.00
				gs_cutdir_bot = 0.00
Else
	parameters gs_cutdir_bot = 90.00
				gs_cutdir_bot = 90.00
Endif

IF NOT(GS_Cutting) Then
	hideparameter	"gs_cutang1",
				"gs_cutang2",
				"gs_cutang3",
				"gs_cutang4",
				"gs_cutdir_top",
				"gs_cutdir_bot",
				"iCutDirBottom",
				"iCutDirTop"
	lock	"gs_cutang1",
		"gs_cutang2",
		"gs_cutang3",
		"gs_cutang4",
		"gs_cutdir_top",
		"gs_cutdir_bot",
		"iCutDirBottom",
		"iCutDirTop"

Endif
!!!=========Cutplanes

!!!=========Parameter settings

IF NOT(gs_debug) Then hideparameter "gs_debug"
LOCK "A","B", "ZZYZX"

call "ui_cw_frame" parameters all


