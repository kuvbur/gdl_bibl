
MUL2 1,-1

PEN gs_cont_pen

unID=1
HOTSPOT2 a/2, 0, unID : unID=unID+1

! HOTSPOT EDITING gs_awbox_x

HOTSPOT2 0, 0, unID, gs_awbox_x, 1+128: unID=unID+1
HOTSPOT2 0, gs_awbox_x, unID, gs_awbox_x, 2: unID=unID+1
HOTSPOT2 0, 0, unID, gs_awbox_x, 3: unID=unID+1

! HOTSPOT EDITING gs_awbox_x

HOTSPOT2 a, 0, unID, gs_awbox_x, 1+128: unID=unID+1
HOTSPOT2 a, gs_awbox_x, unID, gs_awbox_x, 2: unID=unID+1
HOTSPOT2 a, 0, unID, gs_awbox_x, 3: unID=unID+1

! HOTSPOT EDITING overhang

HOTSPOT2 a-0.05, gs_awbox_x, unID, gs_awning_size, 1+128: unID=unID+1
HOTSPOT2 a-0.05, gs_awbox_x+gs_awning_size*cos(gs_alpha), unID, gs_awning_size, 2: unID=unID+1
HOTSPOT2 a-0.05, 0, unID, gs_awning_size, 3: unID=unID+1

HOTSPOT2 0.05, gs_awbox_x, unID, gs_awning_size, 1+128: unID=unID+1
HOTSPOT2 0.05, gs_awbox_x+gs_awning_size*cos(gs_alpha), unID, gs_awning_size, 2: unID=unID+1
HOTSPOT2 0.05, 0, unID, gs_awning_size, 3: unID=unID+1

! HOTSPOT EDITING

HOTSPOT2 0, 0, unID, A, 1+256: unID=unID+1
HOTSPOT2 A, 0, unID, A, 2: unID=unID+1
HOTSPOT2 0, 0, unID, A, 3: unID=unID+1



IF gs_2d_type_m = VIEW_SYMBOL then gs_line1_type=gs_LINE2_type

LINE_TYPE gs_line1_type

RECT2 0.1, gs_awbox_x-gs_rf_aw, a-0.1, gs_awbox_x+(gs_awning_size*cos(gs_alpha))

PEN gs_fill_pen
FILL gs_fill_type

POLY2_b 5, 2+64, gs_fill_pen, gs_back_pen,
		0.1, gs_awbox_x-gs_rf_aw, 0,
		a-0.1, gs_awbox_x-gs_rf_aw, 0,
		a-0.1, gs_awbox_x+(gs_awning_size*cos(gs_alpha)), 0,
		0.1, gs_awbox_x+(gs_awning_size*cos(gs_alpha)), 0,
		0.1, gs_awbox_x-gs_rf_aw, -1

PEN gs_cont_pen
RECT2 0, 0, bb, (gs_awbox_x+gs_rf_aw+gs_arm_size/2)

ADD2 a-bb, 0

RECT2 0, 0, bb, (gs_awbox_x+gs_rf_aw+gs_arm_size/2)

DEL 1

ADD2 0, gs_awbox_x

ADD2 bb, 0

RECT2 0, -gs_rf_aw/2, 0.1-bb, gs_rf_aw/2

DEL 1

ADD2 a-0.1, 0

RECT2 0, -gs_rf_aw/2, 0.1-bb, gs_rf_aw/2

DEL 1

LINE_TYPE gs_LINE2_type

RECT2 0.1, -gs_rf_aw/2, a-0.1, gs_rf_aw/2
LINE2 0.1, gs_rf_aw, a-0.1, gs_rf_aw

DEL 1

LINE_TYPE gs_line1_type

ADD2 0, gs_awbox_x+gs_awning_size*cos(gs_alpha)

LINE2 0.05, gs_ra_aw, a-0.05, gs_ra_aw
LINE2 0.05, gs_ra_aw, 0.05, -gs_ra_aw
LINE2 a-0.05, gs_ra_aw, a-0.05, -gs_ra_aw
LINE2 0.05, -gs_ra_aw, 0.1, -gs_ra_aw
LINE2 a-0.1, -gs_ra_aw, a-0.05, -gs_ra_aw

LINE_TYPE gs_LINE2_type

LINE2 0.1, -gs_ra_aw, a-0.1, -gs_ra_aw
