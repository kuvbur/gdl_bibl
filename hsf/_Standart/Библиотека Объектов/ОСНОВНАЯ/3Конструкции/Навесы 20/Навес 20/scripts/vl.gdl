
call "FM_types" parameters all


values "gs_detlevel_3D" `Детальный`, `Откл.`
values "gs_detlevel_3D_m" DETLEVEL_DETAILED, DETLEVEL_OFF

if GLOB_MODPAR_NAME = "gs_detlevel_3D" then
	gs_detlevel_3D_m = DETLEVEL_DETAILED
	if gs_detlevel_3D = `Откл.` then gs_detlevel_3D_m = DETLEVEL_OFF
	parameters gs_detlevel_3D_m = gs_detlevel_3D_m
else
	gs_detlevel_3D = `Детальный`
	if gs_detlevel_3D_m = DETLEVEL_OFF then  gs_detlevel_3D = `Откл.`
	parameters gs_detlevel_3D = gs_detlevel_3D
endif

values "gs_awning_size" range [gs_rf_aw+gs_ra_aw+2*gs_arm_size, (gs_awning_lenght-gs_awbox_x)]

values "gs_alpha" range [0, 90]

values "gs_awbox_x" range [gs_rf_aw,]
values "gs_awbox_y" range [gs_rf_aw+gs_arm_size/2,]

values "gs_arm_typ" `Круглая`, `Прямоугольный`
values "gs_arm_typ_m" ARM_CIRCLE, ARM_RECT

if GLOB_MODPAR_NAME = "gs_arm_typ" then
	gs_arm_typ_m = ARM_CIRCLE
	if gs_arm_typ = `Прямоугольный` then gs_arm_typ_m = ARM_RECT
	parameters gs_arm_typ_m = gs_arm_typ_m
else
	gs_arm_typ = `Круглая`
	if gs_arm_typ_m = ARM_RECT then gs_arm_typ = `Прямоугольный`
	parameters gs_arm_typ = gs_arm_typ
endif

values "gs_aw_typ" `Прямой`, `Волнистая`
values "gs_aw_typ_m" AW_STRAIGHT, AW_WAWY

if GLOB_MODPAR_NAME = "gs_aw_typ" then
	gs_aw_typ_m = AW_STRAIGHT
	if gs_aw_typ = `Волнистая` then gs_aw_typ_m = AW_WAWY
	parameters gs_aw_typ_m = gs_aw_typ_m
else
	gs_aw_typ = `Прямой`
	if gs_aw_typ_m = AW_WAWY then gs_aw_typ = `Волнистая`
	parameters gs_aw_typ = gs_aw_typ
endif

values "gs_2d_type" `Вид Сверху`, `Символ Плана`
values "gs_2d_type_m" VIEW_TOP, VIEW_SYMBOL

if GLOB_MODPAR_NAME = "gs_2d_type" then
	gs_2d_type_m = VIEW_TOP
	if gs_2d_type = `Символ Плана` then gs_2d_type_m = VIEW_SYMBOL
	parameters gs_2d_type_m = gs_2d_type_m
else
	gs_2d_type = `Вид Сверху`
	if gs_2d_type_m = VIEW_SYMBOL then gs_2d_type = `Символ Плана`
	parameters gs_2d_type = gs_2d_type
endif


lock "b"
parameters b=gs_awbox_x+(gs_awning_size*cos(gs_alpha))

lock "zzyzx"
parameters zzyzx=(gs_awning_size*sin(gs_alpha))

values "gs_resol" range [3, )

if gs_cont_pen=0 then parameters gs_cont_pen=2
if gs_fill_pen=0 then parameters gs_fill_pen=91
