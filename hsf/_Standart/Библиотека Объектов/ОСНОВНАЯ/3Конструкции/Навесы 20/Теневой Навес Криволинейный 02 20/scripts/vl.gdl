
call "FM_types" parameters all

LOCK "A"
LOCK "B"
LOCK "ZZYZX"
values{2} "gs_detlevel_3D_m" 	DLEV3D_DETAILED, `Детальный`,
								DLEV3D_SIMPLE, `Простой`,
								DLEV3D_OFF, `Откл.`
values{2} "iLouvreStyle" 	LOUVRESTYLE_1, `Стиль 1`,
							LOUVRESTYLE_2, `Стиль 2`,
							LOUVRESTYLE_3, `Стиль 3`
values{2} "iSolarFrameType" 	FRAMETYPE_STRAIGHT, `Прямой`,
								FRAMETYPE_CURVED, `Изогнутый`
VALUES "tang" RANGE [-90,90]
VALUES "alfa" RANGE [1,180]
VALUES "rad" RANGE [.001,]
VALUES "srad" RANGE [.001,rad]
VALUES "arms" RANGE [1,]
VALUES "fh" RANGE [.001,]
VALUES "ldepth" RANGE [.001,]
VALUES "fw" RANGE [.001,]
VALUES "lt" RANGE [.001,]

maxlax=(srad-(2*fw))/ldepth
PARAMETERS maxl=maxlax : LOCK "maxl"

IF numl>1 THEN
 PARAMETERS lax=(srad-(2*fw))/numl: LOCK "lax"
ELSE
 PARAMETERS lax=((srad-(2*fw))/(numl+1))+ldepth/2: LOCK "lax"
ENDIF

IF gs_detlevel_3D_m = DLEV3D_SIMPLE THEN
 lock	"iLouvreStyle"
 PARAMETERS iSolarFrameType = FRAMETYPE_STRAIGHT
 lock	"iSolarFrameType"
ENDIF

if gs_cont_pen=0 then parameters gs_cont_pen=2
if gs_fill_pen=0 then parameters gs_fill_pen=91
