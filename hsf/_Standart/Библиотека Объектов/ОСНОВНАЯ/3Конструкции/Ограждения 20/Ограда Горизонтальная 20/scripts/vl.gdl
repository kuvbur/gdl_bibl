
! ==============================================================================
! Fence Horizontal
! ==============================================================================

hideparameter all "A", "B", "ZZYZX"

ac_bottomlevel	= 0
ac_toplevel		= ZZYZX + ph
parameters	ac_bottomlevel	= ac_bottomlevel,
			ac_toplevel		= ac_toplevel

call "FM_types" parameters all


dim stDetail[3]
	stDetail[1] = `Отключено`
	stDetail[2] = `Простое`
	stDetail[3] = `Детальное`

values "gs_detlevel_3D" stDetail[3], stDetail[2], stDetail[1]
values "gs_detlevel_3D_m" 2, 1, 0

if GLOB_MODPAR_NAME = "gs_detlevel_3D" then
	for hi = 1 to vardim1(stDetail)
		if gs_detlevel_3D = stDetail[hi] then
			gs_detlevel_3D_m = hi - 1
		endif
	next hi
	parameters gs_detlevel_3D_m = gs_detlevel_3D_m
else
	gs_detlevel_3D = stDetail[max(1, min(gs_detlevel_3D_m + 1, vardim1(stDetail)))]
	parameters gs_detlevel_3D = gs_detlevel_3D
endif

dim stBoardType[2]
	stBoardType[1] = `Между Стойками`
	stBoardType[2] = `Перед Стойками`

values "bp" stBoardType
values "bp_m" 1, 2

if GLOB_MODPAR_NAME = "bp" then
	bp_m = 1
	if bp = stBoardType[2] then bp_m = 2
	parameters bp_m = bp_m
else
	bp = stBoardType[1]
	if bp_m = 2 then bp = stBoardType[2]
	parameters bp = bp
endif

if gs_cont_pen= 0 then parameters gs_cont_pen=2
IF gs_fill_pen=0 then parameters gs_fill_pen=91

parameters b=pd
lock "b"

values "bh" RANGE (0,)
values "bd" RANGE (0,)
values "gb" RANGE [0,)
values "np" RANGE [0,)
values "pw" RANGE (0,)
values "pd" RANGE (0,)


! ======================================================================
! UI
! ======================================================================

call "ui_tabcontrol" parameters		controlType			= 1,
									nTabs				= 10,
									gs_ui_current_page	= gs_ui_current_page
