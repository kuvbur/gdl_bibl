
targetGUID = TO_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

gosub "paramNameChange_BWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid targetGuid

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! paramNameChange
! ==============================================================================
"paramNameChange_BWM":
	if targetGUID # "" then
			newparameter "p", "Boolean"
			newparameter "wab", "Boolean"

			if not(bSupport) then
				p = 0
				wab = 0
			endif

			if bSupport & iSupportType = SUPPORT_POST then
				p = 1
				wab = 0
			endif

			if bSupport & iSupportType = SUPPORT_WALLBRACKET then
				p = 0
				wab = 1
			endif
			
			parameters p = p, wab = wab
	endif
return

