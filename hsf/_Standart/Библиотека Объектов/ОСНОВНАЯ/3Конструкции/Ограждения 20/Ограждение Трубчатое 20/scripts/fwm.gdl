
actualGUID = FROM_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

	_autoID		= "5CA2A287-1ECF-4FAD-AD0B-35D2D4DE6981"
	_startID	= "47F8738F-797A-48D8-9BE2-D7E4F5E943E2"
	_endID		= "B130735D-C74D-4599-BC2E-4930E56A299B"
gosub "paramNameChange_FWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid actualGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! paramNameChange
! ==============================================================================
"paramNameChange_FWM":
	if actualGuid = _startID | actualGuid = _autoID then
		_bHasParam = DELETED_PAR_VALUE("p", _tempPost)
		_bHasParam2 = STORED_PAR_VALUE("wab", _tempWab)

		if _bHasParam then
			if _tempPost then
				bSupport = 1
				iSupportType = SUPPORT_POST
			endif

			if not(_tempPost) & not(_tempWab) then
				bSupport = 0
			endif

			if not(_tempPost) & _tempWab then
				bSupport = 1
				iSupportType = SUPPORT_WALLBRACKET
			endif

			parameters bSupport = bSupport, iSupportType = iSupportType
		endif

		if _bHasParam2 then
			if _tempWab then
				bSupport = 1
				iSupportType = SUPPORT_WALLBRACKET
			endif

			if not(_tempPost) & not(_tempWab) then
				bSupport = 0
			endif

			if not(_tempWab) & _tempPost then
				bSupport = 1
				iSupportType = SUPPORT_POST
			endif

			parameters bSupport = bSupport, iSupportType = iSupportType
		endif
		
		actualGuid = _endID
	endif
return

