
!! Cube.gsm

if drawrubik then
	add 1,1,1
	call "rubikscube" parameters all
	del 1
else
	if gs_detlevel_3D_m = DLEV3D_OFF then End

	if gs_shadow then
		shadow on
	else
		shadow off
	endif

	rotx angle_1
	roty angle_2

	CALL "Resize_A_B_ZZYZX" parameters all

	pen gs_cont_pen
	material gs_prim_mat

	brick a,b,c
endif

end

