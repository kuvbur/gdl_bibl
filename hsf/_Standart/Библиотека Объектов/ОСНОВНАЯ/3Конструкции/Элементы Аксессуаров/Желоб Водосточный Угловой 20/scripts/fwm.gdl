! ==============================================================================
! toplevel_update
! ==============================================================================
"toplevel_update_FWM":
	if actualGuid = _startID | actualGuid = _autoID then

		_ac_toplevel = 0
		bHas = STORED_PAR_VALUE("ac_toplevel", _ac_toplevel)

		_zzyzx = 0
		bHas = STORED_PAR_VALUE("zzyzx", _zzyzx)

		ac_bottomlevel = _ac_toplevel - _zzyzx
		parameters	ac_bottomlevel = ac_bottomlevel
		
		actualGuid = _endID
	endif
return

