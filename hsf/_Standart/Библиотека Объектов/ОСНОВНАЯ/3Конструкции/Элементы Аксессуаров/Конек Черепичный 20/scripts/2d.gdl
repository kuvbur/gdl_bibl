
!!Ridge Tile 70.gsm

pen gs_cont_pen
fill gs_fill_type
hotspot2 0, 0

if abs(inclAngle-90) < EPS then
	poly2_b 10, 1+2+4+64, gs_fill_pen, gs_back_pen,
			0, innerRadius, 1,
			0, innerRadius + 0.04, 1,
			-1, 0, 800,
			0, -innerRadius  -0.04, 1001,
			0, -innerRadius, 1,
			-1, 0, 800,
			- innerRadius, 0, 1001,
			0, 1, 800,
			0, innerRadius, 1001,
			0, innerRadius, -1
	arc2 0, 0, innerRadius + 0.02, 90, 270
	hotspot2 -innerRadius -0.04, 0
else
poly2_b 4, 1+2+4+64, gs_fill_pen, gs_back_pen,
			0, -innerRadius - 0.04, 1,
			A, -innerRadius - 0.04, 1,
			A,  innerRadius + 0.04, 1,
			0,  innerRadius + 0.04, 1

	rect2 0, -innerRadius - 0.04, A, innerRadius + 0.04
	hotspot2 A, 0
endif
