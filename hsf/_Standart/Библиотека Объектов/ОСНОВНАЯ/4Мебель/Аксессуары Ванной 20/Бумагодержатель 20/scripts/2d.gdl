
pen gs_cont_pen
fill gs_fill_type

! -----------------------------------------------------------------------------
! 2D Hotspots
! -----------------------------------------------------------------------------

unID=1

hotspot2    A/2,    0, unID : unID = unID + 1

if iDispenserstyle = 1 then
	hotspot2    A/2,    -B/2, unID : unID = unID + 1

	hotspot2  0, 0, unID, A, 1+256 :	unID=unID+1
	hotspot2  A, 0, unID, A, 2 : unID=unID+1
	hotspot2  -A, 0, unID, A, 3 : unID=unID+1

	hotspot2  0, -B, unID, A, 1+256 :	unID=unID+1
	hotspot2  A, -B, unID, A, 2 : unID=unID+1
	hotspot2  -A, -B, unID, A, 3 : unID=unID+1


	hotspot2  0, 0, unID, B, 1+256 :	unID=unID+1
	hotspot2  0, -B, unID, B, 2 : unID=unID+1
	hotspot2  0, B, unID, B, 3 : unID=unID+1

	hotspot2  A, 0, unID, B, 1+256 :	unID=unID+1
	hotspot2  A, -B, unID, B, 2 : unID=unID+1
	hotspot2  A, B, unID, B, 3 : unID=unID+1
endif

if iDispenserstyle = 2 then
	if iSymbolType = SYMBTYPE_DETAILED then
		hotspot2  0 - FrameWidth, 0, unID :	unID=unID+1
		hotspot2  A + FrameWidth, 0, unID :	unID=unID+1
	endif

	hotspot2  0 + BoxThk + 0.01,  -RollRadius - RollRadius*0.25 - 0.01 - 0.004, unID :	unID=unID+1
	hotspot2  A - BoxThk - 0.01 , -RollRadius - RollRadius*0.25 - 0.01 - 0.004, unID :	unID=unID+1

	hotspot2  0, 0, unID, A, 1+256 :	unID=unID+1
	hotspot2  -1, 0, unID, A, 3+128 : unID=unID+1
	hotspot2  A, 0, unID, A, 2 : unID=unID+1

	hotspot2  0, 0, unID, B, 1+128 :	unID=unID+1
	hotspot2  0, -1, unID, B, 3+128 : unID=unID+1
	hotspot2  0, B, unID, B, 2 : unID=unID+1

	hotspot2  A, 0, unID, B, 1+128 :	unID=unID+1
	hotspot2  A, -1, unID, B, 3+128 : unID=unID+1
	hotspot2  A, B, unID, B, 2 : unID=unID+1
endif

if iDispenserstyle = 3 then
	if iSymbolType = SYMBTYPE_DETAILED then
		hotspot2  0 - FrameWidth, 0, unID :	unID=unID+1
		hotspot2  A + FrameWidth, 0, unID :	unID=unID+1
	endif

	hotspot2  0 + BoxThk + 0.01, -RollRadius + 0.01, unID :	unID=unID+1
	hotspot2  A - BoxThk - 0.01, -RollRadius + 0.01, unID :	unID=unID+1

	hotspot2  A/2 , 0, unID :	unID=unID+1

	hotspot2   0, 0, unID, A, 1+256 :	unID=unID+1
	hotspot2  -1, 0, unID, A, 3+128 : unID=unID+1
	hotspot2   A, 0, unID, A, 2 : unID=unID+1

	hotspot2  0,  0, unID, B, 1+128 :	unID=unID+1
	hotspot2  0, -1, unID, B, 3+128 : unID=unID+1
	hotspot2  0,  B, unID, B, 2 : unID=unID+1

	hotspot2  A,  0, unID, B, 1+128 :	unID=unID+1
	hotspot2  A, -1, unID, B, 3+128 : unID=unID+1
	hotspot2  A,  B, unID, B, 2 : unID=unID+1
endif

if iDispenserstyle = 4 then
	hotspot2   0,    0, unID : unID = unID + 1
	hotspot2   A,    0, unID : unID = unID + 1
	hotspot2   A,    -B, unID : unID = unID + 1
	hotspot2   0,    -B, unID : unID = unID + 1
endif

! -----------------------------------------------------------------------------
! Styles
! -----------------------------------------------------------------------------
add2 A/2, 0
gosub iDispenserstyle
del 1
end

! =============================================================================
! SUBRUTINES
!
! =============================================================================

! -----------------------------------------------------------------------------
! Wall Mounted  Toilet Paper dispenser
! -----------------------------------------------------------------------------

1:
	add2 -a/2, 0
	line2    0,   -0.002,      a,   -0.002
	line2    0,    -0.01,      a,    -0.01

	poly2_b     13, 1+2+4+64, gs_fill_pen, gs_back_pen,
					 0, -b,      1,
					 a, -b,      1,
					 a,    -0.01,      1,
				   a-0.005,    -0.01,      1,
				   a-0.005,       -0.002,      1,
					 a,       -0.002,      1,
					 a,            0,      1,
					   0,            0,      1,
					   0,       -0.002,      1,
				   0.005,       -0.002,      1,
				   0.005,    -0.01,      1,
					   0,    -0.01,      1,
					   0, -b,     -1
	del 1
return

! -----------------------------------------------------------------------------
! Built-in 1  Toilet Paper dispenser
! -----------------------------------------------------------------------------

2:
	add2 0, b
	line_type CabinetLineType
	add2 0,0
	poly2_b 5, 1+2+64, gs_fill_pen, gs_back_pen,
			-A*0.5,		0,		3,
			A*0.5,		0,		3,
			A*0.5,	-B,		0,
			-A*0.5,	-B,		3,
			-A*0.5,		0,		3

	if iSymbolType = SYMBTYPE_SIMPLE then line2 -A*0.5,	-B, A*0.5,	-B
	del 1

	add2 A*0.5,		-B

	if iSymbolType = SYMBTYPE_DETAILED then
		line_type  1
		poly2_b 5, 1+2+64, gs_fill_pen, gs_back_pen,
				-A-FrameWidth,		0,			3,
				FrameWidth,		0,			3,
				FrameWidth,		-FrameThk,		3,
				-A-FrameWidth,		-FrameThk,		3,
			-A-FrameWidth,		0,			3
	endif
	del 2

	! --- Toilet Tissue ---
	if iSymbolType = SYMBTYPE_DETAILED then
		add2 -a/2 + BoxThk + 0.01, -FrameThk
		poly2_b 5, 1+2+64, gs_fill_pen, gs_back_pen,
				A-(BoxThk)*2-0.02,		0,			1,
				0,		0,			1,
				0,		-RollRadius-RollRadius*0.25-0.01+FrameThk-0.004,		1,
				A-(BoxThk)*2-0.02,		-RollRadius-RollRadius*0.25-0.01+FrameThk-0.004,		1,
				A-(BoxThk)*2-0.02,		0,			3
		del 1
	else
		line_type  1
		add2 -a/2 + BoxThk + 0.01, 0
		poly2_b 5, 1+2+64, gs_fill_pen, gs_back_pen,
				A-(BoxThk)*2-0.02,		0,			1,
				0,		0,			1,
				0,		-RollRadius-RollRadius*0.25-0.01-0.004,		1,
				A-(BoxThk)*2-0.02,		-RollRadius-RollRadius*0.25-0.01-0.004,		1,
				A-(BoxThk)*2-0.02,		0,			3
		del 1
	endif

return

! -----------------------------------------------------------------------------
! Built-in 2  Toilet Paper dispenser
! -----------------------------------------------------------------------------

3:

	! --- Cabinet ---
	add2 0, B
	line_type CabinetLineType
	add2 0,0
	poly2_b 5, 1+2+64, gs_fill_pen, gs_back_pen,
			-A*0.5,		0,		3,
			A*0.5,		0,		3,
			A*0.5,	-B,		0,
			-A*0.5,	-B,		3,
			-A*0.5,		0,		3

	if iSymbolType = SYMBTYPE_SIMPLE then line2 -A*0.5,	-B, A*0.5,	-B
	del 1

	add2 A*0.5,		-B
	if iSymbolType = SYMBTYPE_DETAILED then
		line_type  1
		poly2_b 5, 1+2+64, gs_fill_pen, gs_back_pen,
				-A-FrameWidth,		0,			3,
				FrameWidth,		0,			3,
				FrameWidth,		-FrameThk,		3,
				-A-FrameWidth,		-FrameThk,		3,
				-A-FrameWidth,		0,			3
	endif
	del 2

	! --- Toilet Tissue ---
	if iSymbolType = SYMBTYPE_DETAILED then
		add2 -a/2 + BoxThk + 0.01, -FrameThk
		poly2_b 5, 1+2+64, gs_fill_pen, gs_back_pen,
				A-(BoxThk)*2-0.02,		0,			1,
				0,		0,			1,
				0,		-RollRadius+FrameThk+0.01,		1,
				A-(BoxThk)*2-0.02,		-RollRadius+FrameThk+0.01,		1,
				A-(BoxThk)*2-0.02,		0,			3
		del 1
	else
		line_type  1
		add2 -a/2 + BoxThk + 0.01, 0
		poly2_b 5, 1+2+64, gs_fill_pen, gs_back_pen,
				A-(BoxThk)*2-0.02,		0,			1,
				0,		0,			1,
				0,		-RollRadius+0.01,		1,
				A-(BoxThk)*2-0.02,		-RollRadius+0.01,		1,
				A-(BoxThk)*2-0.02,		0,			3
		del 1
	endif

return

! -----------------------------------------------------------------------------
! Commercial  Toilet Paper dispenser
! -----------------------------------------------------------------------------

4:
	circle2 0, -b/2, lockDia/2
	poly2_b       4, 1+2+4+64,      gs_fill_pen,     gs_back_pen,
			-a/2,  0, 1,
			 a/2,  0, 1,
			 a/2, -b, 1,
			-a/2, -b, 1
return

