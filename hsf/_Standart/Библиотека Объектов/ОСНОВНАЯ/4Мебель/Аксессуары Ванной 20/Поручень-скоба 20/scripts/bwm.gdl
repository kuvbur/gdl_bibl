
targetGUID = TO_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

_targetGUID_USA = "9C3A8C42-8798-4D42-8EAC-42BF2DE1E4ED"
_targetGUID_INT = "E4A52EBF-3373-4D55-A8E1-D639702DED66"

gosub "USA_Merge_BWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid targetGuid

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! Merge the USA Grab Bar object into the INT library
! ==============================================================================
"USA_Merge_BWM":
	if targetGUID # "" then
		if LibraryLangCode = "USA" then
			targetGuid = _targetGUID_USA
		else
			targetGuid = _targetGUID_INT
		endif
					
		if bAutoPlateResize = 0 then
			targetGUID = ""
		else
			if LibraryLangCode = "USA" then			
				if (abs(plateThk - 0.005) < EPS ) | (abs(plateThk - 0.016) < EPS) then
					newparameter "iBarType", "Integer"
					newparameter "an", "Angle"
					newparameter "halfA", "Length"
					
					an 		= inc_angle
					halfA 	= half_a
					if abs(plateThk - 0.005) < EPS then
						iBarType 	= 1
					else
						iBarType 	= 2
					endif
					
					parameters 	iBarType 	= iBarType,
								an 			= an,
								halfA		= halfA
				else
					targetGUID = ""
				endif
			else
				if abs(plateThk - 0.005) > EPS then
					targetGUID = ""
				endif
			endif
		endif
	endif
return

