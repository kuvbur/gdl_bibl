
call "FM_types" parameters all

!Hide all parameters from UI
hideparameter all  "A", "B", "ZZYZX"

IF gs_cont_pen<=0 THEN PARAMETERS gs_cont_pen=8
values{2} "gs_detlevel_3D_m" 	DLEV3D_DETAILED, `Детальный`,
								DLEV3D_SIMPLE, `Простой`,
								DLEV3D_OFF, `Откл.`
VALUES "gs_resol" RANGE [3,)
VALUES "br" RANGE (2*vcs,)
VALUES "tr" RANGE (2*vcs,)

A = 2*max(tr,br)
B = A
PARAMETERS A=A
PARAMETERS B=B
LOCK "A"
LOCK "B"

IF gs_detlevel_3D_m = DLEV3D_SIMPLE THEN HIDEPARAMETER "gs_resol"
IF gs_detlevel_3D_m = DLEV3D_OFF THEN HIDEPARAMETER "gs_shadow", "gs_resol"
