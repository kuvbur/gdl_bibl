
call "FM_types" parameters all

!Hide all parameters from UI
hideparameter all  "A", "B", "ZZYZX"

values{2} "gs_detlevel_3D_m" 	DLEV3D_DETAILED, `Детальный`,
								DLEV3D_OFF, `Откл.`

if gs_cont_pen=0 then parameters gs_cont_pen=7
if gs_fill_pen=0 then parameters gs_fill_pen=91

if gs_detlevel_3D_m = DLEV3D_OFF then hideparameter "gs_shadow"

if bPict then
	lock "carpet_mat"
else
	hideparameter	"gs_picture_custom"
endif

if not(bPict) or (bPict & not(gs_picture_custom)) then
	hideparameter	"gs_picture_name",
					"bUseAlphaChannel"
	lock	"gs_picture_name",
			"bUseAlphaChannel"
endif
