"3DRepresentationpage":
! ------------------------------------------------------------------------------
	dim surfacesArray[]
	SurfItemID = 1

	surfacesArray[SurfItemID]	= "frmat"
	SurfItemID = SurfItemID + 1

	if iBedType = 2 then 
		surfacesArray[SurfItemID]	= "gs_leg_mat"
		SurfItemID = SurfItemID + 1
	endif

	if bHeadboard then
		surfacesArray[SurfItemID]	= "hbmat"
		SurfItemID = SurfItemID + 1
	endif

	surfacesArray[SurfItemID]	= "mmat"
	SurfItemID = SurfItemID + 1

	if pw then
		surfacesArray[SurfItemID]	= "pillow_mat"
		SurfItemID = SurfItemID + 1
	endif

	surfacesArray[SurfItemID]	= "gs_cabinet_mat"
	SurfItemID = SurfItemID + 1

	if bCornice then
		surfacesArray[SurfItemID]	= "gs_cornice_mat"
		SurfItemID = SurfItemID + 1
	endif

	if bBase then
		surfacesArray[SurfItemID]	= "matLeg"
		SurfItemID = SurfItemID + 1
	endif

	surfacesArray[SurfItemID]	= "doorframe_mat"
	SurfItemID = SurfItemID + 1

	if gs_door_type_m = 3 then
		surfacesArray[SurfItemID]	= "doorglas_mat"
		SurfItemID = SurfItemID + 1
	endif

	if bKnob or gs_door_type_m <> 8 then
		surfacesArray[SurfItemID]	= "gs_knob_mat"
		SurfItemID = SurfItemID + 1
	endif

	if iLampOnSides <> 1 then	! None
		surfacesArray[SurfItemID]	= "matFrame"
		SurfItemID = SurfItemID + 1
		surfacesArray[SurfItemID]	= "matShadeOut"
		SurfItemID = SurfItemID + 1
	endif

	dim childFlagArray[]
	for k = 1 to vardim1(surfacesArray)
		childFlagArray[k]	= 0
	next k
	
	call "ui_object_3DRepresentation" parameters all	surfacesArray		= surfacesArray,
														childFlagArray		= childFlagArray,
														iSurfaceListID		= TABID_3DRepresentation

return

