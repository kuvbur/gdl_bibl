"CorniceBasepage":
! ------------------------------------------------------------------------------
	pageStartY = 0
	_infHeight 	= 19
	_outfHeight = 15
	_yOffset 	= 4	
	x1 = 76
	x2 = 0
	x3 = 123
	dy = 24
	_yCurr = pageStartY

	if not(bCornice) then _outfFlag = 4

