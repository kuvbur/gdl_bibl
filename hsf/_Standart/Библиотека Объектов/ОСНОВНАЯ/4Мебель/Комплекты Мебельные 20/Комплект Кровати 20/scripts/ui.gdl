! ------------------------------------------------------------------------------
"StyleAndDimensionpage":
! ------------------------------------------------------------------------------
	pageStartY = 0
	_infHeight = 19
	_outfHeight = 15
	_yOffset = 4
	widthInf2	= 37
	yPict = 93
	PictHeight = 130

	x1 = 0
	x2 = 100
	x3 = 230
	x4 = 331
	x5 = 178
	dy = 24

	_yCurr = pageStartY
	ui_infield{3} "iTypeOfBed", x2, _yCurr, 112, _infHeight,
			8, "",
			0, 0,
			0, 0, 0, 0,
			"", `Односпальная`, 1,
			"", `Двуспальная`, 2 ui_tooltip `Тип Кровати`

	ui_infield{3} "iNightTableType", x4, _yCurr, 112, _infHeight,
			8, "",
			0, 0,
			0, 0, 0, 0,
			"", `Несколько Ящиков`, 1,
			"", `Открытая`, 2 ui_tooltip `Тип Тумбочки`

	_yCurr = _yCurr + _yOffset
	ui_outfield `Тип Кровати`, x1, _yCurr, x2 - 2, _outfHeight
	ui_outfield `Тип Тумбочки`, x3, _yCurr, x2 - 2, _outfHeight

	_yCurr = _yCurr + dy - _yOffset
	ui_infield{3} "iNightTableOnSides", x2, _yCurr, 112, _infHeight,
			8, "",
			0, 0,
			0, 0, 0, 0,
			"", `С одной стороны`, 1,
			"", `С обеих сторон`, 2 ui_tooltip `Тумбочки`

	ui_infield{3} "iLampOnSides", x4, _yCurr, 112, _infHeight,
			8, "",
			0, 0,
			0, 0, 0, 0,
			"", `Нет`, 			1,
			"", `Лампа на Тумбочке 1`, 	2,
			"", `Лампа на Тумбочке 2`, 	3,
			"", `Две Лампы`, 		4 ui_tooltip `Светильники`


	_yCurr = _yCurr + _yOffset
	ui_outfield `Тумбочки`, x1, _yCurr, x2 - 2, _outfHeight
	ui_outfield `Светильники`, x3, _yCurr, x2 - 2, _outfHeight

	_yCurr = _yCurr + dy - _yOffset + 3
	ui_separator 0, _yCurr, 444, _yCurr
	ui_separator 222, 0, 222, 48

	if iTypeOfBed = 1 then	! Single
		xPict = 152
		PictWidth = 139 
		ui_pict 1, xPict, yPict, PictWidth, PictHeight, 1
	else
		xPict = 131
		PictWidth = 182 
		ui_pict 2, xPict, yPict, PictWidth, PictHeight, 1
	endif

	_yCurr = yPict - _infHeight - 3
	ui_infield "tableWidth", xPict - 39, _yCurr, 90, _infHeight ui_tooltip `Ширина Тумбочки`
	ui_infield "bedWidth", xPict + 56, _yCurr, 90, _infHeight ui_tooltip `Ширина Кровати`

	if iTypeOfBed = 1 then
		_yCurr = yPict - 6
		ui_infield "distBedTable", xPict + PictWidth + 3, _yCurr, 90, _infHeight ui_tooltip `Расстояние между Кроватью и Тумбочкой`
	else
		ui_infield "distBedTable", xPict + 151, _yCurr, 90, _infHeight ui_tooltip `Расстояние между Кроватью и Тумбочкой`
	endif


	_yCurr = yPict + 12
	ui_infield "tableDepth", xPict - 93, _yCurr, 90, _infHeight ui_tooltip `Глубина Тумбочки`

	_yCurr = yPict + 46
	ui_infield "bedLength", xPict + PictWidth + 3, _yCurr, 90, _infHeight ui_tooltip `Длина Кровати`

	_yCurr = yPict + 67
	ui_infield "MSBedside", xPict - 93, _yCurr, 90, _infHeight ui_tooltip `Длина Прикроватного Пространства`

	_yCurr = yPict + 103
	ui_infield "MSFrontBed", xPict - 93, _yCurr, 90, _infHeight ui_tooltip `Расстояние Перед Кроватью`

	_yCurr = yPict + PictHeight + 3
	ui_infield "MSSide", xPict - 6, _yCurr, 72, _infHeight ui_tooltip `Ширина Прикроватного Пространства 1`

	_yCurr = yPict + PictHeight + 3
	ui_infield "MSSide2", xPict + 112, _yCurr, 72, _infHeight ui_tooltip `Ширина Прикроватного Пространства 2`

	_outfFlag = 0

return


