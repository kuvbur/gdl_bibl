! ------------------------------------------------------------------------------
"Lamppage":
! ------------------------------------------------------------------------------
	pageStartY = 0
	_infHeight 	= 19
	_outfHeight = 15
	_yOffset 	= 4	

	x1 = 0
	x2 = 169
	x3 = 123
	dy = 24
	_yCurr = pageStartY + _yOffset

	ui_style 0, 1
	ui_outfield `Размеры Светильника`, x1, _yCurr, 444, _outfHeight
	ui_style 0, 0

	if iLampOnSides = LAMP_ON_SIDES_NONE then _outfFlag = 4

	_yCurr = _yCurr + dy - _yOffset	
	ui_infield{3} "lampDiameter", x2, _yCurr, 97, _infHeight ui_tooltip `Нижний Диаметр`

	_yCurr = _yCurr + _yOffset
	ui_outfield `Нижний Диаметр`, x1, _yCurr, 120, _outfHeight, _outfFlag

	_yCurr = _yCurr + dy - _yOffset
	ui_infield{3} "lud", x2, _yCurr, 97, _infHeight ui_tooltip `Верхний Диаметр`

	_yCurr = _yCurr + _yOffset
	ui_outfield `Верхний Диаметр`, x1, _yCurr, 120, _outfHeight, _outfFlag

	_yCurr = _yCurr + dy - _yOffset
	ui_infield{3} "lsh", x2, _yCurr, 97, _infHeight ui_tooltip `Высота Светильника`

	_yCurr = _yCurr + _yOffset
	ui_outfield `Высота Плафона`, x1, _yCurr, 120, _outfHeight, _outfFlag

	_yCurr = _yCurr + dy - _yOffset
	ui_infield{3} "lampHeight", x2, _yCurr, 97, _infHeight ui_tooltip `Общая Высота`

	_yCurr = _yCurr + _yOffset
	ui_outfield `Общая Высота`, x1, _yCurr, 120, _outfHeight, _outfFlag

	_yCurr = _yCurr + dy - _yOffset
	ui_separator x1, _yCurr, 444, _yCurr

 	_yCurr = _yCurr + 5

	ui_infield{3} "tempIntC", x1, _yCurr,27 * 2,	27,
		4, 9,
		2, 1,
		25,	25,	18, 22,
		1, "", 1,
		2, "", 0

 	_yCurr = _yCurr + 27 + 3

	if not(gs_light_switch) then _outfFlag = 4

	ui_outfield `Цвет`, x1, _yCurr, 120, _outfHeight, _outfFlag

 	_yCurr = _yCurr - _yOffset
	ui_colorpicker	"gs_color_red", "gs_color_green", "gs_color_blue", x2, _yCurr, 42, 19

	_yCurr = _yCurr + dy

	ui_outfield `Основная Яркость`, x1, _yCurr, 160, _outfHeight, _outfFlag

 	_yCurr = _yCurr + dy - _yOffset

	ui_slider "gs_light_intensity", x1, _yCurr, x2 - 2,	19, 10, 1

	ui_infield{3} "gs_light_intensity", x2, _yCurr, 60, _infHeight ui_tooltip `Интенсивность`
	_yCurr = _yCurr + _yOffset
	ui_outfield `%`, x2 + 60 + 3, _yCurr, 120, _outfHeight, _outfFlag

	_yCurr = _yCurr + dy
	ui_outfield `Дистанция Света`, x1, _yCurr, x2 - 2, _outfHeight, _outfFlag
	_yCurr = _yCurr - _yOffset
	ui_infield{3} "c4dDetRadius", x2, _yCurr, 97, _infHeight ui_tooltip `Интенсивность`

	_yCurr = _yCurr + dy

	ui_infield{4} "bGenShadow", x1, _yCurr, 210, _infHeight,
					7, "",
					2, 0,
					0, 0, 0, 0,
					"", `Отбрасывание Теней`, 0,
					"", `Отбрасывание Теней`, 1

	_outfFlag = 0

return

