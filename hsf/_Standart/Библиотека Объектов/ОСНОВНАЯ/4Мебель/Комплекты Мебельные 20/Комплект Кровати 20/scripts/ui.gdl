"Drawerpage":
! ------------------------------------------------------------------------------
	pageStartY = 0
	_infHeight = 19
	_outfHeight = 15
	_yOffset = 4
	x1 = 0
	x2 = 76
	x3 = 230
	x4 = 123 + 230
	dy = 24
	pictWidth = 18
	pictHeight = 18
	_yCurr = pageStartY + _yOffset
	_outfFlag 		= 0

	ui_style 0, 1
	ui_outfield `Стиль Ящика`, x1, _yCurr, 210, _outfHeight, _outfFlag
	ui_style 0, 0
	
	if dnum = 0 or iNightTableDrawerPos = DRAWERPOS_NONE then _outfFlag = 4

	_yCurr = _yCurr + _infHeight + 2
	ui_infield{4} "gs_door_type_m", x2, _yCurr, 135, 96,
		2, 3, 4, 1,
		120, 90, 120, 90,
		1, "",	1,
		2, "",	2,
		3, "", 	3,
		4, "",	8 ui_tooltip `Стиль Ящика`
	
	if iNightTableType = 1 then	! Multi Drawer
		ui_infield{3} "dnum", x4 + 30, _yCurr, 60, _infHeight,
			8, "", 3, 1,
			0, 0, 0, 0,
			1, `2`, 	2,
			2, `1`, 	1,
			3, `Нет`, 	0 ui_tooltip `Количество Ящиков`

		_yCurr = _yCurr + _yOffset
		ui_outfield `Количество Ящиков`, x3, _yCurr, 120, _outfHeight, _outfFlag
	else
		ui_infield{3} "iNightTableDrawerPos", x4, _yCurr, 90, _infHeight,
			8, "",
			0, 0,
			0, 0, 0, 0,
			"", `Вверху`, DRAWERPOS_UP,
			"", `Внизу`, DRAWERPOS_DOWN,
			"", `Нет`, DRAWERPOS_NONE ui_tooltip `Размещение Ящика`

		_outfFlag = 0

		_yCurr = _yCurr + _yOffset
		ui_outfield `Размещение Ящика`, x3, _yCurr, 120, _outfHeight, _outfFlag

		if dnum = 0 or iNightTableDrawerPos = DRAWERPOS_NONE then _outfFlag = 4

		_yCurr = _yCurr + dy - _yOffset
		ui_infield{3} "dwh", x4, _yCurr, 90, _infHeight ui_tooltip `Верхняя Секция`

		_yCurr = _yCurr + _yOffset
		ui_outfield `Верхняя Секция`, x3, _yCurr, 120, _outfHeight, _outfFlag
	endif

	_yCurr = _yCurr + dy - _yOffset
	ui_infield{3} "fr_th1", x4, _yCurr, 90, _infHeight ui_tooltip `Толщина`

	_yCurr = _yCurr + _yOffset
	ui_outfield `Толщина`, x3, _yCurr, 120, _outfHeight, _outfFlag

	_yCurr = _yCurr + dy - _yOffset
	ui_infield{3} "doorframe_w", x4, _yCurr, 90, _infHeight ui_tooltip `Ширина Обвязки Ящика`

	_yCurr = _yCurr + _yOffset
	if gs_door_type_m = 1 or gs_door_type_m = 8 then _outfFlag = 4
	ui_outfield `Ширина Рамы`, x3, _yCurr, 120, _outfHeight, _outfFlag

	_yCurr = _yCurr + dy - _yOffset
	ui_infield{3} "hor_gnum", x4 + 36, _yCurr, 54, _infHeight ui_tooltip `Количество Профилей Горизонтальной Раскладки`
	
	_yCurr = _yCurr + _yOffset
	if gs_door_type_m <> 3 then _outfFlag = 4
	ui_outfield `Гориз. Раскладка`, x3, _yCurr, 120, _outfHeight, _outfFlag

	_yCurr = _yCurr + dy - _yOffset
	ui_infield{3} "vert_gnum", x4 + 36, _yCurr, 54, _infHeight ui_tooltip `Количество Профилей Вертикальной Раскладки`
	
	_yCurr = _yCurr + _yOffset
	ui_outfield `Верт. Раскладка`, x3, _yCurr, 120, _outfHeight, _outfFlag

	_yCurr = _yCurr + dy - _yOffset
	ui_infield{3} "gs_bevel", x3, _yCurr, 210, _infHeight,
					7, "",
					2, 0,
					0, 0, 0, 0,
					"", `Фаска Торца`, 0,
					"", `Фаска Торца`, 1


	if iNightTableType = 1 then
		_yCurr = _yCurr + 55
	else
		_yCurr = _yCurr + 55 - dy
	endif

	ui_infield "doorframe_mat", x3 + 20, _yCurr, 213 - 20, 25 ui_tooltip `Покрытие Обвязки Ящика`
	ui_pict 4, x3, _yCurr + 3, pictWidth , pictHeight, 1 ui_tooltip `Покрытие Обвязки Ящика`

	_yCurr = _yCurr + 30
	ui_infield "doorglas_mat", x3 + 20, _yCurr, 213 - 20, 25 ui_tooltip `Покрытие Филенки Ящика`
	ui_pict 5, x3, _yCurr + 3, pictWidth , pictHeight, 1 ui_tooltip `Покрытие Филенки Ящика`

	_outfFlag = 0

return

