
targetGUID = TO_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

gosub "newparameters_BWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid targetGuid

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! newparameters_bUniformSurfaces_bChairBottom_bChairLeft
! ==============================================================================
"newparameters_BWM":
	if targetGUID # "" then
	
		! Migrate in every case.
	
		! If Chair on the Bottom is off, then Chair Type on the Bottom is None.		
		if not(bChairBottom) then
			iBottomSideType = 0
		else
			iBottomSideType = iBottomSideType
		endif
		
		! If Chair on the Left is off, then Chair Type on the Left is None.		
		if not(bChairLeft) then
			iLeftSideType = 0
		else
			iLeftSideType = iLeftSideType
		endif

		parameters	iBottomSideType = iBottomSideType,
					iLeftSideType	= iLeftSideType
	endif
return

