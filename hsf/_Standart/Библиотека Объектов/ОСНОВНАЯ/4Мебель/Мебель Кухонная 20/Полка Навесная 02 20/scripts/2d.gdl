END

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!!!!!!!!!!!!!!!!!!!!! *** TRIANGLE *** !!!!!!!!!!!!!!!!!!!!
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!

30:

		FILL gs_fill_type

		POLY2_b 5, 2+4+64,gs_fill_pen,gs_back_pen,
			0,0,1,
			0.02,0,1,
			a,b-0.02,1,
			a,b,1,
			0,b,1

		LINE2 0, 0,   0.02, 0
