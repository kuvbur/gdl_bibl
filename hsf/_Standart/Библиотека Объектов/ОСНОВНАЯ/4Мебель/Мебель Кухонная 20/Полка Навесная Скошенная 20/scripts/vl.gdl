

call "FM_types" parameters all

values "zzyzx" range [(shelf_num+2)*0.02, )

VALUES "gs_detlevel_3D" `Детальный`,`Простой`,`Откл.`
VALUES "gs_detlevel_3D_m" 	0,1,2

! ------------------------------------------------------------------------------

VALUES "shelf_num" RANGE [0,)
IF shelf_num = 0 THEN HIDEPARAMETER "gs_ShelfMat"

if GLOB_MODPAR_NAME = "gs_detlevel_3D" then
	if gs_detlevel_3D = `Откл.`		then parameters gs_detlevel_3D_m = 0
	if gs_detlevel_3D = `Простой`	then parameters gs_detlevel_3D_m = 1
	if gs_detlevel_3D = `Детальный`	then parameters gs_detlevel_3D_m = 2
else
	if gs_detlevel_3D_m = 0 then parameters gs_detlevel_3D = `Откл.`
	if gs_detlevel_3D_m = 1 then parameters gs_detlevel_3D = `Простой`
	if gs_detlevel_3D_m = 2 then parameters gs_detlevel_3D = `Детальный`
endif

IF gs_detlevel_3D_m = 1 THEN HIDEPARAMETER "gs_resol"
IF gs_detlevel_3D_m = 0 THEN HIDEPARAMETER "gs_resol", "gs_shadow"

VALUES "depth_cab" range [0,b]

! --- Pen<>0 ---
IF gs_cont_pen=0 THEN PARAMETERS gs_cont_pen=7
IF gs_fill_pen=0 THEN PARAMETERS gs_fillpen=91

VALUES "a" RANGE [0.1,)
VALUES "b" RANGE [0.1,)
VALUES "zzyzx" RANGE [(shelf_num+2)*0.02,)

values "side_vis2D" stSideVisValues
values "side_vis2D_mirror" stSideVisValues
values "side_vis2D_m" 0, 1, 2, 3

if GLOB_MODPAR_NAME = "side_vis2D" then
	if side_vis2D = stSideVisValues[1] then
		side_vis2D_m = 0
		side_vis2D_mirror = stSideVisValues[1]
	endif
	if side_vis2D = stSideVisValues[2] then
		side_vis2D_m = 1
		side_vis2D_mirror = stSideVisValues[3]
	endif
	if side_vis2D = stSideVisValues[3] then
		side_vis2D_m = 2
		side_vis2D_mirror = stSideVisValues[2]
	endif
	if side_vis2D = stSideVisValues[4] then
		side_vis2D_m = 3
		side_vis2D_mirror = stSideVisValues[4]
	endif
	parameters	side_vis2D_m = side_vis2D_m,
				side_vis2D_mirror = side_vis2D_mirror
else
	if GLOB_MODPAR_NAME = "side_vis2D_mirror" then
		if side_vis2D_mirror = stSideVisValues[1] then
			side_vis2D_m = 0
			side_vis2D = stSideVisValues[1]
		endif
		if side_vis2D_mirror = stSideVisValues[2] then
			side_vis2D_m = 2
			side_vis2D = stSideVisValues[3]
		endif
		if side_vis2D_mirror = stSideVisValues[3] then
			side_vis2D_m = 1
			side_vis2D = stSideVisValues[2]
		endif
		if side_vis2D_mirror = stSideVisValues[4] then
			side_vis2D_m = 3
			side_vis2D = stSideVisValues[4]
		endif
		parameters	side_vis2D_m = side_vis2D_m,
					side_vis2D = side_vis2D
	else
		if side_vis2D_m = 0 then
			side_vis2D = stSideVisValues[1]
			side_vis2D_mirror = stSideVisValues[1]
		endif
		if side_vis2D_m = 1 then
			side_vis2D = stSideVisValues[2]
			side_vis2D_mirror = stSideVisValues[3]
		endif
		if side_vis2D_m = 2 then
			side_vis2D = stSideVisValues[3]
			side_vis2D_mirror = stSideVisValues[2]
		endif
		if side_vis2D_m = 3 then
			side_vis2D = stSideVisValues[4]
			side_vis2D_mirror = stSideVisValues[4]
		endif
		parameters	side_vis2D = side_vis2D,
					side_vis2D_mirror = side_vis2D_mirror
	endif
endif

IF SYMB_MIRRORED THEN
	HIDEPARAMETER "side_vis2D"
ELSE
	HIDEPARAMETER "side_vis2D_mirror"
ENDIF

parameters ac_bottomlevel = placementHeight
parameters ac_toplevel = placementHeight+zzyzx
