
UI_CURRENT_PAGE gs_ui_current_page

dim pageNames[7]
	pageNames[1] = ""
	pageNames[2] = ""
	pageNames[3] = ""
	pageNames[4] = ""
	pageNames[5] = ""
	pageNames[6] = `Минимальное Пространство`
	pageNames[7] = ""

dim pageIcos[6]
	pageIcos[1] = ""
	pageIcos[2] = ""
	pageIcos[3] = ""
	pageIcos[4] = ""
	pageIcos[5] = ""
	pageIcos[6] = "uiTab_MinSpace_flat(1)"

iPageIDRoot 		=-1
iPageIDMinimalSpace	= 6

pageStartY = 0

! ==============================================================================
!UI_PAGES
! ==============================================================================

CALL "Kitchen Cabinets UI macro" parameters all \
									pageIds = pageIds,
									pageNames = pageNames,
									pageIcos = pageIcos,
									CornCabType = 0,
									bEdgeBevel = 1,
									CabinetType = 0,
									bCounterOverhang = 0,
									bMirror = 1,

									CornerEdgeSizes = 0,
									bToe = 1,
									bShelf = 1,
									bShowSurfaces = 1,

									bPossibleUndermounted = 0,
									possibleSinkTypes = 2,
									bSidePanels = 0,
									bDrainSymb = 1,

									bMoreDoorPanels = 1,
									bPossibleDrawer = 0,
									bPossibleDrawerNum = 0,
									nDoors = 2,

									bDoubleFacadeEdit = 1,
									bOneRearPanelEdit = 1,
									bHorKnobPosition = 1,
									bVerKnobPosition =1,
									bPositionType = 1,

									bCabSymbol = 1,
									bPossibleSinknTap = 1,
									bApplianceEdit = 0,
									bSymbLineEdit = 0,
									bMinSpaceEdit = 1,
									bShadowEdit = 1,
									bHotspotEdit = 1,

									bEnableBackSplash	= 1,

									bWideCab = 1,
									bSideSpace = 1,
									minSpaceTopViewPict = "ui_kitchencab_belfastsink_2D(13)"

FILE_DEPENDENCE "ui_kitchencab_belfastsink_2D(13)"

UI_PAGE iPageIDMinimalSpace, iPageIDRoot, `Минимальное Пространство`, "uiTab_MinSpace_flat(1)"
	gosub "minimalspacePage"
	
! ==============================================================================
end
! ==============================================================================

! ==============================================================================
!Subroutines
! ==============================================================================

! ------------------------------------------------------------------------------
"minimalspacePage":
! ------------------------------------------------------------------------------

	xPict1 = 128
	yPict1 = pageStartY + 45
	xPict2 = 124
	yPict2 = pageStartY + 20
	wGap = 50
	hGap = 18
	xField = 90
	yField = 18
	fieldToPic = 3
	fieldToField = 5
	
	if dob_face = 0 then
		ui_pict 1, xPict1, yPict1, 187, 175, 1 

		ui_infield "A", xPict1 + 40, yPict1 - yField - fieldToPic, xField, yField ui_tooltip `Ширина`
		ui_infield "MSSide2", xPict1 + 40 - fieldToField - xField, yPict1 - yField - fieldToPic, xField, yField ui_tooltip `Минимальное Пространство Сбоку 2`
		ui_infield "MSSide", xPict1 + 40 + fieldToField + xField, yPict1 - yField - fieldToPic, xField, yField ui_tooltip `Минимальное Пространство Сбоку 1`
		
		ui_infield "B", xPict1 + 187 + fieldToPic, yPict1 + 40, xField, yField ui_tooltip `Длина`
		ui_infield "MSFront", xPict1 + 187 + fieldToPic, yPict1 + 120, xField, yField ui_tooltip `Минимальное Пространство Спереди`
	else
		ui_pict 2, xPict2, yPict2, 195, 222, 1
		
		ui_infield "A", xPict2 + 40, yPict2 - yField - fieldToPic, xField, yField ui_tooltip `Ширина`
		ui_infield "RMSSide", xPict2 + 40 - fieldToField - xField, yPict2 - yField - fieldToPic, xField, yField ui_tooltip `Минимальное Пространство Сзади со Стороны 1`
		ui_infield "RMSSide2", xPict2 + 40 + fieldToField + xField, yPict2 - yField - fieldToPic, xField, yField ui_tooltip `Минимальное Пространство Сзади со Стороны 2`
		
		ui_infield "MSRear", xPict2 + 195 + fieldToPic, yPict2 + 27, xField, yField ui_tooltip `Минимальное Пространство Сзади`
		ui_infield "B", xPict2 + 195 + fieldToPic, yPict2 + 90, xField, yField ui_tooltip `Длина`
		ui_infield "MSFront", xPict2 + 195 + fieldToPic, yPict2 + 165, xField, yField ui_tooltip `Минимальное Пространство Спереди`
		
		ui_infield "MSSide2", xPict2 + 60 - fieldToField - xField, yPict2 + 222 + fieldToPic, xField, yField ui_tooltip `Минимальное Пространство Сбоку 2`
		ui_infield "MSSide", xPict2 + 40 + fieldToField + xField, yPict2 + 222 + fieldToPic, xField, yField ui_tooltip `Минимальное Пространство Сбоку 1`
	endif

return
