

values "door_ang" range [0,90]

values "A" range [0.1+right_cab,)
values "B" range [front_cab,)

values "depth_cabl" range [0.05+front_cab+lrear_cab,A-0.1-right_cab-left_cab]

values "depth_cabr" range [front_cab*dob_face+0.05+front_cab+rrear_cab,B-rrear_cab-left_cab]

IF GLOB_MODPAR_NAME="A" OR GLOB_MODPAR_NAME="B" THEN

		IF front_cab>a-depth_cabl AND GLOB_MODPAR_NAME="B" AND count_th>0 THEN
			IF a-depth_cabl>b-depth_cabr THEN PARAMETERS front_cab=b-depth_cabr
			IF a-depth_cabl<b-depth_cabr THEN PARAMETERS front_cab=a-depth_cabl
		ENDIF

		IF front_cab>b-depth_cabr AND GLOB_MODPAR_NAME="A" AND count_th>0 THEN
			IF a-depth_cabl>b-depth_cabr THEN PARAMETERS front_cab=b-depth_cabr
			IF a-depth_cabl<b-depth_cabr THEN PARAMETERS front_cab=a-depth_cabl
		ENDIF

ENDIF

IF ABS(count_th)<EPS THEN
	PARAMETERS front_cab=0, lrear_cab=0, rrear_cab=0, left_cab=0, right_cab=0
	LOCK "front_cab", "lrear_cab", "rrear_cab", "left_cab", "right_cab"
ENDIF


