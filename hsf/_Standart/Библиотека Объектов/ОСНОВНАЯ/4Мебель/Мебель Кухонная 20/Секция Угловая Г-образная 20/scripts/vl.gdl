
values "door_ang" range [0,90]

values "A" range [0.1,)
values "B" range [0.1,)
values "zzyzx" range [count_th-toe_h, )

values "depth_cabl" range [front_cab+front_cab*dob_face+0.05,A-0.1-right_cab-left_cab]

values "depth_cabr" range [front_cab+rrear_cab+0.05,B-rrear_cab-left_cab]


if GLOB_MODPAR_NAME="A" OR GLOB_MODPAR_NAME="B" then

	if front_cab>a-depth_cabl AND GLOB_MODPAR_NAME="B" AND count_th>0 then
		if a-depth_cabl>b-depth_cabr then PARAMETERS front_cab=b-depth_cabr
		if a-depth_cabl<b-depth_cabr then PARAMETERS front_cab=a-depth_cabl
	endif

	if front_cab>b-depth_cabr AND GLOB_MODPAR_NAME="A" AND count_th>0 then
		if a-depth_cabl>b-depth_cabr then PARAMETERS front_cab=b-depth_cabr
		if a-depth_cabl<b-depth_cabr then PARAMETERS front_cab=a-depth_cabl
	endif

	ELSE

	if front_cab>a-depth_cabl OR  front_cab>b-depth_cabr AND count_th>0 then
		if a-depth_cabl>b-depth_cabr then PARAMETERS front_cab=b-depth_cabr
		if a-depth_cabl<b-depth_cabr then PARAMETERS front_cab=a-depth_cabl
		if ABS((a-depth_cabl)-(b-depth_cabr))<EPS then PARAMETERS front_cab=a-depth_cabl
	endif

endif


if gs_door_type_m = 0 Then
		parameters door = 0
					door = 0
Else
		parameters door = 1
					door = 1
Endif

values "gs_hingedDoorSide" `Слева`, `Справа`
values "gs_hingedDoorSide_m" 1, -1

if GLOB_MODPAR_NAME = "gs_hingedDoorSide" then
	gs_hingedDoorSide_m = 1
	if gs_hingedDoorSide = `Справа` then gs_hingedDoorSide_m = -1
	parameters gs_hingedDoorSide_m = gs_hingedDoorSide_m
else
	gs_hingedDoorSide = `Слева`
	if gs_hingedDoorSide_m = -1 then gs_hingedDoorSide = `Справа`
	parameters gs_hingedDoorSide = gs_hingedDoorSide
endif

if not(gs_bHingedDoubleDoor) then
	hideparameter "gs_hingedDoorSide_m", "gs_hingedDoorSide"
	lock "gs_hingedDoorSide_m", "gs_hingedDoorSide"
endif

