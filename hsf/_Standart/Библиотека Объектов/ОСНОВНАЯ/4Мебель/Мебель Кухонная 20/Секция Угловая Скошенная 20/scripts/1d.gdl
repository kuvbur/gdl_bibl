
EPS=0.0001

dim pageIds[7]
	pageIds[1] = 1
	pageIds[2] = 0
	pageIds[3] = 3
	pageIds[4] = 4
	pageIds[5] = 5
	pageIds[6] = 0
	pageIds[7] = 7

DIM st_knobtypes[9]
	st_knobtypes[1]=`Нет`
	st_knobtypes[2]=`Стиль 1`
	st_knobtypes[3]=`Стиль 2`
	st_knobtypes[4]=`Стиль 3`
	st_knobtypes[5]=`Стиль 4`
	st_knobtypes[6]=`Стиль 5`
	st_knobtypes[7]=`Стиль 6`
	st_knobtypes[8]=`Стиль 7`
	st_knobtypes[9]=`Стиль 8`

! if no door, than the cabinet size is A, B
if not(bDrawer) and gs_door_type_m = 0  then fr_th1=0

if gs_toe_type_m = 0 then toe_h = 0

if gs_count_type_m = 0 then count_th = 0

if cab_size=1 then
	a=a+lrear_cab+right_cab
	b=b+left_cab+rrear_cab
endif

! --- Drawer and Door Panel
if bDrawer then
	drawer_h = drawerHeight
else
	drawer_h = 0
endif

if bUseDoorFront then
	_iDrawerType = gs_door_type_m
else
	_iDrawerType = 1
endif

if A<0.15+depth_cab then parameters A=0.15+depth_cab
if B<0.15+depth_cab then parameters A=0.15+depth_cab

if zzyzx-count_th-toe_h<0 then parameters zzyzx=count_th+toe_h

if ABS(count_th)<EPS then parameters gs_sink_type_m = 0
if depth_cab<0.10 then parameters depth_cab=0.10

if GLOB_MODPAR_NAME="A" OR GLOB_MODPAR_NAME="B" then
	if GLOB_MODPAR_NAME="B" AND front_cab>a-depth_cab AND count_th>0 then parameters front_cab=a-depth_cab
	if GLOB_MODPAR_NAME="A" AND front_cab>b-depth_cab AND count_th>0 then parameters front_cab=b-depth_cab
else
	if front_cab>a-depth_cab AND count_th>0 then parameters front_cab=a-depth_cab
	if front_cab>b-depth_cab AND count_th>0 then parameters front_cab=b-depth_cab
endif
