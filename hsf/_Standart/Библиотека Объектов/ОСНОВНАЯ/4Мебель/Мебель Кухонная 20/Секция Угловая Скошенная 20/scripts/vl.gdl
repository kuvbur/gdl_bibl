
call "FM_types" parameters all

values "A" range [0.75, )
values "B" range [0.75, )

values "door_ang" range [0,90+45]

values "gs_resol" range [4,)

! --- Minimal Space ---
minspace_max=(a-depth_cab)/sin(45)/2
values "MSFront" range [0, )
values "MSSide" range (-minspace_max,]
values "MSSide2" range(-minspace_max,]


if ABS(count_th)<EPS then
	parameters front_cab=0, lrear_cab=0, rrear_cab=0, left_cab=0, right_cab=0
	lock "front_cab", "lrear_cab", "rrear_cab", "left_cab", "right_cab"
endif

! Same A,B Size
if GLOB_MODPAR_NAME = "B" 		then
	A=B
	parameters A=A
else
	B=A
	parameters B=B
endif

MEP_NumberConnections = 3
PARAMETERS MEP_NumberConnections = MEP_NumberConnections

dim DimConName[3]
	DimConName[1] = `Подключение Горячей Воды`
	DimConName[2] = `Подключение Холодной Воды`
	DimConName[3] = `Подключение Канализации`

! --- Drawer ---
values "drawerHeight" range (0, )

if not(bDrawer) then
	hideparameter "bUseDoorFront", "drawerHeight"
	lock "bUseDoorFront", "drawerHeight"
endif

CALL "Kitchen Cabinets UI macro" parameters all \
									pageIds = pageIds,
									DimConName = DimConName,
									CornCabType = 1,
									bEdgeBevel = 1,
									CabinetType = 0,
									bMirror = 1,
									bCounterOverhang = 1,

									CornerEdgeSizes = 3,
									bToe = 1,
									bShelf = 1,
									bShowSurfaces = 1,

									bPossibleUndermounted = 1,
									possibleSinkTypes = 3,
									bSidePanels = 0,

									bMoreDoorPanels = 1,
									bPossibleDrawer = 1,
									bPossibleDrawerNum = 0,
									nDoors = 1,

									bDoubleFacadeEdit = 1,
									bOneRearPanelEdit = 0,
									bHorKnobPosition = 1,
									bVerKnobPosition =1,
									bPositionType = 1,

									bCabSymbol = 1,
									bPossibleSinknTap = 1,
									bApplianceEdit = 0,
									bSymbLineEdit = 0,
									bMinSpaceEdit = 1,
									bShadowEdit = 1,
									bHotspotEdit = 1,
									bDrainSymb = 0,

									bEnableBackSplash	= 1,
									bEnableDoorpanel	= 1,
									bEnableEndpanel		= 1,
									bDoubleDoor			= 0,
									bEnableKnobPlacing	= 1,
									bEnableShelf		= 1,
									bEnableSink			= 1,
									isTallCabinet		= 0,

									bKnobParams = 1


if gs_sink_type_m=1 then sink_a=0.40			!!SINK 1
if gs_sink_type_m=2 then sink_a=0.55			!!SINK 2
tapMinDist=0.03
bigTap = 0
if gs_tap_type_m > 3 then bigTap = 1
if gs_tap_type_m=0  then HIDEPARAMETER "tapOffset"
if gs_sink_type_m=1  then VALUES "tapOffset" RANGE[0, 0]																!!(Sink Round)
if gs_sink_type_m=2  then VALUES "tapOffset" RANGE[-sink_a/2+tapMinDist+bigTap*0.1, sink_a/2-tapMinDist-bigTap*0.1]		!!(Sink 1 Bowl)

VALUES "gs_ui_current_page" 1, 2, 3, 4, 5, 6, 7

