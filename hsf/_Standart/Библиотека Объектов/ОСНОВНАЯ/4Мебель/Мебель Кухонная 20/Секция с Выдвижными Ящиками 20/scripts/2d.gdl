
! --- Handling overhungs in case of end panels --------------------------------

CALL "fa_counter_sink" PARAMETERS ALL MSSide=0, MSSide2=0, side_vis2D_m = side_vis2D_m + 1


IF not(cab_size) THEN
	CALL "Resize_A_B_ZZYZX" Parameters ALL
ELSE
	A=a-(left_cab+right_cab)
	B=b-(front_cab+rear_cab)

	ADD2 left_cab,front_cab
	CALL "Resize_A_B_ZZYZX" Parameters ALL
	DEL 1

	A=a+(left_cab+right_cab)
	B=b+(front_cab+rear_cab)

ENDIF
