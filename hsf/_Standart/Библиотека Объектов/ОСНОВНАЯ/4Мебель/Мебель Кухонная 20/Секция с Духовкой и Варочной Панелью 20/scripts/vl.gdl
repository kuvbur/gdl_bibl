! --- Cooktops ---
dim stCooktopType[13]
	stCooktopType[ 1] = `Тип 1`
	stCooktopType[ 2] = `Тип 2`
	stCooktopType[ 3] = `Тип 3`
	stCooktopType[ 4] = `Тип 4`
	stCooktopType[ 5] = `Тип 5`
	stCooktopType[ 6] = `Тип 6`
	stCooktopType[ 7] = `Тип 7`
	stCooktopType[ 8] = `Тип 8`
	stCooktopType[ 9] = `Тип 9`
	stCooktopType[10] = `Тип 10`
	stCooktopType[11] = `Тип 11`
	stCooktopType[12] = `Тип 12`
	stCooktopType[13] = `Тип 13`

dim idxCooktopType[13]
	idxCooktopType[ 1] = 1
	idxCooktopType[ 2] = 2
	idxCooktopType[ 3] = 3
	idxCooktopType[ 4] = 4
	idxCooktopType[ 5] = 5
	idxCooktopType[ 6] = 6
	idxCooktopType[ 7] = 7
	idxCooktopType[ 8] = 8
	idxCooktopType[ 9] = 9
	idxCooktopType[10] = 10
	idxCooktopType[11] = 11
	idxCooktopType[12] = 12
	idxCooktopType[13] = 13

VALUES "CooktopType" 	stCooktopType, `Нет`
VALUES "CooktopType_m" 	idxCooktopType, 0

if GLOB_MODPAR_NAME = "CooktopType" then
	cooktopType_m = 1
	for iCooktopType = 1 to vardim1(idxCooktopType)
		if cooktopType = stCooktopType[iCooktopType] then
			cooktopType_m = idxCooktopType[iCooktopType]
		endif
	next iCooktopType
	if cooktopType = `Нет` then cooktopType_m = 0
	parameters cooktopType_m = cooktopType_m
else
	idx = 1
	for i=1 to vardim1(idxCooktopType)
		if idxCooktopType[i] = cooktopType_m then
			idx = i
		endif
	next i
	cooktopType = stCooktopType[idx]
	if cooktopType_m = 0 then cooktopType = `Нет`
	parameters cooktopType = cooktopType
endif

if GLOB_MODPAR_NAME="CooktopType" or GLOB_MODPAR_NAME="CooktopType_m" then
	if CooktopType_m<>0 THEN CALL "fa_CooktopType" PARAMETERS ALL\
							returned_parameters defaultAA, defaultBB
	PARAMETERS AA=defaultAA, BB=defaultBB
endif
values "A" range [AA, )
values "B" range [BB+2*0.055, )

