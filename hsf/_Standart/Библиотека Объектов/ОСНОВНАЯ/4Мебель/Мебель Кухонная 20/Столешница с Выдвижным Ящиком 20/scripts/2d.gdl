
pen gs_cont_pen

CALL "fa_counter_sink" PARAMETERS a=a, b=b, zzyzx=zzyzx,cab_size=cab_size,
		gs_detlevel_3D_m=gs_detlevel_3D_m, gs_full_edit=0, dob_face=0, count_th=count_th,
		front_cab=front_cab, rear_cab=rear_cab, left_cab=left_cab,
		right_cab=right_cab, bs_h=bs_h, side_vis_m=side_vis_m+1,gs_sink_type_m=0,
		drawer2d=drawer2d,drawer=bDrawer,
		gs_cont_pen=gs_cont_pen, gs_fill_type=gs_fill_type, gs_fill_pen=gs_fill_pen,
		gs_back_pen=gs_back_pen, gs_shadow=gs_shadow,
		side_vis2D_m=side_vis2D_m+1,
		MSFront=MSFront, MSSide=0, MSSide2=0


if cab_size=0 then
	CALL "Resize_A_B_ZZYZX" Parameters ALL

else

	A=a-(left_cab+right_cab)
	B=b-(front_cab+rear_cab)

	ADD2 left_cab,front_cab
	CALL "Resize_A_B_ZZYZX" Parameters ALL
	DEL 1

	A=a+(left_cab+right_cab)
	B=b+(front_cab+rear_cab)

endif
