
! const values for parameter: iEndCabinetStyle
CABEND_ANGULAR  = 1
CABEND_RADIUSED = 2


!gs_knob_p_m
CABINET_KNOB_VER		= 0
CABINET_KNOB_HOR		= 1


dim pageIds[4]
	pageIds[1] = 0
	pageIds[2] = 3
	pageIds[3] = 0
	pageIds[4] = 5

if iEndCabinetStyle = CABEND_ANGULAR then
	DIM st_doortypes[13]
		st_doortypes[1] = `Нет`
		st_doortypes[2] = `Стиль 1`
		st_doortypes[3] = `Стиль 2`
		st_doortypes[4] = `Стиль 3`
		st_doortypes[5] = `Стиль 7`
		st_doortypes[6] = `Стиль 8`
		st_doortypes[7] = `Стиль 9`
		st_doortypes[8] = `Стиль 10`
		st_doortypes[9] = ""
		st_doortypes[10]= `Стиль 6`
		st_doortypes[11]= ""
		st_doortypes[12]= `Стиль 4`
		st_doortypes[13]= `Стиль 5`
else
	DIM st_doortypes[13]
		st_doortypes[1] = `Нет`
		st_doortypes[2] = `Стиль 1`
		st_doortypes[3] = `Стиль 2`
		st_doortypes[4] = `Стиль 3`
	for i = 5 to 13
		st_doortypes[i] = ""
	next i
endif

! --- If no door, than the cabinet size is A, B ---
if gs_door_type_m = 0  then fr_th1 = 0

left_cab = 0
front_cab = 0
szi=ATN(b/a)		! --- 45
fi=szi-22.5								! --- 22.5
ama=a/COS(szi)/2
amb=ama/COS(22.5)
amx=amb*COS(szi-22.5)
amy=amb*SIN(szi-22.5)

amxt=(b-0.03-(a-0.03)/TAN(45-fi))/(TAN(fi)-1/(TAN(45-fi)))
amyt=amxt*TAN(fi)

amxc=(b-a/TAN(45-fi))/(TAN(fi)-1/(TAN(45-fi)))
amxc=(b-fr_th1-(a-fr_th1)/TAN(45-fi))/(TAN(fi)-1/(TAN(45-fi)))
ambcl=amxc/COS(fi)			! --- Left door width
amyc=(a-amxc)/TAN(45-fi)
ambcr=amyc/COS(45-fi)

amxms=(b+0.9-(a+0.9)/TAN(45-fi))/(TAN(fi)-1/(TAN(45-fi)))
amyms=amxms*TAN(fi)
