

! --- Door Height ---
parameters door_h=ZZYZX

! Limits of A,B ZZYZX Parameters
values "A" range [0.1, )
values "B" range [0.1, )
values "ZZYZX" range (0, )
if GLOB_MODPAR_NAME="A" then
	PARAMETERS B=A
else
	PARAMETERS A=B
endif

values "fr_th1" range [0.01,)

if iEndCabinetStyle = CABEND_ANGULAR then
	values "gs_door_type" 	st_doortypes[2],st_doortypes[3],st_doortypes[4],st_doortypes[12],st_doortypes[13],st_doortypes[10],
							st_doortypes[5],st_doortypes[6],st_doortypes[7],st_doortypes[8],`Спец. панель`,st_doortypes[1]
else
	values "gs_door_type" 	st_doortypes
endif

if GLOB_MODPAR_NAME = "gs_door_type" then
	for iDoor = 1 to vardim1(st_doortypes)
		if gs_door_type = st_doortypes[iDoor] then
			gs_door_type_m = iDoor - 1
		endif
	next iDoor
	if gs_door_type = `Спец. панель` then
		gs_door_type_m = 99
	endif
	parameters gs_door_type_m = gs_door_type_m
else
	if gs_door_type_m < 99 then
		if gs_door_type_m >= vardim1(st_doortypes) then
			gs_door_type_m = 1
		endif
		gs_door_type = st_doortypes[gs_door_type_m + 1]
	else
		gs_door_type = `Спец. панель`
	endif
	parameters gs_door_type = gs_door_type
endif

values{2} "iEndCabinetStyle" 	CABEND_ANGULAR, `Угловой Торец`,
								CABEND_RADIUSED, `Радиальный Торец`
if iEndCabinetStyle = CABEND_ANGULAR then hideparameter "gs_res_2"

values "door_ang" range [0,90+20]


! --- LOCK PARAMETERS ----------------------------------------------------------

if iEndCabinetStyle = CABEND_ANGULAR then
	if gs_knob_type_m=1 or gs_knob_type_m=2 or gs_knob_type_m=3 or gs_knob_type_m=9  then
		lock "gs_knob_p", "gs_knob_p_m"
		hideparameter "gs_knob_p", "gs_knob_p_m"
	endif
else
	lock "gs_knob_p", "gs_knob_p_m", "knob_pl_x","door_ang","vert_gnum","hor_gnum"
	hideparameter "gs_knob_p", "gs_knob_p_m", "knob_pl_x","door_ang","vert_gnum","hor_gnum"
	if gs_door_type_m=3 then
		lock "doorglas_mat"
		hideparameter "doorglas_mat"
	endif
endif

