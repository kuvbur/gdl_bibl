
pen gs_cont_pen
line_type gs_line_type

FILL gs_fill_type

POLY2_B 6, 2+64, gs_fill_pen, gs_back_pen,
	0, b, 1,
	a, b, 1,
	a, b-depth_cab, 1,
	depth_cab, 0, 1,
	0, 0, 1,
	0, b, -1

hotline2 0, b,a, b
hotline2 a, b,a, b-depth_cab
hotline2 a, b-depth_cab,depth_cab, 0
hotline2 depth_cab, 0,0, 0
hotline2 0, 0,0, b

unID=1
hotspot2 0, b, unID : unID=unID+1
hotspot2 a, b, unID : unID=unID+1
hotspot2 a, b-depth_cab, unID : unID=unID+1
hotspot2 depth_cab, 0, unID : unID=unID+1
hotspot2 0, 0, unID : unID=unID+1

line2 0, 0,   0, b
line2 0, b,   a, b
line2 depth_cab, 0,   a, b-depth_cab
line2 a, b-depth_cab,   a, b-depth_cab
side_vis2D_m = side_vis2D_m+1
if side_vis2D_m=1 then 15
if side_vis2D_m=2 then 10
line2 a, b,   a, b-depth_cab

10:
if side_vis2D_m=3 then 15
line2 0, 0, depth_cab, 0

15:
cab_size = 0
lrear_cab = 0
left_cab = 0
ADD2 lrear_cab*(1-cab_size), left_cab*(1-cab_size)
If iCabSymbolType2D <> 1 Then
	if iCabSymbolType2D = 2 OR iCabSymbolType2D = 4 then
!		line2 	depth_cab-0.02/(sqr(2)), 0.02/(sqr(2)),	a-0.02/(sqr(2)), b-depth_cab+0.02/(sqr(2))
		line2 depth_cab-0.025,0, a, b-depth_cab+0.025
	endif
	if iCabSymbolType2D = 3 OR iCabSymbolType2D = 4 then
		poly2_ 4,1+4,
				a,b-depth_cab,1,
				depth_cab,0,901,
				0,-15,4001,
				depth_cab,0,1
	endif
endif
del 1

END
