
ADD2 a/2,b/2

unID=1
HOTSPOT2 -a/2,b/2, unID : unID=unID+1
HOTSPOT2 0,b/2, unID : unID=unID+1

!!! *** Minimal Space
ADD2 -a/2,-b/2
call "fa_minimalspace" parameters all MSType = 20, unID = 3000
DEL 1
PEN gs_cont_pen
unID = 10
side_vis2D_m = side_vis2D_m+1

IF side_vis2D_m=1 THEN
	m=0
	n=0
ENDIF

IF side_vis2D_m=2 THEN
	m=0
	n=1
ENDIF

IF side_vis2D_m=3 THEN
	m=1
	n=0
ENDIF

IF side_vis2D_m=4 THEN
	m=1
	n=1
ENDIF

!! Fill of Cabinet
FILL gs_fill_type
POLY2_B 5, 1+2+64,gs_fill_pen,gs_back_pen,
	-a/2,-b/2,n,
	-a/2,b/2,1,
	a/2,b/2,m,
	a/2,-b/2,1,
	-a/2,-b/2,-1

HOTLINE2 -a/2,-b/2,-a/2,b/2
HOTLINE2 -a/2,b/2,a/2,b/2
HOTLINE2 a/2,b/2,a/2,-b/2
HOTLINE2 a/2,-b/2,-a/2,-b/2


DEL 1
CALL "Resize_A_B_ZZYZX" Parameters ALL


IF Appl2D=1 AND ApplType_m <> APPLTYPE_NONE THEN
	LINE_TYPE ApplLineType
	LINE2 0.05,temp,0.05,B-0.05
	LINE2 A-0.05,temp,A-0.05,B-0.05
	LINE2 0.05,B-0.05,A-0.05,B-0.05
ENDIF
