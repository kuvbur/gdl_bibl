
call "FM_types" parameters all

hideparameter all "A", "B", "ZZYZX"

values "gs_resol" range [3,)

values{2} "iDetlevel3D" 2, `Детальный`,
						1, `Простой`,
						0, `Откл.`

if iDetlevel3D = 1 then
	hideparameter "gs_resol"
	lock "gs_resol"
endif

if iDetlevel3D = 0 then 
	hideparameter "gs_resol", "gs_shadow"
	lock "gs_resol", "gs_shadow"
endif

values{2} "iCaseType" 	1, stCaseType[1],
						2, stCaseType[2]

values{2} "iLegTipType" 1, stLegTipType[1],
						2, stLegTipType[2]


if GLOB_MODPAR_NAME = "moduleNumHor" then
	a				= moduleNumHor * moduleWidth
	parameters a	= moduleNumHor * moduleWidth
else
	values "a" range[moduleWidth,] step 0, moduleWidth
	moduleNumHor 	= a	/ moduleWidth
	parameters moduleNumHor 	= a	/ moduleWidth
endif

if GLOB_MODPAR_NAME = "moduleNumVer" then
	b				= moduleNumVer * moduleDepth
	parameters b	= moduleNumVer * moduleDepth
else
	values "b" range[moduleDepth,] step 0, moduleDepth
	moduleNumVer 	= b	/ moduleDepth
	parameters moduleNumVer 	= b	/ moduleDepth
endif

values "zzyzx" range [umbHoldElev+umbHoldHeight+gridHeight+0.01+coverLouverHeight,)

values "moduleNumHor" range [1,)
values "moduleNumVer" range [1,)

values "umbHoldElev" range[legHeight*(iLegTipType>1),)
values "umbHoldHeight" range[umbHoldThk,)
values "coverLouverHeight" range[coverCutDepth*2,)

if not(bLegTip) then
	lock "legHeight", "legTipMat", "gs_resol", "iLegTipType"
	hideparameter "legHeight", "legTipMat", "gs_resol", "iLegTipType"
endif

if iCaseType=1 then
	lock "coverMat", "coverLouverHeight"
	hideparameter "coverMat", "coverLouverHeight"
endif


