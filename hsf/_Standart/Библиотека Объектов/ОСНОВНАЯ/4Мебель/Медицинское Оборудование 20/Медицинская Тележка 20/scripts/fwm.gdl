
actualGUID = FROM_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

	_autoID		= "1A05964C-6D0D-4098-8904-2EC7DA4DE8A4"
	_startID	= "52F9FFE6-E866-4565-847D-A3FC691B59CF"
	_endID		= "2C95E1CF-D084-4B6C-BC05-B48129476DDE"
gosub "newparameters_FWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid actualGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! newparameters
! ==============================================================================
"newparameters_FWM":
	if actualGuid = _startID | actualGuid = _autoID then
	
		bHas =	DELETED_PAR_VALUE("gs_knob_type_m", _gs_knob_type_m) |\
				DELETED_PAR_VALUE("gs_knob_type",	_gs_knob_type)
		
		! If Knob Style is None, than Knob is turned off.
		if _gs_knob_type_m = 0 then
			bKnob = 0
		else
			bKnob = 1
			iKnobStyle = _gs_knob_type_m
		endif
		
		parameters	iKnobStyle	= iKnobStyle,
					bKnob		= bKnob
				
		bHas =	DELETED_PAR_VALUE("gs_detlevel_3D", _gs_detlevel_3D) |\
				DELETED_PAR_VALUE("gs_detlevel_3D_m", _gs_detlevel_3D_m)
		
		if _gs_detlevel_3D_m = 2 then	! Detailed
			iDetlevel3D = 2
		else
			if _gs_detlevel_3D_m = 1 then	! Simple
				iDetlevel3D = 1
			else
				iDetlevel3D = 0	! Off
			endif
		endif
		
		parameters iDetlevel3D = iDetlevel3D
		
		bHas =	DELETED_PAR_VALUE("gs_detlevel_2D", _gs_detlevel_2D) |\
				DELETED_PAR_VALUE("gs_detlevel_2D_m", _gs_detlevel_2D_m)
		
		if _gs_detlevel_2D_m = 2 then	! Detailed
			iDetlevel2D = 2
		else	! Simple
			iDetlevel2D = 1
		endif
		
		parameters iDetlevel2D = iDetlevel2D

		bHas =	DELETED_PAR_VALUE("gs_type", _gs_type) |\
				DELETED_PAR_VALUE("gs_type_m", _gs_type_m)
		
		if _gs_type_m = 1 then	! Basket
			iStyle = 1
		else
			if _gs_type_m = 2 then	! Drawers Only
				iStyle = 2
			else	! With Lower Tray
				iStyle = 3
			endif
		endif
		
		parameters iStyle = iStyle
		
		
		bHas =	STORED_PAR_VALUE ("bSide", _bSide) |\
				STORED_PAR_VALUE ("bUpper", _bUpper)
				
		if _gs_type_m = 1 and _bSide = 0 and _bUpper = 0 then	! Basket
			iCartStyles = 1
		else
			if _gs_type_m = 2 and _bSide = 0 and _bUpper = 1 then	! Drawers Only
				iCartStyles = 2
			else	! With Lower Tray
				iCartStyles = 3
			endif
		endif
		
		parameters iCartStyles = iCartStyles
		
		actualGuid = _endID
	endif
return

