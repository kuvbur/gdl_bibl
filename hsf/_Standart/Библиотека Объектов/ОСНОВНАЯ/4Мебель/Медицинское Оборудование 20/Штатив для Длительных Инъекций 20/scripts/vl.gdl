
dim stDetlevel3D[3]
	stDetlevel3D[1] = `Детальный`
	stDetlevel3D[2] = `Простой`
	stDetlevel3D[3] = `Откл.`

call "FM_types" parameters all

hideparameter all "A", "B", "ZZYZX"

values "a" range [0.2, ]
values "b" range [0.2, ]
poleHeight = (baseHeight - 0.07)
values "zzyzx" range [0.9, baseHeight + 2*poleHeight]

! ----- 3D Representation -----
values{2} "gs_detlevel_3D_m" 	2, stDetlevel3D[1],
								1, stDetlevel3D[2],
								0, stDetlevel3D[3]

values "gs_resol" range [3, )

if gs_detlevel_3D_m = 1 then	! Simple
	lock "gs_resol"
else
	if gs_detlevel_3D_m = 0 then ! Off
		lock "gs_resol", "gs_shadow"
	endif
endif


! ----- AB -----
if GLOB_MODPAR_NAME = "B" then
	a = b
	parameters a = a
else
	b = a
	parameters b = b
endif


! ----- Minimal Space -----
values "MSFront" range [0, ]
