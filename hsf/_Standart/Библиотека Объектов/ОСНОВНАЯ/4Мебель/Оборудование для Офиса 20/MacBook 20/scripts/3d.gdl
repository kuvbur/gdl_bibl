	prism_ 2, portDepth,
		0, 0, 979,
		portHeight/2, 360, 4079
	addx portHeight*1.5
	next pr
	del 2
	del 1

	del 3



	!!! Right Side
	mulx -1
	rotz -90
	add -b+0.5"+cdSlotWidth/2, -a/2+portDepth, -(bodyThk-bottomTopRoundingThk)/2
	rotx 90

	prism_ 7, portDepth,
		cdSlotWidth/2-portHeight/2, -portHeight/2, 79,
