	del 6+(iScreenSize = 3)

	addx portWidth*1.5*(6+(iScreenSize = 3))+cardReaderWidth/2-portWidth/2
	prism_ 7, portDepth,
		cardReaderWidth/2-portHeight/2, -portHeight/2, 79,
