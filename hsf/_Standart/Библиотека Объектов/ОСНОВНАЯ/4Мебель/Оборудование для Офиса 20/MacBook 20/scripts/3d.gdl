		cdSlotWidth/2-portHeight/2, portHeight/2, 1079,
		-cdSlotWidth/2+portHeight/2, portHeight/2, 79,
		-1, 0, 879,
		-cdSlotWidth/2+portHeight/2, -portHeight/2, 1079,
		cdSlotWidth/2-portHeight/2, -portHeight/2, -1

	addx cdSlotWidth/2+portWidth*1.5
	prism_ 7, portDepth,
		portWidth/2-portHeight/2, -portHeight/2, 79,
		1, 0, 879,
		portWidth/2-portHeight/2, portHeight/2, 1079,
		-portWidth/2+portHeight/2, portHeight/2, 79,
		-1, 0, 879,
		-portWidth/2+portHeight/2, -portHeight/2, 1079,
		portWidth/2-portHeight/2, -portHeight/2, -1
	del 1

	del 4

