			buHeight=0.013
			buPos=-0.01
			add a-buttonsXpos*2+buDist-(buWidth+buDist)*3, buPos, 0
			for bu=1 to 3
				prism_ 5, buttonThk,
					0, 0, 15,
					buWidth, 0, 15,
					buWidth, buHeight, 15,
					0, buHeight, 15,
					0, 0, -1
				addx buWidth+buDist
			next bu
			del 3
			del 1
			add a-buttonsXpos*2+buDist-(buWidth+buDist)*2, buPos+buHeight+buDist, 0
			prism_ 5, buttonThk,
				0, 0, 15,
				buWidth, 0, 15,
				buWidth, buHeight, 15,
				0, buHeight, 15,
				0, 0, -1
			del 1
		endif

		if iLaptopType = 3 or iLaptopType = 4 or iLaptopType = 5 then
			!1st row
			buPos=0
			addy buPos
			buHeight=0.017
			for bu=1 to 8
				if bu=1 then
					buWidth=0.02
				endif
				if bu=5 then
					buWidth=0.089
				endif
				if bu<>1 and bu<>5 then
					buWidth=0.017
				endif
				prism_ 5, buttonThk,
					0, 0, 15,
					buWidth, 0, 15,
					buWidth, buHeight, 15,
					0, buHeight, 15,
					0, 0, -1
				addx buWidth+buDist
			next bu
			del 8
			del 1

			!2nd row
			buPos=buHeight+buDist
			addy buPos
			buHeight=0.017
			for bu=1 to 12
				if bu=1 then
					buWidth=0.038
				endif
				if bu=12 then
					buWidth=0.047
				endif
				if bu<>1 and bu<>12 then
					buWidth=0.017
				endif
				prism_ 5, buttonThk,
					0, 0, 15,
					buWidth, 0, 15,
					buWidth, buHeight, 15,
					0, buHeight, 15,
					0, 0, -1
				addx buWidth+buDist
			next bu
			del 12
			del 1

			!3rd row
			buPos=(buHeight+buDist)*2
			addy buPos
			buHeight=0.017
			for bu=1 to 13
				if bu=1 then
					buWidth=0.028
				endif
				if bu=13 then
					buWidth=0.039
				endif
				if bu<>1 and bu<>13 then
					buWidth=0.017
				endif
				prism_ 5, buttonThk,
					0, 0, 15,
					buWidth, 0, 15,
					buWidth, buHeight, 15,
					0, buHeight, 15,
					0, 0, -1
				addx buWidth+buDist
			next bu
			del 13
			del 1

			!4th row
			buPos=(buHeight+buDist)*3
			addy buPos
			buHeight=0.017
			for bu=1 to 14
				if bu=1 then
					buWidth=0.024
				endif
				if bu=14 then
					buWidth=0.025
				endif
				if bu<>1 and bu<>14 then
					buWidth=0.017
				endif
				prism_ 5, buttonThk,
					0, 0, 15,
					buWidth, 0, 15,
					buWidth, buHeight, 15,
					0, buHeight, 15,
					0, 0, -1
				addx buWidth+buDist
			next bu
			del 14
			del 1

			!5th row
			buPos=(buHeight+buDist)*4
			addy buPos
			buHeight=0.017
			for bu=1 to 14
				if bu=1 then
					buWidth=0.017
				endif
				if bu=14 then
					buWidth=0.032
				endif
				if bu<>1 and bu<>14 then
					buWidth=0.017
				endif
				prism_ 5, buttonThk,
					0, 0, 15,
					buWidth, 0, 15,
					buWidth, buHeight, 15,
					0, buHeight, 15,
					0, 0, -1
				addx buWidth+buDist
			next bu
			del 14
			del 1

			!6th row
			buPos=(buHeight+buDist)*5
			addy buPos
			buHeight=0.014
			for bu=1 to 17
				if bu=1 then
					buWidth=0.016
				endif
				if bu<>1 then
					buWidth=0.015
				endif
				prism_ 5, buttonThk,
					0, 0, 15,
					buWidth, 0, 15,
					buWidth, buHeight, 15,
					0, buHeight, 15,
					0, 0, -1
				addx buWidth+buDist+0.004*(bu=1 or bu=5 or bu=9 or bu=13)
			next bu
			del 17
			del 1

			!keys above touchpad
			buHeight=0.017
			buPos=-buHeight-buDist
			add touchpadXpos-buttonsXpos, buPos, 0
			for bu=1 to 2
				buWidth=(0.065-buDist*3)/2
				prism_ 5, buttonThk,
					0, 0, 15,
					buWidth, 0, 15,
					buWidth, buHeight, 15,
					0, buHeight, 15,
					0, 0, -1
				addx buWidth+buDist
			next bu
			del 2
			del 1

			!arrow keys
			buWidth=0.017
			buHeight=0.013
			buPos=-0.01
			add a-buttonsXpos*2+buDist-(buWidth+buDist)*3, buPos, 0
			for bu=1 to 3
				prism_ 5, buttonThk,
					0, 0, 15,
					buWidth, 0, 15,
					buWidth, buHeight, 15,
					0, buHeight, 15,
					0, 0, -1
				addx buWidth+buDist
			next bu
			del 3
			del 1
			add a-buttonsXpos*2+buDist-(buWidth+buDist)*2, buPos+buHeight+buDist, 0
			prism_ 5, buttonThk,
				0, 0, 15,
				buWidth, 0, 15,
				buWidth, buHeight, 15,
				0, buHeight, 15,
				0, 0, -1
			del 1

			!rightside page keys
			buWidth=0.017
			buHeight=0.017
			buPos=buHeight+buDist
			add a-buttonsXpos*2-buWidth, buPos, 0
			for bu=1 to 4
				prism_ 5, buttonThk,
					0, 0, 15,
					buWidth, 0, 15,
					buWidth, buHeight, 15,
					0, buHeight, 15,
					0, 0, -1
				addy buHeight+buDist
			next bu
			del 4
			del 1
		endif
	endif




	del 1

	return


	300:
	!!! Cutting for Touchpad

	add touchpadXpos-0.001, touchpadYpos-0.001, 0.1+topPlateThk-buttonThk
	mulz -1
	material bodyBlkMat
	if iLaptopType = 1 then
		cutform 5, 1, 2+16,
				0, 0, 1, 0.1,
				0.09,        0.026,      1+2+4+8+16,
				0,        0.026,      1+2+4+8+16,
				0,            0,      1+2+4+8+16,
				0.09,            0,      1+2+4+8+16,
				0.09,        0.026,      1+2+4+8+16
	endif
	if iLaptopType = 2 then
		cutform 5, 1, 2+16,
				0, 0, 1, 0.1,
				0.065,        0.044,      1+2+4+8+16,
				0,        0.044,      1+2+4+8+16,
				0,            0,      1+2+4+8+16,
				0.065,            0,      1+2+4+8+16,
				0.065,        0.044,      1+2+4+8+16
	endif
	if iLaptopType = 3 then
		cutform 5, 1, 2+16,
				0, 0, 1, 0.1,
				0.065,        0.044,      1+2+4+8+16,
				0,        0.044,      1+2+4+8+16,
				0,            0,      1+2+4+8+16,
				0.065,            0,      1+2+4+8+16,
				0.065,        0.044,      1+2+4+8+16
	endif
	if iLaptopType = 4 then
		cutform 5, 1, 2+16,
				0, 0, 1, 0.1,
				0.065,        0.059,      1+2+4+8+16,
				0,        0.059,      1+2+4+8+16,
				0,            0,      1+2+4+8+16,
				0.065,            0,      1+2+4+8+16,
				0.065,        0.059,      1+2+4+8+16
	endif
	if iLaptopType = 5 then
		cutform 5, 1, 2+16,
				0, 0, 1, 0.1,
				0.065,        0.072,      1+2+4+8+16,
				0,        0.072,      1+2+4+8+16,
				0,            0,      1+2+4+8+16,
				0.065,            0,      1+2+4+8+16,
				0.065,        0.072,      1+2+4+8+16
	endif
	del 2

	return



	400:
	!!! Touchpad buttons

	add touchpadXpos, touchpadYpos, topPlateThk-buttonThk
	material butMat
	if iLaptopType = 1 then
		prism_ 5, buttonThk,
				0.076,            0,      15,
				0.076,        0.024,      15,
				0.012,        0.024,      15,
				0.012,            0,      15,
				0.076,            0,      -1

		prism_ 5, buttonThk,
				0.077,            0,      15,
				0.088,            0,      15,
				0.088,        0.024,      15,
				0.077,        0.024,      15,
				0.077,            0,      -1

		prism_ 5, buttonThk,
				0.011,            0,      15,
				   0,            0,      15,
				   0,        0.024,      15,
				0.011,        0.024,      15,
				0.011,            0,     -1
	endif
	if iLaptopType = 2 then
		prism_ 5, buttonThk,
				0.063,        0.013,      15,
				0.063,        0.042,      15,
				0,        0.042,      15,
				0,        0.013,      15,
				0.063,        0.013,      -1

		prism_ 5, buttonThk,
				0,            0,      15,
				0.031,            0,      15,
				0.031,        0.012,      15,
				0,        0.012,      15,
				0,            0,      -1

		prism_ 5, buttonThk,
				0.063,            0,      15,
				0.032,            0,      15,
				0.032,        0.012,      15,
				0.063,        0.012,      15,
				0.063,            0,      -1
	endif
	if iLaptopType = 3 then
		prism_ 5, buttonThk,
				0.063,        0.013,      15,
				0.063,        0.042,      15,
				0,        0.042,      15,
				0,        0.013,      15,
				0.063,        0.013,      -1

		prism_ 5, buttonThk,
				0,            0,      15,
				0.031,            0,      15,
				0.031,        0.012,      15,
				0,        0.012,      15,
				0,            0,      -1

		prism_ 5, buttonThk,
				0.063,            0,      15,
				0.032,            0,      15,
				0.032,        0.012,      15,
				0.063,        0.012,      15,
				0.063,            0,      -1
	endif
	if iLaptopType = 4 then
		prism_ 5, buttonThk,
				0.063,        0.018,      15,
				0.063,        0.057,      15,
				0,        0.057,      15,
				0,        0.018,      15,
				0.063,        0.018,      -1

		prism_ 5, buttonThk,
				0,            0,      15,
				0.031,            0,      15,
				0.031,        0.017,      15,
				0,        0.017,      15,
				0,            0,      -1

		prism_ 5, buttonThk,
				0.063,            0,      15,
				0.032,            0,      15,
				0.032,        0.017,      15,
				0.063,        0.017,      15,
				0.063,            0,      -1
	endif
	if iLaptopType = 5 then
		prism_ 5, buttonThk,
				0.063,        0.018,      15,
				0.063,         0.07,      15,
				0,         0.07,      15,
				0,        0.018,      15,
				0.063,        0.018,      -1

		prism_ 5, buttonThk,
				0,            0,      15,
				0.031,            0,      15,
				0.031,        0.017,      15,
				0,        0.017,      15,
				0,            0,      -1

		prism_ 5, buttonThk,
				0.063,            0,      15,
				0.032,            0,      15,
				0.032,        0.017,      15,
				0.063,        0.017,      15,
				0.063,            0,      -1
	endif
	del 1

	return



	500:
	!!! Cutting for On/Off button and leds

	add buttonsXpos-0.001, ledsYpos-0.001, 0.1+topPlateThk-buttonThk
	mulz -1
	material bodyBlkMat
	cutform 5, 1, 2+16,
			0, 0, 1, 0.1,
			0,            0,      1+2+4+8+16,
			keyboardPlaceWidth,            0,      1+2+4+8+16,
			keyboardPlaceWidth,        ledsHeight+0.001*2,      1+2+4+8+16,
			0,       ledsHeight+0.001*2,      1+2+4+8+16,
			0,            0,      1+2+4+8+16
	del 2

	return



	600:
	!!! On/Off button and leds

	add buttonsXpos, ledsYpos, topPlateThk-buttonThk
	material butMat
	prism_ 5, buttonThk,
			0,            0,      15,
			0.031,            0,      15,
			0.031,        ledsHeight,      15,
			0,       ledsHeight,      15,
			0,            0,      -1

	prism_ 5, buttonThk,
			0.032,            0,      15,
			keyboardPlaceWidth-0.001*2,            0,      15,
			keyboardPlaceWidth-0.001*2,        ledsHeight,      15,
			0.032,       ledsHeight,      15,
			0.032,            0,      -1
	del 1

	return



	700:
	!!! Cutting for Screen

	add (a-screenSizeX)/2, screenYpos, -0.1+topPlateThk+topPlateRad
	material bodyMetMat
	cutform 5, 1, 2+16,
			0, 0, 1, 0.1,
			0, 0,						1+2+4+8+16,
			screenSizeX, 0,				1+2+4+8+16,
			screenSizeX, screenSizeY,	1+2+4+8+16,
			0, screenSizeY,				1+2+4+8+16,
			0, 0,						1+2+4+8+16
	del 1

	return



	800:
	!!! Screen

	add (a-screenSizeX)/2, screenYpos, topPlateRad
	if pic then
		if gs_picture_custom and gs_picture_name <> "" then
			material "custDesktop"
		else
			material "pcDesktop"
		endif
	else
		material screenMat
	endif
	prism_ 5, topPlateRad,
			0, 0,						15,
			screenSizeX, 0,				15,
			screenSizeX, screenSizeY,	15,
			0, screenSizeY,				15,
			0, 0,						-1
	del 1
	base
	muly -1
	add (a-screenSizeX)/2, -screenYpos, 0
	vert 0, 0, 0
	vert 1, 0, 0
	vert 0, 1, 0
	vert 0, 0, 1
	coor 2+256, -1, -2, -3, -4
	body 1
	del 2

	return



	900:
	!!! Cutting for Hinges

	addx hingeXpos
	for hg=1 to 2
		cutpolya 4, 1, 0,
			0, b-0.01,				15,
			hingeWidth, b-0.01,		15,
			hingeWidth, 1,	15,
			0, 1,			15

	addx a-hingeXpos*2-hingeWidth
	mulx 1
	next hg
	del 4
	del 1

	return

endif






