
eps = 0.0001

bodyRad				= 0.01

frontPanelRad		= 0.01
frontPanelThk		= 0.02
frontPanelOffset	= 0.01


rackHeight			= 0.042
rackWidth			= 0.145
rackOffset			= 0.003
rackThk				= 0.004

frontpanelCutW		= rackWidth+rackOffset*2
frontpanelCutH		= rackHeight*4+rackOffset*5
frontpanelCutYpos	= zzyzx-0.025


!! Front Panel Cutting Slices

cutSliceWidth		= 0.004
