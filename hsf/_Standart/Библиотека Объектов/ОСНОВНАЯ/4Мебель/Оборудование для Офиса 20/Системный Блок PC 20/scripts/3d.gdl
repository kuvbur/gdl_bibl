
IF gs_shadow=0 THEN SHADOW OFF

IF gs_detlevel_3D_m = 0 THEN END

resol gs_resol * (gs_detlevel_3D_m = 2) + 8 * (gs_detlevel_3D_m = 1)
PEN gs_cont_pen

add a/2, b, 0
rotx 90

!!! Front Panel
add 0, frontpanelCutYpos-rackOffset, b-frontPanelThk
for ra=1 to 4
	material box_mat
	prism_ 4, rackThk/2,
		-rackWidth/2, 0, 15,
		rackWidth/2, 0, 15,
		rackWidth/2, -rackHeight, 15,
		-rackWidth/2, -rackHeight, 15
	if ra=1 then
		addz rackThk/2
		prism_ 4, rackThk/2,
			-rackWidth/2+rackOffset, -rackOffset, 15,
			rackWidth/2-rackOffset, -rackOffset, 15,
			rackWidth/2-rackOffset, -rackHeight/2, 15,
			-rackWidth/2+rackOffset, -rackHeight/2, 15
		prism_ 4, rackThk/2,
			rackWidth/2-rackOffset-0.02, -rackHeight+rackOffset+0.005, 15,
			rackWidth/2-rackOffset, -rackHeight+rackOffset+0.005, 15,
			rackWidth/2-rackOffset, -rackHeight+rackOffset, 15,
			rackWidth/2-rackOffset-0.02, -rackHeight+rackOffset, 15
		del 1
	endif
	if ra=3 then
		addz rackThk/2
		prism_ 4, rackThk/2,
			-rackWidth/2+rackOffset+0.02, -rackOffset*2, 15,
			rackWidth/2-rackOffset-0.02, -rackOffset*2, 15,
			rackWidth/2-rackOffset-0.02, -rackHeight+rackOffset*2, 15,
			-rackWidth/2+rackOffset+0.02, -rackHeight+rackOffset*2, 15
		del 1
	endif

addy -rackHeight-rackOffset
next ra
del 4
del 1



rotx 90
resol 512 * (gs_detlevel_3D_m = 2) + 32 * (gs_detlevel_3D_m = 1)
cutpolya 6, 1, 0,
	0, -rad+b, 979,
	-a/2-frontPanelOffset, b-curvedCutDepth, 79,
	0, b, 3079,
	a/2+frontPanelOffset, b-curvedCutDepth, 3079,
	a/2+frontPanelOffset, b*2, 15,
	-a/2-frontPanelOffset, b*2, 15
del 1
resol gs_resol * (gs_detlevel_3D_m = 2) + 8 * (gs_detlevel_3D_m = 1)



add 0, frontpanelCutYpos-frontpanelCutH-(zzyzx-frontpanelCutYpos+frontPanelOffset), b-frontPanelThk
material mat2
prism_ 4, frontPanelThk,
	-a/2-frontPanelOffset, 0, 15,
	a/2+frontPanelOffset, 0, 15,
	a/2+frontPanelOffset, -rackHeight, 15,
	-a/2-frontPanelOffset, -rackHeight, 15
del 1



addy frontpanelCutYpos
cutpolya 4, 1, 0,
	-frontpanelCutW/2, 0, 15,
	frontpanelCutW/2, 0, 15,
	frontpanelCutW/2, -frontpanelCutH, 15,
	-frontpanelCutW/2, -frontpanelCutH, 15
del 1
addy frontpanelCutYpos-frontpanelCutH-(zzyzx-frontpanelCutYpos+frontPanelOffset)
cutpolya 4, 1, 0,
	-frontpanelCutW, 0, 15,
	frontpanelCutW, 0, 15,
	frontpanelCutW, -rackHeight, 15,
	-frontpanelCutW, -rackHeight, 15
del 1



add -a/2-frontPanelOffset+frontPanelRad/2+cutSliceWidth/2, 0, b-cutSliceDepth
for slc=1 to cutSliceNum
	cutform 4, 1, 1+16,
		0, 0, -1, 0,
		-cutSliceWidth/2, frontPanelRad/2, 1+2+4+8+16,
		cutSliceWidth/2, frontPanelRad/2, 1+2+4+8+16,
		cutSliceWidth/2, frontpanelCutYpos-frontpanelCutH-(zzyzx-frontpanelCutYpos+frontPanelOffset)-rackHeight, 1+2+4+8+16,
		-cutSliceWidth/2, frontpanelCutYpos-frontpanelCutH-(zzyzx-frontpanelCutYpos+frontPanelOffset)-rackHeight, 1+2+4+8+16
addx cutSliceWidth+cutSliceDist
next slc
del cutSliceNum
del 1








addz b-frontPanelThk
material box_mat
prism_ 12, frontPanelThk,
	-a/2-frontPanelOffset, frontPanelRad, 79,
	0, -1, 800,
	-a/2-frontPanelOffset+frontPanelRad, 0, 1079,
	a/2+frontPanelOffset-frontPanelRad, 0, 79,
	1, 0, 800,
	a/2+frontPanelOffset, frontPanelRad, 1079,
	a/2+frontPanelOffset, zzyzx+frontPanelOffset-frontPanelRad, 79,
	0, 1, 800,
	a/2+frontPanelOffset-frontPanelRad, zzyzx+frontPanelOffset, 1079,
	-a/2-frontPanelOffset+frontPanelRad, zzyzx+frontPanelOffset, 79,
	-1, 0, 800,
	-a/2-frontPanelOffset, zzyzx+frontPanelOffset-frontPanelRad, 1079
del 1

for slc=1 to cutSliceNum
	cutend
next slc

cutend
cutend

cutend

!!! End Of Front Panel



!!! Body
material box_mat
prism_ 12, b-frontPanelThk,
	-a/2, bodyRad, 79,
	0, -1, 800,
	-a/2+bodyRad, 0, 1079,
	a/2-bodyRad, 0, 79,
	1, 0, 800,
	a/2, bodyRad, 1079,
	a/2, zzyzx-bodyRad, 79,
	0, 1, 800,
	a/2-bodyRad, zzyzx, 1079,
	-a/2+bodyRad, zzyzx, 79,
	-1, 0, 800,
	-a/2, zzyzx-bodyRad, 1079
!!! End Of Body

del 2

end





