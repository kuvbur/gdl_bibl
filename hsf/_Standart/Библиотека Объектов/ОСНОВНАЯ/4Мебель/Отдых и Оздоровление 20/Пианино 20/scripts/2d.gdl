
!!Pianino

PEN gs_cont_pen
FILL gs_fill_type

HOTSPOT2 0,B
HOTSPOT2 A/2,B
HOTSPOT2 A/2,B/2

CALL "Resize_A_B_ZZYZX" Parameters ALL

! Keyboard
ADD2 topOvhg,0

POLY2_B 5, 1+2+4+64,gs_fill_pen, gs_back_pen,
		0, 0, 1,
		A-2*topOvhg, 0, 1,
		A-2*topOvhg, keyboardDepth-topOvhg, 1,
		0, keyboardDepth-topOvhg, 1,
		0, 0, -1
del 1

! Instrument box
ADD2 0, keyboardDepth-topOvhg
POLY2_B 5, 1+2+4+64,gs_fill_pen, gs_back_pen,
		0, 0, 1,
		0, B-keyboardDepth+topOvhg, 1,
		A, B-keyboardDepth+topOvhg, 1,
		A, 0, 1,
		0, 0, -1

del 1

! Minimal space
drawindex 20
call "fa_minimalspace" parameters all MSType = 5, unID = 3000, diff = keyboardDepth-topOvhg, diff2 = topOvhg
