
targetGUID = TO_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

gosub "newparameters_BWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid targetGuid

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! newparameters
! ==============================================================================
"newparameters_BWM":
	if targetGUID # "" then
	
		newparameter "Openn", "Boolean"
		
		! If Music Stand Position is Opened, then Opened is turned on.

		if iStandPos = 1 then	! Opened
			Openn = 1
		else					! Closed
			Openn = 0
		endif

		parameters	Openn = Openn

	endif
return

