CALL "Resize_A_B_ZZYZX" Parameters ALL

!!! *** Hotspots

PEN gs_cont_pen
RESOL gs_resol
material womat

mulz zzyzx/0.8

add a/2, b/2, 0.5

! Bottom
prism_     17,     0.01,
		-pocketCornerPosX, B/2, 9,
		pocketCornerPosX, B/2, 9,
		pocketCornerPosX, pocketCornerPosY, 901,
		0, -90, 4009,
		A/2, pocketCornerPosY, 9,
		A/2, -pocketCornerPosY, 9,
		pocketCornerPosX, -pocketCornerPosY, 901,
		0, -90, 4009,
		pocketCornerPosX, -B/2, 9,
		-pocketCornerPosX, -B/2, 9,
		-pocketCornerPosX, -pocketCornerPosY, 901,
		0, -90, 4009,
		-A/2, -pocketCornerPosY, 9,
		-A/2, pocketCornerPosY, 9,
		-pocketCornerPosX, pocketCornerPosY, 901,
		0, -90, 4009,
		-pocketCornerPosX, B/2, -1
del 1

! Legs

mulx a/1.5132
addy B/6.5
gosub "legs"
del 1

addy B-B/6.5+0.24
gosub "legs"
del 1

del 1

addz 0.5

material plmat
add A/2, B/2, 0
!Corner pockets
addz 0.01
for i = 1 to 2
	for j = 1 to 2
		add pocketCornerPosX, pocketCornerPosY, 0
		gosub "pocket"
		del 1
		mul 1, -1, 1
	next j
	del j-1
	mul -1, 1, 1
next i
del i-1

!Middle pockets

for i = 1 to 2
	prism_      9,      0.29,
			pocketMiddlePosX, pocketRadius, 13,
			pocketMiddlePosX, railWidth-pocketRadius-EPS, 13,
			pocketMiddlePosX, 0, 900,
			0, -180, 4013,
			pocketMiddlePosX, -(railWidth-pocketRadius-EPS), 13,
			pocketMiddlePosX, -pocketRadius, 13,
			pocketMiddlePosX, 0, 900,
			0, 180, 4013,
			pocketMiddlePosX, pocketRadius, -1

	mul -1, 1, 1
next i
del i-1
del 1

material womat
!Rails
addz 0.01
for i = 1 to 2
	prism_      5,      0.29,
				surfaceWidth/2-pocketString, surfaceLength/2+cushionWidth, 14,
				surfaceWidth/2-pocketString, surfaceLength/2+railWidth, 14,
				-(surfaceWidth/2-pocketString), surfaceLength/2+railWidth, 14,
				-(surfaceWidth/2-pocketString), surfaceLength/2+cushionWidth, 14,
				surfaceWidth/2-pocketString, surfaceLength/2+cushionWidth, -1
	mul 1, -1, 1
next i
del i-1

for i = 1 to 2
	cutpolya 5, 1, 0,
			pocketMiddlePosX, railWidth-pocketRadius-EPS, 12,
			pocketMiddlePosX, 0, 900,
			0, -180, 4010,
			pocketMiddlePosX, -(railWidth-pocketRadius-EPS), 12,
			pocketMiddlePosX, railWidth-pocketRadius-EPS, -1

	prism_      5,      0.29,
				surfaceWidth/2+cushionWidth,	surfaceLength/2-pocketString, 14,
				surfaceWidth/2+railWidth,		surfaceLength/2-pocketString, 14,
				surfaceWidth/2+railWidth,		-(surfaceLength/2-pocketString), 14,
				surfaceWidth/2+cushionWidth,	-(surfaceLength/2-pocketString), 14,
				surfaceWidth/2+cushionWidth,	surfaceLength/2-pocketString, -1

	mul -1, 1, 1
next i
del i-1
del 1
!Play Area
material fabmat
addz 0.23
for i = 1 to 2
	for j = 1 to 2
		add pocketCornerPosX, pocketCornerPosY, 0
		cutpolya 7, 1, 0,
			-pocketRadius, 0, 13,
			0, 0, 900,
			0, 180, 4013,
			pocketRadius, 0, 13,
			0, 0, 900,
			0, 180, 4013,
			-pocketRadius, 0, -1
		del 1
		mul 1, -1, 1
	next j
	del j-1
	add pocketMiddlePosX, 0, 0
	cutpolya 7, 1, 0,
		-pocketRadius, 0, 13,
		0, 0, 900,
		0, 180, 4013,
		pocketRadius, 0, 13,
		0, 0, 900,
		0, 180, 4013,
		-pocketRadius, 0, -1
	del 1
	mul -1, 1, 1
next i
del i-1

