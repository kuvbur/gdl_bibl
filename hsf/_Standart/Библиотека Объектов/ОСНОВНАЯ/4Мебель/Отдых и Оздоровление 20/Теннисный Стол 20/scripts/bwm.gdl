
targetGUID = TO_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

gosub "EnablePlayAreaDimesions_BWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid targetGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! EnablePlayAreaDimesions
! ==============================================================================
"EnablePlayAreaDimesions_BWM":
	if targetGUID # "" then
		if abs(areaLength - 12.2) > EPS | abs(areaWidth - 6.1) > EPS then
			targetGUID = ""
		endif
	endif
return

