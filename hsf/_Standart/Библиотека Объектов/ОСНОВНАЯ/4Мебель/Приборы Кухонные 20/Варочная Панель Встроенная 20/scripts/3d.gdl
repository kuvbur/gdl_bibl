hotspot a+rgtmod, 0, 0, unID, hCountertop, 1+128 : unID=unID+1
hotspot a+rgtmod, 0, -1, unID, hCountertop, 3 : unID=unID+1
hotspot a+rgtmod, 0, hCountertop, unID, hCountertop, 2 : unID=unID+1

hotspot -lftmod, 0, 0, unID, hCountertop, 1+128 : unID=unID+1
hotspot -lftmod, 0, -1, unID, hCountertop, 3 : unID=unID+1
