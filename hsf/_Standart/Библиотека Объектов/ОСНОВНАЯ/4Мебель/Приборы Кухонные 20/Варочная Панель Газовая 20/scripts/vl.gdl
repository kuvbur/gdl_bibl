
call "FM_types" parameters all

values{2} "gs_detlevel_3D_m" 	2, `Детальный`,
								1, `Простой`,
								0, `Откл.`
								
values{2} "iCookTopType" 	COOKTOPTYPE_1, `Варочная Панель 1`,
							COOKTOPTYPE_2, `Варочная Панель 2`,
							COOKTOPTYPE_3, `Варочная Панель 3`

values{2} "iShapeType" 	SHAPETYPE_RECT, `Прямоугольник`,
						SHAPETYPE_OVAL, `Овал`
VALUES "gs_resol" RANGE [3,)

lock "ZZYZX"
hideparameter all "A", "B", "ZZYZX"

parameters ac_bottomlevel = hCountertop - cookh
if iCookTopType = COOKTOPTYPE_1 then
	parameters ZZYZX = cookh + .25
	parameters ac_toplevel = hCountertop + 0.25
endif
if iCookTopType = COOKTOPTYPE_2 then
	parameters ZZYZX = cookh + .028
	parameters ac_toplevel = hCountertop + 0.028
endif
if iCookTopType = COOKTOPTYPE_3 then
	parameters ZZYZX = cookh + .034
	parameters ac_toplevel = hCountertop + 0.034
endif

if iCookTopType <> COOKTOPTYPE_1 then lock "bps"

if iCookTopType <> COOKTOPTYPE_2 then
	lock	"iShapeType"
	hideparameter	"iShapeType"
endif

if iCookTopType <> COOKTOPTYPE_3 then
	LOCK "p_mat"
	HIDEPARAMETER "p_mat"
endif

if gs_cont_pen=0 then PARAMETERS gs_cont_pen=4
if gs_fill_pen=0 then PARAMETERS gs_fill_pen=19

if gs_detlevel_3D_m = 1 then HIDEPARAMETER "gs_resol"
if gs_detlevel_3D_m = 0 then HIDEPARAMETER "gs_resol", "gs_shadow"

if GLOB_MODPAR_NAME="iCookTopType" then
	if LibraryLangCode = "USA" then
		if iCookTopType = COOKTOPTYPE_1 then
			PARAMETERS A=48" , B=24", cookh=6 1/2"
		endif
		if iCookTopType = COOKTOPTYPE_2 then
			PARAMETERS A=36" , B=21", cookh=3 1/2"
		endif
		if iCookTopType = COOKTOPTYPE_3 then
			PARAMETERS A=36" , B=21", cookh=3 1/2"
		endif
	else
		if iCookTopType = COOKTOPTYPE_1 then
			PARAMETERS A=1.5 , B=.9, cookh=0.15
		endif
		if iCookTopType = COOKTOPTYPE_2 then
			PARAMETERS A=.9 , B=.540
		endif
		if iCookTopType = COOKTOPTYPE_3 then
			PARAMETERS A=.88 , B=.5, cookh=0.08
		endif
	endif
endif

! =============================================================================
! Onorm list Settings
! =============================================================================
if LibraryLangCode = "AUT" or LibraryLangCode = "CHE" or LibraryLangCode = "GER" then
	call "Onorm_Storage" parameters all
else
	hideparameter "gs_onorm_Title"
endif
