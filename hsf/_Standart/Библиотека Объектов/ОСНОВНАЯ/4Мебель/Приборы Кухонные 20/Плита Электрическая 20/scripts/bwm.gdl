! ==============================================================================
! New Parameters_iCooktopType
! ==============================================================================
"newparameters_BWM":
	if targetGUID # "" then
		newparameter "CooktopType",	"String"
		newparameter "CooktopType_m",	"Integer"

		if iCooktopType = 1 then
			CooktopType_m = 1
			CooktopType = `Тип 1`
		else
			if iCooktopType = 2 then
				CooktopType_m = 2
				CooktopType = `Тип 2`
			else
				if iCooktopType = 5 then
					CooktopType_m = 5
					CooktopType = `Тип 3`
				else
					if iCooktopType = 6 then
						CooktopType_m = 6
						CooktopType = `Тип 4`
					else
						if iCooktopType = 7 then
							CooktopType_m = 7
							CooktopType = `Тип 5`
						else
							if iCooktopType = 10 then
								CooktopType_m = 10
								CooktopType = `Тип 6`
							else
								if iCooktopType = 11 then
									CooktopType_m = 11
									CooktopType = `Тип 7`
								else						! `Type 8`
									CooktopType_m = 12
									CooktopType = `Тип 8`
								endif
							endif
						endif
					endif
				endif
			endif
		endif
		
		parameters 	CooktopType_m = CooktopType_m,
					CooktopType = CooktopType
		
	endif
return

! ==============================================================================
! FontSizeParamType
! ==============================================================================
"FontSizeParamType_BWM":
	if targetGUID # "" then
		newparameter "fsz", "RealNum"
		newparameter "fsz_point", "RealNum"
		
		fsz = fontSize * 100
		fsz_point = fontSize * 7200 / 25.4
		
		parameters 	fsz = fsz,
					fsz_point = fsz_point
	endif
return

