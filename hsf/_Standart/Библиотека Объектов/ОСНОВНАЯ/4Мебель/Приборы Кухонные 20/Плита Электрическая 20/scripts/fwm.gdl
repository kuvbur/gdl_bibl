! ==============================================================================
! FontSizeParamType
! ==============================================================================
"FontSizeParamType_FWM":
	if actualGuid = _startID | actualGuid = _autoID then
		bHas = DELETED_PAR_VALUE("fsz", _fsz)
		
		fontSize = _fsz / 100
		parameters fontSize = fontSize
		
		actualGuid = _endID
	endif
return

! ==============================================================================
! New Parameters_iCooktopType
! ==============================================================================
"newparameters_FWM":
	if actualGuid = _startID | actualGuid = _autoID then

		bHas = 	DELETED_PAR_VALUE ("CooktopType_m", _CooktopType_m) |\
				STORED_PAR_VALUE ("A", _A)
				
		if _CooktopType_m = 1 then iCooktopType = 1		! 2 Hotplates (1)
		if _CooktopType_m = 2 then iCooktopType = 2		! 2 Hotplates (2)
		if _CooktopType_m = 5 then iCooktopType = 5		! 4 Hotplates (1)
		if _CooktopType_m = 6 then iCooktopType = 6		! 4 Hotplates (2)
		if _CooktopType_m = 7 then iCooktopType = 7		! 4 Hotplates (3)
		if _CooktopType_m = 10 then						! 5 Hotplates (1)
			iCooktopType = 5
			AA = _A
		endif
		if _CooktopType_m = 11 then iCooktopType = 11	! 5 Hotplates (2)
		if _CooktopType_m = 12 then iCooktopType = 12	! 4 Hotplates (4)
		
			
		parameters 	iCooktopType = iCooktopType,
					AA = AA
			
		actualGuid = _endID
	endif
return

