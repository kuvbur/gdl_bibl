! ==============================================================================
! AddingGlazedStyle
! ==============================================================================
"AddingGlazedStyle_FWM":
	if actualGuid = _startID | actualGuid = _autoID then

		iStyle = STYLE_PANEL
		_ControlPanel = 0
		bHas = DELETED_PAR_VALUE ("ControlPanel", _ControlPanel)
		bControlPanel = _ControlPanel

		parameters bControlPanel = bControlPanel

		! The drawing of the main body of the machine is different a little bit so when there is no controlpanel
		! the zzyzx should decrease

		if bControlPanel then
			appl_height = appl_height - 0.2
		endif

		parameters	iStyle 			= iStyle,
					appl_height 	= appl_height

		actualGuid = _endID
	endif
return

! ==============================================================================
! Make the Font Size parameter a Length Type parameter
! ==============================================================================
"FontSizeParamType_FWM":
	if actualGuid = _startID then
		bHas = DELETED_PAR_VALUE("fsz", _fsz)
		
		fontSize = _fsz / 100
		parameters fontSize = fontSize
		
		actualGuid = _endID
	endif
return
