	_autoID		= "5350F9A1-99D3-477F-8350-CB500AB5766B"
	_startID	= "AFAFD39F-FC1B-4432-8F87-3B437CDF20F5"
	_endID		= "82831E75-BBD7-453D-A92F-BFC19BFBE56D"
gosub "newBooleansToeHandle_FWM"

	_startID	= "82831E75-BBD7-453D-A92F-BFC19BFBE56D"
	_endID		= "92F31592-33BF-4933-8442-21E4DF9BEB4D"
gosub "FontSizeParamType_FWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid actualGUID

