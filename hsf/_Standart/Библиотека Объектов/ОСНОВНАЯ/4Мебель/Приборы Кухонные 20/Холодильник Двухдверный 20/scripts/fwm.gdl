! ==============================================================================
! newBooleansToeHandle
! ==============================================================================
"newBooleansToeHandle_FWM":
	if actualGuid = _startID | actualGuid = _autoID then

		! If toe_h was 0, that equals with bToe being off, otherwise it is on.
		bHas = STORED_PAR_VALUE("toe_h", _toe_h)

		if abs(_toe_h) < EPS then
			bToe = 0
		else
			bToe = 1
		endif
		parameters bToe = bToe
		
		! The integer setting the handle style was renamed from gs_handle_st to iHandleType
		bSuccessful = DELETED_PAR_VALUE ("gs_handle_st", _gs_handle_st)
		if bSuccessful then
			parameters iHandleType = _gs_handle_st
		endif

		! Handle can be switched off now with bHandle boolean, but in default, it is on, so nothing to do.

		actualGuid = _endID
	endif
return

! ==============================================================================
! FontSizeParamType
! ==============================================================================
"FontSizeParamType_FWM":
	if actualGuid = _startID then
		bHas = DELETED_PAR_VALUE("fsz", _fsz)
		
		fontSize = _fsz / 100
		parameters fontSize = fontSize
		
		actualGuid = _endID
	endif
return

