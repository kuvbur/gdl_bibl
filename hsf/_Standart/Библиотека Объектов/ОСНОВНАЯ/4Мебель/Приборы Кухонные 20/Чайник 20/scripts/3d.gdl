
! Tea Kettle.gsm

pen gs_cont_pen

if gs_detlevel_3D_m = DLEV3D_OFF then end
if gs_detlevel_3D_m = DLEV3D_SIMPLE then gs_resol = 6

if gs_shadow=0 then SHADOW OFF


HOTSPOT 0,0,0
HOTSPOT H,0,0
HOTSPOT 0,H,0
HOTSPOT -H,0,0
HOTSPOT 0,-H,0

HOTSPOT -x1,0,z1
HOTSPOT H*1.25+H/8,0,0
HOTSPOT 0,0,H*1.25+H/8

Resol 2.5*gs_resol
Material kettle_mat
Ellips H, H
Roty -45
Addz H-H/32
Resol 1.5*gs_resol
Cone H/2, H/8, H/16, 90, 90
Material ac_mat
Addz H/2
Cone H/16, H/16, H/18, 90, 90
Del 3
Material ac_mat
Resol gs_resol
Arme H*1.26, H, H/32, H*0.99, 0
Addz H/32
Roty 90
Material ac_mat
	addy 0.004
	Arme H*1.26, H, H/32, H*0.99, -H/32
	del 1
Material handle_mat
Addx -H*1.25-H/32
Resol 3*gs_resol
Elbow H*1.25+H/16, 90, H/16
Addz -H/16
Material ac_mat
Cylind H/16, H/16
del 4
End
