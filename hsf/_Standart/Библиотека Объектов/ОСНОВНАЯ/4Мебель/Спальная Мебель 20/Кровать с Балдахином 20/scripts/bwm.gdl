
targetGUID = TO_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

gosub "repairboundingbox_BWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid targetGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! A, B size is bounding box instead of structure axis
! ==============================================================================
"repairboundingbox_BWM":
	if targetGUID # "" then

		A = resizeA
		B = resizeB

		parameters	A = A,
					B = B
	endif
return
