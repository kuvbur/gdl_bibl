
actualGUID = FROM_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

	_autoID		= "465A8B8C-03FE-4348-A542-2114F295BDA4"
	_startID	= "D3DB32C7-DE4C-48D5-A516-9030B01BAA3C"
	_endID		= "9BB66A60-3B15-4D7C-A761-D24C6ED83B96"
gosub "bDrawer_FWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid actualGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! bDrawer
! ==============================================================================
"bDrawer_FWM":
	if actualGuid = _startID | actualGuid = _autoID then
		bHas = STORED_PAR_VALUE ("iNightTableType", _iNightTableType)
		if bHas then
			if _iNightTableType = 1 then	! Multi Drawer
				_dnum = 1
				bHas = STORED_PAR_VALUE ("dnum", _dnum)
				if _dnum = 0 then
					bDrawer = 0
					parameters bDrawer = bDrawer
				endif
			else							! Open
				bHas = STORED_PAR_VALUE ("iNightTableDrawerPos", _iNightTableDrawerPos)
				if bHas then
					if _iNightTableDrawerPos = 0 then	! None
						bDrawer = 0
						parameters bDrawer = bDrawer
					endif
				endif
			endif
		endif
		actualGuid = _endID
	endif
return

