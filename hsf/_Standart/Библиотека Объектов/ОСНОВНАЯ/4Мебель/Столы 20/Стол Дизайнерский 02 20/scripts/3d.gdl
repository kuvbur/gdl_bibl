
!! Design Table 02.gsm

IF not(gs_shadow) THEN SHADOW OFF

PEN gs_cont_pen

IF gs_detlevel_3D_m = DLEV3D_OFF THEN END
IF gs_detlevel_3D_m = DLEV3D_SIMPLE THEN gs_resol = 8

RESOL gs_resol
MUL a/1,a/1,1

ROTZ 45
ROTX 90

MULY (zzyzx-0.012)/0.495
MATERIAL gs_leg_mat
FOR n=0 TO 3
	ROTY n*90
	ADDZ -0.01
	PRISM_     20,      0.02,
		-0.499,     0.495,      15,
		-0.499,     0.0,      15,
		-0.454,     0.0,      15,
		-0.454,     0.215,      13,
		-0.439,     0.215,    900,
		0.0,     -90.0,   4001,
		-0.439,     0.23,      13,
		-0.028,     0.23,      0,
		-0.028,     0.275,      13,
		-0.439,     0.275,      13,
		-0.439,     0.29,    900,
		0.0,     -90.0,   4001,
		-0.454,     0.29,      13,
		-0.454,     0.435,      13,
		-0.439,     0.435,    900,
		0.0,     -90.0,   4001,
		-0.439,     0.45,      13,
		-0.028,     0.45,      0,
		-0.028,     0.495,      13,
		-0.499,     0.495,     -1

	gosub 3000
	DEL 2
NEXT n

DEL 2

MULZ (zzyzx-0.012)/0.495
ADDZ 0.45
FOR n=1 TO 2
PRISM_      17,      0.045,
			0.01,    -0.028,      0,
		   -0.01,    -0.028,      13,
		   -0.028,    -0.028,    900,
			0.0,      90.0,   4001,
		   -0.028,    -0.01,      0,
		   -0.028,     0.01,      13,
		   -0.028,     0.028,    900,
			0.0,      90.0,   4001,
		   -0.01,     0.028,      0,
			0.01,     0.028,      13,
			0.028,     0.028,    900,
			0.0,      90.0,   4001,
			0.028,     0.01,      0,
			0.028,    -0.01,      13,
			0.028,    -0.028,    900,
			0.0,      90.0,   4001,
			0.01,    -0.028,     -1
ADDZ -0.22
NEXT n
DEL 2
DEL 1

DEL 1
ADDZ zzyzx-.012

RESOL 2*gs_resol
MATERIAL gs_frame_mat
CYLIND 0.012,0.5

ADDZ 0.018

ROTY -90

REVOLVE      11,      360,  1+2+4+8+16+32,
			0.0185,     0.495027,      1,
			0.018513,     0.497513,    901,
			0,     180.6205,   4001,
			0.0185,     0.5,      1,
			0.0,     0.5,      1,
			0.0,     0.44358,      1,
			0.001,     0.44358,      1,
			0.001,     0.477623,      1,
			0.018452,     0.477575,    901,
			0.0,     -90.0008,   4001,
			0.0185,     0.495027,     1


DEL 4

MATERIAL gs_top_mat
ADDZ zzyzx
CYLIND 0.006,0.465

DEL 1


END

3000:
rotz 90
gosub 5000
del 1
return


5000:

vert 0, 0, 0
vert 1, 0, 0
vert 0, 1, 0
vert 0, 0, 1

coor 2, -1, -2, -3, -4

base

body -1

return



