
unID=1
call "fa_minimalspace" parameters all MSType = 4, unID = 3000

pen gs_cont_pen
FILL gs_fill_type
IF iDet2D = DETLEVEL_2D_DETAILED THEN

	ADD2 a/2,b/2
	mul2 a/1.064, b/0.66

	rect2 -0.4812, -0.2792, 0.4812, 0.2792

	POLY2_B       9, 1+2+4+64,     gs_fill_pen, gs_back_pen,
		   -0.4812,    -0.33,      1,
		   -0.532,    -0.2792,      1,
		   -0.532,     0.2792,      1,
		   -0.4812,     0.33,      1,
			0.4812,     0.33,      1,
			0.532,     0.2792,      1,
			0.532,    -0.2792,      1,
			0.4812,    -0.33,      1,
		   -0.4812,    -0.33,     -1

	del 2

else

	POLY2_B 5, 1+2+64, gs_fill_pen, gs_back_pen,
			0, 0,1,
			a, 0,1,
			a, b,1,
			0, b,1,
			0, 0,-1

ENDIF


HOTLINE2 0,0,A,0
HOTLINE2 A,0,A,B
HOTLINE2 A,B,0,B
HOTLINE2 0,B,0,0


unID=1
HOTSPOT2 A/2,B/2, unID: unID=unID+1

HOTSPOT2 0,B/2, unID: unID=unID+1
HOTSPOT2 A,B/2, unID: unID=unID+1
