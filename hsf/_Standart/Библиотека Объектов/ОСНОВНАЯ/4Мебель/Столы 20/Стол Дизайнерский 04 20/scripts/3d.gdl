
!! Design Table 04.gsm

IF not(gs_shadow) THEN SHADOW OFF


pen gs_cont_pen


nos=abs(nos)
bss=abs(bss)
deth=abs(deth)
tsh=abs(tsh)


	define material	"builtInMatBlackPlastic" 0,
	  0.333333,   0.313725,   0.294118,
	 0.15,  0.15,  0.45,     0,      5,     0,
	  0.784314,   0.784314,   0.784314,
	         0,          0,          0,     0

IF gs_detlevel_3D_m = DLEV3D_OFF THEN END
IF gs_detlevel_3D_m = DLEV3D_SIMPLE THEN gs_resol = 4


MATERIAL cvmat
a1=a-nos*2
b1=b-nos*2
ADD nos,nos,bss
a0=a1
b0=b1
GOSUB 10
ADDZ zzyzx-bss-tsh-deth
GOSUB 10
DEL 1
ADD deth,0,deth
ROTY -90
a0=zzyzx-bss-tsh-deth*2
GOSUB 10
ADDZ deth-a1
GOSUB 10
DEL 2
ADDY (b1-deth)/2
BLOCK a1-deth*2,deth,a0
DEL 1
FOR xx=1 TO numc-1
ADD (a1-deth)/numc*xx-deth,deth,0
BLOCK deth,(b1-deth)/2-deth,a0
ADDY (b1-deth)/2
BLOCK deth,(b1-deth)/2-deth,a0
DEL 2
NEXT xx
DEL 2

	ADD nos+deth*2,nos+deth*2,zzyzx-tsh
	MATERIAL gs_leg_mat
	RESOL gs_resol
	CYLIND tsh-deth/2,deth*0.75
	ADDX a1-deth*4
	CYLIND tsh-deth/2,deth*0.75
	ADDY b1-deth*4
	CYLIND tsh-deth/2,deth*0.75
	ADDX deth*4-a1
	CYLIND tsh-deth/2,deth*0.75
	DEL 4

MATERIAL gs_top_mat
RESOL 2*gs_resol
ADDZ zzyzx-deth/2
BLOCK a,b,deth/2
DEL 1
ADD nos+deth*2,nos+deth*2,bss
GOSUB 20
addx a1-deth*4
GOSUB 20
addy b1-deth*4
GOSUB 20
addx deth*4-a1
GOSUB 20
DEL 4
END

10:
CPRISM_ cvmat,cvmat,trmat,5,deth,0,0,15,a0,0,15,
a0,b0,15,0,b0,15,0,0,-1
RETURN

20:

	ROTZ rnd(360)
	ADD -deth/2,-deth*2,0

		MATERIAL  gs_leg_mat
		RECT deth,deth*3


		ROTY 90
			POLY 4,0,0,0,deth*3,bss*0.65,deth*3,bss*0.65,deth*2
		ADDZ deth
			POLY 4,0,0,0,deth*3,bss*0.65,deth*3,bss*0.65,deth*2
		DEL 3

		ADDZ -bss*0.6
		ROTY 90
		ADDZ -deth*0.3
			MATERIAL "builtInMatBlackPlastic"
			CYLIND deth*0.6,bss*0.3
		ADDZ -deth*0.1

		MATERIAL  gs_leg_mat
			PRISM_ 4,deth*0.8,0,0,979,bss*0.4,360,4079,
			 0,0,979,bss*0.3,360,4079

	DEL 5
RETURN
