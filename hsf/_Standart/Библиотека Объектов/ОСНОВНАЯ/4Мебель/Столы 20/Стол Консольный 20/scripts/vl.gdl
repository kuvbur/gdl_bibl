
call "FM_types" parameters all


values{2} "gs_detlevel_3D_m" 	DLEV3D_DETAILED, `Детальный`,
								DLEV3D_SIMPLE, `Простой`,
								DLEV3D_OFF, `Откл.`

values "gs_resol" RANGE [3,)
values "top_type" `Прямоугольный`,`Эллиптическая`
values "top_type_m" 1,2

if GLOB_MODPAR_NAME = "top_type" then
	top_type_m = 1
	if top_type = `Эллиптическая` then top_type_m = 2
	parameters top_type_m = top_type_m
else
	top_type = `Прямоугольный`
	if top_type_m = 2 then top_type = `Эллиптическая`
	parameters top_type = top_type
endif

IF gs_cont_pen<=0 THEN PARAMETERS gs_cont_pen=4
IF gs_fill_pen<=0 THEN PARAMETERS gs_fill_pen=19

if tableTopUp2D then HIDEPARAMETER "dashedLineType"
IF gs_detlevel_3D_m = DLEV3D_SIMPLE THEN HIDEPARAMETER "gs_resol"
IF gs_detlevel_3D_m = DLEV3D_OFF THEN HIDEPARAMETER "gs_resol", "gs_shadow"


values "A" range [0.3, )
values "B" range [0.3, )

! Minimal Space

VALUES "MSFront" RANGE [0,)
VALUES "MSSide" RANGE [0,)
