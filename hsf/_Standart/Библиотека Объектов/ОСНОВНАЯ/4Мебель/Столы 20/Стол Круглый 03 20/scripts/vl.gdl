
call "FM_types" parameters all


values "gs_detlevel_3D" `Детальный`,`Простой`,`Откл.`
values "gs_detlevel_3D_m" 2, 1, 0
values "A" range [8*ls, )
values "B" range [8*ls, )

if GLOB_MODPAR_NAME = "gs_detlevel_3D" then
	if gs_detlevel_3D = `Детальный` then gs_detlevel_3D_m = 2
	if gs_detlevel_3D = `Простой` then gs_detlevel_3D_m = 1
	if gs_detlevel_3D = `Откл.` then gs_detlevel_3D_m = 0
	parameters gs_detlevel_3D_m = gs_detlevel_3D_m
else
	if gs_detlevel_3D_m = 2 then gs_detlevel_3D = `Детальный`
	if gs_detlevel_3D_m = 1 then gs_detlevel_3D = `Простой`
	if gs_detlevel_3D_m = 0 then gs_detlevel_3D = `Откл.`
	parameters gs_detlevel_3D = gs_detlevel_3D
endif


values "gs_resol" RANGE [3,)

IF gs_cont_pen=0 THEN PARAMETERS gs_cont_pen=4
IF gs_fill_pen=0 THEN PARAMETERS gs_fill_pen=19

IF gs_detlevel_3D_m = 1 THEN HIDEPARAMETER "gs_resol"
IF gs_detlevel_3D_m = 0 THEN HIDEPARAMETER "gs_resol", "gs_shadow"

values "ets" `Без Окантовки`,`Закругленный`,`С Фаской`
values "ets_m" 1, 2, 3
if GLOB_MODPAR_NAME = "ets" then
	if ets = `Без Окантовки` then ets_m = 1
	if ets = `Закругленный` then ets_m = 2
	if ets = `С Фаской` then ets_m = 3
	parameters ets_m = ets_m
else
	if ets_m = 1 then ets = `Плоский`
	if ets_m = 2 then ets = `Закругленный`
	if ets_m = 3 then ets = `С Фаской`
	parameters ets = ets
endif

if  ets = `Без Окантовки` then
	lock  "edmat"
endif



if GLOB_MODPAR_NAME = "temp_RR" then	! hidden parameter for hotspot editing
	A = temp_RR*2
	B = temp_RR*2
	parameters	A = A,
				B = B
else
	if GLOB_MODPAR_NAME = "B" then		! B parameter input
		A = B
		temp_RR = B/2
		parameters	A = A,
					temp_RR = temp_RR
	else								! A parameter input
		B = A
		temp_RR = A/2
		parameters	B = B,
					temp_RR = temp_RR
	endif
endif


!!! ******************************** Minimal Space
VALUES "MSFront" RANGE [0,)
