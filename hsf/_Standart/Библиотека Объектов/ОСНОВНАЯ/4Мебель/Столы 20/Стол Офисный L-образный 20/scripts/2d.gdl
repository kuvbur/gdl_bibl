
unID = 1

! ======================================================================
! Desks (with Front Panel, Desk Cabinet, Chair, Keyboard, LCD Monitor)
! ======================================================================

gosub "desk"



! ======================================================================
! Desk Partitions
! ======================================================================

for iPa = 1 to 4
	if existPartition[iPa] then
		partLengthType	= 1
		if iPa = 4 then anglePartititon	= 90
		if iPa = 2 then anglePartititon	= iDeskAngle
		if iPa = 1 then anglePartititon	= 0
		if iPa = 3 then anglePartititon	= 0
		gosub "partition"
	endif
next iPa

! ======================================================================
end
! ======================================================================



"desk":
	call "completeTable_m" parameters all	iTableTopType			= 1,
											bCalledAsExtension		= 0,
											bPartOfLayout			= 0,
											bPartOfWsSolo			= 0,
											bChair					= 0,
											bComputer 				= 0,
											bEnableDeskHsCorner		= 1,
											bEnableDeskHsHalfEdge	= 1,
											bEnableDeskHsEditingX	= 1,
											bEnableDeskHsEditingY	= 1,
											bEnableDeskHsEditingC	= 1,
											bMirroredCabPos			= 0,
											completeTable_unID		= unID,
											returned_parameters completeTable_unID
	unID = completeTable_unID
return

"partition":
	call "deskClosings_m" parameters all	iExtensionType			= 6,
											partitionFrameThk		= partitionFrameThk,
											bCalledAsExtension		= 0,
											bPartOfLayout			= 0,
											bPartOfWsSolo			= 0,
											bOnlyPartitionColumn	= 0,
											bOnlyPartitionCover		= 0,
											iPartitionLength		= 1,
											lengthPartition			= partLength[iPa],
											iPartitionLoc			= iPa,
											anglePartititon			= anglePartititon,
											xPartOrigPos			= partPos[iPa][1],
											yPartOrigPos			= partPos[iPa][2],
											partCornerCutTypeLeft	= partCornerCutFront1 * (iPa = 1) - partCornerCutFront1 * (iPa = 2),
											partCornerCutTypeRight	= 0,
											deskClosings_unID		= unID,
											returned_parameters deskClosings_unID
	unID = deskClosings_unID
return







