
!! Office Table Round.gsm

pen gs_cont_pen

if A < 0.2 then A = 0.2
if B < 0.2 then B = 0.2

! Minimal Space
call "fa_minimalspace" parameters 	A=A,B=B,
									MSFront = MSFront,
									MSType = 7,
									unID = 3000


unID = 1
hotspot2 0,0,unID :	unID=unID+1

for i=1 to 4
	hotspot2 0, 0, unID, temp_RR, 1+128, A:	unID=unID+1
	hotspot2 -1, 0, unID, temp_RR, 3+128, A: unID=unID+1
	hotspot2 temp_RR, 0, unID, temp_RR, 2, A: unID=unID+1

	rot2 90
next i
del 3

line_type 1

add2 -A/2, -B/2
call "fa_tabletop_macro" parameters A=A, B=B,
	gs_tabletop_style_m	= 9,
	gs_radius_1			= gs_radius_1,
	gs_radius_2			= gs_radius_2,
	gs_cont_pen			= gs_cont_pen,
	gs_fill_type		= gs_fill_type,
	gs_fill_pen			= gs_fill_pen,
	gs_back_pen			= gs_back_pen,
	unID				= unID
del 1

