1:
	R=A/20
	RR=A/42
	MATERIAL gs_frame_mat

	ROTY -90

	REVOLVE 8,360, 1,
			zzyzx/12,0,1,
			zzyzx/12,A/18,1,
			zzyzx/16,A/9,1,
			zzyzx/25,A/4.5,1,
			zzyzx/45,A/3,1,
			zzyzx/90, A/2.2,1,
			zzyzx/180,A/2,1,
			0,A/2,0
	DEL 1

	ADDZ zzyzx/12
	CYLIND zzyzx-gs_seat_thk-zzyzx/12,R
	DEL 1

	ARMC R,RR,(A/2-2*RR)/SIN(57),zzyzx-gs_seat_thk-(A/2-2*RR)/TAN(57)-(zzyzx-gs_seat_thk)/3,0,57
	ROTZ 120
	ARMC R,RR,(A/2-2*RR)/SIN(57),zzyzx-gs_seat_thk-(A/2-2*RR)/TAN(57)-(zzyzx-gs_seat_thk)/3,0,57
	ROTZ 120
	ARMC R,RR,(A/2-2*RR)/SIN(57),zzyzx-gs_seat_thk-(A/2-2*RR)/TAN(57)-(zzyzx-gs_seat_thk)/3,0,57
	DEL 2

	!ring

	ROTX 90
	ADD -A/2+2*RR,zzyzx-gs_seat_thk-(zzyzx-gs_seat_thk)/3,0
	ELBOW A/2-2*RR, 360,RR

	DEL 2

	!seat
	MATERIAL gs_seat_mat

	ADDZ zzyzx-gs_seat_thk
	CONE gs_seat_thk/10,A/2-gs_seat_thk/10,A/2, 90,90
	ADDZ gs_seat_thk/10
	CYLIND gs_seat_thk-2*gs_seat_thk/10,A/2
	ADDZ gs_seat_thk-2*gs_seat_thk/10
	CONE gs_seat_thk/10,A/2,A/2-gs_seat_thk/10, 90,90
	DEL 3
RETURN

2:
	R=A/20
