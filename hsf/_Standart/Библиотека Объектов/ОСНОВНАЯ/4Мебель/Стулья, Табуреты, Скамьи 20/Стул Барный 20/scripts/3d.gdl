	DEL 1
RETURN

3:
	E=13
	F=10
	G=A/30
	BB=(zzyzx-gs_seat_thk-2*G)
	J=ATN(BB/(A-3*G-A/8.6))
	H=180-J
	I=((A-3*G-A/8.6)-2*(A/8.6*TAN(90-J/2))+2*(A/8.6*COS(90-J)))/COS(J)
	ROTZ -90
	ADDX -A/2+2*G

	MATERIAL gs_frame_mat
	ROTX 90
	ADDY G
	ELBOW A/2-2*G,360,G

	ADD 2*G+A/8.6,0,-A/8.6
	ROTY -90
	ROTZ 90
	ADDY -(A/8.6)
	REVOLVE 2,180,1,
		0,A/8.6,901,
		G,360,4001
	DEL 2

	ROTX 180
	PRISM_ 2,(A-3*G-A/8.6)-2*(A/8.6*TAN(90-J/2)),
		0,0,974,
		G,360,4074

	ADDZ (A-3*G-A/8.6)-2*(A/8.6*TAN(90-J/2))
	ROTZ -90
	ROTZ 90
	ADDY -(A/8.6)
	REVOLVE 2,H,1,
		0,A/8.6,901,
		G,360,4001
	DEL 2
	ADDX A/8.6
	ROTY  H
	ADDX -A/8.6

	PRISM_ 2,I,
		0,0,974,
		G,360,4074
	ADDZ I
	ROTZ 180
	ROTZ 90
	ADDY -(A/8.6)
	REVOLVE 2,H,1,
		0,A/8.6,901,
		G,360,4001
	DEL 2
	ADDX A/8.6
	ROTY H
	ADDX -A/8.6
	PRISM_ 2,(A-3*G-A/8.6)-2*(A/8.6*TAN(90-J/2)),
		0,0,974,
		G,360,4074

	DEL 10
	ADDX 2*A/8.6
	PRISM_ 2,(A-3*G-A/8.6)-2*(A/8.6*TAN(90-J/2)),
		0,0,974,
		G,360,4074

	ADDZ (A-3*G-A/8.6)-2*(A/8.6*TAN(90-J/2))
	ROTZ -90
	ROTZ 90
	ADDY -(A/8.6)
	REVOLVE 2,H,1,
		0,A/8.6,901,
		G,360,4001
	DEL 2
	ADDX A/8.6
	ROTY  H
	ADDX -A/8.6

	PRISM_ 2,I,
		0,0,974,
		G,360,4074
	ADDZ I
	ROTZ 180
	ROTZ 90
	ADDY -(A/8.6)
	REVOLVE 2,H,1,
		0,A/8.6,901,
		G,360,4001
	DEL 2
	ADDX A/8.6
	ROTY H
	ADDX -A/8.6
	PRISM_ 2,(A-3*G-A/8.6)-2*(A/8.6*TAN(90-J/2)),
		0,0,974,
		G,360,4074

	DEL 14
	ADD 0,zzyzx-gs_seat_thk-2*G,0
	ELBOW A/2-2*G,360,G

	MATERIAL gs_seat_mat
	ROTX 90
	ADD A/2-2*G,0,-gs_seat_thk-G
	PRISM_ 2,gs_seat_thk,
		0,0,979,
		A/2,360,4079

	MATERIAL gs_frame_mat
	ROTX 90
	ADD -A/2+4*G+A/8.6+(A-3*G-A/8.6)-2*(A/8.6*TAN(90-J/2))+COS(90-J)*A/8.6-(COS(J)*I)/2,gs_seat_thk+G+BB/2-G/COS(J)-A/55,-(A-2*(A/55+COS(35)*A/20))/2
	PRISM_ 2,A-2*(A/55+COS(35)*A/20),
		0,0,974,
		A/55,360,4074
	ROTX 180
	ROTZ 90
	ADDY -A/20
	REVOLVE 2,125,1,
		0,A/20,901,
		A/55,360,4001
	DEL 2
	ADDZ -(A-2*(A/55+COS(35)*A/20))
	ROTX 180
	ROTZ 90
	ADDY -A/20
	REVOLVE 2,125,1,
		0,A/20,901,
		A/55,360,4001
	DEL 2
	ADD A/20+SIN(35)*A/20,0,COS(35)*A/20
	ROTY 125
	ROTZ 90
	ADDY -((A-2*(A/55+COS(35)*A/20))/2+(A/20)*COS(35))/SIN(55)
	REVOLVE 2,110,1,
		0,((A-2*(A/55+COS(35)*A/20))/2+(A/20)*COS(35))/SIN(55),901,
		A/55,360,4001
	DEL 2
	DEL 14
RETURN

