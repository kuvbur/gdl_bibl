5:
	RESOL 40
	K=MIN(A/49,zzyzx/81.6)
	F=16

	MATERIAL gs_frame_mat
	! leg
	ROTZ 45
	FOR I=1 TO 4
		ADD -A/2,0,0
		PRISM_ 2,zzyzx/5.5,
			0,0,975,
			K,360,4074
		ADDZ zzyzx/5.5
		ROTZ 90
		ADDY -MIN(zzyzx/4,A/2.5)
		REVOLVE 2,F,1,
			0,MIN(zzyzx/4,A/2.5),901,
			K,360,4001
		DEL 2
		ADD MIN(zzyzx/4,A/2.5)-COS(F)*MIN(zzyzx/4,A/2.5),0,SIN(F)*MIN(zzyzx/4,A/2.5)
		ROTY F
		PRISM_ 2,(zzyzx-zzyzx/27-2*(zzyzx/5.5+SIN(F)*MIN(zzyzx/4,A/2.5)))/SIN(90-F),
			0,0,974,
			K,360,4074
		ADDZ (zzyzx-zzyzx/27-2*(zzyzx/5.5+SIN(F)*MIN(zzyzx/4,A/2.5)))/SIN(90-F)
		ROTZ 180
		ROTZ 90
		ADDY -MIN(zzyzx/4,A/2.5)
		REVOLVE 2,F,1,
			0,MIN(zzyzx/4,A/2.5),901,
			K,360,4001
		DEL 2
		ADD MIN(zzyzx/4,A/2.5)-COS(F)*MIN(zzyzx/4,A/2.5),0,SIN(F)*MIN(zzyzx/4,A/2.5)
		ROTY F
		PRISM_ 2,zzyzx/5.5,
			0,0,974,
			K,360,4074
		DEL 8
		ROTZ 90
	NEXT I
	DEL 1
	! ring
	DEL 4
	ROTX 90
	ADDX -(A/2)-2*K
	ADD MIN(zzyzx/4,A/2.5)-COS(F)*MIN(zzyzx/4,A/2.5)+((zzyzx-zzyzx/27-2*(zzyzx/5.5+SIN(F)*MIN(zzyzx/4,A/2.5)))/2)/TAN(90-F),(zzyzx-zzyzx/27-2*(zzyzx/5.5+SIN(F)*MIN(zzyzx/4,A/2.5)))/2+SIN(F)*MIN(zzyzx/4,A/2.5)+zzyzx/5.5,0
	ELBOW (A/2)+2*K-MIN(zzyzx/4,A/2.5)+COS(F)*MIN(zzyzx/4,A/2.5)-((zzyzx-zzyzx/27-2*(zzyzx/5.5+SIN(F)*MIN(zzyzx/4,A/2.5)))/2)/TAN(90-F),360,K
	DEL 3

	! seat
	MATERIAL gs_seat_mat
	ADDZ zzyzx-gs_seat_thk-0.005
	CYLIND gs_seat_thk,A/2
	ADDZ gs_seat_thk
	CONE 0.005,A/2,A/2-0.005,90,90
	DEL 2
RETURN

6:
	RESOL 4

	K=MIN(A/49,zzyzx/81.6)
	F=16
	AA=(MIN(zzyzx/4,A/2.5)-COS(F)*MIN(zzyzx/4,A/2.5))*2+(zzyzx-zzyzx/27-2*(zzyzx/5.5+SIN(F)*MIN(zzyzx/4,A/2.5)))/TAN(90-F)
	E=ATN((zzyzx-zzyzx/27)/AA)
	MATERIAL gs_frame_mat
	! --- leg
	ROTZ 45
	FOR I=1 TO 4
		ADD -A/2,0,0
		ROTY 90-E
		CYLIND zzyzx-zzyzx/27,K
		DEL 2
		ROTZ 90
