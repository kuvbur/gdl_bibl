
HIDEPARAMETER ALL "A", "B", "zzyzx"

call "FM_types" parameters all

parameters ac_bottomlevel = 0
parameters ac_toplevel = ZZYZX

! ------------------------------------------------------------------------------
! Accessories
! ------------------------------------------------------------------------------
values "stTrayStyle" stTrayStyleValues
values "iTrayStyle" STYLE_RECTTRAY, STYLE_OVALTRAY

if GLOB_MODPAR_NAME = "stTrayStyle" then
	iTrayStyle = STYLE_RECTTRAY
	if stTrayStyle = stTrayStyleValues[2] then iTrayStyle = STYLE_OVALTRAY
	parameters iTrayStyle = iTrayStyle
else
	stTrayStyle = stTrayStyleValues[1]
	if iTrayStyle = STYLE_OVALTRAY then stTrayStyle = stTrayStyleValues[2]
	parameters stTrayStyle = stTrayStyle
endif

if not(gs_detlevel_3D_m = 2) then 
	lock "stTrayStyle"
	hideparameter "stTrayStyle"
endif

if not(bTray) then 
	lock "gs_tray_mat", "stTrayStyle", "iTrayStyle"
	hideparameter  "gs_tray_mat","stTrayStyle", "iTrayStyle"
endif


if gs_detlevel_3D_m <> 2 then 	
	lock "bEdgeProtect"
  hideparameter "bEdgeProtect"
endif

if not(bEdgeProtect) then 
	lock "gs_edge_mat"
	hideparameter  "gs_edge_mat"
endif


! ------------------------------------------------------------------------------
! restrictions for 3D geometry
! ------------------------------------------------------------------------------
values "ZZYZX" 			range [0.7,1.3]
values "A"				range [0.34,0.54)
values "B"				range [0.5,1.4)
values "gs_seat_height" range [0.7 - 0.4,1.3 - 0.4]
values "seatDepth" 	range [0.22,B-_xLegs5*2]

if  GLOB_MODPAR_NAME = "gs_seat_height" then
	ZZYZX = gs_seat_height + 0.4 
	parameters ZZYZX = ZZYZX 
else 
	gs_seat_height = ZZYZX -0.4 
	parameters gs_seat_height = gs_seat_height
endif


! ------------------------------------------------------------------------------
! 2D Symbol
! ------------------------------------------------------------------------------
call "2dDetailLevel" parameters gs_detlevel_2D	= gs_detlevel_2D,
							gs_detlevel_2D_m	= gs_detlevel_2D_m,
							enable2dDetLevels	= 2+4+8+16+32

if gs_detlevel_2D_m <> 4 then
	HIDEPARAMETER "gs_SymbolType"
	lock "gs_SymbolType", "gs_SymbolType_m"
endif


call "fa_ChairSymbol" parameters all stSymbolTypes = stSymbolTypes

IF gs_cont_pen=0 THEN PARAMETERS gs_cont_pen=4
IF gs_fill_pen=0 THEN PARAMETERS gs_fill_pen=19


! ------------------------------------------------------------------------------
! 3D Detail level
! ------------------------------------------------------------------------------

values "gs_resol" RANGE [3,)
if gs_detlevel_3D_m = 1 then
	lock "gs_resol"
	hideparameter "gs_resol"
endif

if gs_detlevel_3D_m = 0 then
	lock "gs_resol", "gs_shadow"
	hideparameter "gs_resol", "gs_shadow"
endif


values "gs_detlevel_3D" stDetlevel3D           ! Off(0), Simple(1), Detailed(2)
values "gs_detlevel_3D_m" 0, 1, 2
if GLOB_MODPAR_NAME = "gs_detlevel_3D" then
	gs_detlevel_3D_m = 2
	if gs_detlevel_3D = stDetlevel3D[2] then gs_detlevel_3D_m = 1
	if gs_detlevel_3D = stDetlevel3D[3] then gs_detlevel_3D_m = 0
	parameters gs_detlevel_3D_m = gs_detlevel_3D_m
else
	gs_detlevel_3D = stDetlevel3D[1]
	if gs_detlevel_3D_m = 1 then gs_detlevel_3D = stDetlevel3D[2]
	if gs_detlevel_3D_m = 0 then gs_detlevel_3D = stDetlevel3D[3]
	parameters gs_detlevel_3D = gs_detlevel_3D
endif

! ------------------------------------------------------------------------------
! Minimal Space
! ------------------------------------------------------------------------------

values "MSFront"	range [0,)
values "MSSide"		range [0,)
values "MSRear"		range [0,)

