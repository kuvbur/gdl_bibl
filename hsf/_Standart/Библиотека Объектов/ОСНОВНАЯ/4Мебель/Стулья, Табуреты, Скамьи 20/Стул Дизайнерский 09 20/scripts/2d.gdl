	PEN gs_cont_pen
	! --- Minimal Space and Hotspots ---

	CALL "fa_ChairSymbol" PARAMETERS ALL gs_SymbolType_m=0

	! --- Symbol ---

	ADD2 a/2, b/2

	MUL2 a/0.59, b/0.728

	! --- Background Fill, Symbol ---
	FILL gs_fill_type

	poly2_b      29, 1+2+64,     gs_fill_pen, gs_back_pen,
