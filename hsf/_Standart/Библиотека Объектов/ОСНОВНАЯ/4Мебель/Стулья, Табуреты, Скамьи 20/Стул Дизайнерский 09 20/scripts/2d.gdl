			  -0.295, -0.265964779705,   1,
			  -0.295, -0.295964779705,   1,
			  -0.265, -0.295964779705,   1,
			  -0.265, -0.325964779705,   1,
			  -0.235, -0.325964779705,   1,
			  -0.235, -0.356964779705,   1,
			  -0.205, -0.356964779705,   1,
			  -0.205, -0.364,            1,
			   0.205, -0.364,            1,
			   0.205, -0.356964779705,   1,
			   0.235, -0.356964779705,   1,
			   0.235, -0.325964779705,   1,
			   0.265, -0.325964779705,   1,
			   0.265, -0.295964779705,   1,
			   0.295, -0.295964779705,   1,
			   0.295, -0.265964779705,   1,
			   0.235, -0.265964779705,   1,
			   0.235, -0.0969647797046,  1,
			   0.275, -0.0969647797046,  1,
			   0.275,  0.364,            1,
			   0.195,  0.364,            1,
			   0.195,  0.430590085323,   1,
			  -0.195,  0.430590085323,  -1


	hotspot2  0,       -0.364
	hotspot2 -0.295,   -0.364
	hotspot2  0.295,   -0.364
	hotspot2  0.195,    0.43059
	hotspot2 -0.195,    0.43059
	hotspot2 -0.295,    0.364
	hotspot2  0.295,    0.364
	hotspot2  0,        0
	line2    -0.195, 0.15, 0.195, 0.15
	for i = 1 to 2
		line2 -0.195,  0.364,          -0.275,  0.364
