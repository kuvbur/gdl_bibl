
HIDEPARAMETER ALL "A", "B", "zzyzx"

call "FM_types" parameters all

lock "a", "b", "zzyzx"
values "drej" range [0, 20]

call "2dDetailLevel" parameters gs_detlevel_2D	= gs_detlevel_2D,
							gs_detlevel_2D_m	= gs_detlevel_2D_m,
							enable2dDetLevels	= 2+4+8+16+32

VALUES "styp" stSymbolTypes
VALUES "iSymbolType" TYPE_1, TYPE_2, TYPE_3, TYPE_4

IF GLOB_MODPAR_NAME = "styp" THEN
	iSymbolType = TYPE_1
	IF styp = stSymbolTypes[2] THEN iSymbolType = TYPE_2
	IF styp = stSymbolTypes[3] THEN iSymbolType = TYPE_3
	IF styp = stSymbolTypes[4] THEN iSymbolType = TYPE_4
	PARAMETERS iSymbolType = iSymbolType
ELSE
	styp = `Стиль 1`
	IF iSymbolType = TYPE_2 THEN styp = stSymbolTypes[2]
	IF iSymbolType = TYPE_3 THEN styp = stSymbolTypes[3]
	IF iSymbolType = TYPE_4 THEN styp = stSymbolTypes[4]
	PARAMETERS styp = styp
ENDIF

IF gs_detlevel_2D_m <>4 THEN
	LOCK "styp", "iSymbolType"
	HIDEPARAMETER "styp", "iSymbolType"
ENDIF
