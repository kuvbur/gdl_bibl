
targetGUID = TO_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

gosub "newArmrestBoolean_BWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid targetGuid

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! newArmrestBoolean
! ==============================================================================
"newArmrestBoolean_BWM":
	if targetGUID # "" then
	
		! If gs_armrestType_m was 0, that equals with bArmrest being off, otherwise it is on.
		if not(bArmrest) then
			gs_armrestType_m = 0
			parameters	gs_armrestType_m = gs_armrestType_m
		endif
	
		newparameter "gs_knob_type", "String"
		if gs_armrestType_m = 0	then _gs_armrestType = `Нет`
		if gs_armrestType_m = 1	then _gs_armrestType = `Стиль 1`
		if gs_armrestType_m = 2	then _gs_armrestType = `Стиль 2`
		if gs_armrestType_m = 3	then _gs_armrestType = `Стиль 3`
		parameters gs_armrestType = _gs_armrestType
	
	endif
return

