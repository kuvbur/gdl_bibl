! ==============================================================================
! newArmrestBoolean
! ==============================================================================
"newArmrestBoolean_FWM":
	if actualGuid = _startID | actualGuid = _autoID then
	
		! If gs_armrestType_m was 0, that equals with bArmrest being off, otherwise it is on.
		bHas = STORED_PAR_VALUE("gs_armrestType_m", _gs_armrestType_m)

		if _gs_armrestType_m = 0 then
			bArmrest = 0
		else
			bArmrest = 1
		endif
		parameters bArmrest = bArmrest
	
		actualGuid = _endID
	endif
return

