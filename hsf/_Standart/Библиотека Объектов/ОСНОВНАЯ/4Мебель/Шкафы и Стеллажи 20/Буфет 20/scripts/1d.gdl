	stLiningSide[2] = `Спереди и Слева`
	stLiningSide[3] = `Спереди и Справа`
	stLiningSide[4] = `Спереди`

! const values for parameter: gs_lining_m
LINING_SIDE_BOTH	= 1
LINING_SIDE_LEFT	= 2
LINING_SIDE_RIGHT	= 3
LINING_SIDE_NONE	= 4


lffw2 = A-2*0.01
IF gs_lining_m = LINING_SIDE_BOTH then lffw2 = lffw2-2*side_overhang
IF gs_lining_m = LINING_SIDE_LEFT then lffw2 = lffw2-side_overhang
IF gs_lining_m = LINING_SIDE_RIGHT then lffw2 = lffw2-side_overhang
IF gs_lining_m = LINING_SIDE_NONE OR not(bCornice) then lffw2 = lffw2

if not(bCornice) then
	cot=0
	cooh=0
	side_overhang=0
endif

IF cot=0 THEN
cooh=0
ENDIF

_bDrawKnob = 1

if gs_detlevel_3D_m=1 then
	vert_gnum=0
	hor_gnum=0
	_bDrawKnob = 0
endif


