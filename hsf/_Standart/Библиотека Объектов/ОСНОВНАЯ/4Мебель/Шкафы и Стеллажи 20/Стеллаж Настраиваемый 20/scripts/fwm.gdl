! ==============================================================================
! New Symbol 2D Styles
! ==============================================================================
"2DSymbolStyles_FWM":
	if actualGuid = _startID | actualGuid = _autoID then
	
		bHas = DELETED_PAR_VALUE ("gs_detlevel_2D_m", _gs_detlevel_2D_m)
		
		if _gs_detlevel_2D_m = 1 then iSymbolStyle = 1		! Style 1
		if _gs_detlevel_2D_m = 2 then iSymbolStyle = 2		! Style 2	
		if _gs_detlevel_2D_m = 3 then iSymbolStyle = 3		! Style 3
		
		parameters iSymbolStyle = iSymbolStyle

		actualGuid = _endID
	endif
return

! ==============================================================================
! bLeg_and_iLegType
! ==============================================================================
"bLeg_and_iLegType_FWM":
	if actualGuid = _startID then

		bHas = DELETED_PAR_VALUE ("gs_iLegtype", _iLegType)

		if _iLegType = 4 then !LEG_LEG
			wBase = b/10
		else
			wBase = max(0.02, gs_frameWidth/2)
		endif

		if _iLegType = 0 then
			bLeg = 0		!Leg Type None
		else
			bLeg = 1
		endif

		parameters wBase = wBase,
					bLeg = bLeg,
					iLegType = _iLegType
			
		actualGuid = _endID
	endif
return

