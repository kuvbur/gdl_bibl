	_autoID		= "13A9D0DC-BB49-4F3C-8907-5EA1DF89B0D9"
	_startID	= "5BEC40FF-D3D0-4564-ADF3-96C3C1311986"
	_endID		= "CA84ABB0-20D7-4BD7-B7B1-477D9B2CA366"
gosub "2DSymbolStyles_FWM"

	_startID	= "CA84ABB0-20D7-4BD7-B7B1-477D9B2CA366"
	_endID		= "73A0C4DD-0385-46EC-9E69-A603E6BE09A7"
gosub "bLeg_and_iLegType_FWM"

