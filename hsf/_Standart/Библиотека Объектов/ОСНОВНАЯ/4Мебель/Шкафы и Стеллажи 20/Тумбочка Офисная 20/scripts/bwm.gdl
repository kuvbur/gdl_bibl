! ==============================================================================
! newparameters_baseWidth_iKnobNum_drawerKnobSize_deletedParameter_gs_editableKnobs
! ==============================================================================
"newparameters_BWM":
	if targetGUID # "" then

		! Migrate element indepenedently from baseWidth

		! If there were 2 knobs, the knobs' position was hidden,
		! it will have 1 knob after migration, so we set their positions to default.
		if iKnobNum = 2 then
			for m = 1 to drawer_num
				gs_knobPosArray[m][1] = 0
				gs_knobPosArray[m][2] = 0
			next m
			gs_knobHorPos = 0
			parameters	gs_knobPosArray = gs_knobPosArray,
						gs_knobHorPos = gs_knobHorPos
		endif

		! Migrate element indepenedently from drawerKnobSize

		! gs_editableKnobs is 1 in every case 
		newparameter "gs_editableKnobs", "Boolean"
		gs_editableKnobs = 1
		parameters gs_editableKnobs = gs_editableKnobs
		
	endif
return

! ==============================================================================
! 2D New Symbol Styles
! ==============================================================================
"2DSymbolStyles_BWM":
	if targetGUID # "" then
	
		! Migrate every case
		
	endif
return

! ==============================================================================
! AccessoryBooleans
! ==============================================================================
"AccessoryBooleans_BWM":
	if targetGUID # "" then
	
		! If gs_knob_type_m was 0, that equals with bKnob being off, otherwise it is on.
		if not(bKnob) then
			gs_knob_type_m = 0
			parameters	gs_knob_type_m = gs_knob_type_m
		endif

		newparameter "gs_knob_type", "String"
		if gs_knob_type_m = 0	then _gs_knob_type = `Нет`
		if gs_knob_type_m = 1	then _gs_knob_type = `Стиль 1`
		if gs_knob_type_m = 2	then _gs_knob_type = `Стиль 2`
		if gs_knob_type_m = 3	then _gs_knob_type = `Стиль 3`
		if gs_knob_type_m = 4	then _gs_knob_type = `Стиль 4`
		if gs_knob_type_m = 5	then _gs_knob_type = `Стиль 5`
		if gs_knob_type_m = 6	then _gs_knob_type = `Стиль 6`
		if gs_knob_type_m = 7	then _gs_knob_type = `Стиль 7`
		if gs_knob_type_m = 8	then _gs_knob_type = `Стиль 8`
		parameters gs_knob_type = _gs_knob_type

		! If gs_baseType_m was 0, that equals with bBase being off, otherwise it is on.
		if not(bBase) then
			gs_baseType_m = 0
			parameters	gs_baseType_m = gs_baseType_m
		endif
	
		newparameter "gs_baseType", "String"
		if gs_baseType_m = 0	then _gs_baseType = `Откл.`
		if gs_baseType_m = 1	then _gs_baseType = `Ролики`
		if gs_baseType_m = 2	then _gs_baseType = `Основание`
		parameters gs_baseType = _gs_baseType

	endif
return

