! ==============================================================================
! AccessoryBooleans
! ==============================================================================
"AccessoryBooleans_BWM":
	if targetGUID # "" then

		! If gs_knob_type_m was 0, that equals with bKnob being off, otherwise it is on.
		if not(bKnob) then
			gs_knob_type_m = 0
			parameters	gs_knob_type_m = gs_knob_type_m
		endif

		newparameter "gs_knob_type", "String"
		if gs_knob_type_m = 0	then _gs_knob_type = `Нет`
		if gs_knob_type_m = 1	then _gs_knob_type = `Стиль 1`
		if gs_knob_type_m = 2	then _gs_knob_type = `Стиль 2`
		if gs_knob_type_m = 3	then _gs_knob_type = `Стиль 3`
		if gs_knob_type_m = 4	then _gs_knob_type = `Стиль 4`
		if gs_knob_type_m = 5	then _gs_knob_type = `Стиль 5`
		if gs_knob_type_m = 6	then _gs_knob_type = `Стиль 6`
		if gs_knob_type_m = 7	then _gs_knob_type = `Стиль 7`
		if gs_knob_type_m = 8	then _gs_knob_type = `Стиль 8`
		parameters gs_knob_type = _gs_knob_type

		! If gs_knob_type_m_up was 0, that equals with bKnob_up being off, otherwise it is on.
		if not(bKnob_up) then
			gs_knob_type_m_up = 0
			parameters	gs_knob_type_m_up = gs_knob_type_m_up
		endif

		newparameter "gs_knob_type_up", "String"
		if gs_knob_type_m_up = 0	then _gs_knob_type_up = `Нет`
		if gs_knob_type_m_up = 1	then _gs_knob_type_up = `Стиль 1`
		if gs_knob_type_m_up = 2	then _gs_knob_type_up = `Стиль 2`
		if gs_knob_type_m_up = 3	then _gs_knob_type_up = `Стиль 3`
		if gs_knob_type_m_up = 4	then _gs_knob_type_up = `Стиль 4`
		if gs_knob_type_m_up = 5	then _gs_knob_type_up = `Стиль 5`
		if gs_knob_type_m_up = 6	then _gs_knob_type_up = `Стиль 6`
		if gs_knob_type_m_up = 7	then _gs_knob_type_up = `Стиль 7`
		if gs_knob_type_m_up = 8	then _gs_knob_type_up = `Стиль 8`
		parameters gs_knob_type_up = _gs_knob_type_up

	endif
return

! ==============================================================================
! newparameters_bShelves_nDrawerKnob
! ==============================================================================
"newparameters_BWM":
	if targetGUID # "" then

		! If bShelves was off, then Number of Shelves is 0.
		if not(bShelves) then
			shelfNum = 0
		else
			shelfNum = shelfNum
		endif
		parameters	shelfNum = shelfNum

		! Migrate element indepenedently from No. of Knobs by Drawer

	endif
return

! ==============================================================================
!  New Symbol 2D Styles
! ==============================================================================
"2DSymbolStyles_BWM":
	if targetGUID # "" then
		_bMigrationSuccess = 1
		
		newparameter "gs_detlevel_2D", "Boolean"
		
		if 	iSymbolStyle = 1 |\					! In case of 'Style 1', 'Style 6' or 'Style 7' Detailed 2D Symbol is OFF
			iSymbolStyle = 6 |\
			iSymbolStyle = 7 then
		
			gs_detlevel_2D 	= 0
		else
			if 	iSymbolStyle = 2 |\				! In case of 'Style 2' or 'Style 3' Detailed 2D Symbol is ON
				iSymbolStyle = 3 then
				
				gs_detlevel_2D 	= 1
			else								! Cut migration in case of 'Style 4', 'Style 5', 'Style 8' or 'Style 9' 
				_bMigrationSuccess = 0
			endif
		endif
			
		if _bMigrationSuccess = 1 then
			parameters 	gs_detlevel_2D 	= gs_detlevel_2D
		else
			targetGUID = ""
		endif
	endif
return

