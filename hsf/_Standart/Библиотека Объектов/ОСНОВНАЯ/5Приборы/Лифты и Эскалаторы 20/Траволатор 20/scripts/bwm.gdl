
targetGUID = TO_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

gosub "paramNameChanges_BWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid targetGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! paramNameChanges
! ==============================================================================
"paramNameChanges_BWM":
	if targetGUID # "" then
		newparameter "iHandrailType", "Integer"
			iHandrailType = iBalustradeType

		newparameter "matHandrail", "Material"
			matHandrail = matBalustrade

		newparameter "matRubber", "Material"
			matRubber = matHandrailRubber

		parameters	iHandrailType	= iHandrailType,
					matHandrail		= matHandrail,
					matRubber		= matRubber
	endif
return

