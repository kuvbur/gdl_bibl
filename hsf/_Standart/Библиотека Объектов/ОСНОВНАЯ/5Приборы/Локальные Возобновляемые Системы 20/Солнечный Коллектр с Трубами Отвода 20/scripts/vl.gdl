
hideparameter all "A", "B", "ZZYZX"

values "FM_HeatOutput_unit" `Вт/м2`, `(БТЕ/час)/фут2`
values "FM_RefrigeratingOutput_unit" `Вт/м2`, `(БТЕ/час)/фут2`


if FM_HeatOutput_unit = `(БТЕ/час)/фут2` then
	hideparameter "FM_HeatOutput"
else
	hideparameter "FM_HeatOutput_btu"
endif

if FM_RefrigeratingOutput_unit = `(БТЕ/час)/фут2` then
	hideparameter "FM_RefrigeratingOutput"
else
	hideparameter "FM_RefrigeratingOutput_btu"
endif


!1000 W/m2 = 316,998 (Btu/hour)/ft2
if GLOB_MODPAR_NAME = "FM_HeatOutput_btu" then
	FM_HeatOutput = FM_HeatOutput_btu / 0.316998
	parameters FM_HeatOutput = FM_HeatOutput
else
	FM_HeatOutput_btu = FM_HeatOutput * 0.316998
	parameters FM_HeatOutput_btu = FM_HeatOutput_btu
endif

!1000 W/m2 = 316,998 (Btu/hour)/ft2
if GLOB_MODPAR_NAME = "FM_RefrigeratingOutput_btu" then
	FM_RefrigeratingOutput = FM_RefrigeratingOutput_btu / 0.316998
	parameters FM_RefrigeratingOutput = FM_RefrigeratingOutput
else
	FM_RefrigeratingOutput_btu = FM_RefrigeratingOutput * 0.316998
	parameters FM_RefrigeratingOutput_btu = FM_RefrigeratingOutput_btu
endif

!hideparameter "panelLength"

