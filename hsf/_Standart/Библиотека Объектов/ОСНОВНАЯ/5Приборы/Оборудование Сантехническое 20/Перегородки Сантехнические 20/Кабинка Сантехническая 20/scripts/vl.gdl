! ------------------------------------------------------------------------------
! Defining heights
! ------------------------------------------------------------------------------

values "bottomHeightPanel" range [0,)
values "bottomEndPanel" range [0,)
values "topEndPanel" range [0,)

if iPartitionType = PARTITION_TYPE_STALL then
	if iStallType = STALL_TYPE_OVERHEAD_BRACED then
		if GLOB_MODPAR_NAME = "ZZYZX" then
			heightPanel = zzyzx - heightTopBracing - bottomHeightPanel
			parameters heightPanel = heightPanel
		else
			zzyzx = heightPanel + heightTopBracing + bottomHeightPanel
			parameters zzyzx = zzyzx
		endif
	endif
	if iStallType = STALL_TYPE_FLOOR_MOUNTED then
		values "heightPanel" range (0,)
		values "zzyzx" range (bottomHeightPanel,)
		if GLOB_MODPAR_NAME = "ZZYZX" then
			heightPanel = zzyzx - bottomHeightPanel
			parameters heightPanel = heightPanel
		else
			zzyzx = heightPanel + bottomHeightPanel
			parameters zzyzx = zzyzx
		endif
		hideparameter "gs_topBracing"
	endif
	if iStallType = STALL_TYPE_CEILING_MOUNTED then
		values "heightPanel" range (0,)
		values "heightTopPanel" range (0,)
		values "zzyzx" range (heightPanel + bottomHeightPanel,)
		if GLOB_MODPAR_NAME = "heightTopPanel" then
			heightPanel = zzyzx - heightTopPanel - bottomHeightPanel
			parameters heightPanel = heightPanel
		else
			heightTopPanel = zzyzx - heightPanel - bottomHeightPanel
			parameters heightTopPanel = heightTopPanel
		endif
		hideparameter "gs_topBracing"
	endif
endif

if iPartitionType = PARTITION_TYPE_URINAL_SCREEN then
	if iScreenType = SCREEN_TYPE_FLOOR_MOUNTED then
		values "ZZYZX" range [bottomHeightPanel,)
		if GLOB_MODPAR_NAME = "ZZYZX" then
			heightPanel = zzyzx - bottomHeightPanel
			parameters heightPanel = heightPanel
		else
			zzyzx = heightPanel + bottomHeightPanel
			parameters zzyzx = zzyzx
		endif
	endif
	if iScreenType = SCREEN_TYPE_WALL_HUNG then
		values "ZZYZX" range [bottomEndPanel,)
		if GLOB_MODPAR_NAME = "ZZYZX" then
			topEndPanel = zzyzx - bottomEndPanel
			parameters topEndPanel = topEndPanel
		else
			zzyzx = topEndPanel + bottomEndPanel
			parameters zzyzx = zzyzx
		endif
	endif
endif

! ------------------------------------------------------------------------------
! Defining length parameter logic
! ------------------------------------------------------------------------------

if iPartitionType = PARTITION_TYPE_STALL then
	if GLOB_MODPAR_NAME = "widthStall" or GLOB_MODPAR_NAME = "widthDoor" then
		temp_a = widthHingeSidePanel / (widthHingeSidePanel + widthLockSidePanel)
		widthHingeSidePanel = (widthStall - widthDoor) * temp_a
		widthLockSidePanel = widthStall - widthDoor - widthHingeSidePanel
		parameters widthHingeSidePanel = widthHingeSidePanel
		parameters widthLockSidePanel = widthLockSidePanel
	endif

	if GLOB_MODPAR_NAME = "widthHingeSidePanel" then
		widthLockSidePanel = widthStall - widthDoor - widthHingeSidePanel
		parameters widthLockSidePanel = widthLockSidePanel
	endif

	if GLOB_MODPAR_NAME = "widthLockSidePanel" then
		widthHingeSidePanel = widthStall - widthDoor - widthLockSidePanel
		parameters widthHingeSidePanel = widthHingeSidePanel
	endif
endif


! ------------------------------------------------------------------------------
! Defining full length
! ------------------------------------------------------------------------------
if GLOB_MODPAR_NAME = "A" then
	widthStall = A / nStalls
	parameters widthStall = widthStall
endif

if GLOB_MODPAR_NAME = "widthStall" or GLOB_MODPAR_NAME = "nStalls" or GLOB_MODPAR_NAME = "A" then
	for i = 1 to nStalls
		stallWidths[i] = widthStall
		fixturePositions[i][1] = 0
		fixturePositions[i][2] = 0
		parameters stallWidths[i] = stallWidths[i]
		parameters fixturePositions[i][1] = fixturePositions[i][1]
		parameters fixturePositions[i][2] = fixturePositions[i][2]
	next i
endif

fullLength = 0
for i = 1 to nStalls
	fullLength = fullLength + stallWidths[i]
next i

if nStalls = 1 then
	stallWidths[1] = widthStall
	a = widthStall
	parameters a = a, stallWidths[1] = stallWidths[1]
else
	a = fullLength
	parameters a = a
endif

! ------------------------------------------------------------------------------
! Defining side-panels length
! ------------------------------------------------------------------------------

