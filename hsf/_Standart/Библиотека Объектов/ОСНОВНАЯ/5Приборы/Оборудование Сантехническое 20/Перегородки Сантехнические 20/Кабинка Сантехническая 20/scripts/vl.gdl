	or GLOB_MODPAR_NAME = "depthStall" or GLOB_MODPAR_NAME = "screenLength"  or GLOB_MODPAR_NAME = "B" then

	lengthEndPanel1 = lengthPanel
	parameters lengthEndPanel1 = lengthEndPanel1

	for i = 1 to nPanels
		panelLengths[i] = lengthPanel
		parameters panelLengths[i] = panelLengths[i]
	next i

	lengthEndPanel2 = lengthPanel
	parameters lengthEndPanel2 = lengthEndPanel2
	panelLengths[nStalls] = lengthEndPanel2
	parameters panelLengths[nStalls] = panelLengths[nStalls]
endif

if GLOB_MODPAR_NAME = "bEndPanel2" or GLOB_MODPAR_NAME = "lengthEndPanel2" then
	if bEndPanel2 then
		panelLengths[nStalls] = lengthEndPanel2
		parameters panelLengths[nStalls] = panelLengths[nStalls]
	else
		panelLengths[nStalls] = 0
		parameters panelLengths[nStalls] = panelLengths[nStalls]
