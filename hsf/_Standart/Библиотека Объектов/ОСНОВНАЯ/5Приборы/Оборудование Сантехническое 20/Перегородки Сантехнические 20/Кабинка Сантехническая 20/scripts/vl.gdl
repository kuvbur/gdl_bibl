	if iPartitionType = PARTITION_TYPE_STALL then
		depthStall = b
		parameters depthStall = depthStall
	endif
	if iPartitionType = PARTITION_TYPE_URINAL_SCREEN then
		screenLength = b
		parameters screenLength = screenLength
	endif
else
	if iPartitionType = PARTITION_TYPE_STALL then
		b = depthStall
	endif
	if iPartitionType = PARTITION_TYPE_URINAL_SCREEN then
		b = screenLength
	endif
	parameters b = b
endif
if iPartitionType = PARTITION_TYPE_CHINA_DIVISION then
	B = dividerDepth
