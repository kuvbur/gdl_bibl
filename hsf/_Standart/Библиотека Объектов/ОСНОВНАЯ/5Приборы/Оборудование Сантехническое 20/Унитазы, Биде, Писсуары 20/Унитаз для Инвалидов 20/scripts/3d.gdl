hotspot -dAxisFromWall - 1, 0, 0,	newunID, dAxisFromWall, 3: newunID = newunID + 1
hotspot	0, 0, 0,					newunID, dAxisFromWall, 2+128: newunID = newunID + 1


! ------ Toilet paper dispenser wall-mounted

if	bCorner & bToiletPaper then

	addy distanceFromWall

	!bottom center hsps edit
	add 0, 0, tpd_distFromFloor
	hotspot 0, 0, 0,					newunID, tpd_distFromWall,	1+128: newunID = newunID + 1
	hotspot 0, 1, 0,					newunID, tpd_distFromWall,	3: newunID = newunID + 1
	hotspot 0, -tpd_distFromWall, 0,	newunID, tpd_distFromWall,	2: newunID = newunID + 1
	del 1

	add 0, -tpd_distFromWall, 0
	hotspot 0, 0, 0,					newunID, tpd_distFromFloor,	1+128: newunID = newunID + 1
	hotspot 0, 0, -1,					newunID, tpd_distFromFloor,	3: newunID = newunID + 1
	hotspot 0, 0, tpd_distFromFloor,	newunID, tpd_distFromFloor,	2: newunID = newunID + 1
	del 1

	add 0, -tpd_distFromWall + tpd_halfwidth, tpd_distFromFloor
	rotz -90

	mulx 0.44/0.7

	addz 0.08
	addy -0.08633

	rotz 90
	rotx 90
	addz 0.01

	MATERIAL matPaper
		RESOL 2*gs_resol
		PRISM_       7,      0.18,
				0.07,     0.0,      79,
				0.0,     0.0,    979,
				0.0,     180.0,   4079,
			   -0.07,     0.0,      79,
				0.0,     0.0,    979,
				0.0,     180.0,   4079,
				0.07,     0.0,     -1

	del 1
	material matDispenser

	PRISM_      5,      0.2,
			0.08433,     0.08,      15,
			0.08633,     0.08,      15,
			0.08633,    -0.08,      15,
			0.08433,    -0.08,      15,
			0.08433,     0.08,     -1

	RESOL 2.5*gs_resol
	PRISM_     12,      0.2,
			0.08433,     0.01,      15,
			0.076348,     0.01,      79,
		   -0.0,     0.0,    900,
			0.0,     202.5379,   4001,
		   -0.066684,    -0.0385,      15,
		   -0.064952,    -0.0375,      79,
			0.0,    -0.0,    900,
			0.0,    -202.3377,   4001,
			0.07433,     0.01,      15,
			0.07433,    -0.01,      15,
			0.08433,    -0.01,      15,
			0.08433,     0.01,     -1

	addz 0.005
	RESOL gs_resol
	PRISM_      7,      0.005,
			0.022913,    -0.01,      15,
			0.08433,    -0.01,      15,
			0.08433,     0.01,      15,
			0.022913,     0.01,      79,
		   -0.0,    -0.0,    900,
			0.0,     312.8436,   4001,
			0.022913,    -0.01,     -1
	addz 0.185
	PRISM_      7,      0.005,
			0.022913,    -0.01,      15,
			0.08433,    -0.01,      15,
			0.08433,     0.01,      15,
			0.022913,     0.01,      79,
		   -0.0,    -0.0,    900,
			0.0,     312.8436,   4001,
			0.022913,    -0.01,     -1

	del 10
endif

del 1

! ------ Grabs

if	iGrabBars = GRAB_CORNER then
	if vertgb then			!vertical grabbar
		add 0, -vertgb_distFromWall, vertgb_distFromFloor

		!pos hsp edit
		if LibraryLangCode # "USA" then
			hotspot	0, vertgb_distFromWall, 0,		newunID, vertgb_distFromWall,	1+128: newunID = newunID + 1
			hotspot	0, vertgb_distFromWall + 1, 0,	newunID, vertgb_distFromWall,	3: newunID = newunID + 1
			hotspot	0, 0, 0,						newunID, vertgb_distFromWall,	2: newunID = newunID + 1
		endif

		!length hsps edit
		hotspot	0, 0, 0,							newunID, vertgb_length,		1+128: newunID = newunID + 1
		hotspot	0, 0, -1,							newunID, vertgb_length,		3: newunID = newunID + 1
		hotspot	0, 0, vertgb_length,				newunID, vertgb_length,		2: newunID = newunID + 1
		hotspot	0, 0, -vertgb_distFromFloor,		newunID, vertgb_distFromFloor,		1+128: newunID = newunID + 1
		hotspot	0, 0, -vertgb_distFromFloor - 1,	newunID, vertgb_distFromFloor,		3: newunID = newunID + 1
		hotspot	0, 0, 0,							newunID, vertgb_distFromFloor,		2: newunID = newunID + 1

		rotz -90
		roty -90
		ror = 0.05
		di = grabBarDiameter
		aa = vertgb_length
		GOSUB "bar_wall"
		del 3
	endif

	if reargb then			!rear grabbar
		addx -dAxisFromWall
		hotspot	-reargb_length1, 0, 0, 	unID :unID = unID + 1

		addz rearsidegb_distFromFloor

		!length hsps edit
		hotspot	0, 0, 0,				newunID, reargb_length1,	1+128: newunID = newunID + 1
		hotspot	1, 0, 0,				newunID, reargb_length1,	3: newunID = newunID + 1
		hotspot	-reargb_length1, 0, 0,	newunID, reargb_length1,	2: newunID = newunID + 1
		hotspot	0, 0, 0,				newunID, reargb_length2,	1+128: newunID = newunID + 1
		hotspot	-1, 0, 0,				newunID, reargb_length2,	3: newunID = newunID + 1
		hotspot	reargb_length2, 0, 0,	newunID, reargb_length2,	2: newunID = newunID + 1

		!height hsp edit
		hotspot	0, -0.08, -rearsidegb_distFromFloor,			newunID, rearsidegb_distFromFloor,		1+128: newunID = newunID + 1
		hotspot	0, -0.08, -rearsidegb_distFromFloor - 1,		newunID, rearsidegb_distFromFloor,		3: newunID = newunID + 1
		hotspot	0, -0.08, 0,									newunID, rearsidegb_distFromFloor,		2: newunID = newunID + 1


		add -reargb_length1, 0, 0
		ror = 0.05
		di = grabBarDiameter
		aa = reargb_length1 + reargb_length2
		GOSUB "bar_wall"
		del 3
	endif

	if sgb then				!side grabbar
		gosub "grabbar_sidewall"
	endif
