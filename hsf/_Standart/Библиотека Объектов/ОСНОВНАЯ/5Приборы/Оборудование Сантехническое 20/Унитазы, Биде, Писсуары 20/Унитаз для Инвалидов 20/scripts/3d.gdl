	add 0, -distanceFromWall, gs_seat_height
	hotspot 0, 0, 0,				newunID, cisternHeight,	1+128: newunID = newunID + 1
	hotspot 0, 0, -1,				newunID, cisternHeight,	3: newunID = newunID + 1
	hotspot 0, 0, cisternHeight,	newunID, cisternHeight,	2: newunID = newunID + 1
	del 1
endif
if iFlushType = 3 then
	!cistern height hsp edit
	hotspot 0, 0, 0,				newunID, carrierHeight,	1+128: newunID = newunID + 1
	hotspot 0, 0, -1,				newunID, carrierHeight,	3: newunID = newunID + 1
	hotspot 0, 0, carrierHeight,	newunID, carrierHeight,	2: newunID = newunID + 1
endif
	
call "CompleteToiletMacro" parameters all	iWCType 		= 1,
											bShowHotspots 	= 1,
											bDisabledWC		= 1

del 1
! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
