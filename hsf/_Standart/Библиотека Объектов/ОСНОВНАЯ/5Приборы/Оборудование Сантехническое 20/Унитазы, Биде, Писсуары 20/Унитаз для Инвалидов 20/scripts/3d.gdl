if iFlushType = 1 then
	if gs_flushHandlePlace_m <> 0 and (gs_detlevel_3D_m = DETLEV_3D_DETAILED or gs_detlevel_3D_m = DETLEV_3D_SIMPLE) then		!none or draft
		addy -distanceFromWall
