!================================================================================
"bar_wall":
!================================================================================

	hotspot	0, 0, 0, unID :unID = unID + 1
	hotspot	aa, 0, 0, unID :unID = unID + 1

	material matGrabBar
	pen gs_cont_pen

	add ror, -0.08, 0
		add -ror, ror, 0
		rotx -90
			addz 0.08-ror-0.005
			RESOL 1.5*gs_resol
			prism_ 2, 0.005,
				0, 0,				979,
				di/2 + 0.01, 360,	4079
			del 1
		
			RESOL gs_resol
			prism_ 2,.08-ror-0.005,
				0, 0,				978,
				di/2, 360,			4078
		del 2

		add aa-ror,ror,0
		rotx -90
			RESOL 1.5*gs_resol
			addz 0.08-ror-0.005
			prism_ 2, 0.005,
				0, 0,				979,
				di/2 + 0.01, 360,	4079
			del 1
	
			RESOL gs_resol
			prism_ 2,0.08-ror-0.005,
				0, 0,				978,
				di/2, 360,			4078
		del 2

		roty 90
			prism_ 2,aa-2*ror,
				0, 0,				974,
				di/2, 360,			4074

			if GLOB_VIEW_TYPE = 4 | GLOB_VIEW_TYPE = 5 | GLOB_VIEW_TYPE = 7 then	!section, elevation, detail
				flag = 4 + 8 + 16 + 32
			else
				flag = 4 + 8
			endif
			rotx 90
			addy aa-2*ror
			addz -ror
				revolve 2,90, flag,
					0, ror,			901,
					di/2, 360,		4001
	
				addy -aa+2*ror
				rotz 180
					revolve 2,90, flag,
						0, ror,		901,
						di/2, 360,	4001

	del 7
return

!================================================================================
"grabbar_sidewall":
!================================================================================
	add 0, -sgb_dist, rearsidegb_distFromFloor
	hotspot	0, -sgb_length, -rearsidegb_distFromFloor, unID :unID = unID + 1

	!length hsps edit
	hotspot	0, sgb_dist, 0,			newunID, sgb_dist,		1+128: newunID = newunID + 1
	hotspot	0, sgb_dist + 1, 0,		newunID, sgb_dist,		3: newunID = newunID + 1
	hotspot	0, 0, 0,				newunID, sgb_dist,		2: newunID = newunID + 1
	hotspot	0, 0, 0,				newunID, sgb_length,	1+128: newunID = newunID + 1
	hotspot	0, 1, 0,				newunID, sgb_length,	3: newunID = newunID + 1
	hotspot	0, -sgb_length, 0,		newunID, sgb_length,	2: newunID = newunID + 1

	!height hsp edit
	hotspot	0, 0, -rearsidegb_distFromFloor,			newunID, rearsidegb_distFromFloor,		1+128: newunID = newunID + 1
	hotspot	0, 0, -rearsidegb_distFromFloor - 1,		newunID, rearsidegb_distFromFloor,		3: newunID = newunID + 1
	hotspot	0, 0, 0,									newunID, rearsidegb_distFromFloor,		2: newunID = newunID + 1

	rotz -90
	ror = 0.05
	di = grabBarDiameter
	aa = sgb_length
	GOSUB "bar_wall"
	del 2

return


!================================================================================
"grabbar_floor":
!================================================================================
	for hj = 1 to 2
		!height hsp edit
		add -grabWidth/2, -grabLength, 0
		hotspot	0, 0, 0,			newunID, rearsidegb_distFromFloor,	1+128: newunID = newunID + 1
		hotspot	0, 0, -1,			newunID, rearsidegb_distFromFloor,	3: newunID = newunID + 1
		hotspot	0, 0, grabHeight,	newunID, rearsidegb_distFromFloor,	2: newunID = newunID + 1
		del 1

		material matGrabBar
	
		add -grabWidth/2, 0, grabHeight
		rotx 90
		cylind 0.005, grabRadius + 0.01
		del 2
	
		add -grabWidth/2, -0.005, grabHeight
		rotx 90
		cylind grabLength-gripLength-0.005, grabRadius
		del 2
	
		add -grabWidth/2, -grabLength + gripLength*2, 0
		cylind 0.005, grabRadius + 0.01
		del 1
	
		rotx 90
		cutpolya 2, 1, 0,
			-grabWidth/2, grabHeight, 907+64,
			grabRadius, 360, 4007+64
		del 1

		add -grabWidth/2, -grabLength + gripLength*2, 0.005
		cylind grabHeight-0.005, grabRadius
		del 1
	
		cutend

		if hj = 1 & bToiletPaper then gosub "toiletPaper"

		material matGrip
		add -grabWidth/2, gripLength-grabLength, grabHeight

		rotz -90
		if gs_detlevel_3D_m = DETLEV_3D_DETAILED then
			revolve 6, 360, 1+2+4+8+16+32,
				0, 0, 1,
				0, grabRadius, 1,
				gripRadius-grabRadius, gripRadius, 1001,
				gripLength-(gripRadius-grabRadius), gripRadius, 1,
				gripLength, grabRadius, 1001,
				gripLength, 0, 1
		else
			revolve 2, 360, 1+2+4+8+16+32,
				0, gripRadius, 0,
				gripLength, gripRadius, 0
		endif
		del 2
		addx grabWidth
	next hj
	del 2

return


!================================================================================
"grabbar_toilet":
!================================================================================
	material matGrabBar
	for hj = 1 to 2
		nc = ceil(gs_resol/2)
		for hi = 1 to nc
			alfa = hi * 90/nc
			put -grabWidth/2+roundoff2-roundoff2*sin(alfa), yHoldingPos, zHoldingPos+roundoff2-roundoff2*cos(alfa), -90
		next hi
	
		for hk = 1 to nc
			fi = 90 - atn(2/3)
			beta = hk * fi/nc
			put -grabWidth/2, yHoldingPos-roundoff2+roundoff2*cos(beta), grabHeight-roundoff2*2-roundoff2*tan(fi/2)+roundoff2*sin(beta), 0
		next hk

		for hm = 1 to nc
			gi = atn(2/3)
			ceta = hm * gi/nc
			put -grabWidth/2, yHoldingPos-roundoff2*3-roundoff2*tan(gi/2)+roundoff2*sin(ceta*(nc-hm)), grabHeight-roundoff2+roundoff2*cos(ceta*(nc-hm)), 0
		next hm

		if iToiletMounting = 1 then
			dstart = bowlWidth*0.14
			if iFloorMountedBowlType = 2 then dstart = bowlWidth*0.19
			if iFloorMountedBowlType = 3 then dstart = bowlWidth*0.17
			if iFloorMountedBowlType = 4 then dstart = bowlWidth*0.35
			if iFloorMountedBowlType = 5 then dstart = bowlWidth*0.12
			if iFloorMountedBowlType = 6 then dstart = bowlWidth*0.4
		else
			dstart = bowlWidth*0.25
			if iWallHungBowlType = 2 then dstart = bowlWidth*0.32
			if iWallHungBowlType = 3 then dstart = bowlWidth*0.35
			if iWallHungBowlType = 4 then dstart = bowlWidth*0.4
		endif

		tube 2, 7+nsp/4, 1+2+16+32,
			0, 0, 901,
			grabRadius, 360, 4001,

			0, yHoldingPos, zHoldingPos, -90,
			-dstart, yHoldingPos, zHoldingPos, -90,
			-grabWidth/2+roundoff2, yHoldingPos, zHoldingPos, -90,
			get(nsp/3),
			-grabWidth/2, yHoldingPos, grabHeight-roundoff2*2-roundoff2*tan(fi/2), 0,
			get(nsp/2),
			-grabWidth/2, yHoldingPos-roundoff2*3+roundoff2*tan(gi/2), grabHeight-roundoff2+roundoff2*cos(gi), 0,
			get(nsp),
			-grabWidth/2, -grabLength+gripLength, grabHeight, 0,
			-grabWidth/2, -grabLength, grabHeight, 0
		mulx -1
	next hj
	del 2

	if bToiletPaper then gosub "toiletPaper"

	material matGrip
	for hj = 1 to 2

		!height hsp edit
		add -grabWidth/2, -grabLength, 0
		hotspot	0, 0, 0,			newunID, rearsidegb_distFromFloor,	1+128: newunID = newunID + 1
		hotspot	0, 0, -1,			newunID, rearsidegb_distFromFloor,	3: newunID = newunID + 1
		hotspot	0, 0, grabHeight,	newunID, rearsidegb_distFromFloor,	2: newunID = newunID + 1

		add 0, gripLength, grabHeight
		rotz -90
		if gs_detlevel_3D_m = DETLEV_3D_DETAILED then
			revolve 6, 360, 1+2+4+8+16+32,
				0, 0, 1,
				0, grabRadius, 1,
				gripRadius-grabRadius, gripRadius, 1001,
				gripLength-(gripRadius-grabRadius), gripRadius, 1,
				gripLength, grabRadius, 1001,
				gripLength, 0, 1
		else
			revolve 2, 360, 1+2+4+8+16+32,
				0, gripRadius, 0,
				gripLength, gripRadius, 0
		endif
		del 3
		addx grabWidth
	next hj
	del 2

return


!================================================================================
"grabbar_rear":
!================================================================================
	material matGrabBar

	add -wPlate / 2, -0.005, rearsidegb_distFromFloor - hPlate + hConsole / 2 + ePlate
	block wPlate, 0.005, hPlate
	add ePlate, 0, hPlate - ePlate
	rotx -90
	roty 90
	hotspot	lConsole - hConsole/2, hConsole/2, wConsole + grabRadius
	prism_ 6, wConsole,
		0, 0, 15,
		lConsole - hConsole/2, 0, 79,
		lConsole - hConsole/2, hConsole/2, 979,
		0, 180, 4079,
		0, hConsole, 15,
		0, 0, -1
	addz wConsole
	prism_ 2, 2 * grabradius,
		lConsole - hConsole/2, hConsole/2, 979,
		hConsole/2, 360, 4079
	del 3
	addx 2 * grabRadius + wConsole
	rotx -90
	roty 90
	prism_ 6, wConsole,
		0, 0, 15,
		lConsole - hConsole/2, 0, 79,
		lConsole - hConsole/2, hConsole/2, 979,
		0, 180, 4079,
		0, hConsole, 15,
		0, 0, -1
	del 5

	add 0, -grabLength + dBars/2, grabHeight

	!height hsp edit
	hotspot	0, 0, -grabHeight,		newunID, rearsidegb_distFromFloor,	1+128: newunID = newunID + 1
	hotspot	0, 0, -grabHeight-1,	newunID, rearsidegb_distFromFloor,	3: newunID = newunID + 1
	hotspot	0, 0, 0,				newunID, rearsidegb_distFromFloor,	2: newunID = newunID + 1

	rotx 90
	addy -dBars/2
		revolve 2, 180, 0,
					0, dBars/2, 901,
					grabRadius, 360, 4001

		if GLOB_VIEW_TYPE = 4 | GLOB_VIEW_TYPE = 5 | GLOB_VIEW_TYPE = 7 then	!section, elevation, detail
			flag = 79
		else
			flag = 78
		endif

		mulz -1
		addy dBars/2
		prism_ 2, grabLength - dBars/2 - 0.005 - lConsole + hConsole/2,
			0, 0, 900 + flag,
			grabRadius, 360, 4000 + flag

		addy -dBars
		prism_ 2, grabLength - dBars/2 - 0.005,
			0, 0, 900 + flag,
			grabRadius, 360, 4000 + flag
	del 6
return


!================================================================================
"flushHandlePos":
!================================================================================

	material flushHandleMat

	delnum = 0

	if gs_flushHandlePlace_m = 2 then
		mulx -1
		delnum = delnum + 1
	endif

	if iToiletMounting = 1 and (iFloorMountedBowlType = 5 or iFloorMountedBowlType = 6) then
		if iFloorMountedBowlType = 5 then
			addx -(0.407*bowlWidth + 0.015/2)
			addy -bowlDepth/4
			addz gs_seat_height	+ 0.100
			rotz 10
			delnum = delnum + 4
		endif

		if iFloorMountedBowlType = 6 then
			addx -(bowlWidth/2 + 0.015/2)
			addy -bowlDepth/7
			addz gs_seat_height + 0.150
			delnum = delnum + 3
		endif

		roty  90
		rotz -90
		delnum = delnum + 2

		if iFloorMountedBowlType = 5 then
			rotx  -15
			delnum = delnum + 1
		endif
	else
		addx -(cisternWidth/2 + 0.015/2)
		delnum = delnum + 1

		if iCisternType = 1 then addx 0.015/0.435*cisternWidth
		if iCisternType = 2 then addx 0.006/0.271*cisternWidth
		if iCisternType = 3 then addx 0.063/0.374*cisternWidth
		if iCisternType = 4 then addx 0
		if iCisternType = 5 then addx 0.004/0.400*cisternWidth
		delnum = delnum + 1

		addy -cisternDepth/2
		addz gs_seat_height + cisternHeight*0.6
		delnum = delnum + 2

		if iCisternType = 3 then
			rotz 25
			delnum = delnum + 1
		endif

		roty  90
		rotz -90
		delnum = delnum + 2

