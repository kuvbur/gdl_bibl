addx -dAxisFromWall

if iGrabBars = GRAB_PARALLEL then
	resol gs_resol
	if iPositionGrabBars = GRAB_FLOOR then gosub "grabbar_floor"

	if iPositionGrabBars = GRAB_TOILET then gosub "grabbar_toilet"

	if iPositionGrabBars = GRAB_REARREAR then
		addx -grabWidth/2
		gosub "grabbar_rear"
		addx grabWidth
		gosub "grabbar_rear"
		del 2
		if bToiletPaper then
			addz -dBars
			gosub "toiletPaper"
			del 1
		endif
	endif

	if iPositionGrabBars = GRAB_SIDEREAR then
		addx -grabWidth/2
		gosub "grabbar_rear"
		del 1
		addx dAxisFromWall
		gosub "grabbar_sidewall"
		del 1
	endif

	if iPositionGrabBars = GRAB_SIDESIDE then
		addx dAxisFromWall
		gosub "grabbar_sidewall"
		mulx -1
		addx dAxisFromWall + MSSide + bowlWidth / 2
		gosub "grabbar_sidewall"
		del 3
	endif
