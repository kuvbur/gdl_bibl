
!!! *****************
!!! HVAC
!!! *****************
Width=a
Height=ZZYZX
Depth=b

!Width=HVAC_Width
!Height=HVAC_Height
!Depth=HVAC_Depth

ingLength=HVAC_ingLength
PLH=HVAC_PLH
legW=HVAC_legW
legH=HVAC_legH



!!! ***************
!!! To Localise
!!! ***************

DIM txt_Shape[3]
txt_Shape[1] = `Прямоугольный`
txt_Shape[2] = `Круглая`
txt_Shape[3] = `Овал`


!!! ***************
!!! Special Vars
!!! ***************

ang=atn((Height-PLH)/Depth)
D1=(Height-PLH)/sin(ang)

EPS = 0.00001

if GLOB_MODPAR_NAME = "gs_detlevel_3D" or gs_detlevel_3D_m = -1 then
	if gs_detlevel_3D = `Детальный` then gs_detlevel_3D_m = 2
	!if gs_detlevel_3D = `Simple` then gs_detlevel_3D_m = 1
	if gs_detlevel_3D = `Откл.` then gs_detlevel_3D_m = 0
	parameters gs_detlevel_3D_m = gs_detlevel_3D_m
else
	if gs_detlevel_3D_m = 2 then gs_detlevel_3D = `Детальный`
	!if gs_detlevel_3D_m = 1 then gs_detlevel_3D = `Simple`
	if gs_detlevel_3D_m = 0 then gs_detlevel_3D = `Откл.`
	parameters gs_detlevel_3D = gs_detlevel_3D
endif
