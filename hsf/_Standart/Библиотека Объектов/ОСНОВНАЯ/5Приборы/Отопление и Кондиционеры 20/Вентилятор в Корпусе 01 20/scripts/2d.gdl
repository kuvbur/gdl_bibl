
!PROJECT2 3,270,2

W=a
H=ZZYZX
D=b

ADD2 W/2,D/2

HOTSPOT2 0,D/2
HOTSPOT2 0,0

RESOL HVAC_res

PEN gs_cont_pen
FILL gs_fill_type
LINE_TYPE HVAC_linCon

Hatch=0
IF HVAC_shHatch THEN Hatch=2


!******* Contour Hatch

POLY2_B 4, 1+2+4+64, gs_fill_pen, gs_back_pen,
	 W/2, D/2,1,
	 W/2,-D/2,1,
	-W/2,-D/2,1,
	-W/2, D/2,1

LINE2 W/2-legW,-D/2,W/2-legW,D/2
LINE2 -W/2+legW,-D/2,-W/2+legW,D/2

add2 -W/2+legW,D/2
rot2 -90
mul2 D/D1,1
For i=0.03 to D1-0.02 step 0.02
	RECT2 i,0.02,i+0.01,W-2*legW-0.02
Next i
del top

CALL "Resize_A_B_ZZYZX" Parameters ALL
END
