

EPS = 0.00001

if GLOB_MODPAR_NAME="A" then
	b=a
	parameters b=b
	A2=A/2
	parameters A2=A2
endif

if GLOB_MODPAR_NAME="B" then
	a=b
	parameters a=a
endif

If GLOB_MODPAR_NAME="A2" Then
	A=A2*2
	parameters A=A
	b=A2*2
	parameters b=b
endif

IF ABS(A-A2) > EPS THEN
	A2 = A/2
	PARAMETERS A2 = A2
	B = A
	PARAMETERS B = B
ENDIF
![#COMPATIBILITY] - START
COMP_STATE_PLACEDorDEFAULT = 0
COMP_STATE_OLD_UPDATED     = 1
COMP_STATE_NEW_ELEMENT     = 2
![#COMPATIBILITY] - END

! Show MEP Connections -------------------------------------------------------------[

isavailable = 0
isInArchiCAD = APPLICATION_QUERY ("MEPMODELER", "IsAvailable()", isavailable)
isMEPEnabled = (isavailable OR isInArchiCAD = 0)
if isMEPEnabled then
	bShowMEP = 1
	reqBMep = 0
	success = LIBRARYGLOBAL ("LibraryGlobals13", "showMepConnect", reqBMep)
	if success > 0 then
		bShowMEP = reqBMep
	endif
else
	bShowMEP = 0
endif

! Show MEP Connections -------------------------------------------------------------]
