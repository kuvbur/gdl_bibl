		ConStatus = ConStatus_1,
		ConType_1 = ConType_1,
		ConConnectorType_1 = ConConnectorType_1,
		ConSystem_1 = ConSystem_1,
		ui_current_con = ui_current_con,
		shape_typ_to_validation = shape_typ_to_validation,
		MEP_enabled_geometry_mod = 1

	parameters MEP_ConInfo[ConID][1] = STR(ConToolType_1, 1, 0)
	parameters MEP_ConInfo[ConID][2] = ConName
	parameters MEP_ConInfo[ConID][3] = "0"

	ConID = 2
	ConName = `Труба Выпускного Соединения`

	ConDVecX = 0
	ConDVecY = -1
	ConDVecZ = 0

	ConWVecX = -1
	ConWVecY = 0
	ConWVecZ = 0

	parameters ConPosY_2 = -gs_boxWidth/2 + 0.1
	values "ConPosX_2" range [-gs_boxLength/2 + ConWidth_2/2, -gs_boxLength/2 + 0.1875 - ConWidth_2/2]
	values "ConPosZ_2" range [panelThk + ConWidth_2/2, zzyzx - ConWidth_2/2]

