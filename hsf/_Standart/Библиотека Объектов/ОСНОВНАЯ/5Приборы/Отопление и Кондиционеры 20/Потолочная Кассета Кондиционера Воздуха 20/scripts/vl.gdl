	ConName = `Соединение Отвода Конденсата`

	ConDVecX = 0
	ConDVecY = -1
	ConDVecZ = 0

	ConWVecX = -1
	ConWVecY = 0
	ConWVecZ = 0

	values "ConPosX_3" range [gs_boxLength/2 - 0.16 + ConWidth_3/2 + ConWallThickness_3, gs_boxLength/2 - ConWidth_3/2 - ConWallThickness_3]
	values "ConPosY_3" 	(-gs_boxWidth/2 - ConWidth_3/2 - ConWallThickness_3 + (ConPosX_3 - gs_boxLength/2) + 0.16)
	values "ConPosZ_3" range [panelThk + ConWidth_3/2, zzyzx - ConWidth_3/2]

	call "MEP_m_ConnectionsACL_4" parameters SetProgram = SetProgram,
		gs_Connections = gs_Connections,
		MEP_NumConnectionData = MEP_NumConnectionData,
		MEP_StrConnectionData = MEP_StrConnectionData,
		MEP_NumberConnections = MEP_NumberConnections,
