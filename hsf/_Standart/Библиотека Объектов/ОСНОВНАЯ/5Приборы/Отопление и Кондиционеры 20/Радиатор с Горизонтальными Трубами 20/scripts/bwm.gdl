
targetGUID = TO_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

gosub "gaps_BWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid targetGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! New Insert Gaps parameter
! ==============================================================================
"gaps_BWM":
	if targetGUID # "" then

		if iRadiatorType = RADIATORTYPE_FLAT then
			!can't migrate flat type without gaps
			if not(bGaps) then
				targetGUID = ""
			else
				!set structure thickness
				bd = 0.03
				parameters bd = bd
			endif
		endif

	endif
return
