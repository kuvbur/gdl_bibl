"DefineFontStyles":

	define style "sheet1" fontType, sizeFont2, 8, 0
	define style "sheet2" fontType, sizeFont2, 4, 0
	define style "sheet3" fontType, sizeFont2, 2, 0
	define style "sheet4" fontType, sizeFont2, 6, 0
	define style "sheet5" fontType, sizeFont2, 5, 0

	define style "detail" fontType, sizeFont2, 5, 0

	define style "sheet" fontType, sizeFont2, 5, 0
return

! -----------------------------------------------------------------------------
! Polylines
! -----------------------------------------------------------------------------

"polylines":

	s = 0
	if iStyle > 3 then
		if not(bInactivePoints=0 & (bCalcUpper=0 or bCalcUpper>10000)) then s = 1
	else
		if iStyle = 2 then
			if not(bUpper <= 0 & bInactivePoints=0) then s = 1
		else
			if not(iNumUpper <= 0 & bInactivePoints=0) then s = 1
		endif
	endif

	pen cpn
	poly2_ 6,1,
		-a/8**0.5,b/8**0.5,s,
		0,b/2**0.5,s,
		a/8**0.5,b/8**0.5,1,
		0,0,900,
		0,90,4001,
		-a/8**0.5,b/8**0.5,1

	s = 0
	if iStyle > 3 then
		if not(bInactivePoints=0 & (bCalcRight=0 or bCalcRight>10000)) then s = 1
	else
		if iStyle = 2 then
			if not(bRight <= 0 & bInactivePoints=0) then s = 1
		else
			if not(iNumRight <= 0 & bInactivePoints=0) then s = 1
		endif
	endif
	rot2 -90
	poly2_ 6,1,
		-a/8**0.5,b/8**0.5,s,
		0,b/2**0.5,s,
		a/8**0.5,b/8**0.5,1,
		0,0,900,
		0,90,4001,
		-a/8**0.5,b/8**0.5,1

	s = 0
	if iStyle > 3 then
		if not(bInactivePoints=0 & (bCalcLower=0 or bCalcLower>10000)) then s = 1
	else
		if iStyle = 2 then
			if not(bLower <= 0 & bInactivePoints=0) then s = 1
		else
			if not(iNumLower <= 0 & bInactivePoints=0) then s = 1
		endif
	endif
	rot2 -90
	poly2_ 6,1,
		-a/8**0.5,b/8**0.5,s,
		0,b/2**0.5,s,
		a/8**0.5,b/8**0.5,1,
		0,0,900,
		0,90,4001,
		-a/8**0.5,b/8**0.5,1

	s = 0
	if iStyle > 3 then
		if not(bInactivePoints=0 & (bCalcLeft=0 or bCalcLeft>10000)) then s = 1
	else
		if iStyle = 2 then
			if not(bLeft <= 0 & bInactivePoints=0) then s = 1
		else
			if not(iNumLeft <= 0 & bInactivePoints=0) then s = 1
		endif
	endif
	rot2 -90
	poly2_ 6,1,
		-a/8**0.5,b/8**0.5,s,
		0,b/2**0.5,s,
		a/8**0.5,b/8**0.5,1,
		0,0,900,
		0,90,4001,
		-a/8**0.5,b/8**0.5,1
	del 3



return

! -----------------------------------------------------------------------------
! Elevation definition
! -----------------------------------------------------------------------------

"elevationDef":
	nTrans = 0
	x = 0
	if txtElevationDef <> "0" then
		x = 1
		pen dlpen
		line2 -a/2, 0, a/2, 0

		if iStyle = 1 then
			pen dnpen
			style "sheet5"
			add2 0,(papersizeFont2/2+(tf-fsz/8)*papersizeFont2/fsz)*x
		endif
		if iStyle = 2 then
			pen dnpen
			add2 0,(paperSizeFont/2+(tf-fsz/8)*paperSizeFont/fsz)*x
		endif
		if iStyle = 4 then
			pen snpen
			style "sheet5"
			add2 0, (papersizeFont2/2+(tf-fsz/8)*papersizeFont2/fsz)*x
		endif

		nTrans = nTrans + 1
		! Text Rotation - Readable / Align with Symbol
		! ----------------------------------------------------------------------
		if typeTextRotation_m = 2 then								! Readable
			if (totalRotate > (gs_readable_angle + eps) & totalRotate < (gs_readable_angle + 180 + eps)) then
				rot2 180
				nTrans = nTrans + 1
			endif
		endif
		! ----------------------------------------------------------------------
		text2 0, 0, txtElevationDef
		del nTrans
		nTrans = 0
	endif
return






