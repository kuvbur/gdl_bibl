		! Text Rotation - Readable / Align with Symbol
		! ----------------------------------------------------------------------
		if typeTextRotation_m = 2 then								! Readable
			if (totalRotate > (gs_readable_angle + eps) & totalRotate < (gs_readable_angle + 180 + eps)) then
				rot2 180
				add2 0,-textHeight1
				nTrans = nTrans + 2
			endif
		endif
		! ----------------------------------------------------------------------

		text2 0,0,iNumUpper
		del nTrans
		nTrans = 0
	endif

	! -------------------------------------------------------------------------
	! Right (0-No)
	! -------------------------------------------------------------------------

	if not(iNumRight = 0 | iNumRight > 10000) then
		iNumRight = iNumRight + 10000
		rot2 -90
		SET fill fil
		poly2_b{2}	5,2,fpen,fbckpen,
				0,0,0,
				-a/8**0.5,b/8**0.5,1,
				0,b/2**0.5,1,
				a/8**0.5,b/8**0.5,1,
				0,0,900,
				0,90,4001
		del 1
	endif

	if iNumRight <> 0 then
		iNumRight = iNumRight-10000
		pen dnpen
		style "sheet2"
		textWidth2 = stw(STR(iNumRight, 1, 0))*paperToModel

		add2 a/2**0.5+papersizeFont1/4,0
		nTrans = nTrans + 1

		! Text Rotation - Readable / Align with Symbol
		! ----------------------------------------------------------------------
		if typeTextRotation_m = 2 then								! Readable
			if (totalRotate > (gs_readable_angle + eps) & totalRotate < (gs_readable_angle + 180 + eps)) then
				rot2 180
				add2 -textWidth2 ,0
				nTrans = nTrans + 2
			endif
		endif
		! ----------------------------------------------------------------------
		text2  0,0,iNumRight
		del nTrans
		nTrans = 0
	endif

	! -------------------------------------------------------------------------
	! Lower (0-No)
	! -------------------------------------------------------------------------

	if not(iNumLower = 0 | iNumLower > 10000) then
		iNumLower = iNumLower + 10000
		rot2 -180
		fill fil
		poly2_b{2}  5,2,fpen,fbckpen,
					0,0,0,
					-a/8**0.5,b/8**0.5,1,
					0,b/2**0.5,1,
					a/8**0.5,b/8**0.5,1,
					0,0,900,
					0,90,4001
		del 1
	endif

	if iNumLower <> 0 then
		iNumLower=iNumLower-10000
		pen dnpen
		style "sheet3"

		add2 0,-a/2**0.5
		nTrans = nTrans + 1

		! Text Rotation - Readable / Align with Symbol
		! ----------------------------------------------------------------------
		if typeTextRotation_m = 2 then								! Readable
			if (totalRotate > (gs_readable_angle + eps) & totalRotate < (gs_readable_angle + 180 + eps)) then
				rot2 180
				add2 0, textHeight3
				nTrans = nTrans + 2
			endif
		endif
		! ----------------------------------------------------------------------
		text2  0,0,iNumLower
		del nTrans
		nTrans = 0
	endif

	! -------------------------------------------------------------------------
	! Left (0-No)
	! -------------------------------------------------------------------------

	if not(iNumLeft = 0 | iNumLeft > 10000) then
		iNumLeft = iNumLeft + 10000
		rot2 -270
		fill fil
		poly2_b{2} 5,2,fpen,fbckpen,
					0,0,0,
					-a/8**0.5,b/8**0.5,1,
					0,b/2**0.5,1,
					a/8**0.5,b/8**0.5,1,
					0,0,900,
					0,90,4001
		del 1
	endif

	if iNumLeft <> 0 then
		iNumLeft = iNumLeft - 10000
		pen dnpen
		style "sheet4"

		textWidth4 = stw(STR(iNumLeft, 1, 0))*paperToModel

		add2 -a/2**0.5-paperSizeFont1/4,0
		nTrans = nTrans + 1

		! Text Rotation - Readable / Align with Symbol
		! ----------------------------------------------------------------------
		if typeTextRotation_m = 2 then								! Readable
			if (totalRotate > (gs_readable_angle + eps) & totalRotate < (gs_readable_angle + 180 + eps)) then
				rot2 180
				add2 textWidth4 , 0
				nTrans = nTrans + 2
			endif
		endif
		! ----------------------------------------------------------------------
		text2  0,0,iNumLeft
		del nTrans
		nTrans = 0
	endif

return


! -----------------------------------------------------------------------------
! Oriented view
! -----------------------------------------------------------------------------

"OrientedView":
	angleviewRot = 0
	rrr = REQUEST ("view_Rotangle", "", angleviewRot)
	totalRotate = (SYMB_ROTANGLE + angleviewRot) MOD 360
return

! -----------------------------------------------------------------------------
