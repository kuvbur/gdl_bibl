
call "FM_types" parameters all

if gs_cont_pen<=0 then parameters gs_cont_pen=4
if gs_fill_pen<=0 then parameters gs_fill_pen=19

values "minSpaceDiameter" range (1, )

if iSymbolType = ISYMBTYPE_WHEELCHAIR then
	values "WheelchairA" range (0,]
	values "WheelchairB" range (0,]
	values "WheelchairA2" range (0,]
	values "WheelchairB2" range (0,]

	hideparameter "MSContLineType", "MSFillType", "MSContLinePen", "MSFillPen", "MSFillBackPen", "A", "B"
	lock "A", "B"
else	!with circle or only circle
	values "A" range (1, )		!minSpaceDiameter
	values "B" range (1, )		!minSpaceDiameter

	values "WheelchairA" range  (0, minSpaceDiameter * 0.9 * 0.85]
	values "WheelchairB" range  (0, minSpaceDiameter * 0.9]
	values "WheelchairA2" range (0, minSpaceDiameter * 0.9 * 0.85 / 2]
	values "WheelchairB2" range (0, minSpaceDiameter * 0.9 / 2]

	if iSymbolType = ISYMBTYPE_CIRCLE then
		hideparameter "WheelchairA", "WheelchairB"
	endif
endif

values{2}	"iSymbolType"	ISYMBTYPE_WHEELCHAIR,	stSymbType[1],
							ISYMBTYPE_CIRCLE,		stSymbType[2],
							ISYMBTYPE_BOTH,			stSymbType[3]


if GLOB_MODPAR_NAME = "WheelchairB" | GLOB_MODPAR_NAME = "WheelchairB2" then
	if GLOB_MODPAR_NAME = "WheelchairB" then
		WheelchairB2 = WheelchairB / 2
		parameters WheelchairB2 = WheelchairB2
	endif
	if GLOB_MODPAR_NAME = "WheelchairB2" then
		WheelchairB = WheelchairB2 * 2
		parameters WheelchairB = WheelchairB
	endif

	WheelchairA = WheelchairB*0.85
	WheelchairA2 = (WheelchairB*0.85)/2
	parameters	WheelchairA = WheelchairA, WheelchairA2 = WheelchairA2
endif

if GLOB_MODPAR_NAME = "WheelchairA" then

	if GLOB_MODPAR_NAME = "WheelchairA" then
		WheelchairA2 = WheelchairA / 2
		parameters WheelchairA2 = WheelchairA2
	endif

	WheelchairB		= WheelchairA/0.85
	WheelchairB2	= (WheelchairA/0.85)/2
	parameters	WheelchairB = WheelchairB, WheelchairB2 = WheelchairB2
endif

if GLOB_MODPAR_NAME = "minSpaceDiameter" then
	A = minSpaceDiameter
	parameters	A = A
else
	minSpaceDiameter = A
	parameters	minSpaceDiameter = minSpaceDiameter
endif

B = A
parameters	B = B


