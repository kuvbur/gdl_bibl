call "parking_symbols" parameters all A = A*0.8, B = B*0.4, isymboltype = 1, bFillIn2D = 1, gs_fill_pen = spn
del 1

! Letter P

mul2 a/0.063482,b/0.1143
poly2_b 16, 1+2, spn, gs_back_pen,
		0.0359634, 0.100782,      1,
		0.0175031, 0.100782,      1,
		0.0175031, 0.064859,      1,
		0.0222429, 0.064859,      1,
		0.0222429, 0.077831,      1,
		0.0359634, 0.077831,      1,
		0.0359634, 0.0893065,    900,
		   0, 180,   4001,
		0.0359634, 0.100782,     -1,
		0.0222429, 0.095958,      1,
		0.0359634, 0.095958,      1,
		0.0359634, 0.0893475,    900,
		   0, -180,   4001,
		0.0359653, 0.082737,      1,
