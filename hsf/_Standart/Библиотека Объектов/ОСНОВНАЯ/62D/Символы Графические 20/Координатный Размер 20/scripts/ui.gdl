	! --- Marker Symbol
	
	y = pageStartY + 139
	ui_separator 170, y, 440, y
	y = y + 4
	
	ui_style 0,1
	ui_outfield `Маркер`, xo1, y, xolen3, ho
	
	ui_style 0,0
	y = y + dy - 4
	
	ui_infield{3} "bShowMarkerID", xo1, y-di, 150, hi,
			7, "", 2, 0, 0, 0, 0, 0,
			1, `Маркер с ID`, 0,
			2, `Маркер с ID`, 1 	\
