
!=======================================================================
! Tabpage System
!=======================================================================

ui_current_page gs_ui_current_page

file_dependence "uiTab_Dimension_flat(1)"
file_dependence "uiTab_Text_flat(1)"

TABID_ROOT				= -1
TABID_COORDINATE		= 1
TABID_2DTEXT			= 2
TABID_3DTEXT			= 3

if TABID_COORDINATE > 0 then
	ui_page TABID_COORDINATE, TABID_ROOT, `Система Координат и Специальное Начало`, "uiTab_Dimension_flat(1)"
	gosub "coordinatePage"
endif

if TABID_2DTEXT > 0 then
	ui_page TABID_2DTEXT , TABID_ROOT, `2D-текст и Маркер`, "uiTab_Text_flat(1)"
	gosub "2DTextPage"
endif

if TABID_3DTEXT > 0 then
	ui_page TABID_3DTEXT , TABID_ROOT, `3D-текст и Маркер`, "uiTab_Text_flat(1)"
	gosub "3DTextPage"
endif

! =============================================================================
end
! =============================================================================

"coordinatePage":
!=======================================================================
! Page 1 - Coordinate system and custom origin
!=======================================================================
	
	! --------- Field sizes
	
	pageStartY = 5
	dy = 23
	y0 = pageStartY 
	
	! --- Outfields
	xo1 = 10
	xo2 = 30
	xo3 = 185
	xo4 = 235
	xolen1 = 140
	xolen2 = 400
	xolen3 = 60
	xolen4 = 240
	xolen5 = 90
	xolen6 = 220
	xolen7 = 115
	ho = 15
	
	! --- Infields
	xi1 = 10
	xi2 = 30
	xi3 = 75
	xi4 = 235
	xi5 = 185
	xi6 = 290
	xilen1 = 18
	xilen2 = 195
	xilen3 = 90
	xilen4 = 135
	hi = 18
	di = 3
	
	
	! --- Coordinate System
	
	y = y0
	
	ui_style 0,1
	ui_outfield `Выбор Системы координат`, xo1, y, xolen6, ho
	ui_style 0,0
	ui_infield{3} "strCoorSystem", xi5+60, y-di, xilen2, hi,
			8, "", 0, 0, 0, 0, 0, 0,
			"", strCoorSystem_strings[1], strCoorSystem_strings[1],
			"", strCoorSystem_strings[2], strCoorSystem_strings[2],
			"", strCoorSystem_strings[3], strCoorSystem_strings[3]	\
			ui_tooltip `Система Координат`
	y = y + 17
	
	if iCoorSystem = WORLD_COOR_CUST then
		ui_outfield `Выбор Мировой Системы Координат может привести к незначительной`, xo1, y, xolen2, ho-1
		y = y + 15
		ui_outfield `погрешности в отображаемых значениях из-за формы Земли.`, xo1, y, xolen2, ho-1
		y = y + dy
	endif
	
	
	! --- Custom Origo
	
	ui_separator 10, pageStartY + 50, 435, pageStartY + 50
	
	y = pageStartY + 55
	
	ui_style 0,1
	ui_outfield `Специальное Начало`, xo1, y, xolen1, ho
	ui_style 0,0
	y = y + dy
	
	ui_infield{3} "bCustomOrig", xi1, y-di-4, 160, hi+12,
			7, "", 2, 0, 0, 0, 0, 0,
			1, `Специальное Начало`, 0,
			2, `Специальное Начало`, 1	\
			ui_tooltip `Специальное Начало`
	y = y + dy + 2
	
	iFlag = 4*not(bCustomOrig)
	
	ui_outfield `Смещение отн. Начала Проекта`, xo1, y, xolen1, ho,iFlag
	y = y + dy - 2
	
	ui_outfield `X`, xo1, y, xolen3, ho, iFlag
	ui_infield "CustomOrigX", xi3, y-di, xilen3, hi 	ui_tooltip `Смещение (X)`
	y = y + dy
	
	ui_outfield `Y`, xo1, y, xolen3, ho,iFlag
	ui_infield "CustomOrigY", xi3, y-di, xilen3, hi 	ui_tooltip `Смещение (Y)`
	y = y + dy
	
	ui_outfield `Z`, xo1, y, xolen3, ho,iFlag
	ui_infield "CustomOrigZ", xi3, y-di, xilen3, hi 	ui_tooltip `Смещение (Z)`
	y = y + dy
	
	ui_infield{3} "bShowCustomOrig", xi1, y-di, 160, hi,
			7, "", 2, 0, 0, 0, 0, 0,
			1, `Показ на Плане`, 0,
			2, `Показ на Плане`, 1 	\
			ui_tooltip `Показ Специального Начала на Плане`
	y = y + dy
	
	ui_infield{3} "bShowCustomOrig3D", xi1, y-di, 160, hi,
			7, "", 2, 0, 0, 0, 0, 0,
			1, `Показ в 3D`, 0,
			2, `Показ в 3D`, 1 	\
			ui_tooltip `Показ Специального Начала в 3D`
	
	! --- Coordinates
	
	ui_separator 175, pageStartY + 55, 175, pageStartY + 229
	
	y = pageStartY + 55
	
	iFlag = 4*(iCoorSystem = PROJECT_COOR)
	
	ui_style 0,1
	ui_outfield `Мировые Координаты Начала`, xo3, y, xolen4, ho
	ui_style 0,0
	y = y + dy
	
	ui_infield "iLatitudeDegree", xi5, 			y-di, 45, hi 	ui_tooltip `Широта (градусы)`
	ui_outfield "°", xi5+51, y, 5, ho, iFlag
	ui_infield "iLatitudeMinute", xi5+60, 		y-di, 45, hi 	ui_tooltip `Широта (минуты)`
	ui_outfield "'", xi5+60+51, y, 5, ho, iFlag
	ui_infield "rLatitudeSecond", xi5+60*2, 	y-di, 60, hi 	ui_tooltip `Широта (секунды)`
	ui_outfield "''", xi5+60+75+51, y, 5, ho, iFlag
	
	ui_infield "strLatitudeRef", xi5+60*2+75, 	y-di, 60, hi 	ui_tooltip `Широта в Мировых Координатах`
	y = y + dy
	
	ui_infield "iLongitudeDegree", xi5, 		y-di, 45, hi 	ui_tooltip `Долгота (градусы)`
	ui_outfield "°", xi5+51, y, 5, ho, iFlag
	ui_infield "iLongitudeMinute", xi5+60, 		y-di, 45, hi 	ui_tooltip `Долгота (минуты)`
	ui_outfield "'", xi5+60+51, y, 5, ho, iFlag
	ui_infield "rLongitudeSecond", xi5+60*2, 	y-di, 60, hi 	ui_tooltip `Долгота (секунды)`
	ui_outfield "''", xi5+60+75+51, y, 5, ho, iFlag
	
	ui_infield "strLongitudeRef", xi5+60*2+75, 	y-di, 60, hi 	ui_tooltip `Долгота в Мировых Координатах`
	y = y + dy
	
	ui_outfield `Возвышение`, xi5, y, 115, ho, iFlag
	ui_infield "elevWorldCoor", xi5+60+60, y-di, xolen3, hi 	ui_tooltip `Возвышение`
	y = y + dy
	
	if iCoorSystem = WORLD_COOR_CUST then
		ui_outfield `Погрешность Вычисления:` + " " + errorString, xo3, y, 250, ho
	endif
	y = y + dy
	
	
	! --- Height position
	
	ui_separator 180, pageStartY + 165, 435, pageStartY + 165
	
	ui_style 0,1
	ui_outfield `Высота`, xo3, y, xolen4, ho
	ui_style 0,0
	y = y + dy
	
	ui_infield{3} "bHeight", xi5, y-di, 160, hi,
			7, "", 2, 0, 0, 0, 0, 0,
			1, `Показ Высоты`, 0,
			2, `Показ Высоты`, 1 	\
			ui_tooltip `Показ Высоты`
	y = y + dy
	
	iFlag = 4*not(bHeight)
	
	ui_outfield `Показ Высоты отн.`, xo3, y-2, xolen7, ho, iFlag
	ui_infield{3} "strHeightRef", xi5+120, y-di-2, xilen4, hi,
			8, "", 0, 0, 0, 0, 0, 0,
			"", strHeightRef_strings[1], strHeightRef_strings[1],
			"", strHeightRef_strings[2], strHeightRef_strings[2],
			"", strHeightRef_strings[3], strHeightRef_strings[3],
			"", strHeightRef_strings[4], strHeightRef_strings[4] 	\
			ui_tooltip `Показ Высоты отн.`
	y = y + dy
return

