			6, "", 2, 1, 16, 16, 16, 16,
			"ui_bold_flat(1)", "", 0,
			"ui_bold_flat(1)", "", 1\
			ui_tooltip `Жирный`
	
		ui_infield{3} "bItalicCoor", 		290 + 26 - 40, y, 24, 24,
			6, "", 2, 1, 16, 16, 16, 16,
			"ui_italic_flat(1)", "", 0,
			"ui_italic_flat(1)", "", 1\
			ui_tooltip `Курсив`
	
		ui_infield{3} "bUnderlinedCoor", 	290 + 26 * 2 - 40, y, 24, 24,
			6, "", 2, 1, 16, 16, 16, 16,
			"ui_underline_flat(1)", "", 0,
			"ui_underline_flat(1)", "", 1\
			ui_tooltip `Подчеркнутый`
