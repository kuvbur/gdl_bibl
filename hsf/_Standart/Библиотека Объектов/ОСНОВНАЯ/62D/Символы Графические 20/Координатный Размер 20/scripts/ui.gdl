	ui_pict "ui_FontSize_flat(1)", 290 - 40, y,    27, 14, 1
	ui_pict "ui_text_pen_flat(1)", 290 - 40, y + dy + 4, 24, 17, 1
	
	if STRSTR (workingLengthUnit,"m")  > 0 then
