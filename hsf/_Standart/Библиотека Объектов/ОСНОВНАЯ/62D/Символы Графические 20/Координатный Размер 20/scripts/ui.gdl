		ui_infield "iPenFontCustOrig", 		xi2,    y-di, 30 + 40, 		hi 	ui_tooltip `Перо Текста`
		ui_infield "penFontCustOrig",  		xi2+35 + 40, y-di, xilen3, 	hi 	ui_tooltip `Перо Текста`
