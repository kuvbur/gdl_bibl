	if iApplyFontTo = APPLY_TO_ID then
		ui_infield{3} "bBoldID", 			290 - 40, 	y, 24, 24,
			6, "", 2, 1, 16, 16, 16, 16,
			"ui_bold_flat(1)", "", 0,
			"ui_bold_flat(1)", "", 1\
			ui_tooltip `Жирный`
	
		ui_infield{3} "bItalicID", 			290 + 26 - 40, y, 24, 24,
			6, "", 2, 1, 16, 16, 16, 16,
			"ui_italic_flat(1)", "", 0,
			"ui_italic_flat(1)", "", 1\
			ui_tooltip `Курсив`
	
		ui_infield{3} "bUnderlinedID", 		290 + 26 * 2 - 40, y, 24, 24,
			6, "", 2, 1, 16, 16, 16, 16,
			"ui_underline_flat(1)", "", 0,
			"ui_underline_flat(1)", "", 1\
			ui_tooltip `Подчеркнутый`
	endif
	
	if iApplyFontTo = APPLY_TO_CUSTORIG then
		ui_infield{3} "bBoldCustOrig", 			290 - 40, 	y, 24, 24,
			6, "", 2, 1, 16, 16, 16, 16,
			"ui_bold_flat(1)", "", 0,
			"ui_bold_flat(1)", "", 1\
