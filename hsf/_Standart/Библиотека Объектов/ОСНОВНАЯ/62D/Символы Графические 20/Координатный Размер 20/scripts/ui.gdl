		if iApplyFontTo = APPLY_TO_COOR 	then ui_infield "fontSizeCoor_mm", 		xi2, y-di, xilen2, hi 	ui_tooltip `Размер Шрифта (мм)`
		if iApplyFontTo = APPLY_TO_ID 		then ui_infield "fontSizeID_mm", 		xi2, y-di, xilen2, hi 	ui_tooltip `Размер Шрифта (мм)`
		if iApplyFontTo = APPLY_TO_CUSTORIG	then ui_infield "fontSizeCustOrig_mm",	xi2, y-di, xilen2, hi 	ui_tooltip `Размер Шрифта (мм)`
	
