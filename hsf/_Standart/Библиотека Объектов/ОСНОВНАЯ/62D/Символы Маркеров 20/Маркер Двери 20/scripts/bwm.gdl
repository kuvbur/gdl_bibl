
targetGUID = TO_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

gosub "RectFillSettings_BWM"

gosub "NewUnitSizesOption_BWM"

gosub "removeFireExpDACH_BWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid targetGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! RectFillSettings
! ==============================================================================
"RectFillSettings_BWM":
	if targetGUID # "" then
		if iMarkerShape = 3 then		! SHAPE_FRAGMENT
			if gs_fill_pen <> gs_cont_pen | gs_back_pen <> 0 then
				targetGUID = ""
			endif
		endif
	endif

return

! ==============================================================================
! NewUnitSizesOption
! ==============================================================================
"NewUnitSizesOption_BWM":
	if targetGUID # "" then
		if 	iWorkUnitLinear		<> 10	| \		! DIMUNIT_PROJECT
			iWorkUnitSillHgt	<> 10	then	! DIMUNIT_PROJECT
	
			targetGUID = ""
		endif
	endif
return

! ==============================================================================
! removeFireExpDACH
! ==============================================================================
"removeFireExpDACH_BWM":
	if targetGUID # "" then
		if (LibraryLangCode = "CHE" | LibraryLangCode = "GER") then
			targetGUID = ""
		endif
	endif
return

