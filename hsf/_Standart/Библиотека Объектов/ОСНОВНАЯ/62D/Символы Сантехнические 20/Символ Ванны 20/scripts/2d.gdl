
!! Bath Symb.gsm


CALL "Resize_A_B_ZZYZX" Parameters ALL

pen gs_cont_pen

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!!!!!!!!!!!!!!!!!!!!!! *** Bath Symbol *** !!!!!!!!!!!!!!!!!!!!!
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!


	if iBathSymbElevType = ELEV_TYPE_TOP then
		mul2 a/1.8,b/0.75
			fragment2 1,1
		del 1
	endif


	if iBathSymbElevType = ELEV_TYPE_SIDE then
		mul2 a/1.8,b/0.6
			fragment2 2,1
		del 1
	endif


	if iBathSymbElevType = ELEV_TYPE_FRONT then
		mul2 a/0.75,b/0.6
			fragment2 3,1
		del 1
	endif


if iBathSymbDetlevel = SYMBOL_TYPE_DETAILED then

	if iBathSymbElevType = ELEV_TYPE_TOP then
		mul2 a/1.8,b/0.75
			fragment2 4,1
		del 1
	endif


	if iBathSymbElevType = ELEV_TYPE_SIDE then
		mul2 a/1.8,b/0.6
			fragment2 5,1
		del 1
	endif


	if iBathSymbElevType = ELEV_TYPE_FRONT then
		mul2 a/0.75,b/0.6
			fragment2 6,1
		del 1
	endif

endif
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
!!!!!!!!!!!!!!!!!!!!!!!!! *** Bath Fill *** !!!!!!!!!!!!!!!!!!!!
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!

fill gs_fill_type
!pen gs_fill_pen


	if iBathSymbElevType = ELEV_TYPE_TOP then
		mul2 a/1.8,b/0.75
			POLY2_B  5,  2, gs_fill_pen, gs_back_pen,
			0,0,1,
			1.8,0,1,
			1.8,0.75,1,
			0,0.75,1,
			0,0,-1

		del 1
	endif

	if iBathSymbElevType = ELEV_TYPE_SIDE then
		mul2 a/1.8,b/0.6
			POLY2_B  5,  2, gs_fill_pen, gs_back_pen,
			0,0,1,
			1.8,0,1,
			1.8,0.6,1,
			0,0.6,1,
			0,0,-1
		del 1
	endif


	if iBathSymbElevType = ELEV_TYPE_FRONT then
		mul2 a/0.75,b/0.6

			POLY2_B  5,  2, gs_fill_pen, gs_back_pen,
			0,0,1,
			0.75,0,1,
			0.75,0.6,1,
			0,0.6,1,
			0,0,-1
		del 1
	endif
