
pen gs_cont_pen
unID = 1

HOTSPOT2 a/2,a/2,unID :	unID=unID+1

CALL "Resize_A_B_ZZYZX" Parameters A=A,B=A

If iDrainSymbStyle = SYMBOL_STYLE_1 then
	rect2 0,0,a,a
	line2 0,0,a,a
	line2 a,0,0,a

endif

if iDrainSymbStyle = SYMBOL_STYLE_2 then
	add2 A/2, A/2

	for i = 1 to 4
		line2 A/8 * SQR(2), A/8 * SQR(2), A/2, A/2
		rot2 90
	next i
	del 1 + 4

	fill gs_fill_type

	POLY2_b 2, 1+2,gs_fill_pen,gs_back_pen,
		A/2,	A/2,	900,
		A/4,	360,	4001
endif

If iDrainSymbStyle = SYMBOL_STYLE_3 then

circle2 a/2,a/2,a/2
endif
