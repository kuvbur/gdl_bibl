
call "FM_types" parameters all

! -----------------------------------------------------------------------------
! Elevation types
! -----------------------------------------------------------------------------

values "elevtyp" stElevTypes
values "iElevTyp" 1, 2, 3

if GLOB_MODPAR_NAME = "elevtyp" then
	iElevTyp = 1
	if elevtyp = stElevTypes[2] then iElevTyp = 2
	if elevtyp = stElevTypes[3] then iElevTyp = 3
	parameters iElevTyp = iElevTyp
else
	elevtyp = stElevTypes[1]
	if iElevTyp = 2 then elevtyp = stElevTypes[2]
	if iElevTyp = 3 then elevtyp = stElevTypes[3]
	parameters elevtyp = elevtyp
endif

! -----------------------------------------------------------------------------
! Default Size
! -----------------------------------------------------------------------------

if GLOB_MODPAR_NAME = "iElevTyp" | GLOB_MODPAR_NAME = "elevtyp" then
	if iElevTyp = ELEV_TOP then
		a = sizes[1][1]
		b = sizes[1][2]
	endif
	if iElevTyp = ELEV_SIDE then
		a = sizes[1][1]
		b = sizes[1][3]
	endif
	if iElevTyp = ELEV_FRONT then
		a = sizes[1][2]
		b = sizes[1][3]
	endif
	parameters a = a
	parameters b = b
else
	if iElevTyp = ELEV_TOP then
		sizes[1][1] = a
		sizes[1][2] = b
	endif
	if iElevTyp = ELEV_SIDE then
		sizes[1][1] = a
		sizes[1][3] = b
	endif
	if iElevTyp = ELEV_FRONT then
		sizes[1][2] = a
		sizes[1][3] = b
	endif
	parameters sizes[1][1] = sizes[1][1]
	parameters sizes[1][2] = sizes[1][2]
	parameters sizes[1][3] = sizes[1][3]
endif
