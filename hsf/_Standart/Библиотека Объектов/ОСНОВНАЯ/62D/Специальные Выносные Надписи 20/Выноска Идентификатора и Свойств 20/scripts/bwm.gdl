
targetGUID = TO_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

gosub "NewHeadFunction_BWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid targetGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! NewHeadFunction
! ==============================================================================
"NewHeadFunction_BWM":
	if targetGUID # "" then
		bMigrationSuccess = 1

		if _bEnableSeparatorLine & bShowSeparatorLines then
			bMigrationSuccess = 0
		else
			if not(LABEL_CUSTOM_ARROW) then
				if iShapeHead <> 8 & not(bFitFrameToText) then
					bMigrationSuccess = 0
				endif
			else
				if b2ndRowContent	= 0 then	i2ndRowContent	= CONTENT_NONE
				if b3rdRowContent	= 0 then	i3rdRowContent	= CONTENT_NONE

				parameters	i2ndRowContent	= i2ndRowContent,
							i3rdRowContent	= i3rdRowContent
			endif
		endif

		if bMigrationSuccess = 0 then
			targetGUID = ""
		endif
	endif
return

