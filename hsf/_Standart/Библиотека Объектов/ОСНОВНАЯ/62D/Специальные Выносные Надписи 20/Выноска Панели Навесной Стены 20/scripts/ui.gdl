"GeometrySettingsPage":
! ------------------------------------------------------------------------------

	x1		=	10
	x2		=	165
	x3 		= 	210
	yblock	=	32
	xCheckbox	=	15
	heightRow	= 	18

	if iMarkerShape = SHAPE_CIRCLE or iMarkerShape = SHAPE_FRAGMENT then
		rot_flag = 4
	else
		rot_flag = 0
	endif

	! ------------------------------------------------------------------------------
	ui_separator prev_pos_x-5, yblock, prev_pos_x-5, 266

	ui_infield{3} "AC_SameSide",	x1,		yblock,		prev_pos_x - x1 -10,	heightRow,
			7,"",2,1,
			0,0,0,0,
			1,`Развернуть Выноску`, 0,
			2,`Развернуть Выноску`, 1

	yblock	=	yblock + 21

	ui_outfield `Форма Выноски`,		x1 + xCheckbox,	yblock,		x2-(x1 + xCheckbox)-1,		heightRow
	ui_infield "gs_marker_shape",	x2,			yblock-2,		prev_pos_x - x2 -10,		heightRow

	yblock	=	yblock + 21

	if iMarkerShape = SHAPE_CIRCLE then
		ui_infield{3} "gs_markertext_fit",	x1,		yblock,		prev_pos_x - x1 -10,	heightRow,
				7,"",2,1,
				0,0,0,0,
				1,`Форма Маркера по Размеру Шрифта`, 0,
				2,`Форма Маркера по Размеру Шрифта`, 1
	else
		ui_infield{3} "gs_marker_rotation",	x1,		yblock,		prev_pos_x - x1 -10,	heightRow,
				7,"",2,1,
				0,0,0,0,
				1,`Поворот Маркера`, 0,
				2,`Поворот Маркера`, 1
	endif

	yblock = yblock + 21

	ui_outfield `Направление Выноски`,	x1 + xCheckbox,		yblock,		x2-(x1 + xCheckbox)-1,	heightRow
	ui_infield "stMarkerDir",		x2,					yblock-2,	prev_pos_x - x2 -10,	heightRow

	yblock = yblock + 21

	! ------------------------------------------------------------------------------
	ui_separator 	x1,		yblock, 	prev_pos_x-10,	yblock

	yblock = yblock + 6

	ui_outfield `Ед. Изм. Размера Выноски`,		x1 + xCheckbox, yblock,		x3-(x1 + xCheckbox)-1,	heightRow
	ui_infield "markerSizeUnit",		x3,				yblock-2,	prev_pos_x - x3 -10,	heightRow

	yblock = yblock + 21

	ui_outfield `(1)`,					0,				yblock,	x1 + xCheckbox-1,		heightRow
	ui_outfield `Расположение Выноски`,		x1 + xCheckbox, yblock,	x3-(x1 + xCheckbox)-1, 	heightRow
	if iMarkerSizeUnit = 1 then	ui_infield "headDistMm",		x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize
	if iMarkerSizeUnit = 2 then	ui_infield "headDistCm",		x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize
	if iMarkerSizeUnit = 3 then	ui_infield "headDistPoint",		x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize

	yblock = yblock + 21

	ui_outfield `(2)`,					0,				yblock,	x1 + xCheckbox-1,		heightRow
	ui_outfield `Смещение Выноски`,		x1 + xCheckbox, yblock,	x3-(x1 + xCheckbox)-1,	heightRow
	if iMarkerSizeUnit = 1 then	ui_infield "markerOffsetMm",	x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize
	if iMarkerSizeUnit = 2 then	ui_infield "markerOffsetCm",	x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize
	if iMarkerSizeUnit = 3 then	ui_infield "markerOffsetPoint", x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize

	yblock = yblock + 21

	ui_outfield `(3)`,					0,				yblock,	x1 + xCheckbox-1,		heightRow, 4 * not(AC_WidHei_On)
	ui_outfield `Расположение Размеров`,		x1 + xCheckbox, yblock,	x3-(x1 + xCheckbox)-1,	heightRow, 4 * not(AC_WidHei_On)
	if iMarkerSizeUnit = 1 then	ui_infield "whDistMm",			x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize
	if iMarkerSizeUnit = 2 then	ui_infield "whDistCm",			x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize
	if iMarkerSizeUnit = 3 then	ui_infield "whDistPoint",		x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize

	yblock = yblock + 21

	ui_outfield `(4)`,					0,				yblock,	x1 + xCheckbox-1,	heightRow
	ui_infield{3} "GS_Center_Line",	x1 + xCheckbox,	yblock,	x3-(x1 + xCheckbox)-1,	heightRow,
		7,"",2,1,
		0,0,0,0,
		1,`Продление Линии`, 0,
		2,`Продление Линии`, 1
	if iMarkerSizeUnit = 1 then	ui_infield "footLengthMm",		x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize
	if iMarkerSizeUnit = 2 then	ui_infield "footLengthCm",		x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize
	if iMarkerSizeUnit = 3 then	ui_infield "footLengthPoint",	x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize

	yblock = yblock + 21

	ui_outfield `(5)`,					0,				yblock, x1 + xCheckbox-1,		heightRow, 4 * not(AC_Sill_On)
	ui_outfield `Расстояние Высоты Отлива`, x1 + xCheckbox, yblock, x3-(x1 + xCheckbox)-1,	heightRow, 4 * not(AC_Sill_On)
	if iMarkerSizeUnit = 1 then	ui_infield "sillDistMm",		x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize
	if iMarkerSizeUnit = 2 then	ui_infield "sillDistCm",		x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize
	if iMarkerSizeUnit = 3 then	ui_infield "sillDistPoint",		x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize

	yblock = yblock + 21

	ui_outfield `(6)`,					0,				yblock, x1 + xCheckbox-1,		heightRow, 4 * not(AC_Sill_On)
	ui_outfield `Смещение Высоты Отлива`,	x1 + xCheckbox, yblock, x3-(x1 + xCheckbox)-1,	heightRow, 4 * not(AC_Sill_On)
	if iMarkerSizeUnit = 1 then	ui_infield "sillOffsetMm",		x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize
	if iMarkerSizeUnit = 2 then	ui_infield "sillOffsetCm",		x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize
	if iMarkerSizeUnit = 3 then	ui_infield "sillOffsetPoint",	x3, yblock-2, prev_pos_x - x3 -10, heightRow ui_tooltip uiPaperSize

	yblock = yblock + 21

	_bGeometryPage = 1
	gosub "PictureSets"

return


