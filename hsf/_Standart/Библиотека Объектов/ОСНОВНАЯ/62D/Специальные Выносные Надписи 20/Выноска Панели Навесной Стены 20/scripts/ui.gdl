
! ==============================================================================
! Placeholder if there is not valid content for parent tool
! ==============================================================================

if not(_isEnabledContent) then
	gosub "callPlaceholder"
	END
endif


! ==============================================================================
! Tabpage IDs
! ==============================================================================

PAGE_ID_GEOMETRY		= 1
PAGE_ID_CONTENT1		= 2
PAGE_ID_CONTENT2		= 3
PAGE_ID_TEXTFORMAT1		= 4
PAGE_ID_TEXTFORMAT2		= 5
PAGE_ID_INFORMATION		= 6

dim tabTitles[6]
	tabTitles[1] = `Геометрия Выноски`
	tabTitles[2] = `Данные Выноски 1 - ID, Высота Отлива, Размеры`
	tabTitles[3] = `Данные Выноски 2 - Огнестойкость и Шумозащита`
	tabTitles[4] = `Формат Текста 1 - Текст Выноски и Размеры`
	tabTitles[5] = `Формат Текста 2 - Отлив, Огнестойкость и Шумозащита`
	tabTitles[6] = `Информация`


dim tabIcons[6]
	tabIcons[1] = "uiTab_labelGeometry_flat(1)"
	tabIcons[2] = "uiTab_MarkerLabelContent_flat(1)"
	tabIcons[3] = tabIcons[2]
	tabIcons[4] = "uiTab_TextMarkerLabelZone_flat(1)"
	tabIcons[5] = tabIcons[4]
	tabIcons[6] = "uiTab_Info_flat(1)"
	
dim tabIndexes[]
for i = 1 to vardim1(tabTitles)
	tabIndexes[i] = i
next i

uiPaperSize = `Размер Бумаги`
_bGeometryPage = 0

prev_pos_x=278
prev_pos_y=90

file_dependence "uiTab_labelGeometry_flat(1)"
file_dependence "uiTab_MarkerLabelContent_flat(1)"
file_dependence "uiTab_TextMarkerLabelZone_flat(1)"
file_dependence "uiTab_Info_flat(1)"

ui_current_page gs_ui_current_page

ui_page gs_ui_current_page	! Place it on every page


! =============================================================================
! Header for all pages
! =============================================================================

ui_style 0,1

ui_infield{3} "gs_ui_current_page", 0, 0, 350, 25,
	2, "", 0, 0,
	30, 20, 30, 20,
	tabIcons,	tabTitles,	tabIndexes

ui_button ui_prev, "<<", 360, 2, 34, 21, gs_ui_current_page - 1	ui_tooltip `Предыдущая Вкладка`	! Previous
ui_button ui_next, ">>", 398, 2, 34, 21, gs_ui_current_page + 1	ui_tooltip `Следующая Вкладка`		! Next

ui_style 0,0

ui_separator 0, 28, 444, 28


! ==============================================================================
! Tabpages
! ==============================================================================

ui_page PAGE_ID_GEOMETRY
	gosub "GeometrySettingsPage"

ui_page PAGE_ID_CONTENT1
	gosub "ContentSettingsPage1"

ui_page PAGE_ID_CONTENT2
	gosub "ContentSettingsPage2"

ui_page PAGE_ID_TEXTFORMAT1
	gosub "TextFormatPage1"

ui_page PAGE_ID_TEXTFORMAT2
	gosub "TextFormatPage2"

ui_page PAGE_ID_INFORMATION	
	gosub "callPlaceholder"


! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
