
actualGUID = FROM_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

	_autoID		= "21CF9957-62E7-4846-BCDC-5DF009707EF6"
	_startID	= "BF6CE3D1-7EC8-44EC-9AF5-B59B33ADDA24"
	_endID		= "8ECE6529-9030-4DC0-8C94-1EB8DF291CA5"
gosub "ShowSkinThickness_FWM"

	_startID	= "8ECE6529-9030-4DC0-8C94-1EB8DF291CA5"
	_endID		= "6D58031F-8A7F-40A3-B99A-2B680F22D5A5"
gosub "OptionToShowID_FWM"

	_startID	= "6D58031F-8A7F-40A3-B99A-2B680F22D5A5"
	_endID		= "66C88479-75C2-456E-B75D-75F0934903C9"
gosub "NoLabelHeadWithEmptyID_FWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid actualGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! Show Skin Thickness Option
! ==============================================================================
"ShowSkinThickness_FWM":
	if actualGUID = _startID | actualGUID = _autoID then
		bShowSkinThickness = 1

		parameters bShowSkinThickness = bShowSkinThickness

		actualGUID = _endID
	endif
return


! ==============================================================================
! Show ID Option
! ==============================================================================
"OptionToShowID_FWM":
	if actualGUID = _startID then
		bShowID = 1

		parameters bShowID = bShowID

		actualGUID = _endID
	endif
return

! ==============================================================================
! Cut migration for Label Head with empty ID
! ==============================================================================
"NoLabelHeadWithEmptyID_FWM":
	if actualGUID = _startID then
		bhas =  STORED_PAR_VALUE("iIDText", _iIDText)                   &\
				STORED_PAR_VALUE("sIDTextCustom", _sIDTextCustom)       &\
				STORED_PAR_VALUE("bFitFrameToText", _bFitFrameToText)   &\
                STORED_PAR_VALUE("bShowHead", _bShowHead)    

		if  (_iIDText = IDVALUE_CUSTOMTEXT & _sIDTextCustom = "")   &\
            _bFitFrameToText = 1                                    &\
            not(LABEL_TEXT_WRAP)                                    &\
			_bShowHead      = 1                                     then
            
			actualGUID = ""
		else
			actualGUID = _endID
		endif
	endif
return

