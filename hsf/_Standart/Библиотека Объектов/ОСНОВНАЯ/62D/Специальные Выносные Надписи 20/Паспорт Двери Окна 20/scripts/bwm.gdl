
targetGUID = TO_GUID

! ==============================================================================
! Subroutines
! ==============================================================================

gosub "NewUnitSizesOption_BWM"

gosub "NewOrderingOption_BWM"

! ==============================================================================
! Set migration GUID
! ==============================================================================

setmigrationguid targetGUID

! ==============================================================================
end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! end ! en
! ==============================================================================


! ==============================================================================
! NewUnitSizesOption
! ==============================================================================
"NewUnitSizesOption_BWM":
	if targetGUID # "" then
		if iWorkUnitSillHgt <> iWorkUnitLinear then
			targetGUID = ""
		endif
	endif
return

! ==============================================================================
! NewOrderingOption
! ==============================================================================
"NewOrderingOption_BWM":
	if targetGUID # "" then
		bMigrationSuccess = 1

		for iCol = 1 to 2
			if iContentOrderDim[iCol] <> iCol then
				bMigrationSuccess = 0
				iCol = 2				! break;
			endif
		next iCol

		iRow = 1

		for iContent = 1 to vardim1(_bShowContent)			! _bShowContent: Inicialized in the Master Script
			if _bShowContent[iContent] & _iDisplayContentParametersUI[iContent] > INVALID then
				if functionOrder[iRow] <> iContent then
					bMigrationSuccess = 0
					iContent = vardim1(_bShowContent)		! break;
				else
					iRow = iRow + 1
				endif
			endif
		next iContent

		if not(bMigrationSuccess) then
			targetGUID = ""
		endif
	endif
return

