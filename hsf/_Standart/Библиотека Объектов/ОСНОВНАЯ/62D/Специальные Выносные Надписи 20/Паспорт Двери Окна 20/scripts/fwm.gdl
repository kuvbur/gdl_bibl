! ==============================================================================
! NewOrderingOption
! ==============================================================================
"NewOrderingOption_FWM":
	if actualGUID = _startID | actualGUID = _autoID then
		dim	_bCurrContentShow[]
			for iRow = 1 to 14
				_bCurrContentShow[iRow]	= 0
			next iRow

		bHas =	STORED_PAR_VALUE ("bShowID",				_bCurrContentShow[1])	& \
				STORED_PAR_VALUE ("bShowWallholeDimension",	_bCurrContentShow[2])	& \
				STORED_PAR_VALUE ("bShowUnitDimension",		_bCurrContentShow[3])	& \
				STORED_PAR_VALUE ("bShowRevealDimension",	_bCurrContentShow[4])	& \
				STORED_PAR_VALUE ("bShowEgressDimension",	_bCurrContentShow[5])	& \
				STORED_PAR_VALUE ("bShowSillHeight",		_bCurrContentShow[6])	& \
				STORED_PAR_VALUE ("bShowThresholdHeight",	_bCurrContentShow[7])	& \
				STORED_PAR_VALUE ("bShowFireRating",		_bCurrContentShow[8])	& \
				STORED_PAR_VALUE ("bShowAcousticRating",	_bCurrContentShow[9])	& \
				STORED_PAR_VALUE ("bShowUValue",			_bCurrContentShow[10])	& \
				STORED_PAR_VALUE ("bShowMaterial",			_bCurrContentShow[11])	& \
				STORED_PAR_VALUE ("bShowHardwareSet",		_bCurrContentShow[12])	& \
				STORED_PAR_VALUE ("bShowCustomText1",		_bCurrContentShow[13])	& \
				STORED_PAR_VALUE ("bShowCustomText2",		_bCurrContentShow[14])

		dim	iContentOrderDim[2]
			iContentOrderDim[1] = DIM_WDT
			iContentOrderDim[2] = DIM_HGT

		dim functionOrder[]
			functionOrder[1] = 2

		iRow = 1

		for iContent = 1 to 14
			if _bCurrContentShow[iContent] then
				functionOrder[iRow] = iContent + 1
				iRow = iRow + 1
			endif
		next iContent

		parameters	iContentOrderDim	= iContentOrderDim,
					functionOrder		= functionOrder

		actualGUID = _endID
	endif
return

! ==============================================================================
! NewUnitSizesOption
! ==============================================================================
"NewUnitSizesOption_FWM":
	if actualGUID = _startID then
		_iWorkUnitLinear		= DIMUNIT_PROJECT
		_iWorkUnitLinearDecimal	= 1
	
		bHas =	STORED_PAR_VALUE ("iWorkUnitLinear",		_iWorkUnitLinear)	& \
				STORED_PAR_VALUE ("iWorkUnitLinearDecimal",	_iWorkUnitLinearDecimal)
	
		iWorkUnitSillHgt		= _iWorkUnitLinear
		iWorkUnitSillHgtDecimal	= _iWorkUnitLinearDecimal
	
		parameters	iWorkUnitSillHgt		= iWorkUnitSillHgt,
					iWorkUnitSillHgtDecimal	= iWorkUnitSillHgtDecimal
	
		actualGUID = _endID
	endif
return

