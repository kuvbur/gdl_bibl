N=0.01*GLOB_SCALE
MUL2   N,N
DEFINE STYLE "текст" fontType ,H, 5,1
DEFINE STYLE "текст1"  fontType2 ,G, 5,1
SET STYLE "текст"
if c<0 then c=0
if C>75 then C=75
E=E*0.1
B=1.5+E*C
A=9
RECT2 0, 0, A, B
LINE2 2, 0, 2, B
LINE2 0, C*E, A, C*E
FOR W=1 TO C-1
LINE2  0, E*W, A, E*W
NEXT W
pen ff
TEXT2  1, B-.5, "Марка,"
TEXT2  1, B-1.0, "поз."
TEXT2  5.5, B-.75, "Схема сечения"
SET STYLE "текст1"
PEN dd
TEXT2 A/2, B+G*.2, "Ведомость перемычек"
HOTSPOT2 A, B
HOTSPOT2 0,  B
END
