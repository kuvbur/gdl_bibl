R=0.25
U=.3
T=.2
IF GLOB_SCALE<=100 THEN 100
MUL2 .01*GLOB_SCALE, .01*GLOB_SCALE
100:
HOTSPOT2 0, 0
IF C=1 THEN 1
IF C=2 THEN 2
IF C=3 THEN 3
IF C=4 THEN 4
IF C=5 THEN 5
IF C=6 THEN 6
IF C=7 THEN 7
IF C=8 THEN 8
IF C=9 THEN 9
END
1:
ARC2 0,0,R,0,180
LINE2 R,0,-R,0
LINE2 0,0,0,-U
LINE2 -U/2,-U,U/2,-U
END
2:
RECT2 0,-U,U,U
LINE2 U,U/2,2*U,U/2
LINE2 2*U,U/2,2*U,-U/2
LINE2 U,0,2*U,-U/2
END
3:
RECT2 T,-T,-T,T
gosub 20
END
4:
RECT2 T,-2*T,-T,2*T
LINE2 -T,T,T,T
LINE2 -T,-T,T,-T
GOSUB 20
END
5:
RECT2 T,-2*T,-T,2*T
LINE2 -T/2,T,T/2,T
LINE2 -T/2,-T,T/2,-T
GOSUB 20
END
6:
RECT2 T,-T,-T,T
LINE2 -T,T,0,0
LINE2 0,0,T,T
END
7:
RECT2 T,-T,-T,T
LINE2 -T, -T, T,T
END
8:
RECT2 T,-T,-T,T
LINE2 0,T,0,3*T
LINE2 -T/2,2*T,0,3*T
LINE2 T/2,2*T,0,3*T
END
20:
CIRCLE2 0,0,T/2
ROT2 45
LINE2 -T/2,0,T/2,0
LINE2 0,-T/2,0,T/2
del 1
return
9:
RECT2 T,-T,-T,T
CIRCLE2 0,0,T/2
END
