"DrawHandle":
!===============================================================================
! Draw Handle
!===============================================================================

if iHandleType = 0 then return


gosub iHandleType * 10
return


"DrawWinHandle":
!===============================================================================
! Draw Window Handle
!===============================================================================

if iHandleWinType = 0 then return


if WIDO_REVEAL_SIDE then mulx -1

gosub 1000 + iHandleWinType * 10

del WIDO_REVEAL_SIDE

return


