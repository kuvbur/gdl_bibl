				dy = 25
				for i = 1 to 10
					if sashID[i][SASH_FUNCTION] > 0 & sashID[i][SASH_ROLE] = SASHROLE_MAIN then		! is Valid and Main Sash
						iSashType = sashID[i][SASH_TYPE]

						CasementID = sashID[i][SASH_ID]
						gosub "OpeningTypeAndAngleParams"

						if iSashType <> SASHTYPE_DOUBLE_RIGHT then
							cx2 = x2-x1-1
