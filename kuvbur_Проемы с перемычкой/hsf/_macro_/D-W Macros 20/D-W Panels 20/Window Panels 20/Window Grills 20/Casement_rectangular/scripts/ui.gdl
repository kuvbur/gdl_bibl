				endif

				if not(bNEDOpLineSymbole) | \
					(bNEDOpLineSymbole & not(bHideFixGlassDir)) then

					ui_separator x1, y, x4, y
					y = y + 9
	
					ui_style 0, 1
					ui_outfield gs_UI_CasementRect[119], x1, y, 160, 15, Flag
					ui_style 0, 0
					ui_infield{3} "FixGlassDir_m", 300, y-4, 140, 19,
						8, "",
						2,
						1, 20, 20, 20, 20,
						1, stFixGlassDir[1], 1,
						2, stFixGlassDir[2], 2
				endif
			endif
		endif
	endif
return





