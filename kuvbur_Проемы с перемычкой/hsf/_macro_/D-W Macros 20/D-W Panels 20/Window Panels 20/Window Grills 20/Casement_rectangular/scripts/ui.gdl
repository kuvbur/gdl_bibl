			x70 = x6 + 90
			x71 = x6 + 100
		else
			x6 = x5
			x70 = x4
			x71 = x4 + 1
		endif

		if (numSash > 7*(isWindow) + 2*(not(isWindow))) |\
			(numSash = 2 & bSlidingUIalternative) then
