					ui_infield "gs_open_2D_sl_right", x3, y - 4, x4 - x3, 20
				else
					ui_outfield `Угол Открывания в 2D Боковой Панели`, x1, y, x3 - x1, 20, 4 * _FlagLeftSideLight2DOpen
					ui_infield "gs_open_2D_sl_left", x3, y - 4, x4 - x3, 20
				endif
			endif
			if iWindowType = DOOR_TwoSidelight then
				ui_outfield `Угол Открывания в 2D Боковых Панелей`, x1, y, x3 - x1 - (x4 - x3) - 6, 17, 4 * (_FlagRightSideLight2DOpen & _FlagLeftSideLight2DOpen)
				ui_infield "gs_open_2D_sl_right", x3 - (x4 - x3) - 5, y - 4, x4 - x3, 20
				ui_infield "gs_open_2D_sl_left", x3, y - 4, x4 - x3, 20
			endif
		!else handled in "OpeningTypeAndAngleParams"
		endif
	endif

	if	isWindow 										&\
		bShowAngleValues								&\
		not(iWindowClass = WINCLASS_SINGLE_HUNG			|\
			iWindowClass = WINCLASS_DOUBLE_HUNG			|\
			iWindowClass = WINCLASS_SHELL				|\
			iWindowClass = WINCLASS_HORIZONTAL_SLIDING)	then

		if	iWindowType = WIN_MultisashHorizontal	|\
			iWindowType = WIN_MultisashVertical		then
			y = pageStartY - 4
		else
			y = 202 + pageStartY
			if bNEDOpLineSymbole then y = y + 10
			ui_separator x1, y, x4, y
		endif

		if not(bNEDOpLineSymbole) then
			ui_style 0, 1
			y = y + 8
			ui_outfield gs_UI_CasementRect[48], x1, y, x3-x1-1, 15, 4 * not(bNeed2DOpeningAngle)	! 2D Opening Angle
			ui_style 0, 0
			ui_infield "gs_open_2D", x3, y-4, x4-x3, 20
		else
