								else
									ui_outfield gs_UI_CasementRect[57], x1, y - dy, _wx, 20		! Transom Sashes
								endif
							else
								if not(bSlidingUIalternative) then
									ui_outfield sashName[j], x1, y, _wx, 20						! Sidelight / Transom
								endif
							endif
						endif
						if iSashType+1 <> SASHTYPE_DOUBLE_RIGHT | bShowAngleValues then
							y = y + dy
						endif
					endif
				endif
			next i
		endif
		if not(bShowAngleValues) then
			ui_pict_button ui_function, "", "uiFunctionWindowOpening_flat(1)", 406, y-4, 36, 22, -iUiID_OpeningType ! Set GLOB_UI_BUTTON_ID
		endif

		_FlagRightSideLight2DOpen = (gs_optype_m_05 <> SASH_SIDE_HUNG &\
