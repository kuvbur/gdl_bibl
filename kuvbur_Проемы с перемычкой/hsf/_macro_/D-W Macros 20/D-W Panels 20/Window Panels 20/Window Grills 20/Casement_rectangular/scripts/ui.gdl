! Opening Type and 3D Opening Angle Parameters
! =============================================================================

"OpeningTypeAndAngleParams":

	if bSlidingUIalternative & bShowAngleValues then
		_dy = 22
		ui_outfield `Тип Открывания`,		x71, y,			x3 - x71 - 1, 15
		ui_outfield `Угол Открывания в 3D`, x71, y + _dy,	x3 - x71 - 1, 15

		bRightSideLight	= (sashID[2][SASH_FUNCTION] = SET_SIDELIGHT_1 & CasementID = sashID[2][SASH_ID])
		bLeftSideLight	= (sashID[3][SASH_FUNCTION] = SET_SIDELIGHT_2 & CasementID = sashID[3][SASH_ID])

		if bRightSideLight | bLeftSideLight then
			ui_outfield `Угол Открывания в 2D`, x1, y + _dy,	x6 - x1 - 1, 20

			if bRightSideLight then
				ui_infield "gs_open_2D_sl_right", x6, y + _dy - 4, x70 - x6 - 1, 20
			endif
			if bLeftSideLight then
				ui_infield "gs_open_2D_sl_left", x6, y + _dy - 4, x70 - x6 - 1, 20
			endif
		endif

	else
		_dy = 0
	endif

	if CasementID = 1 then
		if iSashType <> SASHTYPE_DOUBLE_RIGHT then
			if iMullionStyle_01 = MULLION_VENT_PANEL then
				_stSashFixed = stLouversinFrame		! Louvers in Frame
			else
				_stSashFixed = stOpeningType[1]
			endif

			_stOptypeParName = "gs_optype_m_01"
			gosub "opTypeTextPopup"
		endif
		if bShowAngleValues then
			if	gs_optype_m_01 = SASH_SLIDING_VER			|\
				gs_optype_m_01 = SASH_DOUBLE_SLIDING_VER	|\
				gs_optype_m_01 = SASH_SLIDING_HOR			|\
				gs_optype_m_01 = SASH_SLIDING_HOR_M			|\
				gs_optype_m_01 = SASH_DOUBLE_SLIDING_HOR	|\
				gs_optype_m_01 = SASH_DOUBLE_SLIDING_HOR_M	then
				ui_infield "openingLength_01", x3, y + _dy - 4, x4-x3, 20
			else
				if	(gs_optype_m_01 = SASH_TILT_TURN |\
					 gs_optype_m_01 = SASH_TILT_TURN_M |\
					 gs_optype_m_01 = SASH_DOUBLE_TILT_TURN |\
					 gs_optype_m_01 = SASH_DOUBLE_TILT_TURN_M) &\
					abs(openingAngle_01) < EPS  & abs(openingAngle2_01) > EPS then
