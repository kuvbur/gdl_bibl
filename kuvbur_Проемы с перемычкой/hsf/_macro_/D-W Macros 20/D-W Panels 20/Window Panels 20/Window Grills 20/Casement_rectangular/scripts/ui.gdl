							endif
						endif

						if iSashType+1 <> SASHTYPE_DOUBLE_RIGHT | bShowAngleValues then
							y = y + dy
						endif
					endif
				next i
				y = y + dy - 10
			else
				dy = 22
			endif

			! Sash to Edit
			ui_outfield gs_UI_CasementRect[49], x1, y, x6 - x1 - 1, 17
			ui_infield{3} "gs_more_sash_set_to_edit", x6, y-4, x70 - x6 - 1, 20,
					8, "",
					10,
					1, 20, 20, 20, 20,
