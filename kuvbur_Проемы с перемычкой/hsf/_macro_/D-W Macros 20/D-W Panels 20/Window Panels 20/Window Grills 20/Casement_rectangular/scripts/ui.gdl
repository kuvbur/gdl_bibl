return


! =============================================================================
! Opening
! =============================================================================

"pageOpeningType":

	pageStartY = 4
	x5 = 311

	if isWindow then
		if	iWindowType = WIN_MultisashHorizontal	|\
			iWindowType = WIN_MultisashVertical		then

			x1 = 2
			x2 = 155
			x3 = 235
			x4 = 315
			y = 32 + pageStartY
		else
			x1 = 134
			x2 = 255
			x3 = 365
			x4 = 442
			y = pageStartY
		endif

		cx = x3-x2-10
		ui_style 0, 1

		if  not(bShowAngleValues) then
			gs_UI_CasementRect[47] = `Тип Открывания`
			x1 = 150
		endif

		if (iWindowType <> WIN_MultisashHorizontal & iWindowType <> WIN_MultisashVertical) then
			ui_outfield gs_UI_CasementRect[47], x1, y, x4 - x1, 17	! Opening Type and 3D Opening Angle
		endif
		if bShowAngleValues then
			y = y + 31
		else
			y = y + 23
		endif
	else
		x2 = 267
		x3 = 370
		x4 = 442
		dy = 22

		if bSlidingUIalternative then
			x1 = 2
			x2 = 140	! = x1 defined in ui_door_general line #625
			x3 = 340	! not ! = x3 defined in ui_door_general line #627
			x5 = 262	! = x2 defined in ui_door_general line #626
			y = 170 + pageStartY
		else
			x1 = 140
			y = 129 + pageStartY
		endif

		if (gs_door_transom | gs_sidelight_left | gs_sidelight_right) then
			cx = x3-x2-5
			ui_style 0, 1
			if gs_door_transom & (gs_sidelight_left | gs_sidelight_right) then
				ui_outfield gs_UI_CasementRect[70], x1, y, x4-x1, 17		! Sidelight and Transom
			else
				if gs_door_transom then
					ui_outfield gs_UI_CasementRect[44], x1, y, x4-x1, 17	! Transom
				endif
				if (gs_sidelight_left | gs_sidelight_right) then
					ui_outfield gs_UI_CasementRect[40], x1, y, x4-x1, 17	! Sidelight
				endif
			endif

			y = y + dy
			ui_style 0, 0
 
			! Opening Direction
			ui_outfield `Направление Открывания`, x1, y, 170, 15	
			ui_infield{3} "gs_opening_dir_sl_m", x5, y-4, max(130, x4 - x5), 19,
					8, "",
					2,
					1, 20, 20, 20, 20,
					0, `Как у Двери`, 1,		
					1, `Противоположно Двери`, 2		

			y = y + dy
			ui_style 0, 0
			
			if not(bSlidingUIalternative) then
				ui_outfield gs_UI_CasementRect[47], x1, y, x4-x1, 17	! Opening Type and 3D Opening Angle
				y = y + dy 
			endif
		else
			end
		endif
	endif

	numSash = 0
	bNeed2DOpeningAngle = 0
	bNeed2DOpeningLength = 0
	bHideFixGlassDir = 1
	for i = 1 to 10
		if sashID[i][SASH_FUNCTION] > 0 then
			numSash = numSash + 1
			if sashID[i][SASH_ROLE] = SASHROLE_MAIN then
				CasementID = sashID[i][SASH_ID]

				gs_optype_m = 0
				if CasementID = 1 then gs_optype_m = gs_optype_m_01
				if CasementID = 2 then gs_optype_m = gs_optype_m_02
				if CasementID = 3 then gs_optype_m = gs_optype_m_03
				if CasementID = 4 then gs_optype_m = gs_optype_m_04
				if CasementID = 5 then gs_optype_m = gs_optype_m_05
				if CasementID = 6 then gs_optype_m = gs_optype_m_06
				if CasementID = 7 then gs_optype_m = gs_optype_m_07
				if CasementID = 8 then gs_optype_m = gs_optype_m_08
				if CasementID = 9 then gs_optype_m = gs_optype_m_09
				if CasementID = 10 then gs_optype_m = gs_optype_m_10
				if CasementID = 11 then gs_optype_m = gs_optype_m_11

				if	gs_optype_m = SASH_SIDE_HUNG |\
					gs_optype_m = SASH_SIDE_HUNG_M |\
					gs_optype_m = SASH_TILT_TURN |\
					gs_optype_m = SASH_TILT_TURN_M |\
!					gs_optype_m = SASH_HOR_PIVOT |\
					gs_optype_m = SASH_VER_PIVOT |\
					gs_optype_m = SASH_VER_PIVOT_M |\
					gs_optype_m = SASH_DOUBLE_SIDE_HUNG |\
					gs_optype_m = SASH_DOUBLE_TILT_TURN |\
					gs_optype_m = SASH_DOUBLE_TILT_TURN_M then bNeed2DOpeningAngle = 1

				if	gs_optype_m = SASH_BOTTOM_HUNG |\
					gs_optype_m = SASH_TOP_HUNG |\
					gs_optype_m = SASH_HOR_PIVOT then bNeed2DOpeningLength = 1

				if gs_optype_m = SASH_FIXED then bHideFixGlassDir = 0
			endif
		endif
	next i

	if	isWindow &\
		(iWindowType = WIN_MultisashHorizontal	|\
		 iWindowType = WIN_MultisashVertical)	then
		if bShowAngleValues then

			listFieldID = 101
			ui_listfield listFieldID, 0, 54 + pageStartY, 444, 160, 0
		
			listItemID = 1
			for i = 1 to gs_panes_number
				ui_listitem listItemID, listFieldID, "", 0, "", gs_UI_CasementRect[99+i]	: listItemID = listItemID + 1		! Sash 1...10
		
				if i < 10 then
					param_name = "gs_optype_0" + str(i, 1, 0)
				else
					param_name = "gs_optype_10"
				endif
				ui_listitem listItemID, listFieldID, param_name, 1, "", gs_UI_CasementRect[110]	: listItemID = listItemID + 1		! Opening Type
		
				if	goptypeID[i] = SASH_SLIDING_VER				|\
					goptypeID[i] = SASH_DOUBLE_SLIDING_VER		|\
					goptypeID[i] = SASH_SLIDING_HOR				|\
					goptypeID[i] = SASH_SLIDING_HOR_M			|\
					goptypeID[i] = SASH_DOUBLE_SLIDING_HOR		|\
					goptypeID[i] = SASH_DOUBLE_SLIDING_HOR_M	then
		
					if i < 10 then
						param_name = "openingLength_0" + str(i, 1, 0)
					else
						param_name = "openingLength_10"
					endif
					ui_listitem listItemID, listFieldID, param_name, 1, "", gs_UI_CasementRect[112]	: listItemID = listItemID + 1		! 3D Opening Distance
				else
					if	bShowAngleValues &\
						(goptypeID[i] = SASH_TILT_TURN |\
						 goptypeID[i] = SASH_TILT_TURN_M |\
						 goptypeID[i] = SASH_DOUBLE_TILT_TURN |\
						 goptypeID[i] = SASH_DOUBLE_TILT_TURN_M) &\
						abs(openingAngle_array[i]) < EPS & abs(openingAngle2_array[i]) > EPS then
						if i < 10 then
							param_name = "openingAngle2_0" + str(i, 1, 0)
						else
							param_name = "openingAngle2_10"
						endif
						ui_listitem listItemID, listFieldID, param_name, 1, "", gs_UI_CasementRect[111]	: listItemID = listItemID + 1		! 3D Opening Angle
					else
						if i < 10 & bShowAngleValues then
							param_name = "openingAngle_0" + str(i, 1, 0)
						else
							param_name = "openingAngle_10"
						endif
						ui_listitem listItemID, listFieldID, param_name, 1, "", gs_UI_CasementRect[111]	: listItemID = listItemID + 1		! 3D Opening Angle
					endif
				endif
