! =============================================================================
! Sash Option
! =============================================================================

"pageSashOptions":
	pageStartY = 0

		x1 = 2
		x2 = 158
		x3 = 306
		x4 = 442
		y = pageStartY
	
		numPotentialSashes = 0	! Number of potential sashes
		numSashes = 0			! Number of actual sashes
		bFoundNonMainSash = 0
		bFoundFixMainSash = 0
		for ii = 1 to 10
			if	sashID[ii][SASH_ID] > 0 then
				numPotentialSashes = numPotentialSashes + 1
				if sashID[ii][SASH_FUNCTION] <> 0 then
	
					numSashes = numSashes + 1
					if sashID[ii][SASH_ROLE] = SASHROLE_MAIN then
						CasementID = sashID[ii][SASH_ID]
						if goptypeID[CasementID] = SASH_FIXED then
							bFoundFixMainSash = 1
						endif
					else
						bFoundNonMainSash = 1
					endif
				endif
			endif
		next ii
	
		if numPotentialSashes > 1 then
			ui_style 0, 1
			ui_infield{3} "gs_sash_uniform", x1, y, x2-x1-1, 20,
				7, "",
				2,
				1, 20, 20, 20, 20,
				0, gs_UI_CasementRect[1], 0,	! Uniform Sashes
				1, gs_UI_CasementRect[1], 1		! Uniform Sashes
	
			y = y + 25
			y3 = y
		else
			y = y + 5
			y3 = y + 20
		endif
	
		cx = 140
		cy = 85
		cx2 = 60
		cx3 = 100
		x2 = 220
		x3 = x2 + 12
	
		ui_style 0, 1
	
		if gs_sash_uniform then
			CasementID = 0
			if	iWindowType = WIN_MultisashHorizontal	|\
				iWindowType = WIN_MultisashVertical		then
	
				MainCasementID = 1
			else
				MainCasementID = CasementIDArray[2][2]
				if CasementTypeArray[2][2] = SASHTYPE_DOUBLE_RIGHT then
					MainCasementID = CasementIDArray[2][1]
				endif
				if MainCasementID = 0 then
					MainCasementID = CasementIDArray[2][1]
				endif
				if MainCasementID = 0 then
					MainCasementID = CasementIDArray[2][3]
				endif
				if MainCasementID = 0 then
					MainCasementID = CasementIDArray[1][2]
				endif
			endif
	
			gosub "isNoGrillSelected"	! -> bNoMullion
			bLockMullionParams = bNoMullion
	
			ui_outfield gs_UI_CasementRect[3], x1, y, x2-x1-1, 20	! Sash Type
			y = y + 21
	
			iSashShape = 0
		else
			bLockMullionParams = 1
			for i = 1 to 10
				if sashID[i][SASH_FUNCTION] > 0 then
					CasementID = sashID[i][SASH_ID]
					gosub "isNoGrillSelected"	! -> bNoMullion
					bLockMullionParams = (bLockMullionParams & bNoMullion)
					if not(bLockMullionParams) then i = 10	! Exit loop
				endif
			next i
	
			! =============================================================================
			! Edit Sash
			! =============================================================================
	
			iSashShape = SASH_SHAPE_RECTANGULAR
			for i = 1 to 10
				if sashID[i][SASH_FUNCTION] = gs_sash_set_to_edit then
					CasementID = sashID[i][SASH_ID]
					iSashShape	= sashID[i][SASH_SHAPE]
					i = 10	! Force loop end
				endif
			next i

			if (numPotentialSashes > 1) & (numSashes > 1) then
				ui_outfield gs_UI_CasementRect[2], x1, y, x2-x1-cx3-1, 20, 0		! Edit Sash

				ui_style 0, 0
				ui_infield{3} "gs_sash_set_to_edit", x2-cx3, y-3, cx3, 20,
					8, "",
					10,
					1, 20, 20, 20, 20,
					1, sashInShowingOrder[1][2], sashInShowingOrder[1][1],
					2, sashInShowingOrder[2][2], sashInShowingOrder[2][1],
					3, sashInShowingOrder[3][2], sashInShowingOrder[3][1],
					4, sashInShowingOrder[4][2], sashInShowingOrder[4][1],
					5, sashInShowingOrder[5][2], sashInShowingOrder[5][1],
					6, sashInShowingOrder[6][2], sashInShowingOrder[6][1],
					7, sashInShowingOrder[7][2], sashInShowingOrder[7][1],
					8, sashInShowingOrder[8][2], sashInShowingOrder[8][1],
					9, sashInShowingOrder[9][2], sashInShowingOrder[9][1],
					10, sashInShowingOrder[10][2], sashInShowingOrder[10][1]
			else
				ui_outfield gs_UI_CasementRect[3], x1, y, x2-x1-1, 20	! Sash Type
			endif
			y = y + 21
		endif
	
		iSashShapePictIdx = iSashShape
		if iSashShape = SASH_SHAPE_ARCHED & iWindowClass = WINCLASS_DOORSASH then
			iSashShapePictIdx = SASH_SHAPE_HALFROUND
		endif
	
		if CasementID = 0 then	! CasementID = 0 in case of Uniform Sashes
			ui_style 0, 0
			_stParaName = "iMullionStyle_00"
			_picIdx = 1
			gosub "mullionStylePopup"
	
			bGrid = (iMullionStyle_00 = MULLION_HVGRID |\
					 iMullionStyle_00 = MULLION_EDITABLEGRID |\
					 iMullionStyle_00 = MULLION_FRENCH_PANEL |\
					 iMullionStyle_00 = MULLION_DIVIDED_FRAME |\
					 iMullionStyle_00 = MULLION_PRAIRIE)
			bFrenchPanel = (iMullionStyle_00 = MULLION_FRENCH_PANEL)
			bDivided = (iMullionStyle_00 = MULLION_DIVIDED)
			bSolidGlazed = (iMullionStyle_00 = MULLION_SOLID_GLAZED)
			bVentPanel = (iMullionStyle_00 = MULLION_VENT_PANEL)
			bCustom	= (iMullionStyle_00 = MULLION_CUSTOM)
			iMullionStyle = iMullionStyle_00
	
			gs_optype_m = gs_optype_m_01
			if MainCasementID =  2 then gs_optype_m = gs_optype_m_02
			if MainCasementID =  3 then gs_optype_m = gs_optype_m_03
			if MainCasementID =  4 then gs_optype_m = gs_optype_m_04
			if MainCasementID =  5 then gs_optype_m = gs_optype_m_05
			if MainCasementID =  6 then gs_optype_m = gs_optype_m_06
			if MainCasementID =  7 then gs_optype_m = gs_optype_m_07
			if MainCasementID =  8 then gs_optype_m = gs_optype_m_08
			if MainCasementID =  9 then gs_optype_m = gs_optype_m_09
			if MainCasementID = 10 then gs_optype_m = gs_optype_m_10
			if MainCasementID = 11 then gs_optype_m = gs_optype_m_11
			bSingleDoubleHung = (gs_optype_m = SASH_SLIDING_VER | gs_optype_m = SASH_DOUBLE_SLIDING_VER)
		endif
		if CasementID = 1 then
			ui_style 0, 0
			_stParaName = "iMullionStyle_01"
			_picIdx = iSashShapePictIdx
			gosub "mullionStylePopup"
	
			bGrid = (iMullionStyle_01 = MULLION_HVGRID |\
					 iMullionStyle_01 = MULLION_EDITABLEGRID |\
					 iMullionStyle_01 = MULLION_FRENCH_PANEL |\
					 iMullionStyle_01 = MULLION_DIVIDED_FRAME |\
					 iMullionStyle_01 = MULLION_PRAIRIE) |\
					(iSashShape = SASH_SHAPE_ARCHED &\
					 (iMullionStyle_01 = MULLION_SUNBURST_1 |\
					  iMullionStyle_01 = MULLION_SUNBURST_2 |\
					  iMullionStyle_01 = MULLION_SUNBURST_3 |\
					  iMullionStyle_01 = MULLION_SUNBURST_4 |\
					  iMullionStyle_01 = MULLION_SUNBURST_5 |\
					  iMullionStyle_01 = MULLION_SUNBURST_6	))
			bFrenchPanel = (iMullionStyle_01 = MULLION_FRENCH_PANEL)
			bDivided = (iMullionStyle_01 = MULLION_DIVIDED)
			bSolidGlazed = (iMullionStyle_01 = MULLION_SOLID_GLAZED)
			bVentPanel = (iMullionStyle_01 = MULLION_VENT_PANEL)
			bCustom	= (iMullionStyle_01 = MULLION_CUSTOM)
			iMullionStyle = iMullionStyle_01
			bSingleDoubleHung = (gs_optype_m_01 = SASH_SLIDING_VER | gs_optype_m_01 = SASH_DOUBLE_SLIDING_VER)
			bDoubleSash =  (gs_optype_m_01 = SASH_DOUBLE_SIDE_HUNG		|\
							gs_optype_m_01 = SASH_DOUBLE_TILT_TURN		|\
							gs_optype_m_01 = SASH_DOUBLE_TILT_TURN_M	|\
							gs_optype_m_01 = SASH_DOUBLE_SLIDING_HOR	|\
							gs_optype_m_01 = SASH_DOUBLE_SLIDING_HOR_M	)
			bSecondSash =  (gs_optype_m_01 = SASH_DOUBLE_SIDE_HUNG		|\
							gs_optype_m_01 = SASH_DOUBLE_TILT_TURN		|\
							gs_optype_m_01 = SASH_DOUBLE_TILT_TURN_M	|\
							gs_optype_m_01 = SASH_SLIDING_HOR			|\
							gs_optype_m_01 = SASH_SLIDING_HOR_M			|\
							gs_optype_m_01 = SASH_DOUBLE_SLIDING_HOR	|\
							gs_optype_m_01 = SASH_DOUBLE_SLIDING_HOR_M	|\
							gs_optype_m_01 = SASH_SLIDING_VER			|\
							gs_optype_m_01 = SASH_DOUBLE_SLIDING_VER	)
		endif
		if CasementID = 2 then
			ui_style 0, 0
			_stParaName = "iMullionStyle_02"
			_picIdx = iSashShapePictIdx
			gosub "mullionStylePopup"
	
			bGrid = (iMullionStyle_02 = MULLION_HVGRID |\
					 iMullionStyle_02 = MULLION_EDITABLEGRID |\
					 iMullionStyle_02 = MULLION_FRENCH_PANEL |\
					 iMullionStyle_02 = MULLION_DIVIDED_FRAME |\
					 iMullionStyle_02 = MULLION_PRAIRIE) |\
					(iSashShape = SASH_SHAPE_ARCHED &\
					 (iMullionStyle_02 = MULLION_SUNBURST_1 |\
					  iMullionStyle_02 = MULLION_SUNBURST_2 |\
					  iMullionStyle_02 = MULLION_SUNBURST_3 |\
					  iMullionStyle_02 = MULLION_SUNBURST_4 |\
					  iMullionStyle_02 = MULLION_SUNBURST_5 |\
					  iMullionStyle_02 = MULLION_SUNBURST_6	))
			bFrenchPanel = (iMullionStyle_02 = MULLION_FRENCH_PANEL)
			bDivided = (iMullionStyle_02 = MULLION_DIVIDED)
