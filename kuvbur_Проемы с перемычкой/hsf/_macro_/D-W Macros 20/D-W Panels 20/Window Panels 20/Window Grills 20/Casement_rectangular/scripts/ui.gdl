				ui_outfield strName, x1, y, x2 - x1 - 1, 20
			endif
			gosub "OpeningTypeAndAngleParams"

			y = y + dy
		else
			if isWindow then
				dy = 25
			else
				dy = 22
			endif

			_countShownSash = 0
			for i = 1 to 10
				j = sashShowingOrder[i]

				bMainSash	= (sashID[j][SASH_ROLE] = SASHROLE_MAIN)
				isValidSash = (sashID[j][SASH_FUNCTION] > 0)
				if (iWindowType = WIN_HistoricWindowSegmented | iWindowType = WIN_HistoricWindowTripleArched) & bMainSash = 0 then isValidSash = 0

				if isValidSash then
					_countShownSash = _countShownSash + 1
					if _countShownSash <= _numSashRange then
						CasementID = sashID[j][SASH_ID]
						iSashType = sashID[j][SASH_TYPE]

						if iSashType+1 <> SASHTYPE_DOUBLE_RIGHT | bShowAngleValues then
							if _numSashRange = 6 then
								dy = 24
							endif
						endif

						gosub "OpeningTypeAndAngleParams"

