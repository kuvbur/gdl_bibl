! =============================================================================
! Display actual tabpages
! =============================================================================

ui_current_page gs_ui_current_page

for iTabs = 1 to vardim1(uiUsedPageIDs)
	! --------------------------------------------------------------------------------
	! Independent pages
	! --------------------------------------------------------------------------------
	if uiUsedPageIDs[iTabs][1] = iUiID_NominalSizes then
		ui_page	uiUsedPageIDs[iTabs][1], uiUsedPageIDs[iTabs][2], `Номинальные Размеры и Допуски`, "uiTab_WindowSizes_flat(1)"
		if gs_ui_current_page = iUiID_NominalSizes then
			gosub "pageCallNominalSizes"
		endif
	endif
	if uiUsedPageIDs[iTabs][1]= iUiID_FloorPlanSection then
		! for Show Reveal on Symbol
		ui_page	uiUsedPageIDs[iTabs][1], uiUsedPageIDs[iTabs][2], uiUsedPageNames[iTabs][1], uiUsedPageNames[iTabs][2]
		if gs_ui_current_page = iUiID_FloorPlanSection then
			gosub "pageCallFloorPlanAndSect"
		endif
	endif

	! --------------------------------------------------------------------------------
	! Window Settings and Opening group
	! --------------------------------------------------------------------------------
	if uiUsedPageIDs[iTabs][1] = iUiID_OpLines then
		ui_page	uiUsedPageIDs[iTabs][1], uiUsedPageIDs[iTabs][2], `Линии Открывания`, "uiTab_DoorOpeningLines_flat(1)"
		if gs_ui_current_page = iUiID_OpLines then
			gosub "pageCallOpeningOptions"
		endif
	endif

	! --------------------------------------------------------------------------------
	! Wall Opening group
	! --------------------------------------------------------------------------------
	if uiUsedPageIDs[iTabs][1] = iUiID_WallOpening then
		ui_page	uiUsedPageIDs[iTabs][1], uiUsedPageIDs[iTabs][2], `Проем в Стене`, "uiTab_WindowWallOpening_flat(1)"
		if gs_ui_current_page = iUiID_WallOpening then
			gosub "pageCallWallOpening"
		endif
	endif
	if uiUsedPageIDs[iTabs][1] = iUiID_Reveal then
		uiUsedPageNames[iTabs][1]	= `Четверть`
		uiUsedPageNames[iTabs][2]	= "uiTab_OpeningReveal_flat(1)"
	endif
	if uiUsedPageIDs[iTabs][1] = iUiID_Closure then
		uiUsedPageNames[iTabs][1]	= `Замыкание Слоев`
		uiUsedPageNames[iTabs][2]	= "uiTab_OpeningClosure_flat(1)"
	endif
	if uiUsedPageIDs[iTabs][1] = iUiID_WallInset then
		uiUsedPageNames[iTabs][1]	= `Подоконная Ниша`
		uiUsedPageNames[iTabs][2]	= "uiTab_WindowWallInset_flat(1)"
	endif
	if uiUsedPageIDs[iTabs][1] = iUiID_MasonryArch then
		uiUsedPageNames[iTabs][1]	= `Декоративная Перемычка`
		uiUsedPageNames[iTabs][2]	= "uiTab_OpeningMasonryArch_flat(1)"
	endif
	if uiUsedPageIDs[iTabs][1] = iUiID_WallOpModelAttr then
		uiUsedPageNames[iTabs][1]	= `Реквизиты Модели`
		uiUsedPageNames[iTabs][2]	= "uiTab_3DRepr_flat(1)"
	endif

	! --------------------------------------------------------------------------------
	! Fixtures and Fittings group
	! --------------------------------------------------------------------------------
	if uiUsedPageIDs[iTabs][1] = iUiID_FixtureFitting then
		ui_page	uiUsedPageIDs[iTabs][1], uiUsedPageIDs[iTabs][2], `Дополнительные Элементы`, "uiTab_WindowFixturesFittings_flat(1)"
		if gs_ui_current_page = iUiID_FixtureFitting then
			gosub "pageCallFixtureFitting"
		endif
	endif
	if uiUsedPageIDs[iTabs][1] = iUiID_Sill then
		uiUsedPageNames[iTabs][1]	= `Отлив`
		uiUsedPageNames[iTabs][2]	= "uiTab_WindowSill_flat(1)"
	endif
	if uiUsedPageIDs[iTabs][1] = iUiID_Board then
		uiUsedPageNames[iTabs][1]	= `Подоконная Доска`
		uiUsedPageNames[iTabs][2]	= "uiTab_WindowBoard_flat(1)"
	endif
	if uiUsedPageIDs[iTabs][1] = iUiID_CasingOut then
		uiUsedPageNames[iTabs][1]	= `Наружный Наличник`
		uiUsedPageNames[iTabs][2]	= "uiTab_DOutsideCasing_flat(1)"
	endif
	if uiUsedPageIDs[iTabs][1] = iUiID_CasingIn then
		uiUsedPageNames[iTabs][1]	= `Внутренний Наличник`
		uiUsedPageNames[iTabs][2]	= "uiTab_DInsideCasing_flat(1)"
	endif
	if uiUsedPageIDs[iTabs][1] = iUiID_Shutter then
		uiUsedPageNames[iTabs][1]	= `Ставни`
		uiUsedPageNames[iTabs][2]	= "uiTab_OpeningSunshade_flat(1)"
	endif
	if uiUsedPageIDs[iTabs][1] = iUiID_CustomCorner then
		uiUsedPageNames[iTabs][1]	= `Специальный Угол`
		uiUsedPageNames[iTabs][2]	= "uiTab_WindowCustomCorner_flat(1)"
	endif
	if uiUsedPageIDs[iTabs][1] = iUiID_FixFittingModAttr then
		uiUsedPageNames[iTabs][1]	= `Реквизиты Модели`
		uiUsedPageNames[iTabs][2]	= "uiTab_3DRepr_flat(1)"
	endif
next iTabs


