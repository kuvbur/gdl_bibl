		values "gs_open2D"	 range [0, side1Length - gs_leaf_num  * gs_leaf_thk]
		values "gs_open2D_2" range [0, side2Length - gs_leaf_num2 * gs_leaf_thk]
		openValues	= 1
	endif
	if	gs_leaf_optype_m = LEAF_1L_EXTERNAL_SLIDING |\
