	if bArchedTop then
		iWindowShape = SHAPE_ARCHED_HALF_T_LEFT
	else
		iWindowShape = SHAPE_HALF_T_LEFT
	endif
endif

if not(gs_sidelight_left) & gs_sidelight_right then
	if bArchedTop then
		iWindowShape = SHAPE_ARCHED_HALF_T_RIGHT
	else
		iWindowShape = SHAPE_HALF_T_RIGHT
	endif
endif

