				if iInnerEdge = ROUNDED then ui_pict 179, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayLinear_dim_cub_rot_rounded.png
			endif
		endif
		if iLayoutType = LAYOUT_WAVE then
			if iInnerEdge = STRAIGHT then ui_pict 180, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayLinear_dim_wave_straight.png
			if iInnerEdge = ROUNDED then ui_pict 181, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayLinear_dim_wave_rounded.png
		endif

		if iLayoutType <> LAYOUT_STRAIGHT then
			if iLayoutType = LAYOUT_CUBICLE then
