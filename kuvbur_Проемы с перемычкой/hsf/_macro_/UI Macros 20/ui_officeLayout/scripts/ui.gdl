		yCenter	= pageStartY+105

		xLeftStr1	= 230 - 220
		Strlength1	= 100 - 30
		xLeftStr2	= 350 - 220 - 15 - 30
		Strlength2	= 85 + 15 + 30
		yLeftStr1	= pageStartY
		yCurrent	= yLeftStr1 + textElevInfield
		pictOffset	= 1

		if iLayoutType = LAYOUT_WAVE then
			ui_outfield `Размер Стола`, xLeftStr1, yCurrent, Strlength1, outfieldHeight
