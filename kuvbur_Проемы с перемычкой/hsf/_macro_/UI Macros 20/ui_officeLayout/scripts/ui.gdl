			105, 105, 90, 90,
			1, `Прямой`, 1,
			2, `Кубическая`, 2,
			3, `Волна`, 3
	endif

return

! -----------------------------------------------------------------------------
"dimensionsPage":
! -----------------------------------------------------------------------------
	if iCallerType = WS_SOLO then
		pageStartY = 0
		_infHeight = 19
		_outfHeight = 15
		_yOffset = 4
		widthInf2	= 37
		xPict = 167
		yPict = 111
		PictWidth = 112 
		PictHeight = 117
		_outfFlag = 0
		x1 = 0
		x2 = 123
		x3 = 231
		x4 = 354
		x5 = xPict - 93
		x6 = xPict + PictWidth + 3
		dy = 24

		_yCurrent = pageStartY
		ui_infield{3} "iDeskStyle", x2, _yCurrent, 90, 68,
			2, 319, 8, 2,		!ui_WsSolo_dim_desks.png
			80, 80, 75, 61,
			1, stDeskStyle[3], DESKSTYLE_120STR,
			2, stDeskStyle[4], DESKSTYLE_120ROUND,
			3, stDeskStyle[1], DESKSTYLE_90STR,
			4, stDeskStyle[2], DESKSTYLE_90ROUND,
			5, stDeskStyle[5], DESKSTYLE_STR ui_tooltip `Стиль Стола`

		ui_infield{3} "tableTopHeight", x4, _yCurrent, 90, _infHeight ui_tooltip `Высота Стола`

		_yCurrent = _yCurrent + _yOffset
		ui_outfield `Стиль Стола`, x1, _yCurrent, x2 - 2, _outfHeight
		ui_outfield `Высота Стола`, x3, _yCurrent, x2 - 2, _outfHeight

		_yCurrent = _yCurrent + dy - _yOffset
		ui_infield{3} "topThk", x4, _yCurrent, 90, _infHeight ui_tooltip `Толщ. Столешницы`

		_yCurrent = _yCurrent + _yOffset
		ui_outfield `Толщ. Столешницы`, x3, _yCurrent, x2 - 2, _outfHeight

		_yCurrent = _yCurrent + dy - _yOffset
		ui_infield{3} "innerEdgeRad", x4, _yCurrent, 90, _infHeight ui_tooltip `Радиус`

		if iDeskStyle <> DESKSTYLE_90ROUND and iDeskStyle <> DESKSTYLE_120ROUND then _outfFlag = 4

		_yCurrent = _yCurrent + _yOffset
		ui_outfield `Радиус`, x3, _yCurrent, x2 - 2, _outfHeight, _outfFlag

		_outfFlag = 0

		_yCurrent = _yCurrent + dy - _yOffset

		ui_separator x1, _yCurrent, 444, _yCurrent

		if iDeskStyle = DESKSTYLE_90STR 	then ui_pict 170, xPict, yPict, PictWidth, PictHeight, 1		!ui_WsSolo_dim_90_straight.png
		if iDeskStyle = DESKSTYLE_90ROUND 	then ui_pict 171, xPict, yPict, PictWidth, PictHeight, 1		!ui_WsSolo_dim_90_rounded.png
		if iDeskStyle = DESKSTYLE_120STR 	then ui_pict 172, xPict, yPict, PictWidth, PictHeight, 1		!ui_WsSolo_dim_120_straight.png
		if iDeskStyle = DESKSTYLE_120ROUND 	then ui_pict 173, xPict, yPict, PictWidth, PictHeight, 1		!ui_WsSolo_dim_120_rounded.png
		if iDeskStyle = DESKSTYLE_STR 		then ui_pict 174, xPict, yPict, PictWidth, PictHeight, 1		!ui_WsSolo_dim_straight.png

		if iDeskStyle = DESKSTYLE_STR then
			_yCurrent= yPict - _infHeight - 3
			ui_infield{3} "tableChairDistX", xPict + 10, _yCurrent, 90, _infHeight ui_tooltip `Расстояние Стул-Стол 1`

			_yCurrent= yPict + 35
			ui_infield{3} "tableChairDistY", x5, _yCurrent, 90, _infHeight ui_tooltip `Расстояние Стул-Стол 2`

			_yCurrent= yPict + 65
			ui_infield{3} "tableDepth1", x5, _yCurrent, 90, _infHeight ui_tooltip `Глубина Стола 1`

			_yCurrent= yPict + PictHeight + 3
			ui_infield{3} "tableLength1", xPict + 10, _yCurrent, 90, _infHeight ui_tooltip `Глубина Стола 1`
		else
			_yCurrent= yPict - _infHeight - 3
			ui_infield{3} "tableDepth2", xPict - 57, _yCurrent, 90, _infHeight ui_tooltip `Глубина Стола 2`
			ui_infield{3} "tableChairDistX", xPict + 35, _yCurrent, 90, _infHeight ui_tooltip `Расстояние Стул-Стол 1`

