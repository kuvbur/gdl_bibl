"3DPage":
! -----------------------------------------------------------------------------
	dim surfacesArray[]
	SurfItemID = 1
   
  	surfacesArray[SurfItemID]	= "gs_top_mat"
	SurfItemID = SurfItemID + 1

	if gs_bOfficeCab or gs_leg_type_m = 0 then
		surfacesArray[SurfItemID]	= "gs_leg_mat"
		SurfItemID = SurfItemID + 1
	endif

	if gs_leg_type_m = 1 or gs_leg_type_m = 2 or gs_leg_type_m = 5 then ! Style 1, Style 2, Style 5
		surfacesArray[SurfItemID]	= "legbase_mat"
		SurfItemID = SurfItemID + 1
	endif

	if bShowPartitionParams then
		surfacesArray[SurfItemID]	= "gs_partitionFrame_mat"
		SurfItemID = SurfItemID + 1

		surfacesArray[SurfItemID]	= "gs_partition_mat"
		SurfItemID = SurfItemID + 1
	endif

	if gs_bFrontPanel then
		surfacesArray[SurfItemID]	= "gs_frontPanel_mat"
		SurfItemID = SurfItemID + 1
	endif

	if bShowCabParams and gs_bOfficeCab then
		surfacesArray[SurfItemID]	= "gs_cabinet_mat"
		SurfItemID = SurfItemID + 1
	endif

	if bCabinetBase and bShowCabParams and gs_bOfficeCab and bCabBase then
		surfacesArray[SurfItemID]	= "gs_base_mat"
		SurfItemID = SurfItemID + 1
	endif

	if iCallerType = WS_SOLO then
		if gs_bOfficeCab then
			surfacesArray[SurfItemID]	= "gs_door_mat"
			SurfItemID = SurfItemID + 1
		endif
	else
		if iCallerType = LAYOUT_ISLAND then
			if ((iExtensionTypeLeft = 4 or iExtensionTypeRight = 4 or iExtensionTypeBottom = 4or iExtensionTypeTop = 4) and iCabinetScheme <> 5) or gs_bOfficeCab then
				surfacesArray[SurfItemID]	= "gs_door_mat"
				SurfItemID = SurfItemID + 1
			endif
		else
			if ((iExtensionTypeLeft = 4 or iExtensionTypeRight = 4) and iCabinetScheme <> 5) or gs_officeCab_m <> 0 then
				surfacesArray[SurfItemID]	= "gs_door_mat"
				SurfItemID = SurfItemID + 1
			endif
		endif
	endif

	if (iCabKnobType <> 0 or gs_bOfficeCab) and bKnob and bShowCabParams then
		surfacesArray[SurfItemID]	= "gs_knob_mat"
		SurfItemID = SurfItemID + 1
	endif

	surfacesArray[SurfItemID]	= "gs_frame_mat"
	SurfItemID = SurfItemID + 1

	surfacesArray[SurfItemID]	= "gs_seat_mat"
	SurfItemID = SurfItemID + 1

