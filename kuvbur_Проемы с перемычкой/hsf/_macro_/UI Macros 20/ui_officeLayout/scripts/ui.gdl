				xCurrent = xCenter - 100
				yCurrent = yCenter - yPict / 2 - 25
			endif
			ui_infield "tableDepth2", xCurrent, yCurrent, 90, infieldHeight	ui_tooltip `Глубина Стола 2`

			if iLayoutType = LAYOUT_CUBICLE then
				xCurrent = xCenter - 150
				yCurrent = yCenter - 28
			endif
			if iLayoutType = LAYOUT_WAVE then
				xCurrent = xCenter - 100
				yCurrent = yCenter + yPict / 2 + 5
			endif
			ui_infield "tableLength2", xCurrent, yCurrent, 90, infieldHeight	ui_tooltip `Длина Стола 2`
			yCurrent = yCurrent + 25
		endif

		if iLayoutType = LAYOUT_STRAIGHT then
			xCurrent = xCenter - 30
			yCurrent = yCenter - yPict / 2 - 25
		endif
		if iLayoutType = LAYOUT_CUBICLE then
			xCurrent = xCenter - 30
			yCurrent = yCenter + yPict / 2 + 5
		endif
		if iLayoutType = LAYOUT_WAVE then
			xCurrent = xCenter + 20
			yCurrent = yCenter + yPict / 2 + 5
		endif
		ui_infield "tableLength1", xCurrent, yCurrent, 90, infieldHeight		ui_tooltip `Длина Стола 1`

		if iLayoutType = LAYOUT_STRAIGHT then
			xCurrent = xCenter + xPict / 2 + 13
			yCurrent = yCenter - 15
		endif
		if iLayoutType = LAYOUT_CUBICLE then
			xCurrent = xCenter + xPict / 2 + 13
			yCurrent = yCenter - 15
		endif
		if iLayoutType = LAYOUT_WAVE then
			xCurrent = xCenter + 20
			yCurrent = yCenter - yPict / 2 - 25
		endif
		ui_infield "tableDepth1", xCurrent, yCurrent, 90, infieldHeight		ui_tooltip `Глубина Стола 1`
		yCurrent = yCurrent + 25



		yCurrent 	= pageStartY+195
		ui_outfield `Смещение Стула 1`, xCenter - 150, yCurrent, 110, outfieldHeight
		yCurrent = yCurrent - textElevInfield
		ui_infield "tableChairDistX", xCenter - 30, yCurrent, 90, infieldHeight ui_tooltip `Расстояние Стул-Стол 1`
		yCurrent = yCurrent + 25

		ui_outfield `Смещение Стула 2`, xCenter - 150, yCurrent, 110, outfieldHeight
		yCurrent = yCurrent - textElevInfield
		ui_infield "tableChairDistY", xCenter - 30, yCurrent, 90, infieldHeight ui_tooltip `Расстояние Стул-Стол 2`
		yCurrent = yCurrent - 20


		if iLayoutType = LAYOUT_WAVE then
			ui_outfield `Угол Оси`, xCenter - 30 + 125, yCurrent, 60, outfieldHeight, 4 * (iDeskSizing = DESKSIZING_FIX_AXIS)
			yCurrent = yCurrent + 20
			ui_infield "axisAngle", xCenter - 30 + 125, yCurrent, 60, infieldHeight ui_tooltip `Угол Оси`
			yCurrent = yCurrent + 25
		endif

		ui_separator 330, pageStartY+25, 330, pageStartY + 230

		xLeftStr1	= 335
		yLeftStr1	= pageStartY+31
		yCurrent	= yLeftStr1

		ui_outfield `Высота Стола`, xLeftStr1, yCurrent, 105, outfieldHeight
		yCurrent = yCurrent + 17
		ui_infield "tableTopHeight", xLeftStr1, yCurrent, 90, infieldHeight ui_tooltip `Высота Стола`
		yCurrent = yCurrent + 24

		ui_outfield `Толщ. Столешницы`, xLeftStr1, yCurrent, 105, outfieldHeight
		yCurrent = yCurrent + 17
		ui_infield "topThk", xLeftStr1, yCurrent, 90, infieldHeight ui_tooltip `Толщ. Столешницы`
		yCurrent = yCurrent + 24

		ui_outfield `Внутренний Угол`, xLeftStr1, yCurrent, 105, outfieldHeight, 4 * (iLayoutType = LAYOUT_STRAIGHT)
		yCurrent = yCurrent + 17
		ui_infield{3} "iInnerEdge", xLeftStr1, yCurrent, 90, infieldHeight,
			8, "",
			2,
			1, 100, 20, 100, 20,
			"", `Прямой`, STRAIGHT,
			"", `Закругленный`, ROUNDED ui_tooltip `Внутренний Угол`
		yCurrent = yCurrent + 24

		ui_outfield `Радиус`, xLeftStr1, yCurrent, 105, outfieldHeight, 4 * (iInnerEdge = STRAIGHT)
		yCurrent = yCurrent + 17
		ui_infield "innerEdgeRad", xLeftStr1, yCurrent, 90, infieldHeight ui_tooltip `Радиус`
		yCurrent = yCurrent + 24

		ui_outfield `Общая Длина`, xLeftStr1, yCurrent, 105, outfieldHeight, 4 * (iLayoutType = LAYOUT_WAVE or iSizing = SIZING_WS_SIZE)
		yCurrent = yCurrent + 17
		ui_infield "totalLength", xLeftStr1, yCurrent, 90, infieldHeight ui_tooltip `Общая Длина`
		yCurrent = yCurrent + 24
	endif

	if iCallerType = LAYOUT_ISLAND then
		xCenter	= 168
		yCenter	= pageStartY+85

		xPict	= 113
		yPict	= 113
		if iArrangement = ARRANGE_4_DESK_MIRR then
			if iInnerEdge = STRAIGHT then ui_pict 182, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_dim_4mir_straight.png
			if iInnerEdge = ROUNDED then ui_pict 183, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_dim_4mir_rounded.png
		endif
		if iArrangement = ARRANGE_4_DESK_ROT then
			if iInnerEdge = STRAIGHT then ui_pict 184, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_dim_4rot_straight.png
			if iInnerEdge = ROUNDED then ui_pict 185, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_dim_4rot_rounded.png
		endif
		if iArrangement = ARRANGE_3_DESK_ALIGN then
			if iInnerEdge = STRAIGHT then ui_pict 186, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_dim_3align_straight.png
			if iInnerEdge = ROUNDED then ui_pict 187, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_dim_3align_rounded.png
		endif
		if iArrangement = ARRANGE_3_DESK_SHIFT then
			if iInnerEdge = STRAIGHT then ui_pict 188, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_dim_3shift_straight.png
			if iInnerEdge = ROUNDED then ui_pict 189, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_dim_3shift_rounded.png
