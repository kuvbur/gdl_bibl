				_yCurrent= yPict + PictHeight + 3
				ui_infield{3} "tableLength1", xPict + 10, _yCurrent, 90, _infHeight ui_tooltip `Длина Стола 1`
	
				_yCurrent= yPict + 38
				ui_infield{3} "tableChairDistY", x6, _yCurrent, 90, _infHeight ui_tooltip `Расстояние Стул-Стол 2`

				_yCurrent= yPict + 38 + dy
				ui_infield{3} "tableDepth1", x6, _yCurrent, 90, _infHeight ui_tooltip `Глубина Стола 1`


			else	! DESKSTYLE_120STR
				_yCurrent= yPict + 30
				ui_infield{3} "tableDepth2", x5, _yCurrent, 90, _infHeight ui_tooltip `Глубина Стола 2`

				_yCurrent= yPict + PictHeight + 3
				ui_infield{3} "tableDepth1", xPict - 3, _yCurrent, 90, _infHeight ui_tooltip `Глубина Стола 1`

				_yCurrent= yPict + 65
				ui_infield{3} "tableChairDistY", x6, _yCurrent, 90, _infHeight ui_tooltip `Расстояние Стул-Стол 2`

				_yCurrent= yPict + 65 + dy
				ui_infield{3} "tableDepth1", x6, _yCurrent, 90, _infHeight ui_tooltip `Глубина Стола 1`
			endif
		endif
	endif

	if iCallerType = LAYOUT_LINEAR then
