		xPict		= 151
		yPict		= 151

		if iArrangement = ARRANGE_4_DESK_MIRR then ui_pict 402, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_mins_4mirr.png
		if iArrangement = ARRANGE_4_DESK_ROT then ui_pict 403, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_mins_4rot.png
		if iArrangement = ARRANGE_3_DESK_ALIGN then ui_pict 404, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_mins_3align.png
		if iArrangement = ARRANGE_3_DESK_SHIFT then ui_pict 405, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_mins_3shift.png

		yCurrent = yCenter - yPict / 2 - 20
		ui_outfield `Все Стороны`, xCenter + xPict / 2 + 5, yCurrent, 90, outfieldHeight
		yCurrent = yCurrent + 20
		ui_infield "MSAllSides", xCenter + xPict / 2 + 5, yCurrent, 90, infieldHeight

		if iArrangement = ARRANGE_4_DESK_ROT then
			yCurrent = yCenter + yPict / 2 - 40
			ui_outfield STR(workingLengthUnit, MSAllSides), xCenter + xPict / 2 + 5, yCurrent, 90, outfieldHeight
		else
			yCurrent = yCenter + yPict / 2 - 65 + 10 * (iArrangement = ARRANGE_4_DESK_MIRR)
			ui_outfield `Перед Столами`, xCenter + xPict / 2 + 5, yCurrent, 90, outfieldHeight
			yCurrent = yCurrent + 20
			ui_infield "MSFront", xCenter + xPict / 2 + 5, yCurrent, 90, infieldHeight
		endif

		if iArrangement <> ARRANGE_3_DESK_SHIFT then
			xCurrent = xCenter - xPict / 2 - 20 + 8 * (iArrangement = ARRANGE_4_DESK_ROT) + 32 * (iArrangement = ARRANGE_3_DESK_ALIGN)
			yCurrent = yCenter + yPict / 2 + 5
			if iArrangement = ARRANGE_3_DESK_ALIGN then
				ui_outfield STR(workingLengthUnit, MSFront), xCurrent, yCurrent, 60, outfieldHeight, 2
			else
				ui_outfield STR(workingLengthUnit, MSAllSides), xCurrent, yCurrent, 60, outfieldHeight, 2
			endif

			xCurrent = xCenter + 25 - 5 * (iArrangement = ARRANGE_4_DESK_ROT) - 25 * (iArrangement = ARRANGE_3_DESK_ALIGN)
			yCurrent = yCenter + yPict / 2 + 5
			ui_outfield STR(workingLengthUnit, MSAllSides), xCurrent, yCurrent, 60, outfieldHeight, 2
		endif
	endif

	if iCallerType = LAYOUT_LINEAR then
		xPict		= 192
		yPict		= 127

		if iLayoutType = LAYOUT_STRAIGHT then ui_pict 40, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_mins_straight.png
		if iLayoutType = LAYOUT_CUBICLE then
			if iArrangement = ARRANGE_MIRR then
				ui_pict 41, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_mins_cub_mirr.png
			else
				ui_pict 42, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_mins_cub_rot.png
			endif
		endif
		if iLayoutType = LAYOUT_WAVE then ui_pict 46, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayIsland_mins_wave.png

		yCurrent = yCenter - yPict / 2 - 20 * (iLayoutType = LAYOUT_CUBICLE)
		ui_outfield `Перед Столами`, xCenter + xPict / 2 + 5, yCurrent, 90, outfieldHeight
		yCurrent = yCurrent + 20
		ui_infield "MSFront", xCenter + xPict / 2 + 5, yCurrent, 90, infieldHeight

		yCurrent = yCenter - yPict / 2 + 80 + 15 * (iLayoutType = LAYOUT_CUBICLE) + 5 * (iLayoutType = LAYOUT_WAVE)
		ui_outfield STR(workingLengthUnit, MSFront), xCenter + xPict / 2 + 5, yCurrent, 90, outfieldHeight


		if iLayoutType = LAYOUT_WAVE then
			xCurrent = xCenter - xPict / 2 - 95
			yCurrent = yCenter - yPict / 2 + 15
		else
			xCurrent = xCenter - xPict / 2
			yCurrent = yCenter + yPict / 2 + 5
		endif
		ui_infield "MSSide", xCurrent, yCurrent, 90, infieldHeight			ui_tooltip `Cо Стороны 1`

		if iLayoutType = LAYOUT_WAVE then
			xCurrent = xCenter - xPict / 2 - 95
			yCurrent = yCenter - yPict / 2 + 50
		else
			xCurrent = xCenter + xPict / 2 - 70
			yCurrent = yCenter + yPict / 2 + 5
		endif
		ui_infield "MSSide2", xCurrent, yCurrent, 90, infieldHeight		ui_tooltip `Cо Стороны 2`
	endif

	if iCallerType = WS_SOLO then
		xPict		= 123
		yPict		= 120
		if iDeskStyle = 5 then	! Straight
			ui_pict 392, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		! ui_LaySolo_mins_straight.png
		
			yCurrent = yCenter - yPict / 2 + 25
			ui_infield "MSFront", xCenter + xPict / 2 + 3, yCurrent, 90, infieldHeight ui_tooltip `Минимальное Пространство перед Столами`
		
			yCurrent = yCenter - yPict / 2 - infieldHeight - 3	
			ui_infield "MSLength", xCenter - xPict / 2 + 10, yCurrent, 90, infieldHeight ui_tooltip `Длина Минимального Пространства`
		else
			yCurrent = yCenter - yPict / 2 + 20
			ui_infield "MSSide2", xCenter - xPict / 2 - 93, yCurrent, 90, infieldHeight ui_tooltip `Минимальное Пространство Сбоку 2`
		
			if iDeskStyle = 1 or iDeskStyle = 2 then	! L90 Straight, L90 Rounded
				ui_pict 390, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		! ui_WsSolo_mins_90.png
				yCurrent = yCenter - yPict / 2 + 32
				ui_infield "MSFront", xCenter + xPict / 2 + 3, yCurrent, 90, infieldHeight ui_tooltip `Минимальное Пространство перед Столами`
		
				yCurrent = yCenter + yPict / 2 + 3
				ui_infield "MSSide", xCenter - xPict / 2 + 25, yCurrent, 90, infieldHeight ui_tooltip `Минимальное Пространство Сбоку 1`
			else	! L120 Straight, L120 Rounded
				ui_pict 391, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		! ui_LaySolo_mins_120.png
		
				yCurrent = yCenter - yPict / 2 + 65
				ui_infield "MSFront", xCenter + xPict / 2 + 3, yCurrent, 90, infieldHeight ui_tooltip `Минимальное Пространство перед Столами`
		
				yCurrent = yCenter + yPict / 2 + 3	
				ui_infield "MSSide", xCenter - xPict / 2 + 3, yCurrent, 90, infieldHeight ui_tooltip `Минимальное Пространство Сбоку 1`
			endif
		endif

	endif

return


