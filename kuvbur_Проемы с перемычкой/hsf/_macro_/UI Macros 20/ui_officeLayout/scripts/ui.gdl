		SurfItemID = SurfItemID + 1
		surfacesArray[SurfItemID]	= "feetmat"
		SurfItemID = SurfItemID + 1
		surfacesArray[SurfItemID]	= "cover_mat"
		SurfItemID = SurfItemID + 1
		surfacesArray[SurfItemID]	= "keyb_mat"
		SurfItemID = SurfItemID + 1
		surfacesArray[SurfItemID]	= "dispmat"
		SurfItemID = SurfItemID + 1
	endif

	dim childFlagArray[]
	for k = 1 to vardim1(surfacesArray)
		childFlagArray[k]	= 0
	next k

	call "ui_object_3DRepresentation" parameters 	bShowAnimation		= 0,
													surfacesArray		= surfacesArray,
													childFlagArray		= childFlagArray,
