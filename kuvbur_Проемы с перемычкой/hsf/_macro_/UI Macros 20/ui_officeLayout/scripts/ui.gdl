		else
			ui_outfield `Размер`, xLeftStr1, yCurrent, Strlength1, outfieldHeight
			yCurrent = yCurrent - textElevInfield
			ui_infield{3} "iSizing", xLeftStr2, yCurrent, Strlength2, infieldHeight,
				8, "",
				2,
				1, 100, 20, 100, 20,
				"", `по Размеру Рабочего Места`, SIZING_WS_SIZE,
				"", `по Количество Рабочих Мест`, SIZING_WS_COUNT ui_tooltip `Размер`
		endif
		yCurrent = yCurrent + 25

		ui_separator 5, yCurrent, 440, yCurrent

		ui_separator 220, pageStartY-3, 220, yCurrent - 2

		xLeftStr1	= 230
		Strlength1	= 100
		xLeftStr2	= 335
		Strlength2	= 90
		yLeftStr1	= pageStartY
		yCurrent	= yLeftStr1 + textElevInfield

		ui_outfield `Кол-во Столов`, xLeftStr1, yCurrent, Strlength1, outfieldHeight
		yCurrent = yCurrent - textElevInfield
		ui_infield{3} "deskNumber", xLeftStr2, yCurrent, Strlength2, infieldHeight ui_tooltip `Кол-во Столов`



		xPict		= 98 * (iLayoutType <> LAYOUT_WAVE) + 139 * (iLayoutType = LAYOUT_WAVE)
		yPict		= 98
		if iLayoutType = LAYOUT_STRAIGHT then
			ui_pict 175, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayLinear_dim_straight.png
		endif
		if iLayoutType = LAYOUT_CUBICLE then
			if iArrangement = ARRANGE_MIRR then
				if iInnerEdge = STRAIGHT then ui_pict 176, xCenter - xPict / 2, yCenter - yPict / 2, xPict, yPict, 1		!ui_LayLinear_dim_cub_mirr_straight.png
