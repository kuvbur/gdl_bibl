"arrangementPage":
! -----------------------------------------------------------------------------
	if iCallerType = LAYOUT_ISLAND then
		ui_infield{3} "iArrangement", 0, pageStartY, 444, 260,
			1, 273, 				!ui_LayIsland_arrange.png
			4, 1,		
			105, 105, 90, 90,
			1, `4-стола (зеркально)`, 1,
			2, `4-стола (повернуты)`, 2,
			3, `3-стола (выровнены)`, 3,
			4, `3-стола (со сдвигом)`, 4
	endif

	if iCallerType = LAYOUT_LINEAR then
		ui_infield{3} "iLayoutType", 0, pageStartY, 444, 260,
