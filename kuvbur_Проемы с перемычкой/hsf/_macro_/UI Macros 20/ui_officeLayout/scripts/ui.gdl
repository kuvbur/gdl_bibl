! =============================================================================
! Tabpage Structure Definition
! =============================================================================
_idxTab = 1
TABID_Root			= -1

TABID_Arrangement		= _idxTab	: _idxTab = _idxTab + 1
TABID_Dim				= _idxTab	: _idxTab = _idxTab + 1
TABID_Layout			= _idxTab	: _idxTab = _idxTab + 1
TABID_Accessories		= _idxTab	: _idxTab = _idxTab + 1
TABID_DeskCabinet		= _idxTab	: _idxTab = _idxTab + 1
TABID_Extensions		= _idxTab	: _idxTab = _idxTab + 1
TABID_ExtensionCabinet	= _idxTab	: _idxTab = _idxTab + 1
TABID_2DRepresentation	= _idxTab	: _idxTab = _idxTab + 1
TABID_3DRepresentation	= _idxTab	: _idxTab = _idxTab + 1
TABID_MinimalSpace		= _idxTab	: _idxTab = _idxTab + 1
TABID_Descriptions		= _idxTab	: _idxTab = _idxTab + 1

! =============================================================================
! Display actual tabpages
! =============================================================================
ui_current_page gs_ui_current_page

if _bShowArrangementPage then
	ui_page TABID_Arrangement, TABID_Root, `Расположение`, "uiTab_Style_flat(1)"
		gosub "arrangementPage"
endif

if iCallerType = WS_SOLO then
	_tabpageTitle = `Стиль и Размеры`
	_tabpageIcon = "uiTab_Dimension_flat(1)"
else
	_tabpageTitle = `Размеры`
	_tabpageIcon = "uiTab_Dimension_flat(1)"
endif

ui_page TABID_Dim, TABID_Root, _tabpageTitle, _tabpageIcon
	gosub "dimensionsPage"

if _bShowLayoutPage then
	ui_page TABID_Layout, TABID_Root, `Компоновка`, "uiTab_OfficeLayout_flat(1)"
		gosub "layoutPage"
endif

if iCallerType = WS_SOLO then 
	_tabpageRoot = TABID_Dim
else
	_tabpageRoot = TABID_Layout
endif

ui_page TABID_Accessories, _tabpageRoot, `Стул, Ножки, Передняя Панель и Компьютер`, "uiTab_FurnitureAccessories2_flat(1)"
	gosub "accessoriesPage"

ui_page TABID_DeskCabinet, _tabpageRoot, `Тумбочка и Ручка`, "uiTab_Cabinet_flat(1)"
	gosub "deskCabinetPage"

if iCallerType = WS_SOLO then
	_tabpageTitle = `Перегородки`
else
	_tabpageTitle = `Перегородки и Приставной Стол`
endif

ui_page TABID_Extensions, _tabpageRoot, _tabpageTitle, "uiTab_FurnitureExtension_flat(1)"
	gosub "extensionsPage"

if _bShowExtCabinetPage then
	ui_page TABID_ExtensionCabinet, _tabpageRoot, `Приставной Шкаф`, "uiTab_FurnitureExtensionCab_flat(1)"
		gosub "extensionCabinetPage"
endif

ui_page TABID_2DRepresentation, TABID_Root, `2D-отображение`, "uiTab_2DRepr_flat(1)"
	gosub "2DPage"

ui_page TABID_3DRepresentation, TABID_Root, `3D-отображение и Покрытия`, "uiTab_3DRepr_flat(1)"
	gosub "3DPage"

ui_page TABID_MinimalSpace, TABID_Root, `Минимальное Пространство`, "uiTab_MinSpace_flat(1)"
	gosub "minimalSpacePage"

ui_page TABID_Descriptions, TABID_Root, `Характеристики`, "uiTab_Descriptions_flat(1)"
	gosub "Descriptionspage"

