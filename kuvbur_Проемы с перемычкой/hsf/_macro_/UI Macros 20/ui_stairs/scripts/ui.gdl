"TreadSetPage":
! =============================================================================
! Tread Settings
! =============================================================================

	pageStartY = 5

	ui_style 0, 0

	pos_x=4    			! start Position X Coordinate
	pos_y=pageStartY 	! start Position Y Coordinate

	y 				= pos_y
	lineSpace 		= 24
	heightRow 		= 18
	widthBox		= 50
	startPictSet	= 70
	endPictSet		= startPictSet + 160
	startPictNose	= 320
	endPictNose		= startPictNose + 100

	ui_style 0, 1
	ui_infield{3} "bRiser", pos_x, y, widthBox+18, heightRow*1.5,
				7, "",
				2,
				1, 20, 20, 20, 20,
				0, `Подступенок`, 0,
				1, `Подступенок`, 1
	ui_style 0, 0

	if bRiser then typeTreadPict = 13 else typeTreadPict = 27
	ui_pict typeTreadPict,					startPictSet,				y+heightRow+5, 159, 183, 1	! ui_stair_settings_tread

	ui_infield "lengthNose", 	startPictSet+5,				y, widthBox,	heightRow	ui_tooltip stStairUI[58]	! Nose Length
	ui_infield "treadDepth",	startPictSet+widthBox+10 ,	y, widthBox+18, heightRow	ui_tooltip stStairUI[59]	! Tread Depth

	y = y + 75
	ui_infield "angleRiser",	pos_x,				y+7,	widthBox+18,heightRow	ui_tooltip stStairUI[66]	! Riser Angle

	ui_infield "treadHeight",	endPictSet+5,		y,		widthBox+18,heightRow	ui_tooltip stStairUI[60]	! Tread Height
	y = y + lineSpace + 8
	ui_infield "treadThickness",endPictSet+5 ,		y,		widthBox+18,	heightRow	ui_tooltip stStairUI[61]	! Tread Cover Thickness
	y = y + lineSpace + 8
	ui_infield "thickRiser",	endPictSet+5 ,		y,		widthBox+18,	heightRow	ui_tooltip stStairUI[62]	! Riser Thickness
	y = y + lineSpace + 8
	ui_infield "thickPan",		endPictSet+5 ,		y,		widthBox+18,	heightRow	ui_tooltip stStairUI[63]	! Pan Thickness
	y = y + lineSpace + 15

	ui_infield "widthOverlapTread", startPictSet+5,					y, widthBox+18,	heightRow	ui_tooltip stStairUI[64]	! Tread Overlap
	ui_infield "distNoseStringer",	startPictSet+widthBox+18+10 ,	y, widthBox,	heightRow	ui_tooltip stStairUI[65]	! Nose - Stringer Top Distance

	ui_separator 308, pageStartY + 3, 308, pageStartY + 231
	y 	= pos_y + 20

	ui_style 0, 1
	ui_outfield stStairUI[67], 320,	y, 80, heightRow	! Nose Type
	ui_style 0, 0

	cell1_x=75		! width
	cell1_y=25		! height

	UI_INFIELD{3} "typeNose", 401, y-10, 35, 35,
		3, 14, 3, 1,								! ui_stair_nose_Type.png
		cell1_x,cell1_y,25,25,
			1,	"",stTypeNose[1],		! Sharp
			2,	"",stTypeNose[2],		! Fillet
			3,	"",stTypeNose[3]		! Chamfer

	for i = 1 to 3
	   if typeNose = stTypeNose[i] then typeNosePict = 14+i
	next i
	y = y + lineSpace + 20

	ui_pict typeNosePict, startPictNose, y+heightRow+5, 98, 104, 1	! ui_stair_nose_Sharp/Fillet/Chamfer

	if typeNose = stTypeNose[2] then
		ui_infield "radiusFillet",startPictNose+42 , y, widthBox+18,heightRow	ui_tooltip stStairUI[68]	! Fillet Radius
	endif
	if typeNose = stTypeNose[3] then
		ui_infield "depthChamfer",startPictNose+42 , y, widthBox+18,heightRow	ui_tooltip stStairUI[69]	! Chamfer Distance
		y = y + lineSpace + 120
		ui_infield "angleChamfer",startPictNose+42 , y, widthBox+18,heightRow	ui_tooltip stStairUI[70]	! Chamfer Angle
	endif
return

"StringerSetPage":
! =============================================================================
! Stringer Settings
! =============================================================================

	pageStartY = 5
	pos_x=4    			! start Position X Coordinate
	pos_y=pageStartY 	! start Position Y Coordinate

	y 				= pos_y
	lineSpace 		= 24
	heightRow 		= 18
	widthBox		= 50
	widthDescript	= 110
	startPictSet	= 28
	endPictSet		= startPictSet + 130

	startPictString	= 160
	endPictString	= startPictString + 200

	ui_style 0, 1
	ui_outfield stStairUI[71], pos_x, y, widthDescript, heightRow, 2	! Stringer Type
	ui_style 0, 0
	y = y + lineSpace + 30

	cell1_x=280		! width
	cell1_y=116		! height

	UI_INFIELD{3} "typeStringer",startPictSet,	y+10, 70,120,
		3, 18, 8, 1,						! ui_stair_stringer_Type.png
		cell1_x,cell1_y,35,116,
			1,	"",stTypeStringer[1],		! Solid Bar
			2,	"",stTypeStringer[2],		! Hollow Core Bar
			3,	"",stTypeStringer[3],		! Rounded Rectangle
			4,	"",stTypeStringer[4],		! Hollow Rounded Rectangle
			5,	"",stTypeStringer[5],		! Solid Tube
			6,	"",stTypeStringer[6],		! Hollow Core Tube
			7,	"",stTypeStringer[7],		! Chanel inside open
			8,	"",stTypeStringer[8]		! Chanel outside open

	for i = 1 to 8
	   if typeStringer = stTypeStringer[i] then typeStringerPict = 18+i
	next i

	pos_x = 120
	ui_separator pos_x, pageStartY + 3, pos_x, pageStartY + 231
	y 	= pos_y

	ui_style 0, 1
	ui_outfield stStairUI[72], pos_x+10,	y,	widthDescript,heightRow,2	! Stringer Geometry
	ui_style 0, 0
	y = y + lineSpace + 15

	ui_infield "thickStringer", startPictString-20,					y, widthBox+18,	heightRow	ui_tooltip stStairUI[73]	! Stringer Thickness
	ui_infield "widthFreeFlight",startPictString+widthBox+18,		y, widthBox,	heightRow	ui_tooltip stStairUI[74]	! Free Flight Width
	ui_infield "widthStringer",	startPictString+widthBox*2+18+30,	y, widthBox+18,	heightRow	ui_tooltip stStairUI[75]	! Stringer Width

	y = y + lineSpace

	ui_pict typeStringerPict, 	startPictString,	y, 200, 		125, 1		! ui_stair_stringer_
	y = y + lineSpace + 30

	ui_infield "heightStringer",endPictString+5,	y, widthBox+18,	heightRow	ui_tooltip stStairUI[76]	! Stringer Height
	y = y + lineSpace + 50

	ui_infield "B",startPictString+widthBox+18,		y, widthBox+18,	heightRow	ui_tooltip stStairUI[78]	! Flight Width
	if typeStringer = stTypeStringer[3] | typeStringer = stTypeStringer[4] then
		ui_outfield stStairUI[79],		endPictString+5,	y-heightRow-3, widthBox+18,	heightRow
		ui_infield "radiusStringer",	endPictString+5,	y , widthBox+18,	heightRow	ui_tooltip stStairUI[77]	! Stringer Radius
	endif
return


"StringerEditPage":
! =============================================================================
! Stringer Settings - Editing
! =============================================================================

	pos_x			= 95    		! start Position X Coordinate
	pos_y			= pageStartY + 4	! start Position Y Coordinate
	heightRow 		= 18
	widthBox		= 55

	ui_pict 28, 	pos_x,	pos_y, 309, 200, 1		! ui_stair_stringer_edit\

	x = pos_x - widthBox - 5
	y = pos_y + 3
	ui_infield "cutAngleUpperEnd", x, y, widthBox, heightRow ui_tooltip stStairUI[82]	! Upper End Cutting Angle

	y = y + 44
	ui_infield "cutDistUpperEnd", x, y, widthBox, heightRow ui_tooltip stStairUI[83]	! Cutting Distance from Tread

	x = pos_x + 175
	y = pos_y + 205
