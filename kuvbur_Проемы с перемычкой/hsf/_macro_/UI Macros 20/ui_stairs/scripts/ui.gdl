		if rightRailType = stRailTypes[ 1] then zzz = 11		! Rail Balustrade
		if rightRailType = stRailTypes[ 2] then zzz = 12		! Rail Parallel Bars
		if rightRailType = stRailTypes[ 3] then zzz = 13		! Rail Simple Bar
		if rightRailType = stRailTypes[ 4] then zzz = 14		! Rail Solid Frame Filled
		if rightRailType = stRailTypes[ 5] then zzz = 15		! Rail Solid Frame
		if rightRailType = stRailTypes[ 6] then zzz = 16		! Rail Solid
		if rightRailType = stRailTypes[ 7] then zzz = 17		! Rail Post with Balusters
		if rightRailType = stRailTypes[ 8] then zzz = 18		! Rail Wired
		if rightRailType = stRailTypes[ 9] then zzz = 19		! Rail Wooden Balustrade
		if rightRailType = stRailTypes[10] then zzz = 20		! Rail Post with Bars

		if NOT(bRightRail) then		! Left
			ui_infield "leftRailType", pos_x, pos_y, cell1_x, cell1_y,
				1, (2 * bLeftRail + 30 * NOT(bLeftRail)), 24, 6,		! ui_stair_railings, ui_stair_railingsLock.png
				140, 105, 120, 85,
				1,	stRailTypesForUI[ 1],		! Balustrade
				2,	stRailTypesForUI[ 2],		! Parallel Bars
				3,	stRailTypesForUI[ 3],		! Simple Bar
				4,	stRailTypesForUI[ 4],		! Filled Solid Frame
				5,	stRailTypesForUI[ 5],		! Solid Frame
				6,	stRailTypesForUI[ 6],		! Solid
				7,	stRailTypesForUI[ 7],		! Post with Balusters
				8,	stRailTypesForUI[ 8],		! Wired
				9,	stRailTypesForUI[ 9],		! Wooden Balustrade
				10,	stRailTypesForUI[10]		! Post with Bars
		else
			ui_infield{3} "leftRailType", pos_x, pos_y, cell1_x, cell1_y,
				1, (2 * bLeftRail + 30 * NOT(bLeftRail)), 24, 6,		! ui_stair_railings, ui_stair_railingsLock.png
				140, 105, 120, 85,
					zzz, stRailTypesForUI[11], stRailTypes[11],		! Same as Right
					1,	stRailTypesForUI[1],  stRailTypes[1],		! Balustrade
					2,	stRailTypesForUI[2],  stRailTypes[2],		! Parallel Bars
					3,	stRailTypesForUI[3],  stRailTypes[3],		! Simple Bar
					4,	stRailTypesForUI[4],  stRailTypes[4],		! Filled Solid Frame
					5,	stRailTypesForUI[5],  stRailTypes[5],		! Solid Frame
					6,	stRailTypesForUI[6],  stRailTypes[6],		! Solid
					7,	stRailTypesForUI[7],  stRailTypes[7],		! Post with Balusters
					8,	stRailTypesForUI[8],  stRailTypes[8],		! Wired
					9,	stRailTypesForUI[9],  stRailTypes[9],		! Wooden Balustrade
					10,	stRailTypesForUI[10], stRailTypes[10]		! Post with Bars
		endif
	endif
return









