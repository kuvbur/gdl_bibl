	bSunshade = 0
	lock "bSunshade"
endif

if bArchedTop then
	gs_stack_left	= 0
	gs_stack_right	= 0
	gs_stack_top	= 0
	parameters 	gs_stack_left	= 0,
				gs_stack_right	= 0,
				gs_stack_top	= 0
	lock "gs_stack_left", "gs_stack_right", "gs_stack_top"
endif

if gs_frame_style_m = FRAME_STYLE_WRAPPED_EXTRUSION then
	gs_stack_left	= 0
	gs_stack_right	= 0
	gs_stack_top	= 0
endif

bEnableNEDFrameStyles = bEnableNEDFrameStyles &\
						not(gs_leaf_optype_m = LEAF_1L_PIVOT				| \
							gs_leaf_optype_m = LEAF_1L_PIVOT_M				| \
							gs_leaf_optype_m = LEAF_1L_EXTERNAL_SLIDING		|\
							gs_leaf_optype_m = LEAF_1L_EXTERNAL_SLIDING_M	|\
							gs_leaf_optype_m = LEAF_2L_EXTERNAL_SLIDING		|\
							gs_leaf_optype_m = LEAF_1L_POCKET				|\
							gs_leaf_optype_m = LEAF_1L_POCKET_M				|\
							gs_leaf_optype_m = LEAF_2L_POCKET				|\
							bBombShelter )

bEnableMasonryArch = not(gs_leaf_optype_m = LEAF_1L_EXTERNAL_SLIDING	|\
						 gs_leaf_optype_m = LEAF_1L_EXTERNAL_SLIDING_M	|\
						 gs_leaf_optype_m = LEAF_2L_EXTERNAL_SLIDING	|\
						 gs_leaf_optype_m = LEAF_1L_POCKET				|\
						 gs_leaf_optype_m = LEAF_1L_POCKET_M			|\
						 gs_leaf_optype_m = LEAF_2L_POCKET				|\
						 bBombShelter )

gs_board_inside = 0
parameters gs_board_inside = 0

bEnableBoard = (gs_sidelight_left | gs_sidelight_right)

bEnableSillOnDoor = not(gs_leaf_optype_m = LEAF_1L_EXTERNAL_SLIDING |\
						gs_leaf_optype_m = LEAF_1L_EXTERNAL_SLIDING_M |\
						gs_leaf_optype_m = LEAF_2L_EXTERNAL_SLIDING |\
						gs_leaf_optype_m = LEAF_1L_POCKET |\
						gs_leaf_optype_m = LEAF_1L_POCKET_M |\
						gs_leaf_optype_m = LEAF_2L_POCKET |\
						gs_leaf_optype_m = LEAF_1L_MULTIPANEL |\
						gs_leaf_optype_m = LEAF_1L_MULTIPANEL_M |\
						gs_leaf_optype_m = LEAF_2L_MULTIPANEL |\
						bBombShelter)

bEnableSill = (gs_sidelight_left | gs_sidelight_right | bEnableSillOnDoor)

bEnableThreshold = (gs_leaf_optype_m = LEAF_1L_SIDE_HUNG	|\
					gs_leaf_optype_m = LEAF_1L_SIDE_HUNG_M	|\
					gs_leaf_optype_m = LEAF_2L_SIDE_HUNG	|\
					gs_leaf_optype_m = LEAF_1L_PIVOT		|\
