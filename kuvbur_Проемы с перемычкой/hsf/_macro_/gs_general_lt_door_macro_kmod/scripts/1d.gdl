	gs_frame_width_left 			= gs_frame_width
	gs_frame_width_left_2			= gs_frame_width
	gs_frame_width_low_left_sl		= gs_frame_width
	gs_frame_width_right			= gs_frame_width
	gs_frame_width_right_2			= gs_frame_width
	gs_frame_width_low_right_sl		= gs_frame_width
	gs_frame_width_transom			= gs_frame_width
	gs_frame_width_upper			= gs_frame_width
	gs_sidelight_fr_hdiv_wdt_left	= gs_frame_width
	gs_sidelight_fr_hdiv_wdt_right	= gs_frame_width
endif

if bArchedTop then
	gs_frame_width_left 			= gs_frame_width_right
	gs_frame_width_upper			= gs_frame_width_right
endif

if abs(gs_frame_width_left_2)		< EPS then gs_frame_width_left_2		= gs_frame_width
if abs(gs_frame_width_right_2)		< EPS then gs_frame_width_right_2		= gs_frame_width
if abs(gs_frame_width_low_left_sl)	< EPS then gs_frame_width_low_left_sl	= gs_frame_width
if abs(gs_frame_width_low_right_sl)	< EPS then gs_frame_width_low_right_sl	= gs_frame_width
if abs(gs_frame_width_transom)		< EPS then gs_frame_width_transom		= gs_frame_width


! =============================================================================
! Ventilation Profile Type Enable
! =============================================================================

if	bArchedTop & not(gs_sidelight_left | gs_sidelight_right) |\
	bBombShelter  |\
	iOpeningMethod = OPENMETHOD_SLIDE_EXT_SIMP |\	! External Sliding
	iOpeningMethod = OPENMETHOD_SLIDE_EXT_DOUB |\	! Double External Sliding
	iOpeningMethod = OPENMETHOD_POCKET_SIMP |\	! Pocket
	iOpeningMethod = OPENMETHOD_POCKET_DOUB then	! Double Pocket

	bEnableVentilationProfile = 0
else
	bEnableVentilationProfile = 1
endif

if bEnableVentilationProfile then
	if (gs_sidelight_left | gs_sidelight_right | gs_door_transom) then
		if bArchedTop then
			gs_VentilationProfileTypeEnable = 2
		else
			gs_VentilationProfileTypeEnable = 1 * (gs_frame_style_m <> FRAME_STYLE_WRAPPED_EXTRUSION) + 2
		endif
	else
		gs_VentilationProfileTypeEnable = 1 * (gs_frame_style_m <> FRAME_STYLE_WRAPPED_EXTRUSION)
	endif
else
	gs_VentilationProfileTypeEnable = 0
endif


! =============================================================================
! External Sliding Door
! =============================================================================

gs_slidingext =	gs_leaf_optype_m = LEAF_1L_EXTERNAL_SLIDING		|\
				gs_leaf_optype_m = LEAF_1L_EXTERNAL_SLIDING_M	|\
				gs_leaf_optype_m = LEAF_2L_EXTERNAL_SLIDING


! =============================================================================
! Pocket Door Hole Width, Position
! =============================================================================

gs_pocket =	gs_leaf_optype_m = LEAF_1L_POCKET |\
			gs_leaf_optype_m = LEAF_1L_POCKET_M |\
			gs_leaf_optype_m = LEAF_2L_POCKET

posHole			= 0.005
widthWallCut	= gs_leaf_thk + 2 * posHole

if gs_pocket then
	if gs_pocket_door_frame & gs_frame_wthk then
		gs_RevealTypeEnable = 1
	endif
	if not(gs_pocket_door_frame) then
		gs_frame_wthk = 0
	endif
else
	if gs_frame_wthk then
		gs_RevealTypeEnable = 1
	else
		gs_RevealTypeEnable = 0
	endif
endif

posLeafy = 0
if gs_pocket_door_frame then
	if gs_leaf_center then
		if gs_frame_wthk then
			posLeafy = (WALL_THICKNESS - widthWallCut)/2
		else
			posLeafy = (gs_frame_thk - widthWallCut)/2
		endif
	else
		posLeafy = gs_leaf_offset
	endif
endif

! For FramingGeometry
widthWallCut_forFG		= widthWallCut + 2 * gs_pocket_frame * gs_pocket_frame_thk
posLeafy_forFG			= posLeafy - gs_pocket_frame * gs_pocket_frame_thk

iDoorType = 0
if	gs_leaf_optype_m = LEAF_1L_SIDE_HUNG |\
	gs_leaf_optype_m = LEAF_1L_SIDE_HUNG_M |\
	gs_leaf_optype_m = LEAF_1L_DOUBLE_ACTING |\
	gs_leaf_optype_m = LEAF_1L_DOUBLE_ACTING_M |\
	gs_leaf_optype_m = LEAF_1L_PIVOT |\
	gs_leaf_optype_m = LEAF_1L_PIVOT_M then iDoorType = 1
if	gs_leaf_optype_m = LEAF_2L_SIDE_HUNG |\
	gs_leaf_optype_m = LEAF_2L_DOUBLE_ACTING |\
	gs_leaf_optype_m = LEAF_2L_DOUBLE_EGRESS |\
	gs_leaf_optype_m = LEAF_2L_DOUBLE_EGRESS_M then iDoorType = 1
if	gs_leaf_optype_m = LEAF_1L_POCKET |\
	gs_leaf_optype_m = LEAF_1L_POCKET_M then iDoorType = 6
if	gs_leaf_optype_m = LEAF_2L_POCKET then iDoorType = 7
if	gs_leaf_optype_m = LEAF_1L_EXTERNAL_SLIDING |\
	gs_leaf_optype_m = LEAF_1L_EXTERNAL_SLIDING_M then iDoorType = 8
if	gs_leaf_optype_m = LEAF_2L_EXTERNAL_SLIDING then iDoorType = 9


gs_sliding_fixpanel_width_left = 0
gs_sliding_fixpanel_width_right = 0

if	gs_leaf_optype_m = LEAF_2L_1SLIDING | gs_leaf_optype_m = LEAF_2L_1SLIDING_M | gs_leaf_optype_m = LEAF_2L_2SLIDING | gs_leaf_optype_m = LEAF_2L_2SLIDING_M then
	if	GLOB_MODPAR_NAME = "ac_egress_width" | \
		(GLOB_MODPAR_NAME = "A" & gs_egress_nominal) then
		gs_sliding_fixpanel_width_right	= ac_egress_width + gs_leaf_overlap
	else
		gs_sliding_fixpanel_width_right	= (ac_leaf_width + gs_leaf_overlap) / 2
	endif
endif
if	gs_leaf_optype_m = LEAF_3L_2SLIDING | gs_leaf_optype_m = LEAF_3L_2SLIDING_M then
	if	GLOB_MODPAR_NAME = "ac_egress_width" | \
		(GLOB_MODPAR_NAME = "A" & gs_egress_nominal) then
		gs_sliding_fixpanel_width_right	= ac_egress_width / 2 + gs_leaf_overlap
	else
		gs_sliding_fixpanel_width_right	= (ac_leaf_width  + 2 * gs_leaf_overlap) / 3
	endif
endif
if	gs_leaf_optype_m = LEAF_4L_3SLIDING | gs_leaf_optype_m = LEAF_4L_3SLIDING_M then
	if	GLOB_MODPAR_NAME = "ac_egress_width" | \
		(GLOB_MODPAR_NAME = "A" & gs_egress_nominal) then
		gs_sliding_fixpanel_width_right	= ac_egress_width / 3 + gs_leaf_overlap
	else
		gs_sliding_fixpanel_width_right	= (ac_leaf_width  + 3 * gs_leaf_overlap) / 4
	endif
endif
if	gs_leaf_optype_m = LEAF_1L_MULTIPANEL then
		gs_sliding_fixpanel_width_left	= gs_leaf_thk * gs_leaf_num
		gs_sliding_fixpanel_width_right	= 0
endif
if	gs_leaf_optype_m = LEAF_1L_MULTIPANEL_M then
		gs_sliding_fixpanel_width_left	= 0
		gs_sliding_fixpanel_width_right	= gs_leaf_thk * gs_leaf_num
endif
if	gs_leaf_optype_m = LEAF_2L_MULTIPANEL then
		gs_sliding_fixpanel_width_left	= (gs_leaf_thk * gs_leaf_num)
		gs_sliding_fixpanel_width_right	= (gs_leaf_thk * gs_leaf_num2)
endif
if	gs_leaf_optype_m = LEAF_4L_2SLIDING | gs_leaf_optype_m = LEAF_4L_4SLIDING | gs_leaf_optype_m = LEAF_4L_4SLIDING_M then
	if	GLOB_MODPAR_NAME = "ac_egress_width" | \
		(GLOB_MODPAR_NAME = "A" & gs_egress_nominal) then
		gs_sliding_fixpanel_width_left	= ac_egress_width / 2 + gs_leaf_overlap
		gs_sliding_fixpanel_width_right	= ac_egress_width / 2 + gs_leaf_overlap
	else
		gs_sliding_fixpanel_width_left	= ac_leaf_width / 4 + gs_leaf_overlap/2
		gs_sliding_fixpanel_width_right	= ac_leaf_width / 4 + gs_leaf_overlap/2
	endif
endif
if	gs_leaf_optype_m = LEAF_1L_PIVOT | gs_leaf_optype_m = LEAF_1L_PIVOT_M then
		gs_sliding_fixpanel_width_right	= gs_pivotOffset + gs_leaf_thk/2
endif


if iOpeningMethod = OPENMETHOD_OPEN_SIMP then
	gs_door_num_m = 1  ! 1 Leaf
else
	gs_door_num_m = 2  ! 2 Leaf, Sliding
endif

