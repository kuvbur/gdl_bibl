if not(gs_sidelight_left | gs_sidelight_right) then gs_tolerance_uniform = 0
if gs_tolerance_uniform then
	if bArchedTop then
		gs_tolerance_upper 		= gs_tolerance_right
	endif
	gs_tolerance_left 			= gs_tolerance_right
	gs_tolerance_left_2			= gs_tolerance_right
	gs_tolerance_low_left_sl	= gs_tolerance_right
	gs_tolerance_right_2		= gs_tolerance_right
	gs_tolerance_low_right_sl	= gs_tolerance_right
endif

! Uniform frame width
