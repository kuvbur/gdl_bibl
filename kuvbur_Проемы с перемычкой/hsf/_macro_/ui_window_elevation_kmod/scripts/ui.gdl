						infield_x	= posx+25+43*(i-2)
						outfield_x	= posx+21+43*(i-2)

						if not(min(gs_panes_number, 5) mod 2) exor not(i mod 2) then
							infield_y	= posy+picy+1
							outfield_y	= posy+picy+5
						else
							infield_y	= posy-22
							outfield_y	= posy-18
						endif

						if gs_panes_number > 5 then
							if i < 5 then
								idx = 86+i-2
								idx2 = 67+i-2
								ui_outfield gs_UI_WindowElevation[idx], outfield_x, outfield_y, cx, 18, 2 ui_tooltip gs_UI_WindowElevation[idx2]
							endif
						else
							param_name = "MullionWidth_0" + str((i-1), 1, 0)
							if gs_uniform_framewidth then
								ui_outfield str(formatt, gs_frame_width), outfield_x, outfield_y, cx, 18, 2
							else
								idx = 67 + (i-2)
								ui_infield param_name, infield_x, infield_y, cx, 20 ui_tooltip gs_UI_WindowElevation[idx]		! Mullion Width 1...9
							endif
						endif
					next i
				endif
			else
				if gs_uniform_framewidth then
					ui_outfield str(formatt, gs_frame_width_upper),	posx-11, posy-19, cx, 18, 2
				else
					ui_infield "gs_frame_width_upper", posx-16, posy-22, cx, 20 ui_tooltip gs_UI_WindowElevation[19]		! Upper Frame Width
				endif

				if not(min(gs_panes_number, 6) mod 2) then
					if gs_uniform_framewidth then
						ui_outfield str(formatt, gs_frame_width_lower),	posx+sizex+6, posy+picy-9, cx, 18, 0
					else
						ui_infield "gs_frame_width_lower", posx+sizex+3, posy+picy-13, cx, 20 ui_tooltip gs_UI_WindowElevation[20]		! Bottom Frame Width
					endif
				else
					if gs_uniform_framewidth then
						ui_outfield str(formatt, gs_frame_width_lower),	posx+sizex-60, posy+picy+7, cx, 18, 2
					else
						ui_infield "gs_frame_width_lower", posx+sizex-37, posy+picy+3, cx, 20 ui_tooltip gs_UI_WindowElevation[20]		! Bottom Frame Width
					endif
				endif

				if gs_panes_number > 1 then
					for i = 2 to min(gs_panes_number, 6)
						infield_x	= posx+27+43*(i-2)
						outfield_x	= posx+17+43*(i-2)

						if (not(i mod 2) & (i <> 6)) | (not(gs_panes_number mod 2) & (i = 6)) then
							infield_y	= posy+picy+3
							outfield_y	= posy+picy+7	
						else
							infield_y	= posy-23
							outfield_y	= posy-19
						endif

						if gs_panes_number > 5 then
							if i < 5 then
								idx = 86+i-2
								idx2 = 67+i-2
								ui_outfield gs_UI_WindowElevation[idx], outfield_x, outfield_y, cx, 18, 2 ui_tooltip gs_UI_WindowElevation[idx2]
							endif
							if i = 6 then
								idx = 86+gs_panes_number-2
								idx2 = 67+gs_panes_number-2
								ui_outfield gs_UI_WindowElevation[idx], outfield_x, outfield_y, cx, 18, 2 ui_tooltip gs_UI_WindowElevation[idx2]
							endif
						else
							param_name = "MullionWidth_0" + str((i-1), 1, 0)
							if gs_uniform_framewidth then
								ui_outfield str(formatt, gs_frame_width), outfield_x, outfield_y, cx, 18, 2
							else
								idx = 67 + (i-2)
								ui_infield param_name, infield_x, infield_y, cx, 20 ui_tooltip gs_UI_WindowElevation[idx]		! Mullion Width 1...9
							endif
						endif
					next i
				endif
			endif
		endif

		if iWindowType = WIN_MultisashVertical then
			_panes_number_showing_limit = (3 + 2*not(gs_repeat_modules))

			if gs_panes_number > _panes_number_showing_limit then
				x1	= 2
				x2	= 156
				y	= 212 + pageStartY
				if gs_repeat_modules then
					dy = 21
				else
					dy = 23
				endif

				ui_style 0, 0

				for i = 1 to gs_panes_number - 1
					idx = 95 + i
					idx2 = 75 + i
					ui_outfield gs_UI_WindowElevation[idx], x1, y, x2-x1-cx-1, 18, 0 ui_tooltip gs_UI_WindowElevation[idx2]	! Transom 1...9
					param_name = "TransomWidth_0" + str(i, 1, 0)
					ui_infield param_name, x2-cx, y-4, cx, 20
					y = y - dy
				next i
				if gs_repeat_modules then
					ui_outfield gs_UI_WindowElevation[85], x1, y, x2-x1-cx-1, 18, 0 ui_tooltip gs_UI_WindowElevation[46]	! Middle
					ui_infield "TransomWidth", x2-cx, y-4, cx, 20
				endif
			endif

			if gs_repeat_modules then
				dx	= 79
				dx2	= 82
				dy	= 17
				dy2	= 4
				iFlag = 0

				ui_outfield str(formatt, gs_axis_distance), posx-cx-7, posy+49+(16*min(gs_panes_number, 5)), cx, 18, 1
				ui_outfield str(formatt, (gs_frame_width_upper - TransomWidth/2)), posx+1, posy-18, cx, 18, 2 ui_tooltip gs_UI_WindowElevation[108]	! Upper Frame Offset
				ui_outfield str(formatt, (gs_frame_width_lower - TransomWidth/2)), posx+1, posy+sizey+4, cx, 18, 2 ui_tooltip gs_UI_WindowElevation[109]	! Bottom Frame Offset

				if gs_panes_number > 3 then
					ui_outfield gs_UI_WindowElevation[85],	posx+picx+7, posy+49, cx, 18, 0 ui_tooltip gs_UI_WindowElevation[46]	! Middle
				else
					if gs_uniform_framewidth then
						ui_outfield str(formatt, MullionWidth),	posx+picx+7, posy+49, cx, 18, 0
					else
						ui_infield "TransomWidth", posx+picx+4, posy+45, cx, 20 ui_tooltip gs_UI_WindowElevation[46]		! Middle Transom Width
					endif
				endif
			else
				dx	= 35
				dx2	= dx
				dy	= 12
				dy2	= 0
				iFlag = 2
			endif

			ui_infield "gs_frame_width", posx+picx+4, posy+dy, cx, 20 ui_tooltip gs_UI_WindowElevation[17]		! Frame Width

			if gs_uniform_framewidth then
				ui_outfield str(formatt, gs_frame_width_left),	posx-cx-7, posy+dy+4, cx, 18, 1
			else
				ui_infield "gs_frame_width_left", posx-cx-4, posy+dy, cx, 20 ui_tooltip gs_UI_WindowElevation[18]		! Frame Width Side 2
			endif

			if gs_uniform_framewidth then
				ui_outfield str(formatt, gs_frame_width_upper),	posx+dx2-6, posy-18, cx, 18, iFlag
			else
				ui_infield "gs_frame_width_upper", posx+dx, posy-22, cx, 20 ui_tooltip gs_UI_WindowElevation[19]		! Upper Frame Width
			endif

			if gs_uniform_framewidth then
				ui_outfield str(formatt, gs_frame_width_lower),	posx+dx2-6, posy+sizey+4, cx, 18, iFlag
			else
				ui_infield "gs_frame_width_lower", posx+dx, posy+sizey, cx, 20 ui_tooltip gs_UI_WindowElevation[20]		! Bottom Frame Width
			endif

			if gs_panes_number > 1 then
				if gs_panes_number > _panes_number_showing_limit then
					for i = 2 to 5 - 1*gs_repeat_modules
						if i = 5 - 1*gs_repeat_modules then
							idx = 86+gs_panes_number-2
							idx2 = 76+gs_panes_number-2
						else
							idx = 86+i-2
							idx2 = 76+i-2
						endif
						outfield_y	= posy+sizey-49-32*(i-2)-dy2
						ui_outfield gs_UI_WindowElevation[idx], posx+picx+7, outfield_y, cx, 18, 0 ui_tooltip gs_UI_WindowElevation[idx2]
					next i
				else
					for i = 2 to gs_panes_number
						param_name = "TransomWidth_0" + str((i-1), 1, 0)
						if gs_uniform_framewidth then
							outfield_y	= posy+sizey-49-32*(i-2)-dy2
							ui_outfield str(formatt, gs_frame_width), posx+picx+7, outfield_y, cx, 18, 0
						else
							idx = 76 + (i-2)
							infield_y	= posy+sizey-53-32*(i-2)-dy2
							ui_infield param_name, posx+picx+4, infield_y, cx, 20 ui_tooltip gs_UI_WindowElevation[idx]		! Transom Width 1...9
						endif
					next i
				endif
			endif
		endif
	endif
endif
