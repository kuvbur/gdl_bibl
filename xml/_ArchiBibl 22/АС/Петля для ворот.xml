<Script_VL SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
dim ttype_petl[] : i = 0
i=i+1 : ttype_petl[i] = "832"
i=i+1 : ttype_petl[i] = "833"
i=i+1 : ttype_petl[i] = "834"

values "type_petl" ttype_petl, CUSTOM
values "isp_petl" "П", "Л", CUSTOM

if ves>0 then
	parameters ves = ves
	parameters naen = "Петля " + type_petl + isp_petl
endif
]]>
</Script_VL>

