<!-- GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT -->

<Script_3D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
t_l = 5/1000
b_l = (P - D)/2+D/4
h_l = H/2

if isp_petl = "Л" then muly -1
hotspot 0,0,0
hotspot 0,P/2,0
hotspot 0,0,H
hotspot 0,-P/2,H
hotspot 0,-P/2,h_l
hotspot 0,P/2,h_l
addy D/2-D/4
BRICK t_l, b_l, h_l
del 1
addy -D/2+D/4-b_l
addz H/2
BRICK t_l, b_l, h_l
del 2
addx B-D/2
hotspot 0,0,0
hotspot H,0,0
hotspot 0,D/2,0
hotspot H,D/2,0
hotspot 0,-D/2,0
hotspot H,-D/2,0
CYLIND H, D/2
]]>
</Script_3D>

<!-- GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT -->

<Script_2D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
