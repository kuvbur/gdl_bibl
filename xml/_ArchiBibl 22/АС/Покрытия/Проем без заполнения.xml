<Script_3D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
rotx ang
gosub "attr"
if tip_proem = "Прямоугольный" then
	if in3d then
		HOTSPOT A, 0, 0
		HOTSPOT A, B, 0
		HOTSPOT 0, B, 0
		if oper then
			HOTSPOT 0, 0, ZZYZX
			HOTSPOT A, 0, ZZYZX
			HOTSPOT A, B, ZZYZX
			HOTSPOT 0, B, ZZYZX
			if GLOB_SEO_TOOL_MODE = 1 then
				gosub "attr"
				BLOCK A, B, ZZYZX
				end
			endif
		endif
		if upr_n = -1 or GLOB_STRUCTURE_DISPLAY = upr_n then
			HOTSPOT 0, 0, 0, unID: unID=unID+1
			HOTSPOT pos_x, 0, 0, unID, pos_y, 1+128: unID=unID+1
			HOTSPOT pos_x, pos_y, 0, unID, pos_y, 2: unID=unID+1
			HOTSPOT 0, pos_y, 0, unID, pos_x, 1+128: unID=unID+1
			HOTSPOT pos_x, pos_y, 0, unID, pos_x, 2: unID=unID+1
			gosub "attr_on"
			POLY  4,
			0,0,
			0,B,
			A,B,
			pos_x,pos_y
		endif
	endif
	if (GLOB_STRUCTURE_DISPLAY <> upr_n or upr_n=-1) then 
		if pokr or obraml then 
			addz ZZYZX-0.101
			gosub "attr_on"
			CALL "Щит" parameters all
			del 1
		endif
	endif	
else
	if in3d then
		if oper then
			HOTSPOT 0, 0, 0
			HOTSPOT A/2, 0, 0
			HOTSPOT 0, A/2, 0
			HOTSPOT 0, 0, ZZYZX
			HOTSPOT A/2, 0, ZZYZX
			HOTSPOT 0, A/2, ZZYZX
			if GLOB_SEO_TOOL_MODE = 1 then
				gosub "attr"
				CYLIND ZZYZX, A/2
				end
			endif
		endif
		if upr_n = -1 or GLOB_STRUCTURE_DISPLAY = upr_n then
			gosub "attr_on"
			HOTSPOT 0, 0, 0
			HOTSPOT A/2, 0, 0
			HOTSPOT 0, A/2, 0
			x = sin(32.75) * A/2
			y = cos(32.75) * A/2
			poly_ 7, 
			-x, -y, 1, 
			A * 0.15, - A * 0.15, 900, 
			0, -155, 4001, 
			y, x, 1, 
			0, 0, 900, 
			0, 205, 4001, 
			-x, -y, 1
		endif
	endif
endif
end
"attr":
	pen i_pen
	SECT_FILL "Пусто", 0, 0, 0,3
	SECT_ATTRS "Пусто", 0, 0, 0,3
	MATERIAL "!Пусто"		
	SHADOW OFF
return
"attr_on":
	pen i_pen
	SECT_FILL "Сталь", 0, i_pen, i_pen,i_pen
	SECT_ATTRS "Сталь", 0, i_pen, i_pen,i_pen
	MATERIAL "!Краска-05"	
	SHADOW ON
return
]]>
</Script_3D>

