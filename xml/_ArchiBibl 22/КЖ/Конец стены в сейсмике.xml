<Script_2D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
del top

add2 0,max_t
if lev_str = "Монолит" then
	hotspot2 bst/2,bst/2
	line2 0,0,bst,bst
	line2 bst,0,0,bst
endif
if lev_str = "Сердечник" then
	hotspot2 bst,0
	hotspot2 bst,bst
	fill f_st
	poly2 4,1+2+4,0,0,0,bst,bst,bst,bst,0
endif
if lev_str <> "Нет" then
	pen p_f_st
	fill sh_kon
	poly2 4,1+2+4,0,0, 0,bst, -0.03,bst, -0.03,0
endif

]]>
</Script_2D>

