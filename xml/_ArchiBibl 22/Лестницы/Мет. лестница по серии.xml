	PARAMETERS otst_stup=0.005
	PARAMETERS lstup=0.2
	PARAMETERS h_st=0.2
	PARAMETERS b_st=0.2
	PARAMETERS otst_fas_niz_a=-0.01
	PARAMETERS otst_fas_niz_b=-0.01
