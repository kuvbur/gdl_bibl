<Script_PR SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
dlin_prof = ZZYZX
spokr = SURFACE3D()
ves = VOLUME3D()*7850
ves = ves + ves*proc_zap
pos = GLOB_ID

if type_spec = "Металл" then
	gosub "spec_m"
endif
if type_spec = "Изделие" then
	gosub "spec_i"
endif

if uch_pokr then
	tip_el = "30"
	komment = "кв.м."
	obozn = "см. примечания"
	naen = "Покрытие " + mat_pokr
	kol_vo_spec = spokr
	call "Спецификатор" parameters tip_el=tip_el,
									subpos = subpos,  
									obozn=obozn,
									naen=naen,
									kol_vo_spec=spokr,
									komment=komment
endif

goto 20
"spec_m":
	tip_el = "20"
	for i = 1 to kol_vo_spec
	call "Спецификатор" parameters tip_el=tip_el,
									subpos = subpos, 
									pos = pos,
									tip_konstr=tip_konstr,
									stal_gost=stal_gost,
									mater=mater,
									obozn=obozn,
									tip_profile=tip_profile,
									dlin_prof=dlin_prof,
									ves=round_int(ves/dlin_prof),
									naen=naen
	next i
return

"spec_i":
	tip_el = "40"
	call "Спецификатор" parameters tip_el=tip_el,
									subpos = subpos, 
									pos = pos, 
									obozn=obozn,
									naen=naen,
									ves=ves,
									kol_vo_spec=1,
									komment=""
return
20:
]]>
</Script_PR>

