<Script_3D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
if viz=1 or ang>0 then
	otst_p=cos(ang_l)*H1
	l_ogr_vert=h_ogr+b_krep_ogr-15/1000
	l_b=(h_l+pop_verh)/sin(90-ang_l)
	l_1=h_ogr*tan(90-ang_l)
	l_2=0.5*l_1
	l_v=l_b+l_1-otst_p+t_krep_ogr*3
	l_s=l_b+l_2-l1_krep_ogr+t_krep_ogr*3
endif
l_ogr_vert=h_ogr+b_krep_ogr-15/1000
GROUP "ugol_ogr_vert"
sj=1+2+4+8+16+32
roty 90
rotz 180
b_ug=b_ug_ogr_vert
h_ug=b_ug
t_ug=t_ug_ogr_vert
PUT 0,0,sj
PUT 0,h_ug,sj
PUT b_ug,h_ug,sj
PUT b_ug,h_ug-t_ug,sj
PUT t_ug,h_ug-t_ug,sj
PUT t_ug,0,sj
PRISM_ NSP/3,l_ogr_vert, get(NSP)
ENDGROUP

del top

GROUP "ugol_verh"
rotx -ang_l
rotz 90
addx -h_ogr-b_krep_ogr
addy t_ug_ogr_vert
addz otst_p
sj=1+2+4+8+16+32
b_ug=b_ug_v
h_ug=b_ug
t_ug=t_ug_v
PUT 0,0,sj
PUT 0,h_ug,sj
PUT b_ug,h_ug,sj
PUT b_ug,h_ug-t_ug,sj
PUT t_ug,h_ug-t_ug,sj
PUT t_ug,0,sj
PRISM_ NSP/3, l_v, get(NSP)
ENDGROUP

del top

GROUP "ugol_sred"
rotx -ang_l
rotz 180
addx b_ug_ogr_vert
addy h_ogr/2+b_krep_ogr-b_ug_s
addz l1_krep_ogr*(ang>0)+t_krep_ogr
sj=1+2+4+8+16+32
b_ug=b_ug_s
h_ug=b_ug
t_ug=t_ug_s
PUT 0,0,sj
PUT 0,h_ug,sj
PUT b_ug,h_ug,sj
PUT b_ug,h_ug-t_ug,sj
PUT t_ug,h_ug-t_ug,sj
PUT t_ug,0,sj
PRISM_ NSP/3, l_s, get(NSP)
ENDGROUP

GROUP "krep_ogr_fas"
sj=1+2+4+8+16+32
addy -a_krep_ogr!-b_ug_ogr_vert
addz -t_krep_ogr
addx -popr
!addy hshv-a_krep_ogr-tpol
PUT 0,0,sj
PUT b_krep_ogr,0,sj
PUT b_krep_ogr,a_krep_ogr,sj
PUT 0,a_krep_ogr,sj
hotspot bshv,a_krep_ogr,0
PRISM_ NSP/3, t_krep_ogr, get(NSP)
ENDGROUP


del top

GROUP "polosa"
rotx -90
addy -b_krep_ogr-0.01-h_p
addx -b_ug_ogr_vert-b_p
sj=1+2+4+8+16+32
PUT 0,0,sj
PUT 0,h_p,sj
PUT b_p,h_p,sj
PUT b_p,0,sj
PRISM_ NSP/3, l_s, get(NSP)



ENDGROUP
del top
rotz -90
roty -ang_l
addz l1_krep_ogr+t_krep_ogr
for i=1 to n_krep_ogr
PLACEGROUP "ugol_ogr_vert"
if ot_fas then PLACEGROUP "krep_ogr_fas"
addz l2_krep_ogr
next i
del top
if ang=0 then PLACEGROUP "polosa"
PLACEGROUP "ugol_verh"
PLACEGROUP "ugol_sred"
]]>
