<![CDATA[
ui_current_page gs_ui_current_page

x1		= 97
x2		= 136
x3		= 235
y1		= 31
y2		= 74
y3		= 133
add_y	= 24

dim stRecess[7]
	stRecess[1]	= `Параметры Пилястра/Ниши`
	stRecess[2]	= `3D-детализация`
	stRecess[3]	= `Проекция (Отрицательная -> Ниша)`
