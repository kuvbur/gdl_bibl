<Script_2D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
! Switch Period Limiting

call "Switch_symbol" parameters all \
		bText = bText,
		iSwitchType = iSwitchType

if iSymbolType = 2 then
	angleViewRot = 0
	rrr = REQUEST ("View_Rotangle", "", angleViewRot)
	totalRotate = (SYMB_ROTANGLE + angleViewRot) MOD 360

	nTrans = 0

	! style
	pen tpen
	define style "textStyle"	fontType, 10/GLOB_SCALE*fsz, 5,
							gs_text_style_bold + 2 * gs_text_style_italic + 4 * gs_text_style_underline

	style "textStyle"

	! Text Rotation
	if typeTextRotation_m = 2 then								! Readable
		if (totalRotate > (gs_readable_angle + EPS) & totalRotate < (gs_readable_angle + 180 + eps)) then
			rot2 180
			nTrans = nTrans + 1
		endif
	else
		if typeTextRotation_m = 1 then							! Horizontal
			rot2 totalRotate * (SYMB_MIRRORED - not(SYMB_MIRRORED))	! 1/-1
			nTrans = nTrans + 1
		endif
	endif

	text2 circleDiameter, circleDiameter, txt
	del nTrans
endif
]]>
</Script_2D>

<!-- GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT -->

<Script_VL SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
call "Switch_symbol" parameters all \
		A = A * 7963/4000,
		bText = 1,
		iSwitchType = 0

values "circleDiameter" range (0, )
values "A" range (0, )
values "B" range (0, )
values "SymbolType" `Внутренняя Буква`, `Наружная буква`

if GLOB_MODPAR_NAME = "SymbolType" then
	if SymbolType = `Наружная буква` then
		iSymbolType = 2
	else
		iSymbolType = 1
	endif
	parameters iSymbolType = iSymbolType
else
	if iSymbolType = 2 then
		SymbolType = `Наружная буква`
	else
		SymbolType = `Внутренняя Буква`
	endif
	parameters SymbolType = SymbolType
endif

if GLOB_MODPAR_NAME = "A" or GLOB_MODPAR_NAME = "B" then
	if GLOB_MODPAR_NAME = "B" then
		circleDiameter = B/2.35
		A = circleDiameter*2
		parameters A = A
	else
		circleDiameter = A/2
		B = circleDiameter*2.35
		parameters B = B
	endif
	parameters circleDiameter = circleDiameter
else
	A = circleDiameter*2
	B = circleDiameter*2.35
	parameters A = A, B = B
endif
]]>
</Script_VL>

