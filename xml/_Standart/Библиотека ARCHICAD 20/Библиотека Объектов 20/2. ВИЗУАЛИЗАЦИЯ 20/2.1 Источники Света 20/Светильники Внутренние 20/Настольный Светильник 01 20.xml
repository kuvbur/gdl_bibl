LIGHTGROUP_BUILTIN_NONVIS_AREA	= 5
LIGHTGROUP_BUILTIN_FALLOFF		= 6

GENTYPE_OMNI		= 1
GENTYPE_SPOT		= 2
GENTYPE_AREA		= 3
GENTYPE_INFINITE	= 4
GENTYPE_IES			= 5

AREASHAPE_DISC			= 1
