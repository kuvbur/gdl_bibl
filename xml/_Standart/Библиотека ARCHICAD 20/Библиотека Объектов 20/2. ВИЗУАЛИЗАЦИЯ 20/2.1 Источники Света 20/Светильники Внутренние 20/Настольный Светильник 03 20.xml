	call "ui_lampDisplaySettings_m" parameters all	bEnableModelType	= 1,
													pageStartY			= 3,
													stDetailLevel		= stDetailLevel,
													surfaceBlock		= surfaceBlock,
													flSurfaceBlk		= flSurfaceBlk

return

