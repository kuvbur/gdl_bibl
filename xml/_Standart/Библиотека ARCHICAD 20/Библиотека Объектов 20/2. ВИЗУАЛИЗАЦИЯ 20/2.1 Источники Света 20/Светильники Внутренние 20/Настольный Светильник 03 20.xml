<Script_2D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
pen gs_cont_pen
fill gs_fill_type

unID=1

poly2_b 2, 1+2+64,gs_fill_pen,gs_back_pen,
		0,0,901,
		0.047,360,4001

hotspot2 0,0,unID : unID=unID+1
hotspot2 0,0.047,unID : unID=unID+1
hotspot2 0,-0.047,unID : unID=unID+1
hotspot2 -0.047,0,unID : unID=unID+1

!!---- Lower Arm
mul2 cos(ala),1
add2 0,0.0405
poly2_b 5, 1+2+64, gs_fill_pen, gs_back_pen,
		-0.137, -0.002, 1,
		 0.383, -0.002, 1,
		 0.383,  0.002, 1,
		-0.137,  0.002, 1,
		-0.137, -0.002,-1

hotspot2 0.383, 0.002,unID : unID=unID+1
del 1

add2 0,-0.0405
poly2_b 5, 1+2+64, gs_fill_pen, gs_back_pen,
		-0.137, -0.002, 1,
		 0.383, -0.002, 1,
		 0.383,  0.002, 1,
		-0.137,  0.002, 1,
		-0.137, -0.002,-1

hotspot2 0.383, -0.002,unID : unID=unID+1
del 1

poly2_b 5, 1+2+64, gs_fill_pen, gs_back_pen,
		-0.137, -0.0425, 1,
		-0.153, -0.0425, 1,
		-0.153,  0.0425, 1,
		-0.137,  0.0425, 1,
		-0.137, -0.0425,-1

hotspot2 -0.153, -0.0425, unID : unID=unID+1
hotspot2 -0.153,  0.0425, unID : unID=unID+1

del 1

add2 0.383*cos(ala),0
mul2 cos(aua),1

!!---- Upper Arm
add2 0,0.0365
poly2_b 5, 1+2+64, gs_fill_pen, gs_back_pen,
		-0.202,  0.002, 1,
		-0.202, -0.002, 1,
		 0.503, -0.002, 1,
		 0.503,  0.002, 1,
		-0.202,  0.002,-1

hotspot2 0.503, 0.002, unID : unID=unID+1
del 1

add2 0,-0.0365
poly2_b 5, 1+2+64, gs_fill_pen, gs_back_pen,
		-0.202,  0.002, 1,
		-0.202, -0.002, 1,
		 0.503, -0.002, 1,
		 0.503,  0.002, 1,
		-0.202,  0.002,-1

hotspot2 0.503, -0.002, unID : unID=unID+1
del 1

poly2_b 5, 1+2+64, gs_fill_pen, gs_back_pen,
		-0.202, -0.0385, 1,
		-0.212, -0.0385, 1,
		-0.212,  0.0385, 1,
		-0.202,  0.0385, 1,
		-0.202, -0.0385,-1
del 1

add2 0.503*cos(aua),0
poly2_b 5, 1+2+64, gs_fill_pen, gs_back_pen,
		-0.060, -0.0345, 1,
		 0.080, -0.0345, 1,
		 0.080,  0.0345, 1,
		-0.060,  0.0345, 1,
		-0.060, -0.0345,-1

line2 -0.030,0.0345,-0.030,-0.0345
line2  0.050,0.0345, 0.050,-0.0345

hotspot2 0.080, 0.0345,unID : unID=unID+1
hotspot2 0.080,-0.0345,unID : unID=unID+1

del 1
]]>
</Script_2D>

