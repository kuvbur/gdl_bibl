	dim	surfaceBlock[3][2]
		surfaceBlock[1][1]	= `Кронштейн`		:	surfaceBlock[1][2] = "matFrame"
		surfaceBlock[2][1]	= `Основание`	:	surfaceBlock[2][2] = "matPlast"
		surfaceBlock[3][1]	= `Стекло`	:	surfaceBlock[3][2] = "matGlass"

