<Script_2D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
! Street Lamp 02

pen gs_cont_pen
fill gs_fill_type

eps=0.0001
unID=1

poly2_b	2, 1+2+64, gs_fill_pen, gs_back_pen,
		0, 0, 901,
		pw, 360, 4001

for i=1 to iLampType
	poly2_b	7, 1+2+64, gs_fill_pen, gs_back_pen,
			pw/2*cos(30), pw/4, 0,
			0, 0, 900,
			pw/2*cos(30), -pw/4, 3001,
			pw+0.02-pw/2*cos(30), -pw/4, 0,
			pw+0.02, 0, 900,
			pw+0.02-pw/2*cos(30), pw/4, 3001,
			pw/2*cos(30), pw/4, -1

	poly2_b	2, 1+2+64, gs_fill_pen, gs_back_pen,
			pw+0.02+0.40, 0, 901,
			B2, 360, 4001

	poly2_b	7, 1+2+64, gs_fill_pen, gs_back_pen,
			pw+0.02, -pw/2, 1,
			pw+0.02+0.40, -pw/2, 1,
			pw+0.02+0.40, 0, 901,
			0, 180, 4001,
			pw+0.02, pw/2, 1,
			pw+0.02, 0, 901,
			0, 180, 4001

	rot2 360/iLampType
next i

poly2_b	2, 1+2+64, gs_fill_pen, gs_back_pen,
		0, 0, 901,
		pw/2, 360, 4001

del iLampType

!!---- Hotspot Editing
hotspot2 0,0,unID : unID=unID+1

for k=1 to iLampType
	add2 pw+0.02+0.40, 0

		!!---- Light Cone Radius
	For i=1 To 4
		hotspot2 0,0,unID,B2,1+128 : unID=unID+1
		hotspot2 -1,0,unID,B2,3 : unID=unID+1
		hotspot2 B2,0,unID,B2,2 : unID=unID+1
		rot2 90
	Next i
	del 4
	del 1
	rot2 360/iLampType
next k
del top

]]>
</Script_2D>

