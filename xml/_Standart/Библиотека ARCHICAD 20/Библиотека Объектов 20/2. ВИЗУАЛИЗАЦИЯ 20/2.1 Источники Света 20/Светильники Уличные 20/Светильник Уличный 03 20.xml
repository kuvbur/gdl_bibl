	TABID_FLOORPLAN		= 120
	TABID_MODEL			= 130
	TABID_FPLAN_MODEL	= 140
TABID_CUSTOM_1			= 200
TABID_LISTPARAMS		= 400


dim uiUsedPageIDs[][2]
