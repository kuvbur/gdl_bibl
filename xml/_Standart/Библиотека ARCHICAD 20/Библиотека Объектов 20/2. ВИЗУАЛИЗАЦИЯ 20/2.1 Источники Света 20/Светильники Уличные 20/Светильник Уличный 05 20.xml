<Script_1D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
EPS = 0.0001


dim stDetailLevel[3]
	stDetailLevel[1] = `Отключено`
	stDetailLevel[2] = `Простой`
	stDetailLevel[3] = `Детально`

