<!-- GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT -->

<Script_3D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
!!DOWNSPOUT HANGER!!


IF gs_detlevel_3D_m=0 OR gs_detlevel_3D_m=1 THEN END

RESOL gs_resol
PEN gs_cont_pen
MATERIAL gs_gutter_ds_h_mat

if not(gs_shadow) then shadow off

if gs_detlevel_3D_m = 2 then

	add 0, -EPS, 0

	call "m_DSHangerProfiles" parameters	gs_gutter_ds_profile_num	= gs_gutter_ds_profile_num,
											gs_mask						= 15+64,
											gs_gutter_aa				= gs_gutter_aa,
											gs_gutter_bb				= gs_gutter_bb,
											gs_gutter_dd				= gs_gutter_dd,
											dis_wall 					= dis_wall

	prism_ nsp/3, sthw, get(nsp)

	del 1
endif
]]>
</Script_3D>

<!-- GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT -->

<Script_2D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[

LINE_TYPE gs_line_type
PEN gs_cont_pen
FILL gs_fill_type

call "m_DSHangerProfiles" parameters 	gs_gutter_ds_profile_num	= gs_gutter_ds_profile_num,
										gs_mask						= 1,
										gs_gutter_aa				= gs_gutter_aa,
										gs_gutter_bb				= gs_gutter_bb,
										gs_gutter_dd				= gs_gutter_dd,
										dis_wall 					= dis_wall


POLY2_B nsp/3, 1+2+4+64, gs_fill_pen, gs_back_pen, get(nsp)






]]>
</Script_2D>

<!-- GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT ===== GDL SCRIPT -->

<Script_1D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
EPS=0.0001							!!precision of zero!!
st=0.001							!!slab thickness!!
sth=0.002							!!hanger thickness!!
sthw=0.03							!!hanger width!!
sthh=0.2							!!hanger long!!
r=0.01								!!Corrugated downspout corrugating-radius!!
dshw=0.04							!!downspout hanger +width!!
pt=0.0002								!!plane thickness!!

AA=gs_gutter_aa : BB=gs_gutter_bb : DD=gs_gutter_dd



]]>
</Script_1D>

