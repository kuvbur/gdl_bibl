<Script_UI SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
pict_name="number_"+str(num,1,0)
file_dependence "number_1"
file_dependence "number_2"
file_dependence "number_3"
file_dependence "number_4"
file_dependence "number_5"
file_dependence "number_6"
file_dependence "number_7"
file_dependence "number_8"
file_dependence "number_9"
file_dependence "number_10"

epos=0
if num>9 then
	if num_pos=1 or num_pos=4 or num_pos=7 then epos=5
	if num_pos=2 or num_pos=5 or num_pos=8 then epos=2
endif

if auto_type and pixel<8 then
	type=type+1
	if dir then
		x=x-1
	else
		y=y-1
	endif
	num_pos=sec_pos
	pixel=pixel+2
endif

gosub type
if num_pos then gosub (dir+1)*10+num_pos

!!!!gosub 100

goto 1001

1:
if dir then
	ui_pict "lgrey_left",x,y-2, 3, 5, 1
	ui_pict "lgrey_right",x+pixel-3,y-2, 3, 5, 1
	ui_pict "black",x,y,pixel,1
else
	ui_pict "lgrey_up",x-2,y, 5, 3, 1
	ui_pict "lgrey_down",x-2,y+pixel-3, 5, 3, 1
	ui_pict "black",x,y,1,pixel
endif
return

2:
if dir then
	ui_pict "lgrey_right",x-3,y-2, 3, 5, 1
	ui_pict "lgrey_left",x+pixel,y-2, 3, 5, 1
	ui_pict "black",x-5,y,pixel+10,1
else
	ui_pict "lgrey_down",x-2,y-3, 5, 3, 1
	ui_pict "lgrey_up",x-2,y+pixel, 5, 3, 1
	ui_pict "black",x,y-5,1,pixel+10
endif
return

3:
if dir then
	ui_pict "dgrey_left",x,y-2, 3, 5, 1
	ui_pict "dgrey_right",x+pixel-3,y-2, 3, 5, 1
	ui_pict "black",x,y,pixel,1
else
	ui_pict "dgrey_up",x-2,y, 5, 3, 1
	ui_pict "dgrey_down",x-2,y+pixel-3, 5, 3, 1
	ui_pict "black",x,y,1,pixel
endif
return

4:
if dir then
	ui_pict "dgrey_right",x-3,y-2, 3, 5, 1
	ui_pict "dgrey_left",x+pixel,y-2, 3, 5, 1
	ui_pict "black",x-5,y,pixel+10,1
else
	ui_pict "dgrey_down",x-2,y-3, 5, 3, 1
	ui_pict "dgrey_up",x-2,y+pixel, 5, 3, 1
	ui_pict "black",x,y-5,1,pixel+10
endif
return

5:
ui_outfield "hello",0,0,40,15
return

!-----------------!
! Picture Numbers !
!-----------------!

!! Vertical
11:
ui_pict pict_name,x-10-epos,y-12, 7, 9, 1
return

12:
if type=2 or type=4 then e_y=2
ui_pict pict_name,x-3-epos,y-12-e_y, 7, 9, 1
return

13:
ui_pict pict_name,x+4-epos,y-12, 7, 9, 1
return

14:
ui_pict pict_name,x-10-epos,y+pixel/2-4, 7, 9, 1
return

15:
ui_pict pict_name,x-3-epos,y+pixel/2-4, 7, 9, 1
return

16:
ui_pict pict_name,x+4-epos,y+pixel/2-4, 7, 9, 1
return

17:
ui_pict pict_name,x-10-epos,y+pixel+3, 7, 9, 1
return

18:
if type=2 or type=4 then e_y=2
ui_pict pict_name,x-3-epos,y+pixel+3+e_y, 7, 9, 1
return

19:
ui_pict pict_name,x+4-epos,y+pixel+3, 7, 9, 1
return

!! Horizontal
21:
ui_pict pict_name,x-10-epos,y-12, 7, 9, 1
return

22:
ui_pict pict_name,x+pixel/2-4-epos,y-12, 7, 9, 1
return

23:
ui_pict pict_name,x+pixel+3-epos,y-12, 7, 9, 1
return

24:
if type=2 or type=4 then
	e_x=2
else
	e_x=0
endif
ui_pict pict_name,x-10-e_x-epos,y-4, 7, 9, 1
return

25:
ui_pict pict_name,x+pixel/2-4-epos,y-4, 7, 9, 1
return

26:
if type=2 or type=4 then
	e_x=2
else
	e_x=0
endif
ui_pict pict_name,x+pixel+3+e_x-epos,y-4, 7, 9, 1
return

27:
ui_pict pict_name,x-10-epos,y+4, 7, 9, 1
return

28:
ui_pict pict_name,x+pixel/2-4-epos,y+4, 7, 9, 1
return

29:
ui_pict pict_name,x+pixel+3-epos,y+4, 7, 9, 1
return

100:
ui_outfield "Horizontal",200,25,70,19
ui_infield "dir",270,25,15,15
ui_outfield "1-ki lgrey",200,50,70,19
ui_outfield "2-be lgrey",200,65,70,19
ui_outfield "3-ki dgrey",200,80,70,19
ui_outfield "4-be dgrey",200,95,70,19
ui_infield "type",270,47,20,19
ui_outfield "Number",200,120,70,19
ui_infield "num",270,117,20,19
ui_outfield "Position",200,145,70,19
ui_infield "num_pos",270,142,20,19
ui_outfield "Pixel",200,170,70,19
ui_infield "pixel",270,167,20,19
return

1001:















]]>
</Script_UI>

