L=0.01*GLOB_SCALE
MUL2   L,L
DEFINE STYLE "текст" fontType,gg, 5,1
DEFINE STYLE "текст1"  fontType2,G, 8,1
D=D*0.1
HOTSPOT2 18.5, 0
HOTSPOT2 0, 0
if c<0 then c=0
if C>75 then C=75
A=18.5
B = D*C+1.5
K=1.5
GOSUB 2
SET STYLE "текст"
PEN z
TEXT2  3, -0.75, "Обозначение"
TEXT2  10.25, -0.75, "Наименование "
TEXT2  17.0, -0.75, "Примечание"
SET STYLE "текст1"
PEN h
TEXT2 9.25, G*.1,"Ведомость основных комплектов рабочих чертежей"
END
2:
LINE2  0,0,A,0
LINE2 0, -K, A, -K
LINE2  0,0,0,-B
LINE2  6,0,6,-B
LINE2  15.5,0,15.5,-B
LINE2  18.5,0,18.5,-B
LINE2  0,-B,18.5,-B

FOR I=1 TO C-1
LINE2  0, -(I*D+K), 18.5, -(I*D+K)
NEXT I
RETURN
]]>
</Script_2D>

