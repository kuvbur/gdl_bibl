<Script_VL SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
values 'ht' "прямоугольное", "круглое"
values 'mp' 1,2,3,4

if ht="круглое" then
values 'bhs' "d "+str("%0.0mm",hb)
values 'hhs' ''
values 'hh' hb
else
values 'bhs' str("%0.0mm",hb)
values 'hhs' str("%0.0mm",hh)

endif

if f=1 then values 'frs' "Обрамить "+e else  values 'frs' ""
]]>
</Script_VL>

