<Script_2D SectVersion="20" SectionFlags="0" SubIdent="0">
<![CDATA[
!   РАЗРАБОТАЛ  С. А. ТИТОВ
!        МОСКВА, 2000

fill hfill
poly2 4, 7,
-hb/2,-wt/2,
-hb/2,wt/2,
hb/2,wt/2,
hb/2,-wt/2
hotspot2 -hb/2,-wt/2
hotspot2 -hb/2,wt/2
hotspot2 hb/2,wt/2
hotspot2 hb/2,-wt/2
hotspot2 0,-wt/2
hotspot2 0,wt/2
hotspot2 -hb/2,0
hotspot2 hb/2,0


define style 'hm' fontType2, fs,8,0

rot2 -SYMB_ROTANGLE

if mp=1 then

line2 0,0,xx*0.001*GLOB_SCALE,yy*0.001*GLOB_SCALE
line2 xx*0.001*GLOB_SCALE,yy*0.001*GLOB_SCALE,xx*0.001*GLOB_SCALE+fs*1.5*0.001*GLOB_SCALE,yy*0.001*GLOB_SCALE

style 'hm'
add2 xx*0.001*GLOB_SCALE+fs*0.75*0.001*GLOB_SCALE,yy*0.001*GLOB_SCALE
text2 0,0,GLOB_ID
del 1

endif

if mp=2 then

line2 0,0,-xx*0.001*GLOB_SCALE,yy*0.001*GLOB_SCALE
line2 -xx*0.001*GLOB_SCALE,yy*0.001*GLOB_SCALE,-xx*0.001*GLOB_SCALE-fs*1.5*0.001*GLOB_SCALE,yy*0.001*GLOB_SCALE

style 'hm'
add2 -xx*0.001*GLOB_SCALE-fs*0.75*0.001*GLOB_SCALE,yy*0.001*GLOB_SCALE
text2 0,0,GLOB_ID
del 1

endif

if mp=3 then

line2 0,0,-xx*0.001*GLOB_SCALE,-yy*0.001*GLOB_SCALE
line2 -xx*0.001*GLOB_SCALE,-yy*0.001*GLOB_SCALE,-xx*0.001*GLOB_SCALE-fs*1.5*0.001*GLOB_SCALE,-yy*0.001*GLOB_SCALE

style 'hm'
add2 -xx*0.001*GLOB_SCALE-fs*0.75*0.001*GLOB_SCALE,-yy*0.001*GLOB_SCALE
text2 0,0,GLOB_ID
del 1

endif

if mp=4 then

line2 0,0,xx*0.001*GLOB_SCALE,-yy*0.001*GLOB_SCALE
line2 xx*0.001*GLOB_SCALE,-yy*0.001*GLOB_SCALE,xx*0.001*GLOB_SCALE+fs*1.5*0.001*GLOB_SCALE,-yy*0.001*GLOB_SCALE


style 'hm'
add2 xx*0.001*GLOB_SCALE+fs*0.75*0.001*GLOB_SCALE,-yy*0.001*GLOB_SCALE
text2 0,0,GLOB_ID
del 1

endif

del 1

pen alp
line_type c
if ht="круглое" then
line2 0,-wt/2-0.2,0,wt/2+0.2
hotspot2 0,-wt/2-0.2
hotspot2 0,wt/2+0.2
endif
]]>
</Script_2D>

